pragma solidity ^0.8.0;

import "./openzeppelin/SafeMath.sol";
import "./openzeppelin/IERC20.sol";
import "./openzeppelin/SafeERC20.sol";
import "./interfaces/IUniswapV2Router.sol";

contract TokenSale {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    address public governance;

    mapping(address => uint256) public balanceOf;
    mapping(address => bool) public whiteList;
    mapping(address => bool) public checker;
    address[] users;

    uint256 public startTime;
    uint256 public endTime;

    uint256 public MIN = 50000000;
    uint256 public MAX = 500000000;

    uint256 public priceWL = 1176;
    uint256 public priceBase = 1000;

    uint256 public total;

    bool public isRefund;

    IUniswapV2Router uniswap = IUniswapV2Router(0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff);

    IERC20 public usdt = IERC20(0xc2132D05D31c914a87C6611C10748AEb04B58e8F);
    IERC20 public oxi = IERC20(0x910083786EcC4d3844929631239e440d71D7C615);

    event Deposit(address indexed _buyer, uint256 indexed _amount);
    event Withdraw(address indexed _buyer, uint256 indexed _amount);
    event Refund(address indexed _buyer, uint256 indexed _amount);

    constructor(uint256 _startTime, uint256 _endTime) {
        governance = msg.sender;
        startTime = _startTime;
        endTime = _endTime;
    }

    modifier onlyGovernance() {
        require(msg.sender == governance, "Governance: !governance");
        _;
    }

    function setUsdt(address _usdt) external virtual onlyGovernance {
        usdt = IERC20(_usdt);
    }

    function setOxi(address _oxi) external virtual onlyGovernance {
        oxi = IERC20(_oxi);
    }

    function setUniswap(address _uni) external virtual onlyGovernance {
        uniswap = IUniswapV2Router(_uni);
    }

    function setStartTime(uint256 _startTime) external virtual onlyGovernance {
        startTime = _startTime;
    }

    function setEndTime(uint256 _endTime) external virtual onlyGovernance {
        endTime = _endTime;
    }

    function addLiquidity() external virtual onlyGovernance {
        uint256 balanceUsdt = usdt.balanceOf(address(this));
        uint256 amountUsdt = balanceUsdt.mul(7500).div(10000);
        uint256 amountOxi = amountUsdt.mul(833).mul(1e12);

        IERC20(address(usdt)).approve(address(uniswap), amountUsdt);
        IERC20(address(oxi)).approve(address(uniswap), amountOxi);

        uint256 current = block.timestamp;

        uniswap.addLiquidity(address(usdt), address(oxi), amountUsdt, amountOxi, amountUsdt, amountOxi, msg.sender, current.add(100));
        usdt.safeTransfer(msg.sender, balanceUsdt.sub(amountUsdt));
    }

    function deposit(uint256 _amount) external virtual {
        require(_amount > 0, "amount >= 0 ");
        require(block.timestamp > startTime, "current < start time");
        require(block.timestamp < endTime, "current > endTime");

        balanceOf[msg.sender] = balanceOf[msg.sender].add(_amount);
        if (!checker[msg.sender]) {
            checker[msg.sender] = true;
            users.push(msg.sender);
        }

        total = total.add(_amount);
        usdt.safeTransferFrom(msg.sender, address(this), uint256(_amount));

        emit Deposit(msg.sender, _amount);
    }

    function withdraw(uint256 _amount) external virtual {
        require(block.timestamp > startTime, "current < start time");
        require(block.timestamp < endTime, "current > endTime");
        require(balanceOf[msg.sender] >= _amount, "amount > balance");

        balanceOf[msg.sender] = balanceOf[msg.sender].sub(_amount);
        total = total.sub(_amount);
        usdt.safeTransfer(msg.sender, _amount);

        emit Withdraw(msg.sender, _amount);
    }

    function safeTokenTransfer(
        address _token,
        address _to,
        uint256 _amount
    ) internal {
        uint256 _tokenBal = IERC20(_token).balanceOf(address(this));
        if (_amount > _tokenBal) {
            _amount = _tokenBal;
        }
        if (_amount > 0) {
            IERC20(_token).safeTransfer(_to, _amount);
        }
    }

    function withdrawGovernance(
        address _token,
        address _to,
        uint256 _amount
    ) public onlyGovernance {
        safeTokenTransfer(_token, _to, _amount);
    }

    function setRefun(bool _status) public onlyGovernance {
        isRefund = _status;
    }

    function refund(uint256 _amount) external virtual {
        require(isRefund, "Not enable");

        require(balanceOf[msg.sender] >= _amount, "amount > balance");

        balanceOf[msg.sender] = balanceOf[msg.sender].sub(_amount);
        total = total.sub(_amount);
        usdt.safeTransfer(msg.sender, _amount);

        emit Refund(msg.sender, _amount);
    }

    function getBalanceUsdt() public view returns (uint256) {
        return usdt.balanceOf(address(this));
    }

    function addWhiteList(address[] memory listUser) external onlyGovernance {
        for (uint256 i = 0; i < listUser.length; i++) {
            whiteList[listUser[i]] = true;
        }
    }

    function getLengthUserList() external view returns (uint256) {
        return users.length;
    }

    function getResult() external virtual returns (address[] memory user, uint256[] memory amount) {
        uint256 index = 0;
        for (uint256 i = 0; i < users.length; i++) {
            if (balanceOf[users[i]] > 0) {
                uint256 number = 0;
                if (whiteList[users[i]]) {
                    number = balanceOf[users[i]].mul(priceWL).mul(1e12);
                } else {
                    number = balanceOf[users[i]].mul(priceBase).mul(1e12);
                }
                user[index] = users[i];
                amount[index] = number;
                index = index + 1;
            }
        }
    }
}

