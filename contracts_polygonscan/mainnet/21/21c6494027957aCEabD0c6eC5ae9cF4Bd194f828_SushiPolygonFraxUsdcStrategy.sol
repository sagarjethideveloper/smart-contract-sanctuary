pragma solidity 0.6.12;
import "../base/MiniChefStrategy.sol";

contract SushiPolygonFraxUsdcStrategy is MiniChefStrategy {
  constructor() public {}

  function initialize(
    address _storage,
    address _vault
  ) public initializer {
    address underlying = address(0x9e20a8d3501BF96EDA8e69b96DD84840058a1cB0); //FraxUsdc SLP
    uint256 poolId = 14; // FraxUsdc Pool id
    address sushi = address(0x0b3F868E0BE5597D5DB7fEB59E1CADBb0fdDa50a);
    address wmatic = address(0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270);
    address dai = address(0x8f3Cf7ad23Cd3CaDbD9735AFf958023239c6A063); // main asset - dai

    MiniChefStrategy.initializeMiniChefStrategy(
      _storage,
      dai, // main asset
      underlying,
      _vault,
      address(0x0769fd68dFb93167989C6f7254cd0D766Fb2841F), // miniChef contract
      sushi,
      wmatic,
      poolId  // Pool id
    );

    address frax = address(0x104592a158490a9228070E0A8e5343B499e125D0);
    address usdc = address(0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174);
    address weth = address(0x7ceB23fD6bC0adD59E62ac25578270cFf1b9f619);

    rewardRoutes[frax] = [sushi, weth, usdc, frax];
    rewardRoutes[usdc] = [sushi, weth, usdc];

    secondRewardRoute = [wmatic, sushi];

    fromMainAssetRoutes[frax] = [dai, weth, usdc, frax];
    fromMainAssetRoutes[usdc] = [dai, weth, usdc];

    toMainAssetRoutes[frax] = [frax, usdc, weth, dai];
    toMainAssetRoutes[usdc] = [usdc, weth, dai];
  }
}

pragma solidity 0.6.12;

import "@openzeppelin/contracts/math/Math.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "../../interface/uniswap/IUniswapV2Router02.sol";
import "../../interface/IVault.sol";
import "../../interface/sushiswap/IMiniChef.sol";
import "../../interface/uniswap/IUniswapV2Pair.sol";
import "hardhat/console.sol";

contract MiniChefStrategy is OwnableUpgradeable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    address public sushiRouter;
    uint256 public poolId;
    address public store;
    address public underlying;
    address public vault;
    address public miniChef;
    address public rewardToken;
    address public secondRewardToken;
    address public mainAsset;

    uint256 public sellFloor;
    bool public sell;

    // this would be reset on each upgrade
    mapping(address => address[]) public rewardRoutes;
    mapping(address => address[]) public fromMainAssetRoutes;
    mapping(address => address[]) public toMainAssetRoutes;
    address[] public secondRewardRoute;

    constructor() public {}

    function initializeMiniChefStrategy(
        address _storage,
        address _mainAsset,
        address _underlying,
        address _vault,
        address _rewardPool,
        address _rewardToken,
        address _secondRewardToken,
        uint256 _poolID
    ) public initializer {
        __Ownable_init();
        mainAsset = _mainAsset;
        underlying = _underlying;
        poolId = _poolID;
        store = _storage;
        vault = _vault;
        miniChef = _rewardPool;
        rewardToken = _rewardToken;
        secondRewardToken = _secondRewardToken;
        sell = true;
        sushiRouter = address(0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506);

        IERC20 _lpt;
        _lpt = IMiniChef(miniChef).lpToken(_poolID);
        require(address(_lpt) == underlying, "Pool Info does not match underlying");

        address token0 = IUniswapV2Pair(underlying).token0();
        address token1 = IUniswapV2Pair(underlying).token1();

        // rewardRoutes[token0] = new address[](0);
        // rewardRoutes[token1] = new address[](0);

        // fromMainAssetRoutes[token0] = new address[](0);
        // fromMainAssetRoutes[token1] = new address[](0);

        // toMainAssetRoutes[token0] = new address[](0);
        // toMainAssetRoutes[token1] = new address[](0);

        // secondRewardRoute = new address[](0);
    }

    modifier restricted() {
        require(msg.sender == vault || msg.sender == owner(),
        "The sender has to be the controller, governance, or vault");
        _;
    }

    function rewardPoolBalance() internal view returns (uint256 bal) {
        (bal, ) = IMiniChef(miniChef).userInfo(poolId, address(this));
    }

    /*
     *   Note that we currently do not have a mechanism here to include the
     *   amount of reward that is accrued.
     */
    function investedUnderlyingBalance() public view returns (uint256) {
        if (miniChef == address(0)) {
            return IERC20(underlying).balanceOf(address(this));
        }
        // Adding the amount locked in the reward pool and the amount that is somehow in this contract
        // both are in the units of "underlying"
        // The second part is needed because there is the emergency exit mechanism
        // which would break the assumption that all the funds are always inside of the reward pool
        return
            rewardPoolBalance().add(
                IERC20(underlying).balanceOf(address(this))
            );
    }

    function investedBalance() public view returns (uint256) {
        uint256 underlyingBalance = investedUnderlyingBalance();
        return
            IERC20(mainAsset).balanceOf(address(this)).add(
                getMainAssetBalanceFromUnderlyingBalance(underlyingBalance)
            );
    }

    function exitRewardPool() internal {
        uint256 bal = rewardPoolBalance();
        if (bal != 0) {
            IMiniChef(miniChef).withdraw(poolId, bal, address(this));
        }
    }

    function withdrawReward() internal {
        IMiniChef(miniChef).harvest(poolId, address(this));
    }

    function emergencyExitRewardPool() internal {
        uint256 bal = rewardPoolBalance();
        if (bal != 0) {
            IMiniChef(miniChef).emergencyWithdraw(poolId, address(this));
        }
    }

    function unsalvagableTokens(address token) public view returns (bool) {
        return (token == rewardToken ||
            token == underlying ||
            token == secondRewardToken);
    }

    function enterRewardPool() internal {
        uint256 entireBalance = IERC20(underlying).balanceOf(address(this));
        IERC20(underlying).safeApprove(miniChef, 0);
        IERC20(underlying).safeApprove(miniChef, entireBalance);
        IMiniChef(miniChef).deposit(poolId, entireBalance, address(this));
    }

    function setLiquidationPath(address _token, address[] memory _route)
        public
        onlyOwner
    {
        rewardRoutes[_token] = _route;
    }

    // We assume that all the tradings can be done on Uniswap
    function _liquidateReward() internal {
        uint256 secondRewardBalance = IERC20(secondRewardToken).balanceOf(
            address(this)
        );

        if (secondRewardRoute.length > 1 && secondRewardBalance > 0) {
            swapSushiswapWithPath(secondRewardRoute, secondRewardBalance);
        }

        uint256 rewardBalance = IERC20(rewardToken).balanceOf(address(this));
        require(sell && rewardBalance >= sellFloor, "Not good to sell");

        // if (rewardBalance > 0) {
        //     payPerformanceFee();
        // }

        uint256 remainingRewardBalance = IERC20(rewardToken).balanceOf(
            address(this)
        );

        if (remainingRewardBalance > 0) {
            address token0 = IUniswapV2Pair(underlying).token0();
            address token1 = IUniswapV2Pair(underlying).token1();
            uint256 toToken0 = remainingRewardBalance.div(2);
            uint256 toToken1 = remainingRewardBalance.sub(toToken0);

            uint256 token0Amount;
            if (rewardRoutes[token0].length > 1) {
                swapSushiswapWithPath(rewardRoutes[token0], toToken0);
                token0Amount = IERC20(token0).balanceOf(address(this));
            } else {
                // otherwise we assme token0 is the reward token itself
                token0Amount = toToken0;
            }

            uint256 token1Amount;
            if (rewardRoutes[token1].length > 1) {
                swapSushiswapWithPath(rewardRoutes[token1], toToken1);
                token1Amount = IERC20(token1).balanceOf(address(this));
            } else {
                token1Amount = toToken1;
            }

            // provide token1 and token2 to SUSHI
            IERC20(token0).safeApprove(sushiRouter, 0);
            IERC20(token0).safeApprove(sushiRouter, token0Amount);

            IERC20(token1).safeApprove(sushiRouter, 0);
            IERC20(token1).safeApprove(sushiRouter, token1Amount);

            // we provide liquidity to sushi
            uint256 liquidity;
            (, , liquidity) = IUniswapV2Router02(sushiRouter).addLiquidity(
                token0,
                token1,
                token0Amount,
                token1Amount,
                1, // we are willing to take whatever the pair gives us
                1, // we are willing to take whatever the pair gives us
                address(this),
                block.timestamp
            );
        }
    }

    /*
     *   Stakes everything the strategy holds into the reward pool
     */
    function investAllUnderlying() public {
        if (IERC20(mainAsset).balanceOf(address(this)) > 0) {
            mainAssetToUnderlying();
        }
        if (IERC20(underlying).balanceOf(address(this)) > 0) {
            enterRewardPool();
        }
    }

    /*
     *   Withdraws all the asset to the vault
     */
    function withdrawAllToVault() public restricted {
        if (address(miniChef) != address(0)) {
            exitRewardPool();
        }
        // _liquidateReward();
        mainAssetFromUnderlying();
        IERC20(mainAsset).safeTransfer(
            vault,
            IERC20(mainAsset).balanceOf(address(this))
        );
    }

    /*
     *   Withdraws all the asset to the vault
     */
    function withdrawToVault(uint256 amount) public restricted {
        // Typically there wouldn't be any amount here
        // however, it is possible because of the emergencyExit
        uint256 entireBalance = IERC20(mainAsset).balanceOf(address(this));

        if (amount > entireBalance) {
            // While we have the check above, we still using SafeMath below
            // for the peace of mind (in case something gets changed in between)
            uint256 needToWithdraw = amount.sub(entireBalance);
            uint256 toWithdraw = needToWithdraw.mul(investedUnderlyingBalance()).div(investedBalance());
            
            toWithdraw = Math.min(rewardPoolBalance(), toWithdraw);

            IMiniChef(miniChef).withdraw(poolId, toWithdraw, address(this));
            mainAssetFromUnderlying();
            amount = IERC20(mainAsset).balanceOf(address(this));
        }

        IERC20(mainAsset).safeTransfer(vault, amount);
    }

    /*
     *   Governance or Controller can claim coins that are somehow transferred into the contract
     *   Note that they cannot come in take away coins that are used and defined in the strategy itself
     */
    function salvage(
        address recipient,
        address token,
        uint256 amount
    ) external onlyOwner {
        // To make sure that governance cannot come in and take away the coins
        require(
            !unsalvagableTokens(token),
            "token is defined as not salvagable"
        );
        IERC20(token).safeTransfer(recipient, amount);
    }

    /*
     *   Get the reward, sell it in exchange for underlying, invest what you got.
     *   It's not much, but it's honest work.
     *
     *   Note that although `onlyNotPausedInvesting` is not added here,
     *   calling `investAllUnderlying()` affectively blocks the usage of `doHardWork`
     *   when the investing is being paused by governance.
     */
    function doHardWork() external restricted {
        withdrawReward();
        _liquidateReward();
        investAllUnderlying();
    }

    /**
     * Can completely disable claiming UNI rewards and selling. Good for emergency withdraw in the
     * simplest possible way.
     */
    function setSell(bool s) public onlyOwner {
        sell = s;
    }

    /**
     * Sets the minimum amount of CRV needed to trigger a sale.
     */
    function setSellFloor(uint256 floor) public onlyOwner {
        sellFloor = floor;
    }

    function swapSushiswapWithPath(address[] memory path, uint256 _amount)
        internal
    {
        require(path[1] != address(0));
        IERC20(path[0]).safeApprove(sushiRouter, 0);
        IERC20(path[0]).safeApprove(sushiRouter, _amount);
        IUniswapV2Router02(sushiRouter).swapExactTokensForTokens(
            _amount,
            0,
            path,
            address(this),
            now.add(60)
        );
    }

    function mainAssetToUnderlying() internal {
        uint256 assetBalance = IERC20(mainAsset).balanceOf(address(this));
        address token0 = IUniswapV2Pair(underlying).token0();
        address token1 = IUniswapV2Pair(underlying).token1();

        uint256 toToken0 = assetBalance.div(2);
        uint256 toToken1 = assetBalance.sub(toToken0);

        uint256 token0Amount;
        if (fromMainAssetRoutes[token0].length > 1) {
            swapSushiswapWithPath(fromMainAssetRoutes[token0], toToken0);
            token0Amount = IERC20(token0).balanceOf(address(this));
        } else {
            // otherwise we assme token0 is the reward token itself
            token0Amount = toToken0;
        }

        uint256 token1Amount;
        if (fromMainAssetRoutes[token1].length > 1) {
            swapSushiswapWithPath(fromMainAssetRoutes[token1], toToken1);
            token1Amount = IERC20(token1).balanceOf(address(this));
        } else {
            token1Amount = toToken1;
        }

        IERC20(token0).safeApprove(sushiRouter, 0);
        IERC20(token0).safeApprove(sushiRouter, token0Amount);

        IERC20(token1).safeApprove(sushiRouter, 0);
        IERC20(token1).safeApprove(sushiRouter, token1Amount);

        uint256 liquidity;
        (, , liquidity) = IUniswapV2Router02(sushiRouter).addLiquidity(
            token0,
            token1,
            token0Amount,
            token1Amount,
            1, // we are willing to take whatever the pair gives us
            1, // we are willing to take whatever the pair gives us
            address(this),
            block.timestamp
        );
    }

    function mainAssetFromUnderlying() internal {
        address token0 = IUniswapV2Pair(underlying).token0();
        address token1 = IUniswapV2Pair(underlying).token1();

        uint256 liquidity = IERC20(underlying).balanceOf(address(this));
        IERC20(underlying).safeApprove(sushiRouter, 0);
        IERC20(underlying).safeApprove(sushiRouter, liquidity);
        IUniswapV2Router02(sushiRouter).removeLiquidity(
            token0,
            token1,
            liquidity,
            1, // we are willing to take whatever the pair gives us
            1, // we are willing to take whatever the pair gives us
            address(this),
            block.timestamp
        );

        uint256 token0Amount = IERC20(token0).balanceOf(address(this));
        uint256 token1Amount = IERC20(token1).balanceOf(address(this));

        if (toMainAssetRoutes[token0].length > 1) {
            swapSushiswapWithPath(toMainAssetRoutes[token0], token0Amount);
            token0Amount = IERC20(token0).balanceOf(address(this));
        }

        if (toMainAssetRoutes[token1].length > 1) {
            swapSushiswapWithPath(toMainAssetRoutes[token1], token1Amount);
            token1Amount = IERC20(token1).balanceOf(address(this));
        }
    }

    function getMainAssetBalanceFromUnderlyingBalance(uint256 _underlying)
        public
        view
        returns (uint256)
    {
        uint256 amount;

        if (_underlying > 0) {
            address token0 = IUniswapV2Pair(underlying).token0();
            address token1 = IUniswapV2Pair(underlying).token1();

            (uint256 reserves0, uint256 reserves1, ) = IUniswapV2Pair(underlying)
                .getReserves();
            uint256 totalSupply = IERC20(underlying).totalSupply();

            uint256 amount0 = reserves0.mul(_underlying).div(totalSupply);
            uint256 amount1 = reserves1.mul(_underlying).div(totalSupply);

            uint256[] memory amounts0 = IUniswapV2Router02(sushiRouter)
                .getAmountsOut(amount0, toMainAssetRoutes[token0]);
            uint256[] memory amounts1 = IUniswapV2Router02(sushiRouter)
                .getAmountsOut(amount1, toMainAssetRoutes[token1]);

            uint256 index0 = toMainAssetRoutes[token0].length - 1;
            uint256 index1 = toMainAssetRoutes[token1].length - 1;

            amount = amounts0[index0] + amounts1[index1];
        }
        return amount;
    }
}

