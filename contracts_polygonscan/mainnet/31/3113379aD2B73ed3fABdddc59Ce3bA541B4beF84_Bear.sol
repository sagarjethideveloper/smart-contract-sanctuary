pragma solidity ^0.8.0;

contract Helper {
    /**
     * @dev Converts the string to lowercase
     */
    function toLower(string memory str) public pure returns (string memory) {
        bytes memory bStr = bytes(str);
        bytes memory bLower = new bytes(bStr.length);
        for (uint256 i = 0; i < bStr.length; i++) {
            // Uppercase character
            if ((uint8(bStr[i]) >= 65) && (uint8(bStr[i]) <= 90)) {
                bLower[i] = bytes1(uint8(bStr[i]) + 32);
            } else {
                bLower[i] = bStr[i];
            }
        }
        return string(bLower);
    }

    function compareStrings(string memory a, string memory b)
        public
        pure
        returns (bool)
    {
        return (keccak256(abi.encodePacked((a))) ==
            keccak256(abi.encodePacked((b))));
    }

    function randomNumber(
        uint16 lowerBound,
        uint16 upperBound,
        uint256 nonce
    ) public view returns (uint32) {
        uint32 randomnumber = uint32(
            uint256(
                keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce))
            )
        ) % upperBound;
        randomnumber = randomnumber + lowerBound;

        return randomnumber;
    }

    function getItemType(uint32 _itemId) public pure returns (string memory) {
        if (_itemId >= 0 && _itemId < 150) {
            return "helmet";
        } else if (_itemId >= 150 && _itemId < 300) {
            return "chest";
        } else if (_itemId >= 300 && _itemId < 450) {
            return "shoulders";
        } else if (_itemId >= 450 && _itemId < 600) {
            return "arm";
        } else if (_itemId >= 600 && _itemId < 750) {
            return "forearm";
        } else if (_itemId >= 750 && _itemId < 900) {
            return "gloves";
        } else if (_itemId >= 900 && _itemId < 1050) {
            return "thigh";
        } else if (_itemId >= 1050 && _itemId < 1200) {
            return "calf";
        } else if (_itemId >= 1200 && _itemId < 1350) {
            return "weapon";
        }
    }

    function getOverallScore(
        uint32 strength,
        uint32 agility,
        uint32 speed,
        uint32 endurance
    ) public pure returns (uint32) {
        return strength + agility + speed + endurance;
    }
}

