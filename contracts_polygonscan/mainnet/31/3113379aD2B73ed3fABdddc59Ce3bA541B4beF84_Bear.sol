pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "./Namable.sol";
import "./Bio.sol";
import "./Item.sol";
import "./Helper.sol";
import "./CharacterDescription.sol";

contract Character is
    CharacterDescription,
    ERC721Enumerable,
    Namable,
    Bio,
    Ownable,
    IERC1155Receiver
{
    event ItemEquipped(uint16 charactedId, uint16 itemId);
    event ItemRemoved(uint16 charactedId, uint16 itemId);

    event LevelUp(uint16 charactedId, uint16 level);

    mapping(uint16 => uint256[][]) public tokenAppearance;
    bool public appereanceChangable;
    bool public pauseMinting;
    uint256 public presalePrice;

    uint32[50] public removedTraits;

    mapping(address => bool) public onlyAuthorized;

    function setAuthorized(address addy, bool value) public onlyOwner {
        onlyAuthorized[addy] = value;
    }

    mapping(address => uint8) public freePass;

    function setFreePass(address _to, uint8 _amount) public onlyOwner {
        freePass[_to] = _amount;
    }

    event StrengthIncreased(
        uint16 charactedId,
        uint16 newStrength,
        uint16 strengthIncreased
    );
    event AgilityIncreased(
        uint16 charactedId,
        uint16 newAgility,
        uint16 agilityIncreased
    );
    event SpeedIncreased(
        uint16 charactedId,
        uint16 newSpeed,
        uint16 speedIncreased
    );
    event EnduranceIncreased(
        uint16 charactedId,
        uint16 newEndurance,
        uint16 enduranceIncreased
    );

    function receive() external payable {}

    address internal arena;

    function setArenaAddress(address _arena) public onlyOwner {
        arena = _arena;
    }

    function giveXp(uint16 _characterId, uint16 _xp) public {
        require(msg.sender == arena);
        experience[_characterId] += _xp;
    }

    function experienceRequiredToNextLevel(uint16 _currentLevel)
        public
        pure
        returns (uint32)
    {
        return 8 * _currentLevel**3;
    }

    function levelUp(uint16 _charactedId) public returns (bool) {
        bool success = false;

        if (
            experience[_charactedId] >=
            experienceRequiredToNextLevel(level[_charactedId] + 1)
        ) {
            level[_charactedId]++;
            abilityPoints[_charactedId] += 5;
            levelUp(_charactedId);

            emit LevelUp(_charactedId, level[_charactedId]);

            success = true;
        }

        return success;
    }

    function increaseStrength(uint16 _tokenId, uint16 _abitilityPoints) public {
        require(ownerOf(_tokenId) == msg.sender);
        require(abilityPoints[_tokenId] >= _abitilityPoints);

        abilityPoints[_tokenId] -= _abitilityPoints;
        strength[_tokenId] += _abitilityPoints;

        emit StrengthIncreased(_tokenId, strength[_tokenId], _abitilityPoints);
    }

    function increaseAgility(uint16 _tokenId, uint16 _abitilityPoints) public {
        require(ownerOf(_tokenId) == msg.sender);
        require(abilityPoints[_tokenId] >= _abitilityPoints);

        abilityPoints[_tokenId] -= _abitilityPoints;
        agility[_tokenId] += _abitilityPoints;

        emit AgilityIncreased(_tokenId, agility[_tokenId], _abitilityPoints);
    }

    function increaseSpeed(uint16 _tokenId, uint16 _abitilityPoints) public {
        require(ownerOf(_tokenId) == msg.sender);
        require(abilityPoints[_tokenId] >= _abitilityPoints);

        abilityPoints[_tokenId] -= _abitilityPoints;
        speed[_tokenId] += _abitilityPoints;

        emit SpeedIncreased(_tokenId, speed[_tokenId], _abitilityPoints);
    }

    function increaseEndurance(uint16 _tokenId, uint16 _abitilityPoints)
        public
    {
        require(ownerOf(_tokenId) == msg.sender);
        require(abilityPoints[_tokenId] >= _abitilityPoints);

        abilityPoints[_tokenId] -= _abitilityPoints;
        endurance[_tokenId] += _abitilityPoints;

        emit EnduranceIncreased(
            _tokenId,
            endurance[_tokenId],
            _abitilityPoints
        );
    }

    uint256 private emissionStart;
    mapping(uint256 => uint256) private _lastClaim;

    function claimStamina(uint16 _tokenId) public {
        require(ownerOf(_tokenId) == msg.sender);

        uint256 accumulatedStamina = accumulated(_tokenId);

        _lastClaim[_tokenId] = block.timestamp;
        stamina[_tokenId] += uint16(accumulatedStamina);

        if (stamina[_tokenId] > 100) stamina[_tokenId] = 100;
    }

    function lastClaim(uint256 tokenIndex) public view returns (uint256) {
        require(ownerOf(tokenIndex) != address(0));

        uint256 lastClaimed = uint256(_lastClaim[tokenIndex]) != 0
            ? uint256(_lastClaim[tokenIndex])
            : emissionStart;
        return lastClaimed;
    }

    function accumulated(uint256 tokenIndex) public view returns (uint256) {
        require(block.timestamp > emissionStart);
        require(ownerOf(tokenIndex) != address(0));

        uint256 lastClaimed = lastClaim(tokenIndex);
        uint256 accumulationPeriod = block.timestamp;

        uint256 emissionPerDay = 100;
        uint256 SECONDS_IN_A_DAY = 86400;

        uint256 totalAccumulated = ((accumulationPeriod - lastClaimed) *
            emissionPerDay) / SECONDS_IN_A_DAY;

        return totalAccumulated;
    }

    function decreaseStamina(uint16 _tokenId, uint16 _amount) public {
        require(onlyAuthorized[msg.sender] == true);
        if (stamina[_tokenId] < _amount) stamina[_tokenId] = 0;
        else stamina[_tokenId] -= _amount;
    }

    function increaseStamina(uint16 _tokenId, uint16 _amount) public {
        require(onlyAuthorized[msg.sender] == true);
        stamina[_tokenId] += _amount;

        if (stamina[_tokenId] > 100) stamina[_tokenId] = 100;
    }

    // Time of when the sale starts.
    uint256 public constant SALE_START_TIMESTAMP = 1630314000;

    // BB token address
    address public bbAddress;

    // BaseURI for the NFTs
    string public baseURI;

    // Max character supply
    uint16 public MAX_SUPPLY = 5000;
    uint16 public TOTAL_MINTED;

    // function setMaxSupply(uint16 _maxSupply) public onlyOwner {
    //     MAX_SUPPLY = _maxSupply;
    // }

    // function setPauseMinting(bool _paused) public onlyOwner {
    //     pauseMinting = _paused;
    // }

    function setTokenAddress(address _bbAddress) public onlyOwner {
        bbAddress = _bbAddress;
    }

    // function setPresalePrice(uint256 _newPrices) public onlyOwner {
    //     presalePrice = _newPrices;
    // }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory __baseURI) public onlyOwner {
        baseURI = __baseURI;
    }

    constructor(string memory _name, string memory _symbol)
        ERC721(_name, _symbol)
    {
        // 45 MATIC
        presalePrice = 45000000000000000000;
    }

    // function setAppereanceChangable(bool _changable) public onlyOwner {
    //     appereanceChangable = _changable;
    // }

    // function changeAppearance(
    //     uint16[][] memory _selectedAppearance,
    //     uint16 tokenID
    // ) public {
    //     require(appereanceChangable == true);

    //     require(ownerOf(tokenID) == msg.sender);
    //     // Check if unique
    //     tokenAppearance[tokenID] = _selectedAppearance;
    // }

    function getPrice() public view returns (uint256) {
        require(block.timestamp >= SALE_START_TIMESTAMP);
        require(TOTAL_MINTED < MAX_SUPPLY);

        return presalePrice;
    }

    function changeName(uint256 tokenId, string memory newName) public {
        address owner = ownerOf(tokenId);

        require(_msgSender() == owner);
        require(validateName(newName) == true);
        require(sha256(bytes(newName)) != sha256(bytes(_tokenName[tokenId])));
        require(isNameReserved(newName) == false);

        ERC20Burnable(bbAddress).transferFrom(
            msg.sender,
            address(this),
            NAME_CHANGE_PRICE
        );
        // If already named, dereserve old name
        if (bytes(_tokenName[tokenId]).length > 0) {
            toggleReserveName(_tokenName[tokenId], false);
        }
        toggleReserveName(newName, true);
        _tokenName[tokenId] = newName;
        ERC20Burnable(bbAddress).burn(NAME_CHANGE_PRICE);
        emit NameChange(tokenId, newName);
    }

    function changeBio(uint256 tokenId, string memory newBio) public {
        address owner = ownerOf(tokenId);

        require(_msgSender() == owner);
        require(validateBio(newBio) == true);
        require(sha256(bytes(newBio)) != sha256(bytes(_tokenBio[tokenId])));
        require(isBioReserved(newBio) == false);

        ERC20Burnable(bbAddress).transferFrom(
            msg.sender,
            address(this),
            BIO_CHANGE_PRICE
        );
        // If bio already set, dereserve old bio
        if (bytes(_tokenBio[tokenId]).length > 0) {
            toggleReserveBio(_tokenBio[tokenId], false);
        }
        toggleReserveBio(newBio, true);
        _tokenBio[tokenId] = newBio;
        ERC20Burnable(bbAddress).burn(BIO_CHANGE_PRICE);
        emit BioChange(tokenId, newBio);
    }

    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes calldata data
    ) external override returns (bytes4) {
        return 0xf23a6e61;
    }

    function onERC1155BatchReceived(
        address operator,
        address from,
        uint256[] calldata ids,
        uint256[] calldata values,
        bytes calldata data
    ) external override returns (bytes4) {
        return 0xbc197c81;
    }

    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }
}

