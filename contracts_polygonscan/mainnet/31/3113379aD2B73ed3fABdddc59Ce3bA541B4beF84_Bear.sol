pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./Character.sol";

contract Bear is Ownable, Character {
    event BearMinted(uint16 bearId, uint256[][] selectedTraits);
    address itemAddress;

    uint16 numberOfTraits;
    uint16 public numberOfRemovedTraits;
    Item item;

    constructor(
        string memory _name,
        string memory _symbol,
        address _itemAddress
    ) Character(_name, _symbol) {
        setBaseURI("www.bullzvbearz.com/assets/bears/metadata/");
        itemAddress = _itemAddress;

        itemAddress = _itemAddress;

        numberOfTraits = 126;
        numberOfRemovedTraits = 0;
    }

    function setItemAddress(address _itemAddress) public onlyOwner {
        itemAddress = _itemAddress;
    }

    function mintBear(uint256[][] memory _selectedAppearance) public payable {
        require(pauseMinting == false, "Minting currenlty paused");

        for (uint8 i = 0; i < _selectedAppearance.length; i++) {
            uint256 traitId = _selectedAppearance[i][0];

            bool traitRemoved = false;
            for (uint8 j = 0; j < numberOfRemovedTraits; j++) {
                if (traitId == removedTraits[j]) {
                    traitRemoved = true;
                    break;
                }
            }

            require(
                !traitRemoved,
                "One of the traits you are trying to mint has been removed"
            );
        }

        uint8 traits = 0;
        uint8 recolors = 0;

        uint8 freeTraits = 5;
        uint8 freeRecolors = 10;

        uint256 traitPrice = 1.03 * 100;
        uint256 recolorPrice = 1.03 * 100;

        uint8[16] memory defaultTraits = [
            1,
            2,
            3,
            4,
            9,
            15,
            21,
            39,
            40,
            41,
            51,
            62,
            63,
            64,
            65,
            110
        ];

        for (uint8 i = 0; i < _selectedAppearance.length; i++) {
            uint256 traitId = _selectedAppearance[i][0];
            uint256 h = _selectedAppearance[i][1];

            if (h != 0) recolors++;

            bool found = true;
            for (uint8 j = 0; j < defaultTraits.length; j++) {
                if (traitId == defaultTraits[j]) found = false;
            }

            if (found) traits++;
        }

        uint8 extraTraits = 0;
        if (traits > freeTraits) extraTraits = traits - freeTraits;

        uint8 extraRecolors = 0;
        if (recolors > freeRecolors) extraRecolors = recolors - freeRecolors;

        uint256 price = getPrice();

        for (uint8 i = 0; i < extraTraits; i++)
            price = (price * traitPrice) / 100;

        for (uint8 i = 0; i < extraRecolors; i++)
            price = (price * recolorPrice) / 100;

        if (freePass[msg.sender] == 0)
            require(msg.value == price, "Amount of Ether sent is not correct");
        else freePass[msg.sender]--;

        // Check if unique

        uint16 tokenID = TOTAL_MINTED;

        _safeMint(msg.sender, tokenID);

        tokenAppearance[tokenID] = _selectedAppearance;
        abilityPoints[tokenID] += 8;

        level[tokenID] = 1;

        emit BearMinted(tokenID, _selectedAppearance);
        TOTAL_MINTED++;

        if (TOTAL_MINTED % 100 == 0) {
            uint32 traitIndex;
            bool alreadyRemoved = false;

            do {
                alreadyRemoved = false;
                uint256 nonce = uint256(uint160(msg.sender)) +
                    block.timestamp +
                    uint256(TOTAL_MINTED);
                traitIndex = randomNumber(0, numberOfTraits, nonce);

                for (uint16 i = 0; i < numberOfRemovedTraits; i++) {
                    if (removedTraits[i] == traitIndex) {
                        alreadyRemoved = true;
                        break;
                    }
                }
            } while (alreadyRemoved);

            removedTraits[numberOfRemovedTraits] = traitIndex;
            numberOfRemovedTraits++;
        }
    }

    function isUnique(uint256[] memory _selectedAppearance) public {}

    function useItem(uint16 _characterId, uint16 _itemId) public {
        require(item.balanceOf(msg.sender, _itemId) >= 1);
        require(ownerOf(_characterId) == msg.sender);

        (bool success, ) = address(item).delegatecall(
            abi.encodeWithSignature(
                "useItem(uint16,uint16,address,address,address)",
                _characterId,
                _itemId,
                address(item),
                msg.sender,
                address(this)
            )
        );
        require(success);

        item.safeTransferFrom(msg.sender, address(this), _itemId, 1, "");
        emit ItemEquipped(_characterId, _itemId);
    }

    function removeItem(uint16 _characterId, uint16 _itemId) public {
        require(ownerOf(_characterId) == msg.sender);
        require(item.balanceOf(address(this), _itemId) >= 1);

        (bool success, ) = address(item).delegatecall(
            abi.encodeWithSignature(
                "removeItem(uint16,uint16,address,address,address)",
                _characterId,
                _itemId,
                address(item),
                address(this),
                msg.sender
            )
        );
        require(success);

        item.safeTransferFrom(address(this), msg.sender, _itemId, 1, "");
        emit ItemRemoved(_characterId, _itemId);
    }
}

