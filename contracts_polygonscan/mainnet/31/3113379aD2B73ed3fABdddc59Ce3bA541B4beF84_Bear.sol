import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./BearBullToken.sol";
import "./Helper.sol";
import "./CharacterDescription.sol";

contract Item is CharacterDescription, ERC1155, Ownable, Helper {
    constructor(string memory _uri) ERC1155(_uri) {}

    mapping(uint16 => uint16) public quantity;
    mapping(uint16 => uint16) public levelRequirement;

    mapping(uint16 => uint16) public totalMinted;
    mapping(uint16 => bool) private itemsInitialized;

    mapping(uint16 => uint256) public itemPrices;

    address internal _bb;

    function setBBAddress(address bb) public onlyOwner {
        _bb = bb;
    }

    event ItemBought(uint32 id, uint32 amount, bytes data);
    event ItemInitialized(
        uint32 itemId,
        uint16 strength,
        uint16 agility,
        uint16 speed,
        uint16 endurance,
        uint16 stamina,
        uint16 minLevel,
        uint16 quantity
    );

    function initializeItem(
        uint16 _itemId,
        uint16 _strength,
        uint16 _agility,
        uint16 _speed,
        uint16 _endurance,
        uint16 _stamina,
        uint16 _minLevel,
        uint16 _quantity
    ) public onlyOwner {
        require(itemsInitialized[_itemId] != true, "Item already initialized");

        strength[_itemId] = _strength;
        agility[_itemId] = _agility;
        speed[_itemId] = _speed;
        endurance[_itemId] = _endurance;
        stamina[_itemId] = _stamina;
        levelRequirement[_itemId] = _minLevel;
        quantity[_itemId] = _quantity;

        itemsInitialized[_itemId] = true;

        emit ItemInitialized(
            _itemId,
            _strength,
            _agility,
            _speed,
            _endurance,
            _stamina,
            _minLevel,
            _quantity
        );
    }

    function getItemStats(uint16 _itemId)
        public
        view
        returns (uint16[6] memory)
    {
        return [
            strength[_itemId],
            agility[_itemId],
            speed[_itemId],
            endurance[_itemId],
            stamina[_itemId],
            levelRequirement[_itemId]
        ];
    }

    function mintItem(
        uint16 _id,
        uint16 _amount,
        bytes memory _data
    ) public payable {
        require(_id != 0, "Items starts with ID: 1");
        require(quantity[_id] >= 1, "All items with this id have been minted");
        require(_amount != 0);
        require(
            getPrice(_id) * _amount == msg.value ||
                BearBullToken(_bb).allowance(msg.sender, address(this)) >=
                getPriceBB(_id) * _amount,
            "Wrong amount of Matic or BB sent"
        );

        if (
            BearBullToken(_bb).allowance(msg.sender, address(this)) >=
            getPriceBB(_id) * _amount
        ) {
            require(
                BearBullToken(_bb).balanceOf(msg.sender) >=
                    getPriceBB(_id) * _amount
            );

            BearBullToken(_bb).transferFrom(
                msg.sender,
                address(this),
                getPriceBB(_id) * _amount
            );

            uint256 totalPrice = getPriceBB(_id) * _amount;
            uint256 ownerFunds = (totalPrice * 5) / 100;

            BearBullToken(_bb).transfer(owner(), ownerFunds);
            BearBullToken(_bb).burn(totalPrice - ownerFunds);
        }

        _mint(msg.sender, _id, _amount, _data);

        totalMinted[_id] += _amount;
        quantity[_id] -= 1;
        emit ItemBought(_id, _amount, _data);
    }

    function getPrice(uint16 _tokenId) public view returns (uint256) {
        if (_tokenId >= 0 && _tokenId < 20) {
            return 1000000000000000;
        } else if (_tokenId >= 20 && _tokenId < 40) {
            return 2000000000000000;
        }
    }

    function getPriceBB(uint16 _tokenId) public view returns (uint256) {
        return itemPrices[_tokenId];
    }

    function setPrice(uint16 _tokenId, uint256 _price) public onlyOwner {
        itemPrices[_tokenId] = _price;
    }

    function useItem(
        uint16 _characterId,
        uint16 _itemId,
        address _itemAddress,
        address _from,
        address _to
    ) public {
        string memory itemType = getItemType(_itemId);
        require(level[_characterId] >= levelRequirement[_itemId]);

        if (compareStrings(itemType, "helmet")) {
            require(helmet[_characterId] != _itemId);
            if (helmet[_characterId] != 0)
                removeItem(
                    _characterId,
                    helmet[_characterId],
                    _itemAddress,
                    _to,
                    _from
                );

            helmet[_characterId] = _itemId;
        } else if (compareStrings(itemType, "chest")) {
            require(chest[_characterId] != _itemId);
            if (chest[_characterId] != 0)
                removeItem(
                    _characterId,
                    helmet[_characterId],
                    _itemAddress,
                    _to,
                    _from
                );

            chest[_characterId] = _itemId;
        } else if (compareStrings(itemType, "shoulders")) {
            require(shoulders[_characterId] != _itemId);
            if (shoulders[_characterId] != 0)
                removeItem(
                    _characterId,
                    helmet[_characterId],
                    _itemAddress,
                    _to,
                    _from
                );

            shoulders[_characterId] = _itemId;
        } else if (compareStrings(itemType, "arm")) {
            require(arm[_characterId] != _itemId);
            if (arm[_characterId] != 0)
                removeItem(
                    _characterId,
                    helmet[_characterId],
                    _itemAddress,
                    _to,
                    _from
                );

            arm[_characterId] = _itemId;
        } else if (compareStrings(itemType, "forearm")) {
            require(forearm[_characterId] != _itemId);
            if (forearm[_characterId] != 0)
                removeItem(
                    _characterId,
                    helmet[_characterId],
                    _itemAddress,
                    _to,
                    _from
                );

            forearm[_characterId] = _itemId;
        } else if (compareStrings(itemType, "gloves")) {
            require(gloves[_characterId] != _itemId);
            if (gloves[_characterId] != 0)
                removeItem(
                    _characterId,
                    helmet[_characterId],
                    _itemAddress,
                    _to,
                    _from
                );

            gloves[_characterId] = _itemId;
        } else if (compareStrings(itemType, "thigh")) {
            require(thigh[_characterId] != _itemId);
            if (thigh[_characterId] != 0)
                removeItem(
                    _characterId,
                    helmet[_characterId],
                    _itemAddress,
                    _to,
                    _from
                );

            thigh[_characterId] = _itemId;
        } else if (compareStrings(itemType, "calf")) {
            require(calf[_characterId] != _itemId);
            if (calf[_characterId] != 0)
                removeItem(
                    _characterId,
                    helmet[_characterId],
                    _itemAddress,
                    _to,
                    _from
                );

            calf[_characterId] = _itemId;
        } else if (compareStrings(itemType, "weapon")) {
            require(weapon[_characterId] != _itemId);
            if (weapon[_characterId] != 0)
                removeItem(
                    _characterId,
                    helmet[_characterId],
                    _itemAddress,
                    _to,
                    _from
                );

            weapon[_characterId] = _itemId;
        }

        // Add item stats
        strength[_characterId] += strength[_itemId];
        agility[_characterId] += agility[_itemId];
        speed[_characterId] += speed[_itemId];
        endurance[_characterId] += endurance[_itemId];
        stamina[_characterId] += stamina[_itemId];
    }

    function removeItem(
        uint16 _characterId,
        uint16 _itemId,
        address _itemAddress,
        address _from,
        address _to
    ) public {
        string memory itemType = getItemType(_itemId);

        if (compareStrings(itemType, "helmet")) {
            require(helmet[_characterId] == _itemId);
            helmet[_characterId] = 0;
        } else if (compareStrings(itemType, "chest")) {
            require(chest[_characterId] == _itemId);
            chest[_characterId] = 0;
        } else if (compareStrings(itemType, "shoulders")) {
            require(shoulders[_characterId] == _itemId);
            shoulders[_characterId] = 0;
        } else if (compareStrings(itemType, "arm")) {
            require(arm[_characterId] == _itemId);
            arm[_characterId] = 0;
        } else if (compareStrings(itemType, "forearm")) {
            require(forearm[_characterId] == _itemId);
            forearm[_characterId] = 0;
        } else if (compareStrings(itemType, "gloves")) {
            require(gloves[_characterId] == _itemId);
            gloves[_characterId] = 0;
        } else if (compareStrings(itemType, "thigh")) {
            require(thigh[_characterId] == _itemId);
            thigh[_characterId] = 0;
        } else if (compareStrings(itemType, "calf")) {
            require(calf[_characterId] == _itemId);
            calf[_characterId] = 0;
        } else if (compareStrings(itemType, "weapon")) {
            require(weapon[_characterId] == _itemId);
            weapon[_characterId] = 0;
        }

        // Remove item stats
        strength[_characterId] -= strength[_itemId];
        agility[_characterId] -= agility[_itemId];
        speed[_characterId] -= speed[_itemId];
        endurance[_characterId] -= endurance[_itemId];
    }
}

