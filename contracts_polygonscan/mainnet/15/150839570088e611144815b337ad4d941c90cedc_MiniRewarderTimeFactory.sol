pragma solidity 0.6.12;

import "./MiniRewarderTime.sol";
import "@boringcrypto/boring-solidity/contracts/libraries/BoringERC20.sol";
import "@boringcrypto/boring-solidity/contracts/BoringOwnable.sol";

contract MiniRewarderTimeFactory is BoringOwnable {
  address public defaultMiniApe;
  address public defaultOwner;
  using BoringERC20 for IERC20;

  event DeployedRewarderContract(address indexed rewarder, address indexed rewardToken, uint256 indexed rewardPerSecond);
  event UpdatedDefaults(address indexed defaultOwner, address indexed defaultMiniApe);

  constructor (address _defaultMiniApe, address _defaultOwner) public {
    defaultOwner = _defaultOwner;
    defaultMiniApe = _defaultMiniApe;
  }

  function updateDefaultAddresses (address _defaultMiniApe, address _defaultOwner) external onlyOwner {
    defaultOwner = _defaultOwner;
    defaultMiniApe = _defaultMiniApe;
    emit UpdatedDefaults(defaultOwner, defaultMiniApe);
  }

  function deployDefaultRewarder(IERC20 _rewardToken, uint256 _rewardPerSecond) external {
    deployRewarder(defaultMiniApe, defaultOwner, _rewardToken, _rewardPerSecond);
  }

  function deployRewarder (address _miniApe, address _owner, IERC20 _rewardToken, uint256 _rewardPerSecond) public {
    MiniRewarderTime rewarder = new MiniRewarderTime();

    rewarder.initialize(_rewardToken, _rewardPerSecond, _miniApe);

    rewarder.transferOwnership(_owner, true, false);

    emit DeployedRewarderContract(address(rewarder), address(_rewardToken), _rewardPerSecond);
  }

}

