contract Nursery is Ownable, ReentrancyGuard {
    using SafeMath for uint256;

    event Deposit(address indexed user, uint256 amount);
    event Withdraw(
        address indexed user,
        uint256 pendingGuppy,
        uint256 frySurvival,
        uint256 fryMortality
    );
    event Harvest(address indexed user, uint256 pendingGuppy);
    event UpdateEmissionRate(address indexed user, uint256 fryPerBlock);
    event SetDevAddress(address indexed user, address indexed newAddress);
    event SetTeamAddress(address indexed user, address indexed newAddress);
    event SetDonationAddress(address indexed user, address indexed newAddress);
    event UpdateFullMaturityTime(uint256 fullMaturityTime);
    event SetReferralAddress(
        address indexed user,
        IReferral indexed newAddress
    );
    event ReferralCommissionPaid(
        address indexed user,
        address indexed referrer,
        uint256 commissionAmount
    );

    struct UserInfo {
        uint256 amount;
        uint256 depositTimestamp;
    }

    FryToken public fry;
    GuppyToken public guppy;

    address public devAddress;
    address public donationAddress;
    address public teamAddress;
    uint16 public constant PRECISION_POINT = 10000;
    // GUPPY tokens created per block.
    uint256 public guppyPerBlock;

    // Time it takes to get max maturity to prevent mortality
    uint256 public fullMaturityTime;
    uint256 lastRewardBlock; // Last block number that GUPPY distribution occurs.
    uint256 accGuppyPerShare; // Accumulated GUPPYs per share, times 1e18. See below.
    // Fry referral contract address.
    IReferral public referral;
    // Referral commission rate in basis points.
    uint16 public referralCommissionRate = 200;
    // Max referral commission rate: 5%.
    uint16 public constant MAXIMUM_REFERRAL_COMMISSION_RATE = 500;

    // Info of each user that stakes into the nursery
    mapping(address => UserInfo[]) public userInfo;
    mapping(address => uint256) public userRewardDebt;

    constructor(
        FryToken _fry,
        GuppyToken _guppy,
        IReferral _referral,
        uint256 _fullMaturityTime,
        uint256 _guppyPerBlock,
        address _donationAddress,
        address _devAddress,
        address _teamAddress
    ) public {
        fry = _fry;
        guppy = _guppy;
        referral = _referral;
        fullMaturityTime = _fullMaturityTime;
        guppyPerBlock = _guppyPerBlock;
        devAddress = _devAddress;
        teamAddress = _teamAddress;
        donationAddress = _donationAddress;
        lastRewardBlock = block.number;
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to)
        public
        pure
        returns (uint256)
    {
        return _to.sub(_from);
    }

    // View function to see pending GUPPYs on frontend.
    function pendingGuppy(address _user) external view returns (uint256) {
        UserInfo[] storage user = userInfo[_user];
        uint256 rewardDebt = userRewardDebt[_user];
        uint256 accGuppyPerShareMem = accGuppyPerShare;
        uint256 frySupply = fry.balanceOf(address(this));
        uint256 userAmount = 0;
        for (uint256 i; i < user.length; i++) {
            userAmount = userAmount.add(user[i].amount);
        }
        if (block.number > lastRewardBlock && frySupply != 0) {
            uint256 multiplier = getMultiplier(lastRewardBlock, block.number);
            uint256 guppyReward = multiplier.mul(guppyPerBlock);
            accGuppyPerShareMem = accGuppyPerShareMem.add(
                guppyReward.mul(1e18).div(frySupply)
            );
        }
        return userAmount.mul(accGuppyPerShareMem).div(1e18).sub(rewardDebt);
    }

    // Update reward
    function update() public {
        if (block.number <= lastRewardBlock) {
            return;
        }
        uint256 frySupply = fry.balanceOf(address(this));
        if (frySupply == 0) {
            lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(lastRewardBlock, block.number);
        uint256 guppyReward = multiplier.mul(guppyPerBlock);
        guppy.mint(devAddress, guppyReward.mul(200).div(PRECISION_POINT));
        guppy.mint(teamAddress, guppyReward.mul(700).div(PRECISION_POINT));
        guppy.mint(donationAddress, guppyReward.mul(100).div(PRECISION_POINT));
        guppy.mint(address(this), guppyReward);
        accGuppyPerShare = accGuppyPerShare.add(
            guppyReward.mul(1e18).div(frySupply)
        );
        lastRewardBlock = block.number;
    }

    // View function to get info of the nursery for a user (front end use)
    function nurseryInfo(address _user) public view returns (uint256, uint256) {
        UserInfo[] storage user = userInfo[_user];
        if (user.length == 0) return (0, 0);
        uint256 totalAmountTime;
        uint256 totalAmount;
        for (uint256 i = 0; i < user.length; i++) {
            totalAmountTime = totalAmountTime.add(
                user[i].amount.mul(
                    block.timestamp.sub(user[i].depositTimestamp)
                )
            );
            totalAmount = totalAmount.add(user[i].amount);
        }
        // Time weighted average
        uint256 twa = totalAmountTime.div(totalAmount).min(fullMaturityTime);
        // (total amount, time weighted average, pending Guppy)
        return (totalAmount, twa);
    }

    function deposit(uint256 _amount, address _referrer) external nonReentrant {
        require(_amount > 0, "deposit: !amount");
        update();
        (uint256 userAmount, ) = nurseryInfo(msg.sender);
        if (
            _amount > 0 &&
            address(referral) != address(0) &&
            _referrer != address(0) &&
            _referrer != msg.sender
        ) {
            referral.recordReferral(msg.sender, _referrer);
        }
        if (userAmount > 0) {
            uint256 pending = userAmount.mul(accGuppyPerShare).div(1e18).sub(
                userRewardDebt[msg.sender]
            );
            if (pending > 0) {
                // Mint Guppies for users
                _safeGuppyTransfer(msg.sender, pending);
                payReferralCommission(msg.sender, pending);
            }
        }
        UserInfo[] storage user = userInfo[msg.sender];
        fry.transferFrom(address(msg.sender), address(this), _amount);
        user.push(UserInfo(_amount, block.timestamp));

        userRewardDebt[msg.sender] = userAmount
            .add(_amount)
            .mul(accGuppyPerShare)
            .div(1e18);
        emit Deposit(msg.sender, _amount);
    }

    function withdraw() external nonReentrant {
        update();
        (uint256 userAmount, uint256 twa) = nurseryInfo(msg.sender);
        uint256 pending = userAmount.mul(accGuppyPerShare).div(1e18).sub(
            userRewardDebt[msg.sender]
        );
        if (pending > 0) {
            // Mint Guppies for users
            _safeGuppyTransfer(msg.sender, pending);
            payReferralCommission(msg.sender, pending);
        }

        uint256 frySurvival = userAmount.mul(twa).div(fullMaturityTime);
        // Transfer matured Fry back to user
        fry.transfer(msg.sender, frySurvival);
        // Fry mortality D:
        fry.transfer(
            0x000000000000000000000000000000000000dEaD,
            userAmount.sub(frySurvival)
        );
        delete userInfo[msg.sender];

        userRewardDebt[msg.sender] = 0;

        emit Withdraw(
            msg.sender,
            pending,
            frySurvival,
            userAmount.sub(frySurvival)
        );
    }

    function harvest() external nonReentrant {
        (uint256 userAmount, ) = nurseryInfo(msg.sender);
        update();
        uint256 pending = userAmount.mul(accGuppyPerShare).div(1e18).sub(
            userRewardDebt[msg.sender]
        );
        if (pending > 0) {
            // Mint Guppies for users
            _safeGuppyTransfer(msg.sender, pending);
            payReferralCommission(msg.sender, pending);
        }
        userRewardDebt[msg.sender] = userAmount.mul(accGuppyPerShare).div(1e18);
        emit Harvest(msg.sender, pending);
    }

    function _safeGuppyTransfer(address _to, uint256 _amount) internal {
        uint256 guppyBalance = guppy.balanceOf(address(this));
        bool transferSuccess = false;
        if (_amount > guppyBalance) {
            transferSuccess = guppy.transfer(_to, guppyBalance);
        } else {
            transferSuccess = guppy.transfer(_to, _amount);
        }
        require(transferSuccess, "safeGuppyTransfer: Transfer failed");
    }

    // Update referral commission rate by the owner
    function setReferralCommissionRate(uint16 _referralCommissionRate)
        external
        onlyOwner
    {
        require(
            _referralCommissionRate <= MAXIMUM_REFERRAL_COMMISSION_RATE,
            "setReferralCommissionRate: invalid referral commission rate basis points"
        );
        referralCommissionRate = _referralCommissionRate;
    }

    // Update the referral contract address by the owner
    function setReferralAddress(IReferral _referral) external onlyOwner {
        referral = _referral;
        emit SetReferralAddress(msg.sender, _referral);
    }

    // Pay referral commission to the referrer who referred this user.
    function payReferralCommission(address _user, uint256 _pending) internal {
        if (address(referral) != address(0) && referralCommissionRate > 0) {
            address referrer = referral.getReferrer(_user);
            uint256 commissionAmount = _pending.mul(referralCommissionRate).div(
                PRECISION_POINT
            );

            if (referrer != address(0) && commissionAmount > 0) {
                guppy.mint(referrer, commissionAmount);
                emit ReferralCommissionPaid(_user, referrer, commissionAmount);
            }
        }
    }

    // Update dev address by the previous dev.
    function setDevAddress(address _devAddress) external onlyOwner {
        devAddress = _devAddress;
        emit SetDevAddress(msg.sender, _devAddress);
    }

    function setTeamAddress(address _teamAddress) external onlyOwner {
        teamAddress = _teamAddress;
        emit SetTeamAddress(msg.sender, _teamAddress);
    }

    function updateEmissionRate(uint256 _guppyPerBlock) external onlyOwner {
        update();
        guppyPerBlock = _guppyPerBlock;
        emit UpdateEmissionRate(msg.sender, _guppyPerBlock);
    }

    function setDonationAddress(address _donationAddress) external onlyOwner {
        donationAddress = _donationAddress;
        emit SetDonationAddress(msg.sender, _donationAddress);
    }

    function updateFullMaturityTime(uint256 _fullMaturityTime)
        external
        onlyOwner
    {
        fullMaturityTime = _fullMaturityTime;
        emit UpdateFullMaturityTime(_fullMaturityTime);
    }
}

