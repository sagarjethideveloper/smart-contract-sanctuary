pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Multicall.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

import "../bank/IBank.sol";
import "../rngOracle/RNGOracleConsumer.sol";

import "hardhat/console.sol";

/// @title The base for Games
/// @author Romuald Hog
abstract contract Game is
    Ownable,
    ReentrancyGuard,
    Pausable,
    Multicall,
    RNGOracleConsumer
{
    using SafeERC20 for IERC20;

    /// This is the struct that keeps the data of one bet
    struct Bet {
        uint32 id;
        uint32 timestamp;
        address payable user;
        bool resolved;
        address token;
        uint256 amount;
    }

    /// @dev Mapping between users and bets ids
    mapping(address => uint32[]) internal _bets;
    /// Minimum bet amount needs to cover the rngOracle's gas fees
    mapping(address => uint256) public tokensBetResolutionGas;
    /// This is the games tokens house edge. In basis point.
    mapping(address => uint16) public tokensHouseEdges;
    /// The bank that manage the cash flow
    IBank public immutable bank;

    event SetAllowedTokenBetResolutionGas(
        address token,
        uint256 betResolutionGas
    );
    event SetHouseEdge(address token, uint16 houseEdge);

    event BetAmountTransferFail(uint32 id, uint256 amount, string reason);
    event BetPayoutTransferFail(uint32 id, uint256 amount, string reason);
    event BankTransferFail(uint32 id, uint256 amount, string reason);

    /// Insufficient transaction value.
    /// @param amount of the transaction value.
    /// @param minBetAmount minimum bet amount to cover the bet resolution cost.
    error InsufficientTxValue(uint256 amount, uint256 minBetAmount);
    /// Bet value is too high
    /// @param token provided token.
    /// @param amount provided amount.
    /// @param maxBetAmount maximum bet amount allowed by the bank based on the leverage.
    error ExcessiveBetValue(
        address token,
        uint256 amount,
        uint256 maxBetAmount
    );
    /// Bet provided doesn't exist or was already resolved.
    /// @param betId provided by the resolution function.
    error NotPendingBet(uint32 betId);
    /// House edge is capped at 4%.
    /// @param houseEdge provided house edge.
    error ExcessiveHouseEdge(uint16 houseEdge);

    constructor(address _rngOracle, address _bankAddress)
        RNGOracleConsumer(_rngOracle)
    {
        bank = IBank(_bankAddress);
    }

    receive() external payable {}

    /// Only allowed token are accepted
    modifier onlyAllowedToken(address token) {
        if (bank.isAllowedToken(token) == false) {
            revert IBank.ForbiddenToken(token);
        }
        _;
    }
    /// Only pending bet could be resolved
    modifier onlyPendingBet(Bet memory bet) {
        if (bet.user == address(0) || bet.resolved == true) {
            revert NotPendingBet(bet.id);
        }
        _;
    }

    function _newBet(
        address token,
        uint256 tokenAmount,
        uint256 multiplier
    )
        internal
        nonReentrant
        whenNotPaused
        onlyAllowedToken(token)
        returns (Bet memory)
    {
        address user = msg.sender;
        bool isNativeToken = token == address(0);

        uint256 minTxValue = tokensBetResolutionGas[token] * tx.gasprice;
        if (isNativeToken ? msg.value <= minTxValue : msg.value < minTxValue) {
            revert InsufficientTxValue(msg.value, minTxValue);
        }
        uint256 betAmount = isNativeToken
            ? msg.value - minTxValue
            : tokenAmount;
        // TODO define a slippage
        uint256 maxBetAmount = bank.getMaxBetAmount(token, multiplier);
        if (betAmount > maxBetAmount) {
            revert ExcessiveBetValue(token, betAmount, maxBetAmount);
        }

        // If ERC20, transfer the tokens
        if (!isNativeToken) {
            IERC20(token).safeTransferFrom(user, address(this), betAmount);
        }

        // Pay bet resolution gas price
        _addPaidGas(minTxValue);

        // Create bet
        uint32 id = _requestRandomness();
        Bet memory newBet = Bet(
            id,
            uint32(block.timestamp),
            payable(user),
            false,
            token,
            betAmount
        );
        _bets[user].push(id);

        return newBet;
    }

    /// Should not revert as it resolves the bet with the randomness
    function _resolveBet(
        Bet storage bet,
        bool wins,
        uint256 payout
    ) internal nonReentrant onlyPendingBet(bet) returns (uint256) {
        address token = bet.token;
        uint256 betAmount = bet.amount;
        address payable user = bet.user;
        bool isNativeToken = bet.token == address(0);

        // Check for the result
        if (wins) {
            // Transfer the bet amount from the contract
            if (isNativeToken) {
                if (!user.send(betAmount)) {
                    emit BetAmountTransferFail(
                        bet.id,
                        betAmount,
                        "Missing native token funds"
                    );
                }
            } else {
                try IERC20(token).transfer(user, betAmount) {} catch Error(
                    string memory reason
                ) {
                    emit BetAmountTransferFail(bet.id, betAmount, reason);
                }
            }

            uint256 bankPayout = payout - betAmount;
            uint256 fees = getPayoutFees(token, payout);
            payout -= fees;

            // Transfer the payout from the bank
            try bank.payout(user, token, bankPayout, fees) {} catch Error(
                string memory reason
            ) {
                emit BetPayoutTransferFail(bet.id, bankPayout - fees, reason);
            }
        } else {
            payout = 0;
            try
                bank.cashIn{value: isNativeToken ? betAmount : 0}(
                    token,
                    betAmount
                )
            {} catch Error(string memory reason) {
                emit BankTransferFail(bet.id, betAmount, reason);
            }
        }

        // Maintain Oracle node account balance
        _transferPaidGas(tokensBetResolutionGas[token]);

        bet.resolved = true;

        return payout;
    }

    /// Get some funds out
    /// @dev only necessary for contract migration
    function withdraw(address token, uint80 amount)
        external
        onlyOwner
        whenPaused
    {
        if (token == address(0)) {
            Address.sendValue(payable(msg.sender), amount);
        } else {
            IERC20(token).safeTransfer(msg.sender, amount);
        }
    }

    function setTokenBetResolutionGas(address token, uint256 betResolutionGas)
        external
        onlyOwner
    {
        tokensBetResolutionGas[token] = betResolutionGas;
        emit SetAllowedTokenBetResolutionGas(token, betResolutionGas);
    }

    function togglePause(bool toPause) external onlyOwner {
        if (toPause) {
            _pause();
        } else {
            _unpause();
        }
    }

    function _setHouseEdge(address token, uint16 houseEdge) internal onlyOwner {
        if (houseEdge > 400) {
            revert ExcessiveHouseEdge(houseEdge);
        }
        tokensHouseEdges[token] = houseEdge;
        emit SetHouseEdge(token, houseEdge);
    }

    function getPayoutFees(address token, uint256 payout)
        public
        view
        returns (uint256)
    {
        return (tokensHouseEdges[token] * payout) / 10000;
    }
}

