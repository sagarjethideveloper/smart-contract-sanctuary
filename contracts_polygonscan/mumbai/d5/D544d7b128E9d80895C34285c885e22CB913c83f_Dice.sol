pragma solidity ^0.8.7;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Address.sol";

import "./IRNGOracle.sol";

import "hardhat/console.sol";

/// @title The Oracle consumer base contract
/// @author Romuald Hog
abstract contract RNGOracleConsumer is ReentrancyGuard {
    /// This is the Oracle contract
    IRNGOracle private _rngOracle;

    uint256 private _paidGas;

    event MigrateOracle(address oldRNGOracle, address newRNGOracle);
    event PayGas(address oracleNodeAccount, uint256 gas);

    /// Caller is not the Oracle contract
    /// @param caller is the caller
    /// @param rngOracle is the Oracle contract address.
    error WrongCaller(address caller, address rngOracle);

    constructor(address rngOracle) {
        _rngOracle = IRNGOracle(rngOracle);
    }

    /// @dev Only rngOracle can reveal the result
    modifier onlyOracle() {
        if (msg.sender != address(_rngOracle)) {
            revert WrongCaller(msg.sender, address(_rngOracle));
        }
        _;
    }

    function _requestRandomness() internal returns (uint32) {
        return _rngOracle.requestRandomness();
    }

    function _addPaidGas(uint256 gas) internal {
        _paidGas += gas;
    }

    /// @dev `nonReentrant` couldn't be applied here as it's called from a super `nonReentrant` function
    function _transferPaidGas(uint256 betResolutionGas) internal {
        IRNGOracle oracle = IRNGOracle(_rngOracle);
        address oracleSignerAccount = oracle.getRoleMember(
            keccak256("SIGNER_ROLE"),
            0
        );
        // If oracle node account's balance is less than specified amount, transfer the collected gas amount
        if (
            oracleSignerAccount.balance <
            tx.gasprice *
                betResolutionGas *
                oracle.getSignerBalanceMinResolutionsAmount()
        ) {
            Address.sendValue(payable(oracleSignerAccount), _paidGas);
            emit PayGas(oracleSignerAccount, _paidGas);
            _paidGas = 0;
        }
    }

    function _getNewRandomnessFromEntropy(uint256 entropy)
        internal
        view
        returns (uint256 randomness)
    {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        entropy,
                        block.coinbase,
                        block.timestamp,
                        block.gaslimit,
                        block.difficulty,
                        gasleft()
                    )
                )
            );
    }

    function migrateOracle(address _newRNGOracle) external onlyOracle {
        address oldRNGOracle = address(_rngOracle);
        _rngOracle = IRNGOracle(_newRNGOracle);
        emit MigrateOracle(oldRNGOracle, _newRNGOracle);
    }

    function receiveRandomness(uint256 entropy, uint32 requestId)
        external
        virtual;
}

