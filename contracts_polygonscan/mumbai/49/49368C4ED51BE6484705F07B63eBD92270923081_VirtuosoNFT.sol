    function _burn(uint256 tokenId) internal override (ERC721, ERC721URIStorage) {

             delete isBlocked[tokenId];
             uint saleID = salesIndex[tokenId];
             if( saleID != 0) 
             { 
                 if( sales[saleID].status == SaleStatus.ON_APPROVAL ) { sales[saleID].status = SaleStatus.CANCELED; onApproval--; canceled++; }
                 if( sales[saleID].status == SaleStatus.ON_SALE ) { sales[saleID].status = SaleStatus.CANCELED; onSale--; canceled++; }
             }
             ERC721URIStorage._burn(tokenId);   
    }
    
    
