//import "hardhat/console.sol";
import "@openzeppelin/contracts/metatx/ERC2771Context.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Pausable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";


//import "@openzeppelin/contracts/access/Ownable.sol";
//learn more: https://docs.openzeppelin.com/contracts/3.x/erc721

// GET LISTED ON OPENSEA: https://testnets.opensea.io/get-listed/step-two




contract VirtuosoNFT is ERC2771Context, ERC721URIStorage, ERC721Enumerable, ERC721Pausable {

  using Counters for Counters.Counter;
  Counters.Counter private _tokenIds;
  
  //owner
  
  address private _owner;
  address private _owner2;
  address private _owner3;
  address private _escrow; // Can add unlockable content later in case there is no public key
  
  // Address properties
  mapping(address => string) public publicKeys;
  mapping(address => bool) private _publicKeyWritten;
  mapping(address => int256) public virtuosoBalances; // decimals: 2, for example 200 => 2.00
  
  int256 private _welcomeBalance;
  int256 private _mintPrice;
  
  event PermanentURI(string _value, uint256 indexed _id);
  event Balance(address indexed _to, int256 _value, string description);
  event OnSale(uint256 _id);
  event OnMint(uint256 _id);
  event SaleApproved(uint256 _id);
  event Sold(uint256 _id);
  
  // Freeze metadatain arweave
  mapping(string => string) public frozenMetadata; // IPFS hash => arweave hash
  
   // License agreements 
  struct License {
        string name;
        string URL;
    }
    
   License[] public licenses;
   mapping (uint256 => mapping(address => string)) private _signatures; //Signatures of licenses licenceId => address => signature

// Token sales metadata
  enum SaleStatus{ ON_APPROVAL, ON_SALE, SOLD, CANCELED }
  struct Sale {
        uint256 tokenID;
        SaleStatus status;
        string ipfsHash; // IPFS hash with JSON containing sale conditions
    }

   Sale[] public sales;  
   mapping(uint256 => uint256) public salesIndex; // tokenID => sales
   uint public onApproval;
   uint public onSale;
   uint public sold;
   uint public canceled;
   
  // Whitelisted for sale without prior approval accounts
  mapping(address => bool) private _whitelisted;  
  address[] private _whitelisted_list;
  
  // Moderator accounts
  mapping(address => bool) private _moderators;
  address[] private _moderators_list;
  
 // Tokens metadata:
 // tokens that were blocked and cannot be displayed or sold 
  mapping(uint256 => bool) public isBlocked; 
  
  // Parent - child token relationships
  struct ParentToken {
    uint totalChildTokens;
    uint[] childs;
  }
  
  ParentToken[] public parentTokens; // all parent tokens
  mapping(uint256 => uint256) public parentTokensIndex; // parent tokenId => ParentToken index
  mapping(uint256 => uint256) public parents; // child tokenId => parent tokenId  

 // Mapping from tokens to owners and operators IPFS hash with unlockable content keys
  mapping(uint256 => mapping(address => string)) private _unlockableContentKey;
  mapping(uint256 => mapping(address => bool)) private _unlockableContentOnEscrow;

  constructor(address owner)
   ERC2771Context(0x4d4581c01A457925410cd3877d17b2fd4553b2C5) //  forwarder //localhost (0x9fE46736679d2D9a65F0992F2272dE9f3c7fa6e0) mumbai (0x4d4581c01A457925410cd3877d17b2fd4553b2C5)
   ERC721("Virtuoso NFT", "VRT1" )
   ERC721URIStorage()
   ERC721Enumerable()
   ERC721Pausable()
   {
        _welcomeBalance = 500;
        _owner = owner;
        onApproval = 0;
        onSale = 0;
        sold = 0;
        canceled = 0;
        _mintPrice = 100;
        sales.push();
   }
   
function getVirtuosoVersion() public pure
    returns (uint)
{
        return 100; // 234 = version 2.34
}
   
function getUnlockableContentKey(uint256 tokenId, address adr) public view
    returns (string memory unlockableContentKey)
{
//        require(_isApprovedOrOwner(_msgSender(), tokenId) || (_moderators[_msgSender()] == true), "Virtuoso NFT:  getUnlockableContentKey caller is not owner, approved or moderator");
        return _unlockableContentKey[tokenId][adr];
}


 function virtuosoSafeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        string memory unlockableContentKey,
        bool onEscrow
    ) public whenNotPaused 
{
        uint saleID = salesIndex[tokenId];
        address operator = getApproved(tokenId);
        if( saleID != 0) 
        { 
            if( sales[saleID].status == SaleStatus.ON_APPROVAL ) { sales[saleID].status = SaleStatus.CANCELED; onApproval--; canceled++; }
            if( sales[saleID].status == SaleStatus.ON_SALE ) 
            { 
                sales[saleID].status = SaleStatus.SOLD; 
                onSale--; 
                sold++; 
                emit Sold(tokenId);
            }
        }
        ERC721.safeTransferFrom(from, to, tokenId, "");
        _unlockableContentKey[tokenId][to] = unlockableContentKey;
        _unlockableContentOnEscrow[tokenId][to] = onEscrow;
        _unlockableContentKey[tokenId][from] = "";
        if( operator != address(0) ) _unlockableContentKey[tokenId][operator] = "";
}

function addUnlockableContent(
        address to,
        uint256 tokenId,
        string memory unlockableContentKey) public whenNotPaused 
{
        require(_msgSender() == _escrow, "Virtuoso NFT addUnlockableContent: not a _escrow"); 
        require(_unlockableContentOnEscrow[tokenId][to]==true, "Virtuoso NFT addUnlockableContent: unlockable content is not in escrow"); 
        require(_publicKeyWritten[to] == true, "Virtuoso NFT addUnlockableContent: no public key"); 

        _unlockableContentKey[tokenId][to] = unlockableContentKey;
        _unlockableContentOnEscrow[tokenId][to]=false;
}


function changeVirtuosoBalance(address to, int256 changeBalance, string memory description) public whenNotPaused
{
        require((_moderators[_msgSender()] == true) || _msgSender() == _owner , "Virtuoso NFT changeVirtuosoBalance: not a moderator or owner"); 
        int oldBalance = virtuosoBalances[to];
        virtuosoBalances[to] = oldBalance + changeBalance;
        emit Balance(to, changeBalance, description );
}


function sell( uint256 tokenId, string memory ipfsHash, address approvedOperator, string memory unlockableContentKey) public whenNotPaused
    {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "Virtuoso NFT sell: caller is not owner nor approved");
        uint saleID = salesIndex[tokenId];
        if( saleID != 0) 
        { 
            if( sales[saleID].status == SaleStatus.ON_APPROVAL ) { sales[saleID].status = SaleStatus.CANCELED; onApproval--; canceled++; }
            if( sales[saleID].status == SaleStatus.ON_SALE ) { sales[saleID].status = SaleStatus.CANCELED; onSale--; canceled++; }
        }


       _unlockableContentKey[tokenId][approvedOperator] = unlockableContentKey;
        emit OnSale(tokenId);
        
        sales.push();
        uint newSale = sales.length - 1;
        sales[newSale].tokenID = tokenId ;
        sales[newSale].ipfsHash = ipfsHash ;
        if( _whitelisted[_msgSender()] == true)
        {
            sales[newSale].status = SaleStatus.ON_SALE ; 
            onSale++;            
        }
        else 
        {
            sales[newSale].status = SaleStatus.ON_APPROVAL ;
            onApproval++;
        }
        salesIndex[tokenId] = newSale;
        approve(approvedOperator, tokenId);
        
    }
    
   function approveSale(uint256 tokenId)
      public whenNotPaused
  {
     require(_moderators[_msgSender()] == true, "Virtuoso NFT approveSale: not a moderator");  
     uint saleID = salesIndex[tokenId];
     require( saleID!= 0 && sales[saleID].status == SaleStatus.ON_APPROVAL, "Virtuoso NFT approveSale: not on approval");  
     sales[saleID].status = SaleStatus.ON_SALE;
     onSale++;
     onApproval--;
     emit SaleApproved(tokenId);
  }    
    
 function cancelSale( uint256 tokenId) public whenNotPaused
    {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "Virtuoso NFT cancelSale: sale caller is not owner nor approved");
        uint saleID = salesIndex[tokenId];
        require( saleID!= 0, "Virtuoso NFT: not on sale");  

        if( sales[saleID].status == SaleStatus.ON_APPROVAL ) { sales[saleID].status = SaleStatus.CANCELED; onApproval--; canceled++; }
        if( sales[saleID].status == SaleStatus.ON_SALE ) { sales[saleID].status = SaleStatus.CANCELED; onSale--; canceled++; }

    }
    
       


