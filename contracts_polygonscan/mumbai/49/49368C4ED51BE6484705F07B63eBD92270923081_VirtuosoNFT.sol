  function mintItem(address to, string memory newTokenURI, string memory unlockableContentKey, bool onEscrow)
      public whenNotPaused
      returns (uint256)
  {
      int oldBalance = virtuosoBalances[_msgSender()];
      require( oldBalance >= _mintPrice, "Virtuoso NFT mintItem: insufficient virtuoso balance to mint");
        
       virtuosoBalances[_msgSender()] = oldBalance - _mintPrice;
       emit Balance(_msgSender(), (0 - _mintPrice), "Mint fee" );
       
      _tokenIds.increment();

      uint256 id = _tokenIds.current();
      _mint(to, id);
      ERC721URIStorage._setTokenURI(id, newTokenURI);

      isBlocked[id] = false;
      _unlockableContentKey[id][to] = unlockableContentKey;
      _unlockableContentOnEscrow[id][to] = onEscrow;
      emit OnMint( id );
      return id;
  }
  
   function mintChildItem(uint256 parentId, address to, string memory newTokenURI, string memory unlockableContentKey, bool onEscrow)
      public whenNotPaused
      returns (uint256)
  {
        require(_exists(parentId), "Virtuoso NFT mintChildItem: parent does not exist");
        uint id = mintItem( to, newTokenURI, unlockableContentKey, onEscrow);
        parents[id] = parentId;
        
        uint parentIndex = parentTokensIndex[parentId];
        if( parentIndex == 0)
        {
            parentTokens.push();
            parentIndex= parentTokens.length-1;
            parentTokens[parentIndex].totalChildTokens=1;
        }
        else
        {
            parentTokens[parentIndex].totalChildTokens++;
        }
        
        parentTokens[parentIndex].childs.push();
        parentTokens[parentIndex].childs[parentTokens[parentIndex].childs.length-1]= id;   
        return id;   
  }
   
  
    function totalParents()
    public view returns (uint256)
{
    return parentTokens.length;
}

    function totalChilds(uint tokenID)
    public view returns (uint256)
{
    uint index = parentTokensIndex[tokenID];
    if(index == 0) return 0;
    else return parentTokens[index].childs.length;
}
    
  
