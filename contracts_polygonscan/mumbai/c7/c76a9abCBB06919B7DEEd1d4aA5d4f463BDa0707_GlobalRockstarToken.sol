pragma solidity ^0.8.0;

import "./ERC1155PermitUpgradeable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

import "hardhat/console.sol";

contract GlobalRockstarToken is
    AccessControlUpgradeable,
    ERC1155PermitUpgradeable,
    UUPSUpgradeable
{
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    uint256 private _supplyLimitForSingleToken;

    // token id -> supply
    mapping(uint256 => uint256) private _tokensSupply;

    bool private _isTransferEnabled;
    event TransferEnabledStatusChanged(bool indexed isTransferEnabled);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(
        string memory _baseURI,
        address _minterAddress,
        bool isTransferEnabled
    ) public initializer {
        ERC1155PermitUpgradeable.initialize(_baseURI);
        _isTransferEnabled = isTransferEnabled;
        _supplyLimitForSingleToken = 1000;
        _setupRole(MINTER_ROLE, _minterAddress);
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    function upgrade(string memory _baseURI) public {
        _setURI(_baseURI);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        view
        override
    {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()),
            "ERC1155: must have miner role to mint"
        );
    }

    function setTransferStatus(bool isTransferEnabled) public {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()),
            "ERC1155: must have admin role to set transfer status"
        );
        _isTransferEnabled = isTransferEnabled;
        emit TransferEnabledStatusChanged(isTransferEnabled);
    }

    function mintBatch(
        uint256 id,
        address[] memory addresses,
        uint256[] memory amounts
    ) public {
        require(
            addresses.length == amounts.length,
            "ERC1155: addresses and amounts length mismatch"
        );

        uint256 numberOfTokensToMint = 0;

        for (uint256 i = 0; i < amounts.length; i++) {
            numberOfTokensToMint += amounts[i];
        }

        require(
            numberOfTokensToMint <= _supplyLimitForSingleToken,
            "ERC1155: Cannot batchmint more than set cap"
        );

        for (uint256 i = 0; i < addresses.length; i++) {
            mint(addresses[i], id, amounts[i]);
        }
    }

    function mint(
        address _to,
        uint256 _id,
        uint256 _amount
    ) public {
        require(
            hasRole(MINTER_ROLE, _msgSender()),
            "ERC1155: must have minter role to mint"
        );
        uint256 currentTokenSupply = _tokensSupply[_id];
        uint256 newTokenSupply = currentTokenSupply + _amount;
        require(
            newTokenSupply <= _supplyLimitForSingleToken,
            "ERC1155: cannot mint more than the set cap"
        );

        _tokensSupply[_id] = newTokenSupply;

        _mint(_to, _id, _amount, "");
    }

    function transfer(
        address from,
        address to,
        uint256 id,
        uint256 amount
    ) public {
        require(_isTransferEnabled, "ERC1155: transfer is disabled");

        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()),
            "ERC1155: caller is not owner nor approved"
        );
        _safeTransferFrom(from, to, id, amount, "");
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(AccessControlUpgradeable, ERC1155Upgradeable)
        returns (bool)
    {
        return
            AccessControlUpgradeable.supportsInterface(interfaceId) ||
            ERC1155Upgradeable.supportsInterface(interfaceId);
    }
}

