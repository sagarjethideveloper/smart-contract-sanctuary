pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "base64-sol/base64.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "@openzeppelin/contracts/utils/Strings.sol";

import { VRFInfoProvider } from "./VRFInfoProvider.sol";

contract VRFBase64SVGNFT is ERC721URIStorage, VRFConsumerBase, Ownable {
    using SafeMath for uint256;

    string internal _SVGEncodingBase = "data:image/svg+xml;base64,";
    string internal _JSONEncodingBase = "data:application/json;base64,";

    struct SVGConfig {
        uint256 maxPaths;
        uint256 maxPathCommands;
        uint256 height;
        uint256 width;
        string[] colors;
        string[] pathCommands;
    }

    SVGConfig _config = _defaultConfig();

    string internal _description;
    uint256 internal immutable TOTAL_SUPPLY;
    VRFInfoProvider _VRFInfoProvider; 

    uint256 internal _currentTokenId;

    bool internal _initialized;
    mapping(bytes32 => uint256) public requestIdToTokenId;
    mapping(uint256 => uint256) public tokenIdToRandomNumber;
    uint256 internal _requestsFulfilled;

    function requestsFulfilled() external view returns(uint256) {
        return _requestsFulfilled;
    }

    event VRFRequest(uint256 indexed tokenId, bytes32 indexed requestId);
    event VRFRequestFullfilled(uint256 indexed tokenId, bytes32 indexed requestId, uint256 randomNumber); 

    constructor(
        string memory name_, 
        string memory symbol_, 
        string memory description_, 
        uint256 totalSupply_,
        VRFInfoProvider VRFInfoProvider_
    ) 
        ERC721(name_, symbol_)
        VRFConsumerBase(VRFInfoProvider_.VRF_COORDINATOR(), address(VRFInfoProvider_.LINK_TOKEN()))
    {      
        _description = description_;
        TOTAL_SUPPLY = totalSupply_;
        _VRFInfoProvider = VRFInfoProvider_;
    }

    function init() external onlyOwner {
        require(!_initialized, "VRFBase64SVGNFT: already initiated");
        _requestRandomNumbers();
        _initialized = true;
    }

    function isInitiated() external view onlyOwner returns(bool) {
       return _initialized;
    }

    function isReadyToMint() external view onlyOwner returns(bool) {
       return _requestsFulfilled == TOTAL_SUPPLY;
    }

    function setConfig(SVGConfig memory newConfig) external onlyOwner {
        _config = newConfig;
    }

    function config() public view returns(SVGConfig memory) {
        return _config;
    }

    function totalSupply() public view returns (uint256)  {
        return TOTAL_SUPPLY;
    }

    function description() public view returns (string memory)  {
        return _description;
    }

    function nextTokenId() external view returns (uint256) {
        return _getNextTokenId();
    }

    function linkBalance() public view returns(uint256 balance) {
        balance = _VRFInfoProvider.LINK_TOKEN().balanceOf(address(this));
    }

    function withdrawLink() external onlyOwner {
        _VRFInfoProvider.LINK_TOKEN().transfer(owner(), linkBalance());
    }

    modifier readyToMint {
        require(_initialized && _requestsFulfilled == TOTAL_SUPPLY, "VRFBase64SVGNFT: not ready to mint");
        _;
    }

    function mint() external readyToMint onlyOwner {
        uint256 tokenId = _getNextTokenId();
        require(tokenId <= TOTAL_SUPPLY, "VRFBase64SVGNFT: total supply reached");
        _mint(tokenId);
        _incrementTokenId();
    }

    function _requestRandomNumbers() internal {
        bytes32 keyHash = _VRFInfoProvider.KEY_HASH();
        uint256 fee = _VRFInfoProvider.FEE();

        for (uint256 tokenId = 1; tokenId <= TOTAL_SUPPLY; tokenId++) {
            bytes32 requestId = requestRandomness(keyHash, fee);
            requestIdToTokenId[requestId] = tokenId;
            emit VRFRequest(tokenId, requestId);
        }
    }

    function _getNextTokenId() internal view returns (uint256) {
        return _currentTokenId.add(1);
    }

    function _incrementTokenId() internal {
        _currentTokenId = _currentTokenId.add(1);
    }

    function _defaultColors() internal pure returns(string[] memory) {
        string[] memory colors = new string[](6);
        colors[0] = "red";
        colors[1] = "blue";
        colors[2] = "green";
        colors[3] = "yellow";
        colors[4] = "black";
        colors[5] = "white";
        return colors;
    }

    function _defaultPathCommands() internal pure returns(string[] memory) {
        string[] memory pathCommands = new string[](2);
        pathCommands[0] = "M";
        pathCommands[1] = "L";
        return pathCommands;
    }

    function _defaultConfig() internal pure returns(SVGConfig memory defaultConfig) {
        defaultConfig.maxPaths = 10;
        defaultConfig.maxPathCommands = 5;
        defaultConfig.height = 250;
        defaultConfig.width = 250;
        defaultConfig.colors = _defaultColors();
        defaultConfig.pathCommands = _defaultPathCommands();
    }

  
    function fulfillRandomness(bytes32 requestId, uint256 randomNumber) internal override {
        uint256 tokenId = requestIdToTokenId[requestId];
        require(tokenIdToRandomNumber[tokenId] == 0, "VRFBase64SVGNFT: request already fulfilled");
        tokenIdToRandomNumber[tokenId] = randomNumber;
        _requestsFulfilled++;
        emit VRFRequestFullfilled(tokenId, requestId, randomNumber);

    }

    function _mint(uint256 tokenId) internal {
        uint256 randomNumber = tokenIdToRandomNumber[tokenId];
        _safeMint(owner(), tokenId);
        string memory encodedSVG = _encodeSVG(_generateSVG(randomNumber));
        _setTokenURI(tokenId, _encodeMetadata(tokenId, encodedSVG));
    }

    function _generateSVG(uint256 randomness) internal view returns (string memory SVG) {
        // We will only use the path element, with stroke and d elements
        uint256 paths = (randomness % _config.maxPaths) + 1;
        SVG = string(abi.encodePacked(
            "<svg xmlns='http://www.w3.org/2000/svg' height='", 
            Strings.toString(_config.height), 
            "' width='", 
            Strings.toString(_config.width), "'>")
        );
        string memory currentPath;
        for(uint i = 0; i < paths; i++) {
            currentPath = _generatePath(uint256(keccak256(abi.encode(randomness, i))));
            SVG = string(abi.encodePacked(SVG, currentPath));
        }
        SVG = string(abi.encodePacked(SVG, "</svg>"));
    }

    function _size() internal view returns(uint256) {
        return _config.height.mul(_config.height).div(2);
    }

    function _generatePath(uint256 randomness) internal view returns(string memory path) {
        uint256 pathCommands = (randomness % _config.maxPathCommands) + 1;
        path = "<path d='";
        for(uint i = 0; i < pathCommands; i++) {
            string memory pathCommand = _generatePathCommand(uint256(keccak256(abi.encode(randomness, _size() + i))));
            path = string(abi.encodePacked(path, pathCommand));
        }
        string memory color = _config.colors[randomness % _config.colors.length];
        path = string(abi.encodePacked(path, "' fill='transparent' stroke='", color,"'/>"));
    }

    function _generatePathCommand(uint256 randomness) internal view returns(string memory pathCommand) {
        pathCommand = _config.pathCommands[randomness % _config.pathCommands.length];
        uint256 paramOne = uint256(keccak256(abi.encode(randomness, _size() * 2))) % _size();
        uint256 paramTwo = uint256(keccak256(abi.encode(randomness, _size() * 2 + 1))) % _size();
        pathCommand = string(abi.encodePacked(pathCommand, " ", Strings.toString(paramOne), " ", Strings.toString(paramTwo)));
    }

    function _encodeSVG(string memory SVG) internal view returns (string memory) {
        string memory encoded = Base64.encode(bytes(string(abi.encodePacked(SVG))));
        return string(abi.encodePacked(_SVGEncodingBase, encoded));
    }

     function _encodeMetadata(uint256 tokenId, string memory encodedSVG) internal view returns (string memory) {
        return string(
                abi.encodePacked(
                    _JSONEncodingBase,
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                name(),
                                '", "symbol":"',
                                symbol(),
                                '", "id":"',
                                Strings.toString(tokenId),
                                '", "image":"',
                                encodedSVG,
                                '", "description":"',
                                description(),
                                '", "attributes":{}}'
                            )
                        )
                    )
                )
            );
    }

}

