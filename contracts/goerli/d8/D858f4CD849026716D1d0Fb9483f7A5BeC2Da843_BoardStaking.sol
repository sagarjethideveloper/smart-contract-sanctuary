import "hardhat/console.sol";

contract BoardStaking {
  uint256 public constant FACTOR = 1e18;
  address public USDT_TOKEN_ADDRESS;
  address public VNLA_TOKEN_ADDRESS;
  address public VNLA_INCENTIVE_ADDRESS;
  uint256 public POOL_VNLA_APR; // Annual percentage rate
  uint256 public POOL_VNLA_MINIMUM_STAKING; // Minimal staking amount

  address public owner;
  uint256 public startBlock;
  uint256 public VNLABalance;
  uint256 public USDTBalance;

  // uint256 constant ONE_YEAR = 1576800; // 60 * 60 * 24 * 365 / 3
  // uint256 constant HALF_YEAR = 788400; // 60 * 60 * 24 * 365 / 6

  // Test Only
  uint256 constant ONE_YEAR = 1000;
  uint256 constant HALF_YEAR = 500;

  // Pool VNLA
  mapping(address => uint256) public poolVNLABalance; // Current balance in the pool VNLA
  mapping(address => uint256) public poolVNLABlock; // Block number when the balance was last updated
  mapping(address => uint256) public poolVNLAInterest; // Interest for the pool VNLA

  modifier onlyOwner {
    require(msg.sender == owner, "Only the owner can perform this action");
    _;
  }

  constructor(
    uint256 _startBlock,
    address _VNLA_TOKEN_ADDRESS,
    address _VNLA_INCENTIVE_ADDRESS,
    uint256 _POOL_VNLA_APR,
    uint256 _POOL_VNLA_MINIMUM_STAKING,
    address _owner
  ) {
    VNLA_TOKEN_ADDRESS = _VNLA_TOKEN_ADDRESS;
    VNLA_INCENTIVE_ADDRESS = _VNLA_INCENTIVE_ADDRESS;
    POOL_VNLA_APR = _POOL_VNLA_APR;
    POOL_VNLA_MINIMUM_STAKING = _POOL_VNLA_MINIMUM_STAKING;
    startBlock = _startBlock;
    owner = _owner;
  }

  /**
   * @notice A method to change owner by current contract owner
   */
  function changeOwner(address _newOwner) public onlyOwner {
    require(((_newOwner != address(0)) && (_newOwner != owner)), 'Owner cannot be neither zero address or current owner');
    owner = _newOwner;
  }

  /**
   * @notice A method to check whether address is a stake holder
   * @return boolean If the address is a stake holder
   */
  function isOwner() public view returns (bool) {
    return msg.sender == owner;
  }

  /**
   * @notice A method to set start block
   * @param _startBlock The start block
   */
  function setStartBlock(uint256 _startBlock) public onlyOwner {
    startBlock = _startBlock;
  }

  /**
   * @notice A method to view start block
   * @return uint256 The start block
   */
  function getStartBlock() public view returns (uint256) {
    return startBlock;
  }

  /**
   * @notice A method to set VNLA incentive address
   * @param _VNLA_INCENTIVE_ADDRESS The VNLA incentive address
   */
  function setVNLAIncentiveAddress(address _VNLA_INCENTIVE_ADDRESS) public onlyOwner {
    VNLA_INCENTIVE_ADDRESS = _VNLA_INCENTIVE_ADDRESS;
  }

  /**
   * @notice A method to view VNLA incentive address
   * @return address The VNLA incentive address
   */
  function getVNLAIncentiveAddress() public view onlyOwner returns (address) {
    return VNLA_INCENTIVE_ADDRESS;
  }

  /**
   * @notice A method to set VNLA minimum staking amount
   */
  function setVNLAMinimumStaking(uint256 _POOL_VNLA_MINIMUM_STAKING) public onlyOwner {
    POOL_VNLA_MINIMUM_STAKING = _POOL_VNLA_MINIMUM_STAKING;
  }

  /**
   * @notice A method to view VNLA minimum staking amount
   * @return uint256 The VNLA minimum staking amount
   */
  function getVNLAMinimumStaking() public view returns (uint256) {
    return POOL_VNLA_MINIMUM_STAKING;
  }

  /**
   * @notice A method to stake the VNLA token into pool
   * @param _amount The amount to stake
   * @return uint256, uint256 The current balance of account and current interest in account
   */
  function stakeVNLA(uint256 _amount) public returns (uint256, uint256) {
    require((block.number > startBlock), "The staking have not begun");
    require((_amount + poolVNLABalance[msg.sender] >= POOL_VNLA_MINIMUM_STAKING), "Less than mininal staking threshold");
    require((_amount <= (IERC20(VNLA_TOKEN_ADDRESS).balanceOf(msg.sender))), "Insufficient balance");
    require((IERC20(VNLA_TOKEN_ADDRESS).allowance(msg.sender, address(this)) >= _amount), "Insufficient allowance");
    require((block.number < startBlock + HALF_YEAR), "The staking period has ended");

    IERC20(VNLA_TOKEN_ADDRESS).transferFrom(msg.sender, address(this), _amount);
    poolVNLAInterest[msg.sender] = queryVNLAProfit(msg.sender);
    poolVNLABalance[msg.sender] += _amount;
    poolVNLABlock[msg.sender] = block.number;
    return (poolVNLABalance[msg.sender], poolVNLAInterest[msg.sender]);
  }

  /**
   * @notice A method to withdraw the principle in staking pool
   * @param _amount The amount to withdraw
   * @return uint256, uint256 The current balance of account and current interest in account
   */
  function withdrawVNLA(uint256 _amount) public returns (uint256, uint256) {
    require((block.number > startBlock), "The staking have not begun");
    require((poolVNLABalance[msg.sender] >= _amount), "Insufficient balance");

    IERC20(VNLA_TOKEN_ADDRESS).transfer(msg.sender, _amount);
    poolVNLAInterest[msg.sender] = queryVNLAProfit(msg.sender);
    poolVNLABalance[msg.sender] = poolVNLABalance[msg.sender] - _amount;
    
    poolVNLABlock[msg.sender] = block.number;
    if(block.number > startBlock + HALF_YEAR) {
      poolVNLABlock[msg.sender] = startBlock + HALF_YEAR;
    }

    return (poolVNLABalance[msg.sender], poolVNLAInterest[msg.sender]);
  }

  /**
   * @notice A method to withdraw profit has been produced 
   * @return uint256, uint256 The current balance of account and current interest in account
   */
  function withdrawVNLAProfit() public returns (uint256, uint256) {
    require((block.number > startBlock + HALF_YEAR), "The lock period has not passed yet");
    require((IERC20(VNLA_TOKEN_ADDRESS).allowance(VNLA_INCENTIVE_ADDRESS, address(this)) >= poolVNLAInterest[msg.sender]), "Insufficient allowance");
    require((IERC20(VNLA_TOKEN_ADDRESS).balanceOf(VNLA_INCENTIVE_ADDRESS) >= poolVNLAInterest[msg.sender]), "Insufficient VNLA tokens");

    // Need calculate profit first
    poolVNLAInterest[msg.sender] = queryVNLAProfit(msg.sender);
    IERC20(VNLA_TOKEN_ADDRESS).transferFrom(VNLA_INCENTIVE_ADDRESS, msg.sender, poolVNLAInterest[msg.sender]);
    // Update Status
    poolVNLAInterest[msg.sender] = 0;
    poolVNLABlock[msg.sender] = block.number;
    if(block.number > startBlock + HALF_YEAR) {
      poolVNLABlock[msg.sender] = startBlock + HALF_YEAR;
    }

    return (poolVNLABalance[msg.sender], poolVNLAInterest[msg.sender]);
  }

  /**
   * @notice A method to query the amount of profit has been produced 
   * @return uint256 the amount of profit
   */
  function queryVNLAProfit(address _address) public view returns (uint256) {
    require((block.number > startBlock), "The staking have not begun");

    (bool _isStakeholder) = isVNLAStakeholder(_address);
    if (_isStakeholder) {
      uint256 senderInterests = 0;
      uint256 stakeDuration = 0;
      if (block.number >= startBlock + HALF_YEAR) {
        stakeDuration = startBlock + HALF_YEAR - poolVNLABlock[_address];
      } else {
        stakeDuration = block.number - poolVNLABlock[_address];
      }
      senderInterests = poolVNLAInterest[_address];
      senderInterests += poolVNLABalance[_address] * stakeDuration / ONE_YEAR * POOL_VNLA_APR / 100;
      return senderInterests;
    }
    return 0;
  }

  /**
   * @notice A method to query the balance of current staking
   * @return uint256 the amount of balance
   */
  function queryVNLABalance(address _address) public view returns (uint256) {
    require((block.number > startBlock), "The staking have not begun");

    return poolVNLABalance[_address];
  }

  /**
   * @notice A method to query if the address is a VNLA stakeholder
   * @param _address The address to query
   * @return bool The result of query
   */
  function isVNLAStakeholder(address _address) public view returns (bool) {
  return (poolVNLABalance[_address] > 0 || poolVNLAInterest[_address] > 0);
  }
}

