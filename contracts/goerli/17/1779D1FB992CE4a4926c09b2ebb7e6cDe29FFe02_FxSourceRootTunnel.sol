pragma solidity ^0.8.0;

import "../token/ERC721/IERC721.sol";
import "../token/ERC721/IERC721Receiver.sol";
import "../ISource.sol";
import "./FxBaseRootTunnel.sol";

/**
 * @title FxSourceRootTunnel
 */
contract FxSourceRootTunnel is FxBaseRootTunnel {
    bytes32 public constant WITHDRAW_SINGLE = keccak256("WITHDRAW_SINGLE");
    bytes32 public constant WITHDRAW_BATCH = keccak256("WITHDRAW_BATCH");
    bytes32 public constant DEPOSIT_SINGLE = keccak256("DEPOSIT_SINGLE");
    bytes32 public constant DEPOSIT_BATCH = keccak256("DEPOSIT_BATCH");
    uint256 public constant BATCH_LIMIT = 20;

    event TokenMap(address indexed rootToken, address indexed childToken);
    event FxWithdrawn(address indexed rootToken, address indexed to, uint256 tokenId);
    event FxDepositing(address indexed rootToken, address indexed depositor, address indexed to, uint256 tokenId);

    mapping(address => address) public rootToChildTokens;

    constructor(address _checkpointManager, address _fxRoot)
        FxBaseRootTunnel(_checkpointManager, _fxRoot) 
    {
    }

    /**
     * @notice Map a token to enable its movement via the PoS Portal
     * @param rootToken address of token on root chain
     * @param childToken address of token on child chain
     */
    function mapToken(address rootToken, address childToken) public {
        // check if token is already mapped
        require(rootToChildTokens[rootToken] == address(0x0), "FxSourceRootTunnel: ALREADY_MAPPED");
        rootToChildTokens[rootToken] = childToken;
        emit TokenMap(rootToken, childToken);
    }

    // DEPOSIT_SINGLE
    function deposit(address rootToken, address receiveAddress, uint256 tokenId, bytes memory tokenData) public {
        require (rootToChildTokens[rootToken] != address(0x0), "FxSourceRootTunnel: NOT_MAPPED");

        uint256 seed = ISource(rootToken).seeds(tokenId);

        // transfer from depositor to this contract
        IERC721(rootToken).transferFrom(
            msg.sender,    // depositor
            address(this),
            tokenId
        );
        emit FxDepositing(rootToken, msg.sender, receiveAddress, tokenId);

        // DEPOSIT_SINGLE, encode(rootToken, depositor, user, tokenId, seed, extra data)
        _sendMessageToChild(abi.encode(DEPOSIT_SINGLE, abi.encode(rootToken, msg.sender, receiveAddress, tokenId, seed, tokenData)));
    }

    // DEPOSIT_BATCH
    function depositBatch(address rootToken, address receiveAddress, uint256[] memory tokenIds, bytes[] memory tokenDatas) public {
        require(tokenIds.length <= BATCH_LIMIT, "FxSourceRootTunnel: EXCEEDS_BATCH_LIMIT");
        require(tokenDatas.length == tokenIds.length || tokenDatas.length == 0, "FxSourceRootTunnel: TOKEN_DATAS_MISMATCHED");
        require(rootToChildTokens[rootToken] != address(0x0), "FxSourceRootTunnel: NOT_MAPPED");

        uint256[] memory seeds = new uint256[](tokenIds.length);

        for (uint i=0; i<tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            seeds[i] = ISource(rootToken).seeds(tokenId);

            // transfer from depositor to this contract
            IERC721(rootToken).transferFrom(
                msg.sender,    // depositor
                address(this),
                tokenId
            );

            emit FxDepositing(rootToken, msg.sender, receiveAddress, tokenId);
        }

        // DEPOSIT_BATCH, encode(rootToken, depositor, user, tokenIds, seeds, extra datas)
        _sendMessageToChild(abi.encode(DEPOSIT_BATCH, abi.encode(rootToken, msg.sender, receiveAddress, tokenIds, seeds, tokenDatas)));
    }

    // exit processor
    function _processMessageFromChild(bytes memory data) internal override {
        // decode incoming data
        (bytes32 syncType, bytes memory syncData) = abi.decode(data, (bytes32, bytes));

        if (syncType == WITHDRAW_SINGLE) {
            _syncWithdrawSingle(syncData);
        }  else if (syncType == WITHDRAW_BATCH) {
            _syncWithdrawBatch(syncData);
        } else {
            revert("FxSourceRootTunnel: INVALID_SYNC_TYPE");
        }
    }

    function genSyncWithdraw(bytes memory syncData) external view returns (address, address, address, uint256, bytes memory) {
        (address rootToken, address childToken, address to, uint256 tokenId, bytes memory tokenData) = abi.decode(syncData, (address, address, address, uint256, bytes));
        // validate mapping for root to child
        require(rootToChildTokens[rootToken] == childToken, "FxSourceRootTunnel: INVALID_MAPPING_ON_EXIT");

        return (rootToken, childToken, to, tokenId, tokenData);
    }

    function genSyncWithdrawBatch(bytes memory syncData) external view returns (address, address, address, uint256[] memory, bytes[] memory) {
        (address rootToken, address childToken, address to, uint256[] memory tokenIds, bytes[] memory tokenDatas) = abi.decode(syncData, (address, address, address, uint256[], bytes[]));
        // validate mapping for root to child
        require(rootToChildTokens[rootToken] == childToken, "FxSourceRootTunnel: INVALID_MAPPING_ON_EXIT");

        return (rootToken, childToken, to, tokenIds, tokenDatas);
    }


    function _syncWithdrawSingle(bytes memory syncData) internal {
        (address rootToken, address childToken, address to, uint256 tokenId, bytes memory tokenData) = abi.decode(syncData, (address, address, address, uint256, bytes));
        // validate mapping for root to child
        require(rootToChildTokens[rootToken] == childToken, "FxSourceRootTunnel: INVALID_MAPPING_ON_EXIT");

        // transfer from token to
        IERC721(rootToken).safeTransferFrom(
            address(this),
            to,
            tokenId, 
            tokenData
        );
        emit FxWithdrawn(rootToken, to, tokenId);
    }

    function _syncWithdrawBatch(bytes memory syncData) internal {
        (address rootToken, address childToken, address to, uint256[] memory tokenIds, bytes[] memory tokenDatas) = abi.decode(syncData, (address, address, address, uint256[], bytes[]));
        // validate mapping for root to child
        require(rootToChildTokens[rootToken] == childToken, "FxSourceRootTunnel: INVALID_MAPPING_ON_EXIT");

        for (uint i=0; i<tokenIds.length; i++) {
            // transfer from tokens to
            IERC721(rootToken).safeTransferFrom(
                address(this),
                to,
                tokenIds[i], 
                tokenDatas.length == tokenIds.length ? tokenDatas[i] : bytes('')
            );
            emit FxWithdrawn(rootToken, to, tokenIds[i]);
        }
    }
}

