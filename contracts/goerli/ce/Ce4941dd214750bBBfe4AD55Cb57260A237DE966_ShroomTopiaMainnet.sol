     * @dev Initializes the contract by setting a `name` and a `symbol` to the token collection.
     */


  // Events
  // event NameChange (uint256 indexed shroomID, string name);
  event MemoChange (uint256 indexed shroomID, string memo);


    constructor () {
      // LibDiamond.DiamondStorage storage ds = LibDiamond.diamondStorage();
      LibDiamond.setContractOwner(msg.sender);

      // adding ERC165 data
      // ds.supportedInterfaces[type(IERC165).interfaceId] = true;
      // ds.supportedInterfaces[type(IERC721).interfaceId] = true;
      // ds.supportedInterfaces[type(IERC721Metadata).interfaceId] = true;
    }

    function initERC721(string memory baseURI_, string memory name_, string memory symbol_) external {
      LibDiamond.enforceIsContractOwner();
      s._name = name_;
      s._symbol = symbol_;
      s._baseURI = baseURI_;

      LibDiamond.DiamondStorage storage ds = LibDiamond.diamondStorage();
      ds.supportedInterfaces[type(IERC165).interfaceId] = true;
      ds.supportedInterfaces[type(IERC721).interfaceId] = true;
      ds.supportedInterfaces[type(IERC721Metadata).interfaceId] = true;
    }


  function currentPrice() public view returns (uint256) {
    require(totalSupply() < MAX_SHROOMS, "ERC721: Sale has already ended");

    uint currentSupply = totalSupply();

    if (currentSupply < 2000) {
      return 20000000000000000;          // 0 - 1999     0.02 ETH
    } else if (currentSupply < 4000) {
      return 40000000000000000;          // 2000-3999:   0.04 ETH
    } else if (currentSupply < 8000) {
      return 80000000000000000;          // 4000-7999:   0.08 ETH
    } else if (currentSupply < 16000) {
      return 160000000000000000;          // 8000-1500:   0.16 ETH
    } else if (currentSupply < 20500) {
      return 320000000000000000;          // 500-1500:   0.32 ETH
    } else if (currentSupply < 23000) {
      return 640000000000000000;          // 500-1500:   0.64 ETH
    } else if (currentSupply < 24500) {
      return 1280000000000000000;          // 500-1500:   1.28 ETH
    } else {
      return 3000000000000000000;          // 500-1500:   0.64 ETH
    }
  } 


  function spawnShrooms(uint256 shroomCount) external payable {
    require(totalSupply() < MAX_SHROOMS, "ERC721: Sale has already ended");
    require(totalSupply() + shroomCount <= MAX_SHROOMS, "ERC721: Exceeds the maximum available shrooms");
    require(msg.value >= currentPrice() * shroomCount, "ERC721: Ether value below sale price");
    
    for (uint i = 0; i < shroomCount; i++) {
      uint mintIndex = totalSupply();
      _safeMint(msg.sender, mintIndex);  
    }
  }

  function tokensOfOwner(address _owner) external view returns(uint256[] memory ) {
    uint256 tokenCount = balanceOf(_owner);

    if (tokenCount == 0) {
      // Return an empty array
      return new uint256[](0);
    } else {
      uint256[] memory result = new uint256[](tokenCount);
      uint256 index;
      for (index = 0; index < tokenCount; index++) {
        result[index] = tokenOfOwnerByIndex(_owner, index);
      }
      return result;
    }
  }

  function setProvenanceHash(string memory _hash) external {
    LibDiamond.enforceIsContractOwner();
    METADATA_PROVENANCE_HASH = _hash;
  }

  function withdraw() external {
    LibDiamond.enforceIsContractOwner();
      uint balance = address(this).balance;
      payable(msg.sender).transfer(balance);
  }

    /**
  * @dev Changes the memo of a Shroom 
  */
  function changeMemo(uint256 tokenId, string memory memo) external {
    address owner = ownerOf(tokenId);
    require(msg.sender == owner, "ERC721: caller is not the owner");

    s._tokenMemo[tokenId] = memo;
    emit MemoChange(tokenId, memo);
  }

    // Base URI
    

