pragma solidity ^0.7.0;

import "./FxBaseRootTunnel.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721Holder.sol";
import "../PolymorphWithGeneChanger.sol";
import "./PolymorphBaseTunnel.sol";

contract PolymorphRootTunnel is
    FxBaseRootTunnel,
    ERC721Holder,
    PolymorphBaseTunnel
{
    constructor(
        address _checkpointManager,
        address _fxRoot,
        address payable _daoAddress
    )
        FxBaseRootTunnel(_checkpointManager, _fxRoot)
        PolymorphBaseTunnel(_daoAddress)
    {}

    modifier onlyOwner(uint256 tokenId) {
        require(
            polymorphContract.ownerOf(tokenId) == msg.sender,
            "Only owner can move polymorph"
        );
        _;
    }

    function _processMessageFromChild(bytes memory data) internal override {
        require(
            address(polymorphContract) != address(0),
            "Polymorph contract hasn't been set yet"
        );
        console.log("Starting to move from polygon to mainnet");
        (
            uint256 tokenId,
            address ownerAddress,
            uint256 gene,
            bool isNotVirgin,
            uint256 genomeChanges
        ) = _decodeMessage(data);

        polymorphContract.approve(ownerAddress, tokenId);
        console.log("Approved to transfer polymorph to owner");

        polymorphContract.safeTransferFrom(
            address(this),
            ownerAddress,
            tokenId
        );

        polymorphContract.wormholeUpdateGene(
            tokenId,
            ownerAddress,
            gene,
            isNotVirgin,
            genomeChanges
        );

        console.log("Transfered polymorph ownership to owner");

        console.log("Moved successfully from polygon to mainnet");
    }

    function moveThroughWormhole(
        uint256 tokenId,
        uint256 gene,
        bool isNotVirgin,
        uint256 genomeChanges
    ) public override onlyOwner(tokenId) {
        console.log("Starting to move from mainnet to polygon");
        //TODO: Approval should happen on the front end

        // polymorphContract.approve(address(this), tokenId);
        // console.log("Approved to transfer polymorph to bridge");
        polymorphContract.safeTransferFrom(msg.sender, address(this), tokenId);
        console.log("Transfered polymorph ownership to bridge");

        _sendMessageToChild(
            abi.encode(tokenId, msg.sender, gene, isNotVirgin, genomeChanges)
        );
        console.log("Message sent through wormhole");
    }
}

