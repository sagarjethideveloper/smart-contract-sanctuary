import "hardhat/console.sol";

pragma solidity 0.7.6;

import "@openzeppelin/contracts/contracts/drafts/ERC20Permit.sol";

contract CrowdTools is ERC20Permit {
  constructor() public ERC20Permit("CrowdTools Token") ERC20("CrowdTools Token", "CrowdTools") {
    _mint(msg.sender, 28000000 * (10 ** uint256(decimals())));
  }
}

