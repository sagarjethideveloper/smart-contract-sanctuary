import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";
import "./Libraries/Math.sol";
import "./interface/ILP.sol";
import "./interface/ICore.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

contract Core is OwnableUpgradeable, ICore {
    struct Bet {
        uint256 conditionID;
        uint256 amount;
        uint256 rate;
        uint256 outcome;
        bool payed;
    }

    struct Condition {
        uint256 reinforcement;
        uint256[2] fundBank;
        uint256[2] prizing;
        uint256 marginality;
        string ipfsHash;
        uint256 outcomeWin;
        uint256 maxPrizes; // maximum sum of prizes to be paid on some result
        uint256 timestamp; // after this time user cant put bet on condition
    }

    uint256 public decimals;
    address public oracle;
    uint256 public conditionalReinforcementFix; // should be 20k
    uint256 public conditionalMarginality;

    address public lpAddress;

    mapping(uint256 => Condition) public conditions;
    mapping(uint256 => Bet) public bets; // tokenID -> BET

    uint256 public lastBetNumber; //start from 1

    event ConditionCreated(uint256 conditionID, uint256 timestamp);
    event ConditionResolved(
        uint256 conditionID,
        uint256 outcomeWin,
        uint256 amountForLP
    );

    // totally locked prizes value - sum of maximum payouts of all execution Condition.
    // on each Condition at betting calculate sum of maximum payouts and put it here
    // after Condition finished on each user payout decrease its value
    uint256 public totallyPrizesLocked;

    modifier onlyOracle {
        require(msg.sender == oracle, "Core:Only Oracle");
        _;
    }

    modifier OnlyLP() {
        require(msg.sender == lpAddress, "Core:Only LP");
        _;
    }

    /**
     * init
     */

    function initialize(
        uint256 reinforcement_,
        address oracle_,
        uint256 marginality_
    ) public virtual initializer {
        __Ownable_init();
        oracle = oracle_;
        conditionalMarginality = marginality_; // in decimals ^9
        conditionalReinforcementFix = reinforcement_; // in token decimals
        decimals = 10**9;
    }

    function getLockedPrizes() external view override returns (uint256) {
        return totallyPrizesLocked;
    }

    function createCondition(bytes memory data) external override onlyOracle {
        require(lpAddress != address(0), "lpAddress!");
        (
            uint256 oracleConditionID,
            uint256[] memory odds,
            uint256 timestamp,
            string memory ipfsHash
        ) = abi.decode(data, (uint256, uint256[], uint256, string));
        uint256 rate1_ = odds[0];
        uint256 rate2_ = odds[1];

        require(
            ILP(lpAddress).getPossibilityOfReinforcement(
                conditionalReinforcementFix
            ),
            "Not enough liquidity"
        );

        Condition storage newCondition = conditions[oracleConditionID];

        newCondition.fundBank[0] =
            (conditionalReinforcementFix * rate1_) /
            (rate1_ + rate2_);
        newCondition.fundBank[1] =
            (conditionalReinforcementFix * rate2_) /
            (rate1_ + rate2_);
        newCondition.reinforcement = conditionalReinforcementFix;
        newCondition.timestamp = timestamp;
        newCondition.ipfsHash = ipfsHash;
        ILP(lpAddress).lockReserve(conditionalReinforcementFix);
        newCondition.marginality = conditionalMarginality; //not used yet
        emit ConditionCreated(oracleConditionID, timestamp);
    }

    function createConditionTest(
        uint256 oracleConditionID,
        uint256 rate1_,
        uint256 rate2_,
        uint256 timestamp,
        string memory ipfsHash
    ) external {
        require(
            ILP(lpAddress).getPossibilityOfReinforcement(
                conditionalReinforcementFix
            ),
            "Not enough liquidity"
        );

        Condition storage newCondition = conditions[oracleConditionID];

        newCondition.fundBank[0] =
            (conditionalReinforcementFix * rate1_) /
            (rate1_ + rate2_);
        newCondition.fundBank[1] =
            (conditionalReinforcementFix * rate2_) /
            (rate1_ + rate2_);
        newCondition.reinforcement = conditionalReinforcementFix;
        newCondition.timestamp = timestamp;
        newCondition.ipfsHash = ipfsHash;
        ILP(lpAddress).lockReserve(conditionalReinforcementFix);

        // save new condition link

        emit ConditionCreated(oracleConditionID, timestamp);
    }

    function putBet(
        uint256 conditionID,
        uint256 amount,
        uint256 outcomeWin,
        uint256 minRate
    )
        external
        override
        OnlyLP
        returns (
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        Condition storage condition = conditions[conditionID];
        require(
            condition.fundBank[1] / condition.fundBank[0] < 10000 &&
                condition.fundBank[0] / condition.fundBank[1] < 10000,
            "Core: Big difference"
        );
        require(
            block.timestamp < condition.timestamp,
            "Core: steak time exceeded"
        );

        require(outcomeWin == 1 || outcomeWin == 2, "Core: Wrong bet outcome");
        lastBetNumber += 1;

        uint256 rate =
            Math.getRateFromBanks(
                condition.fundBank[0],
                condition.fundBank[1],
                amount,
                outcomeWin,
                condition.marginality,
                decimals
            );
        require(rate >= minRate, "Core: rate too small");

        Bet storage newBet = bets[lastBetNumber];

        newBet.rate = rate;
        newBet.amount = amount;
        newBet.outcome = outcomeWin;
        newBet.conditionID = conditionID;

        condition.fundBank[outcomeWin - 1] =
            condition.fundBank[outcomeWin - 1] +
            amount;
        condition.prizing[outcomeWin - 1] += (rate * amount) / decimals;

        // calc maximum prizes value
        uint256 _maxPrizes =
            (
                condition.prizing[0] > condition.prizing[1]
                    ? condition.prizing[0]
                    : condition.prizing[1]
            );
        if (_maxPrizes > condition.maxPrizes) {
            // if new maxPrizes greater than previouse saved -> save new value
            // and add greater delta to global totallyPrizesLocked
            totallyPrizesLocked += (_maxPrizes - condition.maxPrizes);
            condition.maxPrizes = _maxPrizes;
        }
        //emit FundsChange(newBet.conditionID,condition.fund1Bank, condition.fund2Bank);

        return (
            lastBetNumber,
            rate,
            condition.fundBank[0],
            condition.fundBank[1]
        );
    }

    function resolvePrize(uint256 tokenId)
        external
        override
        OnlyLP
        returns (bool success, uint256 amount)
    {
        Bet storage currentBet = bets[tokenId];

        Condition storage condition = conditions[currentBet.conditionID];

        require(condition.outcomeWin != 0, "Event not happened yet");

        // if condition resulted (any result)
        // and exists amount of locked prizes -> release locked prizes from global state
        if (condition.maxPrizes != 0) {
            // decrease global totallyPrizesLocked on prize payment value
            totallyPrizesLocked -= condition.maxPrizes;
            condition.maxPrizes = 0;
        }

        (success, amount) = _viewPrize(tokenId);

        if (success && amount > 0) {
            currentBet.payed = true;
        }

        return (success, amount);
    }

    function resolveCondition(bytes memory data) external override onlyOracle {
        (uint256 conditionID_, uint8 outcomeWin_) =
            abi.decode(data, (uint256, uint8));

        Condition storage condition = conditions[conditionID_];
        // require(now > condition.timestamp, "Azuro: condition cant be resolve before timelimit");
        // todo remove in prod
        // todo Q: should we add additional delay to timelimit
        require(condition.outcomeWin == 0, "Condition already set");
        uint256 bettersPrizing;
        condition.outcomeWin = outcomeWin_;
        if (outcomeWin_ == 1) {
            bettersPrizing = condition.prizing[0];
        } else {
            bettersPrizing = condition.prizing[1];
        }

        uint256 profitReserve =
            (condition.fundBank[0] + condition.fundBank[1]) - bettersPrizing;
        ILP(lpAddress).addReserve(condition.reinforcement, profitReserve);
        emit ConditionResolved(conditionID_, outcomeWin_, profitReserve);
    }

    function resolveConditionTest(uint256 conditionID_, uint256 outcomeWin_)
        external
    {
        Condition storage condition = conditions[conditionID_];
        // require(now > condition.timestamp, "Azuro: condition cant be resolve before timelimit");
        // todo remove in prod
        // todo Q: should we add additional delay to timelimit
        require(condition.outcomeWin == 0, "Condition already set");
        uint256 bettersPrizing;
        condition.outcomeWin = outcomeWin_;
        if (outcomeWin_ == 1) {
            bettersPrizing = condition.prizing[0];
        } else {
            bettersPrizing = condition.prizing[1];
        }
        uint256 profitReserve =
            (condition.fundBank[0] + condition.fundBank[1]) - bettersPrizing;
        ILP(lpAddress).addReserve(condition.reinforcement, profitReserve);
        emit ConditionResolved(conditionID_, outcomeWin_, profitReserve);
    }

    function setLP(address lpAddress_) external override onlyOwner {
        lpAddress = lpAddress_;
    }

    // for tests
    // todo remove in prod
    function setOracle(address oracle_) external onlyOwner {
        oracle = oracle_;
    }

    function viewPrize(uint256 tokenId)
        external
        view
        override
        returns (bool success, uint256 amount)
    {
        return (_viewPrize(tokenId));
    }

    function getconditions(uint256 id)
        external
        view
        returns (Condition memory)
    {
        return (conditions[id]);
    }

    /**
     * internal view, used resolve prize and ezternal views
     * @param tokenId - NFT token id
     */

    function _viewPrize(uint256 tokenId)
        internal
        view
        returns (bool success, uint256 amount)
    {
        Bet storage currentBet = bets[tokenId];
        Condition storage condition = conditions[currentBet.conditionID];

        if (
            !currentBet.payed &&
            (condition.outcomeWin == 1) &&
            (currentBet.outcome == 1)
        ) {
            uint256 winAmount =
                (currentBet.rate * currentBet.amount) / decimals;
            return (true, winAmount);
        }

        if (
            !currentBet.payed &&
            (condition.outcomeWin == 2) &&
            (currentBet.outcome == 2)
        ) {
            uint256 winAmount =
                (currentBet.rate * currentBet.amount) / decimals;
            return (true, winAmount);
        }
        return (false, 0);
    }
}
