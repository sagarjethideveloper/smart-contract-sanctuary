pragma solidity =0.8.3;
pragma abicoder v2;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./HeroInfo.sol";
import "../main/ERC721Tradable.sol";

contract AbbeyOfHeroes is ERC721Tradable, ReentrancyGuard {

    string private _contractURI;
    address private _admin;
    HeroInfo private heroInfo;

    modifier onlyAdmin {
        require(_msgSender() == _admin, "!admin");
        _;
    }

    constructor(address _proxyRegistryAddress, address _heroInfo)
        ERC721Tradable(
            "Abbey of Heroes (for Loot) - DnD complete characters", "AHLoot",
            _proxyRegistryAddress
        )
    {
        _admin = _msgSender();
        heroInfo = HeroInfo(_heroInfo);
    }

    function setAdmin(address __admin) external onlyAdmin {
        _admin = __admin;
    }

    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _tokenId
    ) internal override {
        return super._beforeTokenTransfer(_from, _to, _tokenId);
    }

    function supportsInterface(bytes4 _interfaceId)
        public
        view
        override
        returns(bool)
    {
        return super.supportsInterface(_interfaceId);
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        return heroInfo.build(_tokenId);
    }

    function claim(uint256 tokenId) public nonReentrant {
        require(tokenId >= 0 && tokenId < 7778, "invalidId");
        _safeMint(_msgSender(), tokenId);
    }

    function adminClaim(uint256 tokenId) public nonReentrant onlyAdmin {
        require(tokenId > 7777 && tokenId < 8001, "invalidId");
        _safeMint(owner(), tokenId);
    }

    function contractURI() public view returns(string memory) {
        return _contractURI;
    }

    function setContractURI(string calldata newContractURI) external onlyOwner {
        _contractURI = newContractURI;
    }
}

