pragma solidity ^0.7.6;


library AddressSet {

    struct Set {
        address[]  _values;
        mapping (address => uint256)  _indexes;
    }

    function add (Set storage set, address value) internal returns (bool) {
        if (!_contains(set, value)) {
            set._values.push(value);
            set._indexes[value] = set._values.length;
            return true;
        } else {
            return false;
        }
    }

    function remove (Set storage set, address value) internal returns (bool) {
        uint256 valueIndex = set._indexes[value];
        if (valueIndex != 0) {
            uint256 toDeleteIndex = valueIndex - 1;
            uint256 lastIndex = set._values.length - 1;
            address lastvalue = set._values[lastIndex];
            set._values[toDeleteIndex] = lastvalue;
            set._indexes[lastvalue] = toDeleteIndex + 1;
            set._values.pop();
            delete set._indexes[value];
            return true;
        } else {
            return false;
        }
    }

    function length (Set storage set) internal view returns (uint256) {
        return set._values.length;
    }

    function values (Set storage set) internal view returns (address[] memory) {
        return set._values;
    }

    function at (Set storage set, uint256 index) internal view returns (address) {
        require(set._values.length > index, "AddressSet | set._values.length > index");
        return set._values[index];
    }

    function _contains (Set storage set, address value) private view returns (bool) {
        return set._indexes[value] != 0;
    }
}


