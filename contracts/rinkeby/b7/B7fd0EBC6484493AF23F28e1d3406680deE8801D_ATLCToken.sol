pragma solidity ^0.7.6;
// pragma abicoder v2;


import "@openzeppelin/contracts/utils/Counters.sol";
import "./IERC165.sol";
import "./IERC721.sol";
import "./IERC721Enumerable.sol";
import "./IERC721Metadata.sol";
import "./Library.sol";


contract ATLCToken is IERC165, IERC721, IERC721Enumerable, IERC721Metadata {
    using AddressSet for AddressSet.Set;
    using Counters for Counters.Counter;
    using TokenSet for TokenSet.Set;



    // IERC165

    mapping(bytes4 => bool) private _interfaces;



    // IERC721Metadata

    string private constant _NAME = "ATLCToken";

    string private constant _SYMBOL = "ATLC";

    string private constant _BASE_URI = "https://gateway.pinata.cloud/ipfs/";



    // ATLCToken

    Counters.Counter private _tokenId;

    uint256[] private _tokens;

    struct TokenInfo {
        address payable owner;
        string uri;
        uint8 sharesAvailable;
        uint256 shareCost;
    }

    mapping (uint256 => TokenInfo) private _tokenInfo;

    mapping (uint256 => AddressSet.Set) private _tokenInvestors;

    mapping (address => TokenSet.Set) private _ownerTokens;

    mapping (address => TokenSet.Set) private _investorTokens;

    mapping (bytes32 => uint8) private _tokenInvestorShares;

    uint8 private _fees = 5;



    constructor () public {
        _interfaces[0x01ffc9a7] = true; // IERC165
        _interfaces[0x80ac58cd] = true; // IERC721
        _interfaces[0x5b5e139f] = true; // IERC721Metadata
        _interfaces[0x780e9d63] = true; // IIERC721Enumerable
    }



    // IERC165

    function supportsInterface (bytes4 interfaceId) external view override returns (bool) {
        return _interfaces[interfaceId];
    }



    // IERC721

    function balanceOf (address owner) external view override returns (uint256) {
        return _ownerTokens[owner].length();
    }


    function ownerOf (uint256 tokenId) external view override returns (address) {
        return _tokenInfo[tokenId].owner;
    }


    // solhint-disable-next-line no-unused-vars
    function safeTransferFrom (address from, address to, uint256 tokenId, bytes memory data) external payable override {
        revert("ATLCToken | not supported");
    }


    // solhint-disable-next-line no-unused-vars
    function safeTransferFrom (address from, address to, uint256 tokenId) external payable override {
        revert("ATLCToken | not supported");
    }


    // solhint-disable-next-line no-unused-vars
    function transferFrom (address from, address to, uint256 tokenId) external payable override {
        revert("ATLCToken | not supported");
    }


    // solhint-disable-next-line no-unused-vars
    function approve (address approved, uint256 tokenId) external payable override {
        revert("ATLCToken | not supported");
    }


    // solhint-disable-next-line no-unused-vars
    function setApprovalForAll (address operator, bool approved) external pure override {
        revert("ATLCToken | not supported");
    }


    // solhint-disable-next-line no-unused-vars
    function getApproved (uint256 tokenId) external pure override returns (address) {
        revert("ATLCToken | not supported");
    }


    // solhint-disable-next-line no-unused-vars
    function isApprovedForAll (address owner, address operator) external pure override returns (bool) {
        revert("ATLCToken | not supported");
    }



    // IERC721Enumerable

    function totalSupply () external view override returns (uint256) {
        return _tokens.length;
    }


    function tokenByIndex (uint256 index) external view override returns (uint256) {
        return _tokens[index];
    }


    function tokenOfOwnerByIndex (address owner, uint256 index) external view override returns (uint256) {
        return _ownerTokens[owner].at(index);
    }



    // IERC721Metadata

    function name () external pure override returns (string memory) {
        return _NAME;
    }


    function symbol () external pure override returns (string memory) {
        return _SYMBOL;
    }


    function tokenURI (uint256 tokenId) external view override returns (string memory) {
        return string(abi.encodePacked(_BASE_URI, _tokenInfo[tokenId].uri));
    }



    // ATLCToken

    function baseURI () public pure returns (string memory) {
        return _BASE_URI;
    }


    function createNFT (string memory uri, uint8 sharesAvailable, uint256 shareCost) public {
        require(msg.sender != address(0x0), "ATLCToken | msg.sender != address(0x0)");
        require(sharesAvailable <= 100, "ATLCToken | sharesAvailable <= 100");

        _tokenId.increment();
        uint256 tokenId = _tokenId.current();
        require(!_exists(tokenId), "ATLCToken | !_exists(tokenId)");

        _tokens.push(tokenId);
        _tokenInfo[tokenId] = TokenInfo(msg.sender, uri, sharesAvailable, shareCost);
        _tokenInvestors[tokenId].add(msg.sender);
        _ownerTokens[msg.sender].add(tokenId);
        _investorTokens[msg.sender].add(tokenId);
        _tokenInvestorShares[keccak256(abi.encodePacked(tokenId, msg.sender))] = 100;

        emit Transfer(address(0x0), msg.sender, tokenId);
    }


    function updateNFT (uint256 tokenId, uint8 sharesAvailable, uint256 shareCost) public {
        require(_exists(tokenId), "ATLCToken | _exists(tokenId)");
        require(msg.sender == _tokenInfo[tokenId].owner, "ATLCToken | msg.sender == _tokenInfo[tokenId].owner");
        require(sharesAvailable <= _tokenInfo[tokenId].sharesAvailable, "ATLCToken | shares <= _tokenInfo[tokenId].sharesAvailable");

        _tokenInfo[tokenId].sharesAvailable = sharesAvailable;
        _tokenInfo[tokenId].shareCost = shareCost;
    }


    function buyNFT (uint256 tokenId, uint8 shares) public payable {
        require(_exists(tokenId), "ATLCToken | _exists(tokenId)");
        require(shares <= _tokenInfo[tokenId].sharesAvailable, "ATLCToken | shares <= _tokenInfo[tokenId].sharesAvailable");
        require(msg.value == (shares * _tokenInfo[tokenId].shareCost), "ATLCToken | msg.value == (shares * _tokenInfo[tokenId].shareCost)");

        _tokenInfo[tokenId].sharesAvailable -= shares;
        _tokenInvestors[tokenId].add(msg.sender);
        _investorTokens[msg.sender].add(tokenId);
        _tokenInvestorShares[keccak256(abi.encodePacked(tokenId, _tokenInfo[tokenId].owner))] -= shares;
        _tokenInvestorShares[keccak256(abi.encodePacked(tokenId, msg.sender))] += shares;

        _tokenInfo[tokenId].owner.transfer(msg.value * (100 - _fees) / 100);
    }


    // function token (uint256 tokenId) public view returns (TokenInfo memory) {
    //     return _tokenInfo[tokenId];
    // }


    function tokenInvestors (uint256 tokenId) public view returns (address[] memory) {
        return _tokenInvestors[tokenId].values();
    }


    function tokenInvestorShares (uint256 tokenId, address investor) public view returns (uint8) {
        require(_exists(tokenId), "ATLCToken | _exists(tokenId)");
        return _tokenInvestorShares[keccak256(abi.encodePacked(tokenId, investor))];
    }


    function _exists (uint256 tokenId) internal view virtual returns (bool) {
        return _tokenInfo[tokenId].owner != address(0x0);
    }
}

