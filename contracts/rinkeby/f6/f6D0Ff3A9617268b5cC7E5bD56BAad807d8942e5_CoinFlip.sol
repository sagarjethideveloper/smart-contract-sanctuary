contract CoinFlip is CoreGame, LocalRandom {
    bytes32 public constant CONTRACT_ROLE = bytes32(keccak256("CONTRACT_ROLE"));
    enum BetStatus {
        NOTSTARTED,
        STARTED,
        FINISHED
    }

    event NewBet(
        address game,
        address indexed player,
        address token,
        uint256 setBetTime,
        uint256 tokenAmount,
        uint256 amountUSD,
        uint256 option
    );

    event Result(
        address game,
        address indexed player,
        uint256 indexed gameId,
        bool result,
        uint256 betAmount,
        uint256 setBetTime,
        uint256 payout,
        uint256 side,
        uint256 randomNumber
    );

    struct bet {
        uint256 tokenAmount;
        uint256 amountUSD;
        uint256 timestamp;
        BetStatus status;
        uint8 option;
        address token;
        bool win;
    }

    mapping(address => bet) public playerBet;

    modifier isBetOpen(address _player) {
        require(
            block.timestamp - playerBet[_player].timestamp >= waitPerGame,
            "You have to wait before play again"
        );
        _;
    }

    modifier isBetSet(address _player) {
        require(
            playerBet[_player].status == BetStatus.STARTED,
            "You have to choose bet before play"
        );
        _;
    }

    modifier isBetNotSet(address _player) {
        require(
            playerBet[_player].status != BetStatus.STARTED,
            "You already have a bet now"
        );
        _;
    }

    constructor(
        uint256 minDelay,
        address[] memory proposers,
        address[] memory executors
    ) CoreGame(minDelay, proposers, executors) {
        waitPerGame = 180;
        houseEdge = 2;
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function payoutAmount(address _player) public view returns (uint256) {
        return (playerBet[_player].tokenAmount * 2);
    }

    function won(
        address[] memory _winners,
        uint256[] memory _amounts,
        address[] memory _tokens,
        uint256 _gameId,
        uint256 _headOrTail,
        uint256 _randomNumb
    ) private {
        address _player = _winners[0];
        playerBet[_player].win = true;
        controller.payment(_winners, _amounts, _tokens, address(this));

        emit Result(
            address(this),
            _player,
            _gameId,
            true,
            playerBet[_player].tokenAmount,
            playerBet[_player].timestamp,
            _amounts[0],
            _headOrTail,
            _randomNumb
        );
    }

    /**
     *@notice this function should be access by only admin for security. Because we send seed.
     *@param playAddresses[0] is player
     *@param playVar[0] is seed
     */

    function play(
        address[] memory playAddresses,
        uint256[] memory playVar,
        bytes32[] memory hashes
    ) external onlyRole(TIMELOCK_ADMIN_ROLE) isBetSet(playAddresses[0]) {
        address player = playAddresses[0];
        uint256 seed = playVar[0];
        uint256 randomNumber = getRandom(100 + houseEdge, seed) + 1;
        uint8 headOrTail = playerBet[player].option;
        playerBet[player].status = BetStatus.FINISHED;

        uint256[] memory amounts = new uint256[](playAddresses.length);
        address[] memory winners = new address[](playAddresses.length);
        address[] memory tokens = new address[](playAddresses.length);

        winners[0] = player;
        amounts[0] = payoutAmount(player);
        tokens[0] = playerBet[player].token;

        if (randomNumber >= 50 && randomNumber <= 100 && headOrTail == 1) {
            won(winners, amounts, tokens, gameId, headOrTail, randomNumber);
        } else if (randomNumber >= 1 && randomNumber <= 50 && headOrTail == 0) {
            won(winners, amounts, tokens, gameId, headOrTail, randomNumber);
        }
        if (playerBet[player].win == false) {
            emit Result(
                address(this),
                player,
                gameId,
                false,
                playerBet[player].tokenAmount,
                playerBet[player].timestamp,
                0,
                headOrTail,
                randomNumber
            );
        }
    }

    /**
    @param adresses[0] is player
    @param adresses[1] is token
    @param gameVar[0] is amountUSD
    @param gameVar[1] is tokenAmount
    @param gameVar[2] is option
 
    */
    function setBet(
        address[] memory adresses,
        uint256[] memory gameVar,
        bytes32[] memory hashes
    )
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
        isBetOpen(adresses[0])
        isBetNotSet(adresses[0])
    {
        address player = adresses[0];
        address token = adresses[1];

        uint256 amountUSD = gameVar[0];
        uint256 tokenAmount = gameVar[1];
        uint8 betOption = uint8(gameVar[2]);

        require(betOption <= 1, "you can only choose heads or tails");
        require(betOption >= 0, "you can only choose heads or tails");

        playerBet[player].tokenAmount = tokenAmount;
        playerBet[player].amountUSD = amountUSD;
        playerBet[player].option = betOption;
        playerBet[player].timestamp = block.timestamp;
        playerBet[player].status = BetStatus.STARTED;
        playerBet[player].token = token;
        playerBet[player].win = false;

        emit NewBet(
            address(this),
            player,
            token,
            playerBet[player].timestamp,
            tokenAmount,
            amountUSD,
            betOption
        );
    }
}

// SPDX-License-Identifier: MIT
pragma solidity 0.8.6;

contract LocalRandom {
    function getRandom(uint256 gamerange, uint256 seed)
        public
        view
        virtual
        returns (uint256)
    {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        block.timestamp +
                            block.difficulty +
                            uint256(
                                keccak256(abi.encodePacked(block.coinbase))
                            ) +
                            seed
                    )
                )
            ) % gamerange;
    }
}

