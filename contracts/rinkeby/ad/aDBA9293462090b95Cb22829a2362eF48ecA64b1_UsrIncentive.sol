    function CanDrawAmount(address account) public view returns (uint256 total){
        uint256 total;
        uint256 dayTime = 10;
        uint256 curTime = currentBlockTimestamp();
        for (uint32 i = 0; i < curRoundIndex; i++) {
            uint32 nTimes = RoundsInfo[i][account].nTimes;
