pragma solidity ^0.8.0;
import '@openzeppelin/contracts/utils/Strings.sol';
import '@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol';
import '@openzeppelin/contracts/access/AccessControl.sol';
import '@openzeppelin/contracts/proxy/utils/Initializable.sol';
import 'prb-math/contracts/PRBMathSD59x18.sol';

contract NamelessVecToken is ERC721Enumerable, AccessControl, Initializable {
  using PRBMathSD59x18 for int256;
  event TokenMetadataChanged(uint256 tokenId);

  bytes32 public constant INFRA_ROLE = keccak256('INFRA_ROLE');
  bytes32 public constant MINTER_ROLE = keccak256('MINTER_ROLE');

  // Duplicate Token name for cloneability
  string private _name;
  // Duplicate Token symbol for cloneability
  string private _symbol;

  string private _uriBase;

  address payable public royaltyAddress;
  uint256 public royaltyBps;

  bytes[4] private vecParts;

  function initialize (
    string memory name_,
    string memory symbol_,
    address initialAdmin
  ) public initializer {
    _name = name_;
    _symbol = symbol_;
    _uriBase = 'data:application/json;base64,';
    vecParts[0] = '<svg xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" id=\"fade\" width=\"1000\" height=\"1000\" viewBox=\"-0.5 -0.5 1 1\"><defs><linearGradient id=\"g\" x1=\"0\" x2=\"0\" y1=\"0\" y2=\"1\"><stop id=\"g1\" offset=\"0%\" stop-color=\"hsl(';
    vecParts[1] = ')\"/><stop id=\"g2\" offset=\"50%\" stop-color=\"hsl(';
    vecParts[2] = ')\"/><stop id=\"g3\" offset=\"100%\" stop-color=\"hsl(0,0%,15%)\"/></linearGradient></defs><g><rect id=\"rect\" x=\"-0.5\" y=\"-0.5\" width=\"1\" height=\"1\" fill=\"hsl(0,0%,15%)\"/><circle id=\"circ\" cx=\"0\" cy=\"0\" r=\"';
    vecParts[3] = '\" fill=\"url(#g)\"><animateTransform attributeName=\"transform\" type=\"rotate\" from=\"0\" to=\"360\" dur=\"60s\" repeatCount=\"indefinite\"/></circle></g></svg>';
    _setupRole(DEFAULT_ADMIN_ROLE, initialAdmin);
  }

  constructor(
    string memory name_,
    string memory symbol_
  ) ERC721(name_, symbol_) {
    initialize(name_, symbol_, msg.sender);
  }

  bool public isSealed;

  modifier onlyUnsealed() {
    require(!isSealed, 'tokens are sealed');
    _;
  }

  modifier onlySealed() {
    require(isSealed, 'tokens are not sealed');
    _;
  }

  function sealTokens() public onlyRole(DEFAULT_ADMIN_ROLE) onlyUnsealed {
    isSealed = true;
  }

  function setURIBase(string calldata uriBase_) public onlyRole(INFRA_ROLE) {
    _uriBase = uriBase_;
  }

  /**
    * @dev See {IERC721Metadata-name}.
    */
  function name() public view virtual override returns (string memory) {
    return _name;
  }

  /**
    * @dev See {IERC721Metadata-symbol}.
    */
  function symbol() public view virtual override returns (string memory) {
    return _symbol;
  }

  bytes4 private constant _INTERFACE_ID_FEES = 0xb7799584;

  function getFeeRecipients(uint256) public view returns (address payable[] memory) {
    address payable[] memory result = new address payable[](1);
    result[0] = royaltyAddress;
    return result;
  }

  function getFeeBps(uint256) public view returns (uint256[] memory) {
    uint256[] memory result = new uint256[](1);
    result[0] = royaltyBps;
    return result;
  }

  function setRoyalties( address payable newRoyaltyAddress, uint256 newRoyaltyBps ) public onlyRole(DEFAULT_ADMIN_ROLE) {
    royaltyAddress = newRoyaltyAddress;
    royaltyBps = newRoyaltyBps;
  }

  /*function tokenURI(uint256 tokenId) public view override returns (string memory) {
    require(_exists(tokenId), 'no such token');
    string memory arweaveContentApi;
    string memory ipfsContentApi;
    (arweaveContentApi, ipfsContentApi) = INamelessTemplateLibrary(_templateLibrary).getContentApis();

    if (templateIndex == type(uint256).max) {
      return NamelessMetadataURIV1.makeDataURI(_uriBase, tokenId, ownerOf(tokenId), arweaveContentApi, ipfsContentApi, templateData, templateCode);
    } else {
      bytes32[] memory libraryTemplateData;
      bytes32[] memory libraryTemplateCode;
      (libraryTemplateData, libraryTemplateCode) = INamelessTemplateLibrary(_templateLibrary).getTemplate(templateIndex);
      return NamelessMetadataURIV1.makeDataURI(_uriBase, tokenId, ownerOf(tokenId), arweaveContentApi, ipfsContentApi, libraryTemplateData, libraryTemplateCode);
    }
  }*/

  mapping(uint256 => uint256) private lastTransferTimestamp;
  function processTokenTransferTimeExtension(uint256 tokenId) internal {
    // solhint-disable-next-line not-rely-on-time
    lastTransferTimestamp[tokenId] = block.timestamp;
  }

  function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal virtual override {
    super._beforeTokenTransfer(from, to, tokenId);
    processTokenTransferTimeExtension(tokenId);
    emit TokenMetadataChanged(tokenId);
  }

  function mint(address to, uint256 tokenId) public onlyRole(MINTER_ROLE) onlySealed {
    _safeMint(to, tokenId);
  }

  function mint(address creator, address recipient, uint256 tokenId) public onlyRole(MINTER_ROLE) onlySealed {
    _safeMint(creator, tokenId);
    _safeTransfer(creator, recipient, tokenId, '');
  }

  function calculateCompletion(uint256 start, uint256 duration, uint256 curTime) internal pure returns (int) {
    require(start <= curTime, 'time reversed');
    int offset = int(curTime - start);
    if (offset > int(duration)) {
      return 0;
    }
    return int(1).fromInt() - (offset.fromInt().div(PRBMathSD59x18.fromInt(int(duration))));
  }

  function calculateDistance(int completionFp ) internal pure returns (int) {
    return completionFp.mul(completionFp.mul(completionFp));
  }

  function calculateC1Curve(int completionFp ) internal pure returns (int) {
    int constant075fp = (75 * PRBMathSD59x18.SCALE) / 100;
    return completionFp.pow(constant075fp);
    //return /*PRBMathSD59x18.exp2*/(/*PRBMathSD59x18.mul*/(PRBMathSD59x18.log2(completionFp)/*, constant075fp*/));
  }

  function calculateC2Curve(int completionFp ) internal pure returns (int) {
    int constant125fp = (125 * PRBMathSD59x18.SCALE) / 100;
    return completionFp.pow(constant125fp);
  }

  function calculateHues(address owner, int completionFp) internal pure returns (int, int) {
    uint160 ownerUint = uint160(owner);
    int hue = int256(uint256((ownerUint & 0xFF) * 360));
    int offset = int256(uint256(((ownerUint) >> 8 & 0xFF) * 180));

    int hueFp = hue.fromInt().div(int(255).fromInt()) + completionFp.mul(int(60).fromInt());
    int hue2Fp = hueFp + offset.fromInt().div(int(255).fromInt()) - int(90).fromInt();
    return (hueFp, hue2Fp);
  }

  function calculateShades(int c1curveFp, int c2curveFp ) internal pure returns (int, int) {
    return (c1curveFp.mul(int(100).fromInt()), c2curveFp.mul(int(50).fromInt()));
  }

  function calculateVariances(int c1curveFp, int c2curveFp ) internal pure returns (int, int) {
    return (c1curveFp.mul(int(70).fromInt()) + (int(15).fromInt()), c2curveFp.mul(int(35).fromInt())+(int(15).fromInt()));
  }

  function calculateRadius(int distanceFp ) internal pure returns (int) {
    int constant048fp = (48 * PRBMathSD59x18.SCALE) / 100;
    int constant002fp = (2 * PRBMathSD59x18.SCALE) / 100;
    return distanceFp.mul(constant048fp) + constant002fp;
  }

  struct DrawData {
    int hue1Fp;
    int shade1Fp;
    int variance1Fp;
    int hue2Fp;
    int shade2Fp;
    int variance2Fp;
    int radius;

    int distanceFp;
    int c1curveFp;
    int c2curveFp;
  }

  function fpToString(int value) internal pure returns (string memory) {
    bytes memory sign = '';
    if (value < 0) {
      value = value * -1;
      sign = '-';
    }

    int wholePart = value.toInt();
    string memory wholeStr = Strings.toString(uint(wholePart));

    bytes memory fracPart = new bytes(4);
    uint fracVal = uint(((value - wholePart.fromInt()) * 10000) / PRBMathSD59x18.SCALE);
    for(uint i = 0; i < 4; i ++) {
      fracPart[3 - i] = bytes1(uint8(0x30 + fracVal % 10));
      fracVal /= 10;
    }

    return string(abi.encodePacked(sign, wholeStr, '.', fracPart ));
  }

  function makeData(address wallet, uint progress, uint duration) internal pure returns (DrawData memory) {
    DrawData memory data;
    int completionFp = calculateCompletion(0, duration, progress);
    data.distanceFp = calculateDistance(completionFp);
    data.c1curveFp = calculateC1Curve(completionFp);
    data.c2curveFp = calculateC2Curve(completionFp);
    (data.hue1Fp, data.hue2Fp) = calculateHues(wallet, completionFp);
    (data.shade1Fp, data.shade2Fp) = calculateShades(data.c1curveFp, data.c2curveFp);
    (data.variance1Fp, data.variance2Fp) = calculateVariances(data.c1curveFp, data.c2curveFp);
    data.radius = calculateRadius(data.distanceFp);
    //revert(fpToString(data.distanceFp));

    return data;
  }

  function emitC1(DrawData memory data) internal pure returns (bytes memory) {
    return abi.encodePacked(fpToString(data.hue1Fp), ',', fpToString(data.shade1Fp), '%,', fpToString(data.variance1Fp), '%');
  }

  function emitC2(DrawData memory data) internal pure returns (bytes memory) {
    return abi.encodePacked(fpToString(data.hue2Fp), ',', fpToString(data.shade2Fp), '%,', fpToString(data.variance2Fp), '%');
  }

  function emitSvg(address wallet, uint progress, uint duration) public view returns (string memory) {
    DrawData memory data = makeData(wallet, progress, duration);

    return string(abi.encodePacked(
      vecParts[0],
      emitC1(data),
      vecParts[1],
      emitC2(data),
      vecParts[2],
      fpToString(data.radius),
      vecParts[3]
    ));
  }

  function calcSvgParams(address wallet, uint progress, uint duration) public pure returns (
    string memory g1_h,
    string memory g1_s_pct,
    string memory g1_v_pct,
    string memory g2_h,
    string memory g2_s_pct,
    string memory g2_v_pct,
    string memory radius
  ) {
    DrawData memory data = makeData(wallet, progress, duration);
    g1_h = fpToString(data.hue1Fp);
    g1_s_pct = fpToString(data.shade1Fp);
    g1_v_pct = fpToString(data.variance1Fp);
    g2_h = fpToString(data.hue2Fp);
    g2_s_pct = fpToString(data.shade2Fp);
    g2_v_pct = fpToString(data.variance2Fp);
    radius = fpToString(data.radius);
  }

  /**
    * @dev See {IERC165-supportsInterface}.
    */
  function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721Enumerable, AccessControl) returns (bool) {
    return interfaceId == _INTERFACE_ID_FEES
      || ERC721Enumerable.supportsInterface(interfaceId)
      || AccessControl.supportsInterface(interfaceId);
  }
}

