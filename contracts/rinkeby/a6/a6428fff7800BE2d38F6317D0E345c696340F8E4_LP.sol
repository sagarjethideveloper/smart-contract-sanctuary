pragma solidity ^0.8.0;
import '@openzeppelin/contracts/utils/math/SafeMath.sol';

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./LP.sol";

contract Core is Ownable {

    address public oracle;
    uint public currentConditionID;
    uint public conditionalReinforcementRate; // 10%
    uint public conditionalMarginality; //not used yet
    uint public totalSupply;
    address public lpAddress;

    struct Condition {
        uint reinforcement;
        uint currentBetID;
        uint fund1Bank;
        uint fund2Bank;
        uint prizing1;
        uint prizing2;
        uint marginality;
        bool result1;
        bool result2;
        mapping (address => mapping (uint => Bet)) bets;
    }


    //uint coinRate = 1900;       // for testing purposes
    uint public decimals = 10**9;
    mapping (uint => Condition) public conditions;

    event BetterLose(address better, uint amount);
    event BetterWin(address better, uint amount);
    event PoolFundedBack(uint team, uint amount);
    event PutBet(uint indexed id,uint team, uint amount, uint rate);

    struct Bet {
        uint amount;
        uint coef;
        uint betEvent;
        bool payed;
    }

    constructor (uint reinforcement_, address oracle_, uint marginality_){
        oracle = oracle_;
        conditionalMarginality = marginality_; // in decimals ^9
        conditionalReinforcementRate = reinforcement_; // in decimals ^9
    }

    function createCondition(uint fund1BankRate, uint fund2BankRate ) onlyOracle external {
        require(lpAddress != address(0));
        currentConditionID += 1;
        uint reserveSupply = LP(lpAddress).getReserve();
        uint reinforcement = conditionalReinforcementRate * reserveSupply / decimals;
        Condition storage newCondition = conditions[currentConditionID];
        newCondition.fund1Bank =  reinforcement * fund1BankRate / (fund1BankRate + fund2BankRate);
        newCondition.fund2Bank =  reinforcement * fund2BankRate / (fund1BankRate + fund2BankRate);
        newCondition.reinforcement = reinforcement;
        LP(lpAddress).lockReserve(reinforcement);
        newCondition.marginality = conditionalMarginality; //not used yet
    }

    function putBet(address better, uint conditionID, uint amount_, uint teamWin_ )  public{

        require(msg.sender == lpAddress);

        Condition storage condition = conditions[conditionID];
        condition.currentBetID += 1;
        // todo check teamWins range
        uint rate;

        Bet storage newBet = condition.bets[better][condition.currentBetID];

        if(teamWin_ == 1) {
            rate = getRateFromBanks(condition.fund1Bank, condition.fund2Bank, amount_, teamWin_, condition.marginality);
            newBet.coef = rate;
            newBet.amount = amount_;
            newBet.betEvent = 1;
            condition.fund1Bank = condition.fund1Bank + amount_;
            condition.prizing1 += rate * amount_ / decimals;
            console.log("log PutBet %s %s %s", condition.currentBetID, amount_, rate);
            emit PutBet(condition.currentBetID, teamWin_, amount_, rate);
            return;
        }

        if(teamWin_ == 2) {
            rate = getRateFromBanks(condition.fund1Bank, condition.fund2Bank, amount_, teamWin_, condition.marginality);
            newBet.coef = rate;
            newBet.amount = amount_;
            newBet.betEvent = 2;
            condition.fund2Bank = condition.fund2Bank + amount_;
            condition.prizing2 += rate * amount_ / decimals;
            console.log("log PutBet %s %s %s", condition.currentBetID, amount_, rate);
            emit PutBet(condition.currentBetID, teamWin_, amount_, rate);
            return;
        }
    }

    function resolvePrize(address better, uint conditionID, uint id) public returns (bool success, uint amount) {
        // todo only LP can call
        require(msg.sender == lpAddress);
        Condition storage condition = conditions[conditionID];

        require(condition.result1 || condition.result2, "Event not happened yet");
        Bet storage currentBet = condition.bets[better][id];
        if (condition.result1 && (currentBet.betEvent == 1) && !currentBet.payed) {
            // todo send some money to better
            uint winAmount = currentBet.coef * currentBet.amount / decimals;
            currentBet.payed = true;
            console.log("log BetterWin %s %s", better, winAmount );
            emit BetterWin(better, winAmount);
            return (true, winAmount);
        }

        if (condition.result2 && (currentBet.betEvent == 2) && !currentBet.payed) {
            // todo send some money to better
            uint winAmount = currentBet.coef * currentBet.amount / decimals;
            currentBet.payed = true;
            emit BetterWin(better, winAmount);
            return (true, winAmount);
        }

        console.log("log BetterLose  %s %s ",better , currentBet.amount );
        emit BetterLose(better, currentBet.amount);
        return (false, 0);
    }

    function resolveCondition(uint conditionID, uint teamWin_) onlyOracle external{
        Condition storage condition = conditions[conditionID];

        require(!(condition.result1 || condition.result2), "Condition already set");
        uint bettersPrizing;
        if (teamWin_ == 1) {
            condition.result1 = true;
            bettersPrizing = condition.prizing1;
        } else {
            condition.result2 = true;
            bettersPrizing = condition.prizing2;
        }

        uint profitReserve = (condition.fund1Bank + condition.fund2Bank) -  bettersPrizing;
        LP(lpAddress).addReserve(condition.reinforcement,profitReserve);
        console.log("log PoolFundedBack %s %s ", teamWin_, (condition.fund1Bank + condition.fund2Bank) -  bettersPrizing );
        emit PoolFundedBack(teamWin_, (condition.fund1Bank + condition.fund2Bank) -  bettersPrizing );
    }

    modifier onlyOracle {
        require(msg.sender == oracle);
        _;
    }
    function ceil(uint a, uint m) pure public returns (uint ) {
        return ((a + m - 1) / m) * m;
    }

    function getRateFromBanks(uint fund1Bank_,uint fund2Bank_, uint amount, uint team, uint marginality_) view public returns (uint ) {

        if (team == 1) {
            uint pe1 = (fund1Bank_ + amount) * decimals / (fund1Bank_ + fund2Bank_ + amount);
            uint ps1 = fund1Bank_ * decimals / (fund1Bank_ + fund2Bank_);
            uint cAmount = ceil((amount * decimals / (fund1Bank_ / 100) ), decimals) / decimals; // step?
            uint rate = (decimals ** 3) / ( (pe1 * cAmount + ps1 * 2 - pe1 * 2  ) * decimals / cAmount);
            return addMarginality(rate, marginality_);
        }

        if (team == 2) {
            uint pe2 = (fund2Bank_ + amount) * decimals / (fund1Bank_ + fund2Bank_ + amount);
            console.log(pe2);
            uint ps2 = fund2Bank_ * decimals / (fund1Bank_ + fund2Bank_);
            console.log(ps2);
            uint cAmount = ceil( (amount * decimals / (fund2Bank_ / 100) ), decimals ) / decimals;
            console.log(cAmount);
            uint rate = (decimals ** 3) / ( (pe2 * cAmount  + ps2 * 2  - pe2 * 2) * decimals / cAmount);
            return addMarginality(rate, marginality_);
        }
        return 0;
    }

    function fund1OfCondition(uint id) external view returns (uint) {
        return conditions[id].fund1Bank;
    }

    function fund2OfCondition(uint id) external view returns (uint) {
        return conditions[id].fund2Bank;
    }

    function setLP(address lpAddress_) onlyOwner external {
        lpAddress = lpAddress_;
    }

    function addMarginality(uint rate, uint marginality) view public returns(uint newRate){
        console.log("Rate %s, margin %s", rate, marginality);
        console.log("1 - 1/kef   ", decimals - decimals ** 2 /rate);
        uint revertRate = decimals ** 2 / (decimals - decimals ** 2 /rate);
        console.log("revert rate", revertRate);
        uint marginEUR = decimals + marginality;        // decimals
        console.log("marginEUR rate", marginEUR);
        uint a = marginEUR * (revertRate - decimals) / (rate - decimals);
        console.log("a ", a);

        uint b = (((revertRate - decimals) * decimals / (rate - decimals)) * marginality + decimals * marginality)/decimals;
        console.log("b ", b);


        uint c =   (2 * decimals - marginEUR);
        console.log("c ", c);
        newRate = (sqrt(b ** 2 + 4 * a * c ) -b ) * decimals/ (2 * a) + decimals;
        return newRate;

    }

    function sqrt(uint x) pure public returns (uint y) {
        uint z = (x + 1) / 2;
        y = x;
        while (z < y) {
            y = z;
            z = (x / z + z) / 2;
        }
    }

}

