pragma solidity ^0.8.0;
import '@uniswap/lib/contracts/libraries/TransferHelper.sol';
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import '@openzeppelin/contracts/utils/math/SafeMath.sol';
import "@openzeppelin/contracts/access/Ownable.sol";
import "./Core.sol";



contract LP is ERC20, Ownable{
    using SafeMath  for uint;

    uint public totalReserve;
    uint public lockedReserve;
    address public token;
    address public core;
    uint public BettingPending; // reserve amounts + bets




    event LiquidityAdded(uint amount);
    event LiquidityRemoved(uint amount);


    function changeCore(address addr_) onlyOwner external {
        core =addr_;
    }

    constructor (address token_) ERC20 ("BTTS LP tolen", "lp-BTTS"){
       token = token_ ;
    }

    function addLiquidity(uint _amount)  external {
        TransferHelper.safeTransferFrom(token, msg.sender, address(this), _amount);
        totalReserve = totalReserve.add(_amount);
        //todo
        //        totalReserve += _amount;
//        liquidity = _amount.mul(_totalSupply) / totalReserve;
//        _mint(msg.sender, liquidity);
        emit LiquidityAdded(_amount);
    }

    function withdrawLiquidity(uint _amount) external {
        TransferHelper.safeTransfer(token, msg.sender, _amount);
        totalReserve = totalReserve.sub(_amount);
        //todo
//        totalReserve -= _amount;
//        amount0 = liquidity.mul(balance0) / _totalSupply; // using balances ensures pro-rata distribution

        emit LiquidityRemoved(_amount);
    }

    function withdrawPrize(uint conditionID, uint betID) external {
        (bool success,uint amount) = Core(core).resolvePrize(msg.sender ,conditionID, betID);
        require(success);
        BettingPending = BettingPending.sub(amount);
        TransferHelper.safeTransfer(token, msg.sender, amount);
    }

    function bet(uint conditionID_, uint amount_, uint teamID_) external {
        TransferHelper.safeTransferFrom(token, msg.sender, address(this), amount_);
        BettingPending = BettingPending.add(amount_);
        Core(core).putBet(msg.sender, conditionID_, amount_, teamID_);
    }

    function addReserve(uint initReserve, uint profitReserve) public {
        require(msg.sender == core);

        if (profitReserve >= initReserve) {
            totalReserve = totalReserve.add(profitReserve - initReserve); // pool win
        } else {
            totalReserve = totalReserve.sub(initReserve - profitReserve); // pool lose
        }
        BettingPending = BettingPending.sub(profitReserve);
        lockedReserve = lockedReserve.sub(initReserve);
    }

    // reserve some reinforcement
    function lockReserve(uint amount) public {
        require(msg.sender == core);
        lockedReserve = lockedReserve.add(amount);
        BettingPending = BettingPending.add(amount);
        require(lockedReserve < totalReserve);
    }

    // reserve some reinforcement
    function getReserve() public view returns (uint reserve) {
      return totalReserve;
    }
}

