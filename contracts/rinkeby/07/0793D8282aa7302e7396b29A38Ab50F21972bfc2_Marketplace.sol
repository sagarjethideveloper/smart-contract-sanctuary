pragma solidity ^0.8.0;

import '@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol';

import '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol';
import '@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol';

import './ERC721/CryptoNyteNFT.sol';

contract Marketplace is Initializable, OwnableUpgradeable, ReentrancyGuardUpgradeable, ERC721HolderUpgradeable {
    using SafeMathUpgradeable for uint256;
    using AddressUpgradeable for address;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    CryptoNyteNFT public cryptoNyteNFT;
    IERC20Upgradeable public cryptoNyteToken;

    /// @dev Parameters of an auction
    struct Auction {
        uint256 reservePrice;
        uint256 startBlock;
        uint256 endBlock;
        uint256 higherRate;
        uint256 tokenId;
        bool resulted;
    }

    /// @dev Information about the sender that placed a bit on an auction
    struct HighestBid {
        address bidder;
        uint256 bid;
        uint256 lastBidBlock;
    }

    /// @dev Structure to store NFT information on sale
    struct CARD {
        uint256 quantity;
        uint256 sold;
        uint256 price;
        address pokerPro;
        address artist;
    }

    event AuctionCreated(uint256 indexed tokenId);
    event PauseToggled(bool isPaused);
    event BidPlaced(uint256 indexed tokenId, address indexed bidder, uint256 bid);
    event BidRefunded(address indexed bidder, uint256 bid);
    event AuctionResulted(uint256 indexed tokenId, address indexed winner, uint256 winningBid);
    event AuctionCancelled(uint256 indexed tokenId);

    mapping(uint256 => CARD) public cards;
    mapping(uint256 => Auction) public auctions;
    mapping(uint256 => HighestBid) public highestBids;
    mapping(uint256 => address) public artists;
    mapping(uint256 => address) public pokerPros;

    /// @notice for switching off auction creations, bids and withdrawals
    bool public isPaused;

    modifier whenNotPaused() {
        require(!isPaused, 'Function is currently paused');
        _;
    }

    function initialize(CryptoNyteNFT cryptoNyteTokenNft, IERC20Upgradeable cryptoNyteTokenToken) public initializer {
        __Ownable_init_unchained();
        __Context_init_unchained();
        __ReentrancyGuard_init_unchained();
        __ERC721Holder_init();

        cryptoNyteNFT = cryptoNyteTokenNft;
        cryptoNyteToken = cryptoNyteTokenToken;
    }

    function _blockNow() private view returns (uint256) {
        return block.number;
    }

    /**
     @notice Toggling the pause flag
     @dev Only admin
     */
    function toggleIsPaused() external onlyOwner {
        isPaused = !isPaused;
        emit PauseToggled(isPaused);
    }

    function setCryptoNyteNFT(CryptoNyteNFT cryptoNyteTokenNft) external onlyOwner {
        cryptoNyteNFT = cryptoNyteTokenNft;
    }

    function setCryptoNyteToken(IERC20Upgradeable cryptoNyteTokenToken) external onlyOwner {
        cryptoNyteToken = cryptoNyteTokenToken;
    }

    function isCardPayable(uint256 cardId) external view returns (bool) {
        if (cards[cardId].quantity == cards[cardId].sold) {
            return false;
        }

        return true;
    }

    /**
     * @dev Function to list NFT on Sale
     * only Owners can call it
     * @param cardId is cardId
     * @param quantity is total count of nfts can be sold
     * @param price is the amount of erc20 tokens required to purchase nft
     * @param pokerPro is the address of the poker pro address for payment
     * @param artist is the address of the artist who drew nft
     */
    function addCard(
        uint256 cardId,
        uint24 quantity,
        uint256 price,
        address pokerPro,
        address artist
    ) public onlyOwner {
        require(cards[cardId].price == 0, 'The card is already exists!');

        cards[cardId] = CARD(quantity, 0, price, pokerPro, artist);
    }

    function addCardBatch(
        uint256[] memory _ids,
        uint24[] memory _quantities,
        uint256[] memory _amounts,
        address[] memory _pokerPros,
        address[] memory _artists
    ) public onlyOwner {
        require(_ids.length == _quantities.length && _ids.length == _amounts.length, "Cards aren't consistent!");

        for (uint24 i = 0; i < _ids.length; i++) {
            addCard(_ids[i], _quantities[i], _amounts[i], _pokerPros[i], _artists[i]);
        }
    }

    /**
     * @dev Function to purchase NFT
     * @param cardId - tokenId to purchase
     * @param count - amount to purchase
     */
    function purchaseNFT(
        uint256 cardId,
        uint256 count,
        string calldata rarity,
        string calldata ipfsHash
    ) external {
        address sender = _msgSender();

        CARD storage card = cards[cardId];

        uint256 remaining = card.quantity.sub(card.sold);

        require(remaining > 0, 'Sold out!');
        require(remaining >= count, 'Not enough remaining!');

        uint256 requiredAmount = count.mul(card.price);

        cryptoNyteToken.safeTransferFrom(sender, card.pokerPro, requiredAmount.mul(50).div(100));
        cryptoNyteToken.safeTransferFrom(sender, card.artist, requiredAmount.mul(10).div(100));
        cryptoNyteToken.safeTransferFrom(sender, owner(), requiredAmount.mul(40).div(100));

        for (uint256 i = 0; i < count; i = i.add(1)) {
            pokerPros[cryptoNyteNFT.totalMinted()] = card.pokerPro;
            artists[cryptoNyteNFT.totalMinted()] = card.artist;
            cryptoNyteNFT.mintNFT(sender, cryptoNyteNFT.totalMinted(), rarity, ipfsHash);
        }

        card.sold = card.sold.add(count);
    }

    /**
     * @dev a function to put Legendary NFT for auction.
     */
    function createAuction(
        uint256 _startBlock,
        uint256 _endBlock,
        uint256 _reservePrice,
        uint256 _minHigherRate,
        uint256 _tokenId,
        address _pokerPro,
        address _artist
    ) external onlyOwner {
        require(auctions[_tokenId].endBlock == 0, "MarketPlace.createAuction: Can't relist the NFT!");
        require(_endBlock > _startBlock, 'MarketPlace.createAuction: End block must be greater than start');
        require(_endBlock > _blockNow(), 'MarketPlace.createAuction: End block passed. Nobody can bid.');

        auctions[_tokenId] = Auction({
            reservePrice: _reservePrice,
            startBlock: _startBlock,
            endBlock: _endBlock,
            higherRate: _minHigherRate,
            tokenId: _tokenId,
            resulted: false
        });

        pokerPros[_tokenId] = _pokerPro;
        artists[_tokenId] = _artist;

        emit AuctionCreated(_tokenId);
    }

    /**
     @notice Used for sending back escrowed funds from a previous bid
     @param _currentHighestBidder Address of the last highest bidder
     @param _currentHighestBid Ether or Mona amount in WEI that the bidder sent when placing their bid
     */
    function _refundHighestBidder(address _currentHighestBidder, uint256 _currentHighestBid) private {
        cryptoNyteToken.transfer(_currentHighestBidder, _currentHighestBid);
        emit BidRefunded(_currentHighestBidder, _currentHighestBid);
    }

    /**
     @notice Places a new bid, out bidding the existing bidder if found and criteria is reached
     @dev Only callable when the auction is open
     @dev Bids from smart contracts are prohibited to prevent griefing with always reverting receiver
     @param _tokenId TokenId for the NFT on Auction
     @param _bidAmount Bid amount of the bidder
     */
    function placeBid(uint256 _tokenId, uint256 _bidAmount) external nonReentrant whenNotPaused {
        require(_msgSender().isContract() == false, 'MarketPlace.placeBid: No contracts permitted');

        Auction storage auction = auctions[_tokenId];

        require(_blockNow() >= auction.startBlock && _blockNow() <= auction.endBlock, 'MarketPlace.placeBid: Bidding outside of the auction window');

        HighestBid storage highestBid = highestBids[_tokenId];
        // Ensure bid adheres to outbid increment and threshold
        uint256 minBidRequired = highestBid.bid.mul(100 + auction.higherRate).div(100);

        require(_bidAmount >= minBidRequired, 'MarketPlace.placeBid: Failed to outbid highest bidder');

        cryptoNyteToken.transferFrom(_msgSender(), address(this), _bidAmount);

        // Refund existing top bidder if found
        if (highestBid.bidder != address(0)) {
            _refundHighestBidder(highestBid.bidder, highestBid.bid);
        }

        // assign top bidder and bid time
        highestBid.bidder = _msgSender();
        highestBid.bid = _bidAmount;
        highestBid.lastBidBlock = _blockNow();

        emit BidPlaced(auction.tokenId, _msgSender(), _bidAmount);
    }

    //////////
    // Admin /
    //////////

    /**
     @notice Results a finished auction
     @dev Only owner
     @dev Auction can only be resulted if there has been a bidder and reserve met.
     @dev If there have been no bids, the auction needs to be cancelled instead using `cancelAuction()`
     */
    function resultAuction(uint256 _tokenId, string calldata ipfsHash) external nonReentrant {
        Auction storage auction = auctions[_tokenId];

        // Check the auction real
        require(auction.endBlock > 0, 'MarketPlace.resultAuction: Auction does not exist');

        // Check the auction has ended
        require(_blockNow() > auction.endBlock, 'MarketPlace.resultAuction: The auction has not ended');

        // Ensure auction not already resulted
        require(!auction.resulted, 'MarketPlace.resultAuction: auction already resulted');

        // Get info on who the highest bidder is
        HighestBid storage highestBid = highestBids[_tokenId];

        address winner = highestBid.bidder;
        uint256 winningBid = highestBid.bid;

        require(winner == _msgSender() || _msgSender() == owner(), 'Only owner or winner can call this function!');

        // Ensure auction not already resulted
        require(winningBid >= auction.reservePrice, 'MarketPlace.resultAuction: reserve not reached');

        // Ensure there is a winner
        require(winner != address(0), 'MarketPlace.resultAuction: no open bids');

        // Result the auction
        auction.resulted = true;

        cryptoNyteToken.transfer(pokerPros[0], winningBid.mul(50).div(100));
        cryptoNyteToken.transfer(artists[0], winningBid.mul(10).div(100));

        // Clean up the highest bid
        delete highestBids[_tokenId];

        // Transfer the token to the winner
        cryptoNyteNFT.mintNFT(winner, auction.tokenId, 'Legendary', ipfsHash); // rarity need to be fixed

        emit AuctionResulted(auction.tokenId, winner, winningBid);
    }

    /**
     @notice Cancels and inflight and un-resulted auctions, returning the funds to the top bidder if found
     @dev Only admin
     @param _tokenId is the TokenId of Auction to cancel
     */
    function cancelAuction(uint256 _tokenId) external nonReentrant onlyOwner {
        // Check valid and not resulted
        Auction storage auction = auctions[_tokenId];

        // Check auction is real
        require(auction.endBlock > 0, 'MarketPlace.cancelAuction: Auction does not exist');

        // Check auction not already resulted
        require(!auction.resulted, 'MarketPlace.cancelAuction: auction already resulted');

        // refund existing top bidder if found
        HighestBid storage highestBid = highestBids[_tokenId];
        if (highestBid.bidder != address(0)) {
            _refundHighestBidder(highestBid.bidder, highestBid.bid);

            // Clear up highest bid
            delete highestBids[_tokenId];
        }

        // Remove auction and top bidder
        delete auctions[_tokenId];

        emit AuctionCancelled(auction.tokenId);
    }

    /**
     * @notice Reclaims ERC20 Compatible tokens for entire balance
     * @dev Only access controls admin
     */
    function reclaimERC20() external onlyOwner {
        uint256 balance = cryptoNyteToken.balanceOf(address(this));
        require(cryptoNyteToken.transfer(_msgSender(), balance), 'Transfer failed');
    }
}

