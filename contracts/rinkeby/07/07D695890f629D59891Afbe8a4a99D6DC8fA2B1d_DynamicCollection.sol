pragma solidity ^0.8.6;

import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "../structs/collections/DynamicColData.sol";
import "../structs/characters/Guardian.sol";
import "../library/Base64.sol";
import "../library/NFTmetadata.sol";

enum PowerType {
    BATTLE,
    PORTAL,
    KYBER
}

enum PaymentType {
    TOKEN,
    ETHER
}

contract DynamicCollection is ERC721, VRFConsumerBase, AccessControl {
    using SafeMath for uint256;
    using Strings for string;
    using EnumerableSet for EnumerableSet.UintSet;
    using Strings for uint256;

    struct Request {
        string username;
        address sender;
        uint256 tokenId;
        PowerType[2] powerChoices;
    }

    event Sold(
        address indexed operator,
        address indexed to,
        uint256 indexed id,
        uint256 price,
        uint8 payType
    );

    event Minted(
        address indexed operator,
        address indexed to,
        uint256 indexed id,
        uint256 amount
    );

    event newChar(
        uint256 tokenId,
        uint256 discount,
        uint256 battlePower,
        uint256 portalPower,
        uint256 kyberPower,
        string username,
        string guardClass,
        string race,
        string realm
    );

    event PaymentShared(address account, uint256 amount, uint8 payType);
    event PaymentTreasure(address account, uint256 amount, uint8 payType);
    event ExternalAddressesAdded(address token, address treasury);
    event NewUsdAmount(uint256 amount);
    event NewStartTime(uint256 startTime);
    event NewEndTime(uint256 endTime);

    event NewBattlePow(uint256 tokenId, uint256 newPow);
    event NewPortalPow(uint256 tokenId, uint256 newPow);
    event NewKyberPow(uint256 tokenId, uint256 newPow);
    event NewExperience(uint256 tokenId, uint256 newExp);
    event NewUsername(uint256 tokenId, string newName);

    EnumerableSet.UintSet soldCards;

    IERC20 private token;

    // Constructor variables
    bytes32 internal keyHash;
    string internal cName;
    string public baseUri;

    uint256 public amount;
    uint256 public percent;
    uint256 public available;
    uint256 public sold;
    uint256 public total;
    uint256 internal fee;
    uint256 internal randomResult;
    uint256 public startTime;
    uint256 public endTime;

    address public artist;
    address public VRFCoordinator;
    // rinkeby: 0xb3dCcb4Cf7a26f6cf6B120Cf5A73875B7BBc655B
    address public LinkToken;
    // rinkeby: 0x01BE23585060835E02B77ef475b0Cc51aA1e0709a
    address public facAddress;
    address public dqTreasury;

    GuardianData[] public guardians;
    GuardianData public defGuardian;

    mapping(bytes32 => Request) requests;
    mapping(uint256 => uint256) tokenIdToIndex;

    /**
     * Constructor inherits VRFConsumerBase
     *
     * Network: Rinkeby
     * Chainlink VRF Coordinator address: 0xb3dCcb4Cf7a26f6cf6B120Cf5A73875B7BBc655B
     * LINK token address:                0x01BE23585060835E02B77ef475b0Cc51aA1e0709
     * Key Hash: 0x2ed0feb3e7fd2022120aa84fab1945545a9f2ffc9076fd6156fa96eaff4c1311
     */
    constructor(DynColData memory collecData, GuardianData memory guardianData)
        VRFConsumerBase(collecData.VRFCoordinator, collecData.linkToken)
        ERC721(collecData.name, collecData.symbol)
    {
        _setupRole(DEFAULT_ADMIN_ROLE, collecData.admin);
        _setupRole(DEFAULT_ADMIN_ROLE, collecData.factory);

        baseUri = collecData.baseUri;
        total = collecData.total;
        available = collecData.total;
        startTime = collecData.startTime;
        endTime = collecData.endTime;
        amount = collecData.amount;
        percent = collecData.percent;
        VRFCoordinator = collecData.VRFCoordinator;
        LinkToken = collecData.linkToken;
        keyHash = collecData.keyHash;
        fee = 0.1 * 10**18; // 0.1 LINK

        defGuardian = guardianData;
    }

    /** Contract Setter */
    function setStarTime(uint256 _starTime)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        startTime = _starTime;
        emit NewStartTime(startTime);
    }

    function setEndTime(uint256 _endTime)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        endTime = _endTime;
        emit NewEndTime(endTime);
    }

    function setAmount(uint256 _newAmount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        amount = _newAmount;

        emit NewUsdAmount(amount);
    }

    function setExternalAddresses(
        address _token,
        address _treasury,
        address _artist
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        token = IERC20(_token);
        dqTreasury = _treasury;
        artist = _artist;
        emit ExternalAddressesAdded(_token, _treasury);
    }

    /** Character Setter */
    function setBattlePow(uint256 _tokenId, uint256 _battlePower)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        guardians[getIndex(_tokenId)].battlePower = _battlePower;
        emit NewBattlePow(_tokenId, _battlePower);
    }

    function setPortalPow(uint256 _tokenId, uint256 _portalPower)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        guardians[getIndex(_tokenId)].portalPower = _portalPower;
        emit NewPortalPow(_tokenId, _portalPower);
    }

    function setKyberPow(uint256 _tokenId, uint256 _kyberPower)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        guardians[getIndex(_tokenId)].kyberPower = _kyberPower;
        emit NewKyberPow(_tokenId, _kyberPower);
    }

    function setExperience(uint256 _tokenId, uint256 _experience)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        guardians[getIndex(_tokenId)].experience = _experience;
        emit NewExperience(_tokenId, _experience);
    }

    function setUsername(uint256 _tokenId, string memory _newName)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        guardians[getIndex(_tokenId)].username = _newName;
        emit NewUsername(_tokenId, _newName);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function buy(
        address _buyer,
        uint256 _id,
        string memory _username,
        PowerType[2] memory _choices,
        PaymentType _payType
    ) external {
        require(
            msg.sender == facAddress,
            "This function can only be called by factory contract"
        );
        require(!(soldCards.contains(_id)), "This card already sold");
        require(available > 0, "Sold Out");
        require(
            startTime <= block.timestamp && endTime > block.timestamp,
            "Sale did not start yet"
        );

        if (_payType == PaymentType.TOKEN) {
            _withToken(_buyer);
        } else {
            _withEther(_buyer);
        }

        requestNewRandomCharacter(_username, _buyer, _id, _choices);

        available -= 1;
        sold += 1;
        soldCards.add(_id);

        emit Sold(address(this), _buyer, _id, amount, uint8(_payType));
    }

    function mint(
        address _to,
        uint256 _id,
        string memory _username,
        PowerType[2] memory _choices
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(!(soldCards.contains(_id)), "This card already sold");
        require(available > 0, "Sold Out");

        requestNewRandomCharacter(_username, _to, _id, _choices);

        available -= 1;
        sold += 1;
        soldCards.add(_id);

        emit Minted(address(this), _to, _id, amount);
    }

    function calcPerc(uint256 _amount) public view returns (uint256) {
        uint256 sellmul = SafeMath.mul(_amount, percent);
        uint256 sellAmount = SafeMath.div(sellmul, 10**18);
        return sellAmount;
    }

    function requestNewRandomCharacter(
        string memory _username,
        address _sender,
        uint256 _tokenId,
        PowerType[2] memory _choices
    ) internal onlyRole(DEFAULT_ADMIN_ROLE) returns (bytes32) {
        require(
            LINK.balanceOf(address(this)) >= fee,
            "Not enough LINK - fill contract with faucet"
        );
        bytes32 requestId = requestRandomness(keyHash, fee);

        Request storage request = requests[requestId];
        request.username = _username;
        request.sender = _sender;
        request.tokenId = _tokenId;
        request.powerChoices[0] = _choices[0];
        request.powerChoices[1] = _choices[1];

        return requestId;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomNumber)
        internal
        override
    {
        Request storage request = requests[requestId];

        // powers[0] is batlle power
        // powers[1] is portal power
        // powers[2] is kyber power
        uint64[3] memory powers;

        uint256 tokenId = request.tokenId;
        uint256 experience = 0;

        powers[uint256(request.powerChoices[0])] = uint64(randomNumber % 100);
        powers[uint256(request.powerChoices[1])] = uint64(
            (randomNumber / block.timestamp) % 100
        );

        tokenIdToIndex[tokenId] = guardians.length;

        guardians.push(
            GuardianData(
                request.username,
                defGuardian.gClass,
                defGuardian.race,
                defGuardian.realm,
                defGuardian.discount,
                powers[0],
                powers[1],
                powers[2],
                experience
            )
        );
        _safeMint(request.sender, tokenId);

        emit newChar(
            tokenId,
            defGuardian.discount,
            defGuardian.battlePower,
            defGuardian.portalPower,
            defGuardian.kyberPower,
            request.username,
            defGuardian.gClass,
            defGuardian.race,
            defGuardian.realm
        );
    }

    /** Contract Getter */
    function _baseURI() internal view override returns (string memory) {
        return baseUri;
    }

    // function tokenURI(uint256 tokenId)
    //     public
    //     view
    //     override
    //     returns (string memory)
    // {
    //     (
    //         string memory username,
    //         string memory gClass,
    //         string memory race,
    //         string memory realm,
    //         uint256 discount,
    //         uint256 battlePower,
    //         uint256 portalPower,
    //         uint256 kyberPower,
    //         uint256 experience
    //     ) = getCharacterStats(tokenId);

    //     return
    //         NFTmetadata.generateURI(
    //             NFTmetadata.MetadataParams({
    //                 image: baseUri,
    //                 username: username,
    //                 gClass: gClass,
    //                 race: race,
    //                 realm: realm,
    //                 discount: discount,
    //                 battlePower: battlePower,
    //                 portalPower: portalPower,
    //                 kyberPower: kyberPower,
    //                 experience: experience,
    //                 tokenId: tokenId
    //             })
    //         );
    // }

    function getIndex(uint256 _tokenId) public view returns (uint256) {
        return tokenIdToIndex[_tokenId];
    }

    function getSold() external view returns (uint256) {
        return sold;
    }

    function getNumberOfCharacters() external view returns (uint256) {
        return guardians.length;
    }

    function getCharacterStats(uint256 tokenId)
        public
        view
        returns (
            string memory,
            string memory,
            string memory,
            string memory,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        uint256 index = tokenIdToIndex[tokenId];
        return (
            guardians[index].username,
            guardians[index].gClass,
            guardians[index].race,
            guardians[index].realm,
            guardians[index].discount,
            guardians[index].battlePower,
            guardians[index].portalPower,
            guardians[index].kyberPower,
            guardians[index].experience
        );
    }

    function _withToken(address _buyer) private {
        require(
            token.balanceOf(_buyer) >= amount,
            "Insufficient funds: Cannot buy this NFT"
        );

        uint256 treasAmount = calcPerc(amount);
        uint256 shareAmount = amount - treasAmount;

        token.transferFrom(_buyer, dqTreasury, treasAmount);
        token.transferFrom(_buyer, artist, shareAmount);

        emit PaymentTreasure(_buyer, treasAmount, 0);
        emit PaymentShared(_buyer, shareAmount, 0);
    }

    function _withEther(address _buyer) private {
        // require(
        //     _buyer.balance >= amount,
        //     "Insufficient funds: Cannot buy this NFT"
        // );
        // console.log("msg.sender",msg.sender);
        // (bool success, ) = payable(dqTreasury).call{value: amount}("");

        // require(success, "Revert: There was a problem sending ether");

        uint256 treasAmount = calcPerc(amount);
        uint256 shareAmount = amount - treasAmount;

        emit PaymentTreasure(_buyer, treasAmount, 1);
        emit PaymentShared(_buyer, shareAmount, 1);
    }
}

