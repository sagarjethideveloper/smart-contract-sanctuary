pragma solidity ^0.8.6;

import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "../interfaces/IUserData.sol";
import "../interfaces/IDynamicCollection.sol";
import "../token/ERC721Locked.sol";
import "../structs/collections/DynamicColData.sol";

contract DynamicCollection is
    IDynamicCollection,
    ERC721Locked,
    VRFConsumerBase,
    AccessControl
{
    using Strings for string;
    using EnumerableSet for EnumerableSet.UintSet;
    using Strings for uint256;

    uint256 fee = 0.1 * 10**18;
    bytes32 keyHash;

    IERC20 internal token;
    IUserData public userData;

    EnumerableSet.UintSet soldCards;

    CollectionInfo public collectionInfo;
    PropertiesInfo public propertiesInfo;
    Metadata public metadata;

    mapping(bytes32 => uint256) requests;

    constructor(
        string memory name,
        string memory symbol,
        address userDataAddress,
        CollectionInfo memory _collectionInfo,
        PropertiesInfo memory _propertiesInfo,
        Metadata memory _metadata,
        VRFInfo memory _VRFInfo
    )
        VRFConsumerBase(_VRFInfo.VRFCoordinator, _VRFInfo.linkToken)
        ERC721Locked(name, symbol)
    {
        _setupRole(DEFAULT_ADMIN_ROLE, _collectionInfo.admin);

        userData = IUserData(userDataAddress);

        collectionInfo = _collectionInfo;
        propertiesInfo = _propertiesInfo;
        metadata = _metadata;

        keyHash = _VRFInfo.keyHash;
        fee = _VRFInfo.fee;

        emit Initialize(true);
    }

    function mint(
        address _to,
        uint256 _id,
        string memory _username
    ) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        require(!(soldCards.contains(_id)), "Mint: This card is already sold");
        require(collectionInfo.available > 0, "Mint: Sold out!");

        _createChar(_to, _id, _username);

        collectionInfo.available -= 1;
        collectionInfo.sold += 1;
        soldCards.add(_id);

        requestNewVault(_id);

        emit Minted(address(this), _to, _id, collectionInfo.amount);
    }

    function _createChar(
        address _to,
        uint256 _id,
        string memory _username
    ) private {
        userData.setDynamicStringData(_id, "username", _username);

        _safeMint(_to, _id);

        emit NewCardOwner(_to, _id, _username);
    }

    function requestNewVault(uint256 vaultID)
        internal
        virtual
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (bytes32)
    {
        require(
            LINK.balanceOf(address(this)) >= fee,
            "Not enough LINK - fill contract with faucet"
        );
        bytes32 requestId = requestRandomness(keyHash, fee);

        requests[requestId] = vaultID;

        return requestId;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomNumber)
        internal
        override
    {
        uint256 vaultID = requests[requestId];

        userData.setDynamicUintData(vaultID, "randomNumber", randomNumber);

        emit NewRandomNumber(vaultID, randomNumber);
    }

    /** Setters & Getters */

    function setMarketplace(address _marketplace)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        admin = _marketplace;

        emit NewMarketplace(_marketplace);
    }

    function setMetadata(Metadata memory _metadata)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        metadata = _metadata;

        emit NewMetadata(
            _metadata.imageUrl,
            _metadata.animationUrl,
            _metadata.externalUrl,
            _metadata.backgroundColor
        );
    }

    function getMetadata() public view returns (Metadata memory _metadata) {
        return metadata;
    }

    function setCollectionInfo(CollectionInfo memory _collectionInfo)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        collectionInfo = _collectionInfo;

        emit NewCollectionInfo(
            _collectionInfo.admin,
            _collectionInfo.artist,
            _collectionInfo.baseUrl,
            _collectionInfo.description,
            _collectionInfo.startTime,
            _collectionInfo.endTime,
            _collectionInfo.amount,
            _collectionInfo.sold,
            _collectionInfo.available,
            _collectionInfo.total
        );
    }

    function getCollectionInfo()
        public
        view
        returns (CollectionInfo memory _collectionInfo)
    {
        return collectionInfo;
    }

    function setPropertiesInfo(PropertiesInfo memory _propertiesInfo)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        propertiesInfo = _propertiesInfo;

        emit NewPropertiesInfo(
            _propertiesInfo.cardClass,
            _propertiesInfo.cardName,
            _propertiesInfo.cardType,
            _propertiesInfo.elemental,
            _propertiesInfo.race,
            _propertiesInfo.realm,
            _propertiesInfo.rarity
        );
    }

    function getPropertiesInfo()
        public
        view
        returns (PropertiesInfo memory _propertiesInfo)
    {
        return propertiesInfo;
    }

    function setDynamicStringData(
        uint256 tokenID,
        string memory attribute,
        string memory value
    ) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        userData.setDynamicStringData(tokenID, attribute, value);

        emit NewStringAttribute(tokenID, attribute, value);
    }

    function setDynamicUintData(
        uint256 tokenID,
        string memory attribute,
        uint256 value
    ) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        userData.setDynamicUintData(tokenID, attribute, value);

        emit NewUintAttribute(tokenID, attribute, value);
    }

    function setPower(
        uint256 tokenID,
        string memory power,
        uint256 value
    ) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        userData.setPower(tokenID, power, value);

        if (value == 10) {
            string memory title = userData.getPowerTitle(power);

            userData.setDynamicStringData(tokenID, "title", title);

            emit NewStringAttribute(tokenID, "title", title);
        }

        emit NewUintAttribute(tokenID, power, value);
    }

    function setPowerTitle(string memory power, string memory title)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        userData.setPowerTitle(power, title);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721Locked, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function lockUnlockNFT(uint256 tokenID, bool lockUnlock)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        isLocked[tokenID] = lockUnlock;

        emit TokenLockUnlock(tokenID, lockUnlock);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return collectionInfo.baseUrl;
    }
}

