        vk.delta = Pairing.G2Point([uint256(0x0e17d43e5c229c535c55ab7533fe4689a100445d7b6551359ccfb8c706f3acb8), uint256(0x158a2e52d638d00c9317967330140605bd8ad586822977da01c054f33ba22038)], [uint256(0x021cf6a73339f04d7e78ba4ccccdea6b78b3fe85f299e6abcd7da8536b1c9249), uint256(0x158e329c06b587ae5aaf6b499bed245526c17be8b61f44d348576927a20776f4)]);
