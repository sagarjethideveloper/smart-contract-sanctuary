        vk.gamma_abc[0] = Pairing.G1Point(uint256(0x2f15d36e3dc8f9aa4febc3b8f410e8034847a752ef7c60af185814833648239d), uint256(0x1ebcc3fdd142311c58c2ca0cf46b28ae0224701f59da9482afbf552bfed6d4be));
        vk.gamma_abc[1] = Pairing.G1Point(uint256(0x1c5ddf0f1955726c569f6b62c41e778ad3349636e425574d4fbd2d603b4ca5e9), uint256(0x215afde2252192ebef6357947c3d845371a555c689e8aca3210f7ec6e1d5cce9));
