pragma solidity ^0.8.0;

import '@openzeppelin/contracts/token/ERC721/presets/ERC721PresetMinterPauserAutoId.sol';
import "@openzeppelin/contracts/utils/Counters.sol";
import "./SolnSquareVerifier.sol";
import "./Verifier.sol";

contract PropertyToken is 
ERC721PresetMinterPauserAutoId(
    "Property", 
    "PROP",
    'https://s3-us-west-2.amazonaws.com/udacity-blockchain/capstone/' 
),
SolnSquareVerifier() {

    event PropertyTitleCreated(
        address owner,
        string instructions
    );

    event SolutionAdded (
        address addr,
        bytes32 solutionKey 
    );

    constructor(address payable verifierAddress) {
        verifier = Verifier(verifierAddress);
    }

    function addProof(
        address propertyOwner, 
        uint[2] memory a,
        uint[2][2] memory b,
        uint[2] memory c, 
        uint[2] memory input
    ) public {
        require(hasRole(
            MINTER_ROLE, 
            _msgSender()), 
            "ERC721PresetMinterPauserAutoId: must have minter role to mint"
        );
       bytes32 key = addSolution(a, b, c, input, propertyOwner);
       emit SolutionAdded(propertyOwner, key);
    }
    
    function createPropertyTitle(
        address propertyOwner, 
        bytes32 solutionKey
    )  public {
        //Verify solutionExists and isValid
        require(solutionExistsAndIsValid(solutionKey), "Invalid Solution");
        
        mint(propertyOwner);

        emit PropertyTitleCreated(
            propertyOwner,
            "See previous 'Transfer' Event to obtain the tokenID"
        );
    }
    
}

pragma solidity ^0.8.0;

// TODO define a contract call to the zokrates generated solidity contract <Verifier> or <renamedVerifier>
import "./Verifier.sol";

// TODO define another contract named SolnSquareVerifier that inherits from your ERC721Mintable class
abstract contract SolnSquareVerifier {

    // TODO define a solutions struct that can hold an index & an address
    struct Solution {
        address addr;
        uint[2] a;
        uint[2][2] b;
        uint[2] c; 
        uint[2] input;
        bool exists;
        bool isValid;
    }

    // TODO define a mapping to store unique solutions submitted
    mapping(bytes32 => Solution) private solutions;

    Verifier verifier;

    event SolutionIsValid (
        bytes32 solutionKey
    );

    function addSolution(
        uint[2] memory a,
        uint[2][2] memory b,
        uint[2] memory c, 
        uint[2] memory input,
        address propertyOwner
    ) public returns(bytes32) {
        bytes32 hashedValue = keccak256(abi.encodePacked(a, b, c, input, propertyOwner));
        //  - make sure the solution is unique (has not been used before) 
        require(solutions[hashedValue].exists == false, "Solution has been used previously");

        Solution storage newSolution = solutions[hashedValue];
        newSolution.addr = msg.sender;
        newSolution.a = a;
        newSolution.b = b;
        newSolution.c = c;
        newSolution.input = input;
        newSolution.exists = true;

       if (verifier.verifyTx(newSolution.a, newSolution.b, newSolution.c, newSolution.input)) {
           emit SolutionIsValid(hashedValue);
           newSolution.isValid = true;
       }
       return hashedValue;
    }

    function solutionExistsAndIsValid(bytes32 solutionHash)
        public 
        returns (bool) 
    {
        require(
            solutions[solutionHash].exists && solutions[solutionHash].isValid,
            "zkSNARK proof doesn't exist or is not valid"
        );
        return true;
    }
}

