//utility
import "./Context.sol";
import "./SafeMath.sol";
import "./AccessControlEnumerable.sol";
import "./Address.sol";

import "./IPancakeRouter02.sol";
import "./IPancakeFactory.sol";

//dependable contracts
import "./ERC20Capped.sol";
import "./draft-ERC20Permit.sol";



contract Ragetoken is Context,AccessControlEnumerable ,ERC20Capped,ERC20Permit  {
    using SafeMath for uint256;
    using Address for address;

    string private _name = "Ragetoken";
    string private _symbol = "FUU";
    uint256 private _initialsupply = 2e27;
    uint256 private _supplycap = 10e27;
    
    address private pancakeRouter = 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;
    
    uint256 private _releasetime;
    uint256 private _mintable;
    uint256 private _minttime;
    
    uint256 private _fee_rate = 3000;
    
    IPancakeRouter02 public router;
    address public pair;
    
    event LiquidityProvided(uint256 tokenAmount, uint256 nativeAmount, uint256 exchangeAmount);
    
    
    uint256 private constant _releaserate = 432e18;
    uint256 private constant _releaseratedev = 12e18;
    
    bytes32 public DEV_ROLE = keccak256("DEV_ROLE");
    bytes32 public FEELESS_ROLE = keccak256("FEELESS_ROLE");
    bytes32 public BOT_ROLE = keccak256("BOT_ROLE");
    
    uint256 private _devmint = 200e24;
    
    struct devlock {
        uint256 _amount;
        uint256 _minttime;
    }
    
    mapping(address => devlock) private _dev;
    
    constructor () ERC20(_name,_symbol) ERC20Capped(_supplycap) ERC20Permit (_name){

        _releasetime = block.timestamp;
        _minttime = _releasetime;
        _mintable = _supplycap.sub(_initialsupply);
          
         
         //setup dev address
        _dev[0xd88328Ba5e1c641ECe66c89Ce423853Cbe7D462b]._amount = _devmint;
        _dev[0xBC9D9807f52A24CB18dd6896120DFEA7c0F5974F]._amount = _devmint;
        _dev[0xbcd2C02B3d4f4e6084830C446C4D7F0E260B60e0]._amount = _devmint;
        _dev[0xF4B902c63ec53E6A9A99e5379bb92fdbDcCF1592]._amount = _devmint;
        _dev[0x90aa08ade3E4cdA66d0BB33F91cE6D3dF1E18488]._amount = _devmint;
        _dev[0xEA018a3D9CA4939F3E5186692B46EA4006daEFF1]._amount = _devmint;
        
        _dev[0xd88328Ba5e1c641ECe66c89Ce423853Cbe7D462b]._minttime = _releasetime;
        _dev[0xBC9D9807f52A24CB18dd6896120DFEA7c0F5974F]._minttime = _releasetime;
        _dev[0xbcd2C02B3d4f4e6084830C446C4D7F0E260B60e0]._minttime = _releasetime;
        _dev[0xF4B902c63ec53E6A9A99e5379bb92fdbDcCF1592]._minttime = _releasetime;
        _dev[0x90aa08ade3E4cdA66d0BB33F91cE6D3dF1E18488]._minttime = _releasetime;
        _dev[0xEA018a3D9CA4939F3E5186692B46EA4006daEFF1]._minttime = _releasetime;
        
        _mintable = _mintable.sub(_devmint.mul(6));
        
        _setupRole(DEV_ROLE, 0xd88328Ba5e1c641ECe66c89Ce423853Cbe7D462b);
        _setupRole(DEV_ROLE, 0xBC9D9807f52A24CB18dd6896120DFEA7c0F5974F);
        _setupRole(DEV_ROLE, 0xbcd2C02B3d4f4e6084830C446C4D7F0E260B60e0);
        _setupRole(DEV_ROLE, 0xF4B902c63ec53E6A9A99e5379bb92fdbDcCF1592);
        _setupRole(DEV_ROLE, 0x90aa08ade3E4cdA66d0BB33F91cE6D3dF1E18488);
        _setupRole(DEV_ROLE, 0xEA018a3D9CA4939F3E5186692B46EA4006daEFF1);
        
         
        //set roles to owner
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(DEV_ROLE, _msgSender());
        
        _setupRole(FEELESS_ROLE, pancakeRouter);
        
        //mint initial supply
        _mint(_msgSender(),_initialsupply);
        
        //setup pancake
        IPancakeRouter02 _router = IPancakeRouter02(pancakeRouter);
        router = _router;

        pair = IPancakeFactory(_router.factory()).createPair(address(this), _router.WETH());
    }

    
    /**
     * @dev See {ERC20-_mint}.
     */
    function _mint(address account, uint256 amount) internal virtual override(ERC20,ERC20Capped) {
       super._mint(account, amount);
    }
    
   
    
    function mint(address account, uint256 amount) external {
        require(hasRole(DEV_ROLE, _msgSender()) || hasRole(BOT_ROLE, _msgSender()), "must have minter role to mint");
        //handle how many devs and bot can mint
        if (hasRole(DEV_ROLE,_msgSender())){
            require(_dev[_msgSender()]._amount>=amount, "dev doesn't have enough allowance left");
            require((block.timestamp).sub(_dev[_msgSender()]._minttime).mul(_releaseratedev)>=amount, "dev doesn't have enough allowance right now");
            
            //substract amount and change minttime
            _dev[_msgSender()]._amount=_dev[_msgSender()]._amount.sub(amount);
            //add time to existing minttime
            uint256 _mintt = amount.div(_releaseratedev);
            _dev[_msgSender()]._minttime=_dev[_msgSender()]._minttime.add(_mintt);
        }
        else if(hasRole(BOT_ROLE,_msgSender())){
            require(_mintable>=amount, "amount exceeds cap");
            require((block.timestamp).sub(_minttime).mul(_releaserate)>=amount, "not that many mintable right now");
            
             //substract ammount and change minttime
            _mintable=_mintable.sub(amount);
            //add time to existing minttime
            uint256 _mintt = amount.div(_releaserate);
            _minttime=_minttime.add(_mintt);
        }
        
        _mint(account,amount);
    }
    
    function get_mintable () public view returns (uint256 mintable){
        return _mintable;
    }
    
     function get_currently_mintable () public view returns (uint256 mintable){
        return (block.timestamp).sub(_minttime).mul(_releaserate);
    }
    
   
    //gets how many tokens a dev is able to mint
    function get_dev_stats(address account) public view returns (uint256 amount,uint256 mintable){
        return (_dev[account]._amount,(block.timestamp).sub(_dev[account]._minttime).mul(_releaseratedev));
    }
    


    // transfer any accidentily sent tokens from the contract
    function transferAnyERC20Token(address tokenAddress, uint256 tokens) external returns (bool success) {
        require(hasRole(DEV_ROLE, _msgSender()) , "must be dev");
        require(tokenAddress != address(this), "can't get own tokens");
        
        IERC20 (tokenAddress).transfer(_msgSender(), tokens);
        
        return true;
    }
    
    
    //returns uncollected fees
    function get_fees() public view returns  (uint256 fees){
        return _balances[address(this)];
    }
    
     //change fees
    function change_fees(uint256 new_fee ) external {
        require(hasRole(DEV_ROLE, _msgSender()), "must be dev");
        require(new_fee<_fee_rate, "new fee must be smaller");
        _fee_rate=new_fee;
    }
    

    function _transfer(address sender, address recipient, uint256 amount) internal virtual override {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");

        if (hasRole(FEELESS_ROLE, _msgSender()) || hasRole(BOT_ROLE, _msgSender()) || hasRole(DEV_ROLE, _msgSender())){
            super._transfer(sender,recipient,amount);
        }
        else {
            uint256 senderBalance = _balances[sender];
            require(senderBalance >= amount, "ERC20: transfer amount exceeds balance");
            uint256 fee = amount.mul(_fee_rate).div(100000);
            
            //take fee
            _balances[sender] = senderBalance.sub(fee);
            
            //add fee to contract
            _balances[address(this)] = _balances[address(this)].add(fee);
            
            super._transfer(sender,recipient,amount.sub(fee));
        }
    }
    
    
    //New Pancakeswap router version?
    //No problem, just change it!
    function setRouterAddress(address newRouter) external {
        require(hasRole(DEV_ROLE, _msgSender()), "must be dev");
        IPancakeRouter02 _newPancakeRouter = IPancakeRouter02(newRouter);
        pair = IPancakeFactory(_newPancakeRouter.factory()).createPair(address(this), _newPancakeRouter.WETH());
        router = _newPancakeRouter;
        _setupRole(FEELESS_ROLE, newRouter);
    }
    
   
    function provideLiquidity(uint256 tokenAmount) external {
        require(hasRole(DEV_ROLE, _msgSender()) || hasRole(BOT_ROLE, _msgSender()), "must be dev");
        require(tokenAmount<=_balances[address(this)], "amount exceeds fees");
        
        uint256 exchangeAmount = tokenAmount.div(2);
        
        //get BNB
        uint256 ignore = address(this).balance;
        exchangeTokenToNativeCurrency(exchangeAmount);
        uint256 profit = address(this).balance.sub(ignore);

        addToLiquidityPool(exchangeAmount, profit);
        emit LiquidityProvided(tokenAmount, profit, exchangeAmount);
        
    }

    function exchangeTokenToNativeCurrency(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();

        _approve(address(this), address(router), tokenAmount);
        router.swapExactTokensForETHSupportingFeeOnTransferTokens(tokenAmount, 0, path, address(this), block.timestamp);
    }

    function addToLiquidityPool(uint256 tokenAmount, uint256 nativeAmount) private {
        _approve(address(this), address(router), tokenAmount);
        router.addLiquidityETH{value: nativeAmount}(address(this), tokenAmount, 0, 0, address(0), block.timestamp);
    }
    
    event Received(address, uint);
    receive() external payable {
        emit Received(_msgSender(), msg.value);
    }
    
    function cap() public view virtual override returns (uint256) {
        return _supplycap;
    }
}


