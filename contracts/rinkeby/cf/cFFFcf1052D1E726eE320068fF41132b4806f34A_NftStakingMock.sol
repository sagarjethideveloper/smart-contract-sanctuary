pragma solidity 0.6.8;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/EnumerableSet.sol";

contract Collection is ERC1155, AccessControl {

    event Minted(
        address indexed operator,
        address indexed to,
        uint256 indexed id
    );

    event MintedBatch(
        address indexed operator,
        address indexed to,
        uint256 [] indexed id
    );

   
    mapping(uint256 => uint64) public weightByTokenAttribute;
   
    
    

    using EnumerableSet for EnumerableSet.UintSet;

    EnumerableSet.UintSet soldSeeds;
    string public seedName;
    uint256 public available;
    uint256 public sold;
    uint256 public total;
    address factory;
    constructor( 
        string memory uri,
        string memory _seedName,
        uint256 _total,
        address _admin,
        address _factory
        ) public 
        ERC1155(uri){

        factory = _factory;
        total = _total;
        available = _total;
        seedName = _seedName;

        _setupRole(DEFAULT_ADMIN_ROLE, _admin);
    }

    modifier factoryOrAdmin() {
      require(msg.sender == factory || hasRole(DEFAULT_ADMIN_ROLE, msg.sender) ,"This function can only be called by factory contract or Admin");
      _;
    }

    
    function addCollecAndWeig(uint256[] memory tokenIds, uint64[] memory weights) public factoryOrAdmin {
        require(tokenIds.length == weights.length, "NftStakingMock: inconsistent array lengths");
        for (uint256 i = 0; i < tokenIds.length; ++i) {
           weightByTokenAttribute[tokenIds[i]] = weights[i];
        }
      
    }

    function mint(address _to, uint256 _id) external factoryOrAdmin {
        require(!(soldSeeds.contains(_id)), "This card already sold");
        require(available > 0, "Sold Out");

        _mint(_to, _id, 1, "");

        available -= 1;
        sold += 1;
        soldSeeds.add(_id);

        emit Minted(address(this), _to, _id);
    }

    function mintBatch(address _to, uint256[] calldata _ids,  uint256[] calldata amount_) external factoryOrAdmin {
        require(available > _ids.length, "Sold Out");
        
        for(uint256 i=0; i < _ids.length; i++){
            require(!(soldSeeds.contains(_ids[i])), "This card already sold");

        }
       

        _mintBatch(_to, _ids, amount_, "");

        available -= _ids.length;
        sold += _ids.length;

        for(uint256 i=0; i < _ids.length; i++){
            soldSeeds.add(_ids[i]);
        }
        
        emit MintedBatch(address(this), _to, _ids);
    }

    
   
    

}

