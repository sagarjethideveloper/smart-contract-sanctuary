pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Enumerable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./WeiBlocksCore.sol";

contract WeiBlocksEngineV2 is Ownable {
  using Counters for Counters.Counter;

  struct State {
    address player;
    uint8 moveNumber;
    // white is 1 and black is 2
    uint8 color;
    uint8 stoneId;
    uint8 blackStonesCaptured;
    uint8 whiteStonesCaptured;
    uint8 koPoint;
    uint8 positionX;
    uint8 positionY;
  }

  struct FinishedGame {
    State lastState;
    uint8 color;
    bool minted;
  }

  // board matrix 19x19
  uint16[19][19] private board;

  Counters.Counter private stateCounter;
  Counters.Counter private gamesCounter;

  State[381] internal states;
  FinishedGame[] internal games;

  address private weiBlocksStoneAddress;
  address private weiBlocksNFT;

  uint16 lastMove = 0;

  WeiBlocksCore engine = new WeiBlocksCore();

  event UserInitiated(uint16 indexed _stoneId, address indexed _user);
  event BoardState(address indexed _player, uint16 indexed _stoneId, uint8 indexed _moveNumber);
  event EndGame(uint8 indexed _moveNumber, uint8 indexed _winningColor, address indexed _mover);
  event MintEndedGame(uint256 indexed _no, address indexed _to);

  constructor() {}

  function doMove(uint16[2] calldata coordinates) public {
    engine.engine(coordinates);
  }

  function getGameState() public view returns (Game.GameState memory) {
    return engine.getCurrentGame();
  }

  function saveState(
    uint8 _stoneId,
    uint8 _moveNumber,
    uint8 _color,
    uint8 _blackStonesCaptured,
    uint8 _whiteStonesCaptured,
    uint8 _positionX,
    uint8 _positionY,
    uint8 _koPoint,
    bool _gameWon,
    uint8 _winningColor
  ) public {
    uint16 stoneId = getStoneTokenId(_color, _stoneId);

    //Who's next? Back or white?
    require(msg.sender != address(0), "Address 0 can not be a player");
    require(_moveNumber > 0, "Move number > 0");
    require(_moveNumber > lastMove, "Move is invalid");
    require(WeiBlocksStoneI(weiBlocksStoneAddress).exists(stoneId), "Stone not valid");
    require(WeiBlocksStoneI(weiBlocksStoneAddress).ownerOf(stoneId) == msg.sender, "Not owner of stone");
    require(board[_positionX][_positionY] == 0, "Position not valid");

    State memory currentState = State(
      msg.sender,
      _moveNumber,
      _color,
      _stoneId,
      _blackStonesCaptured,
      _whiteStonesCaptured,
      _koPoint,
      _positionX,
      _positionY
    );

    states[stateCounter.current()] = currentState;
    stateCounter.increment();
    board[_positionX][_positionY] = _stoneId;
    lastMove = _moveNumber;

    emit BoardState(msg.sender, stoneId, _moveNumber);

    if (_gameWon) {
      games.push(FinishedGame(currentState, _winningColor, false));
      gamesCounter.increment();
      emit EndGame(_moveNumber, _winningColor, msg.sender);
    }
  }

  function initiateUser(
    uint8 _color,
    uint8 _stoneNo,
    address _user
  ) public onlyOwner {
    require(_user != address(0), "Address 0 can not be a player");
    WeiBlocksStoneI(weiBlocksStoneAddress).mint(_user, getStoneTokenId(_color, _stoneNo));
    emit UserInitiated(getStoneTokenId(_color, _stoneNo), _user);
  }

  function getState(uint16 _stateIndex) public view returns (State memory) {
    return states[_stateIndex];
  }

  function getWinningGame(uint16 _stateIndex) public view returns (FinishedGame memory) {
    return games[_stateIndex];
  }

  function setWeiBlocksStoneAddress(address _address) public onlyOwner {
    weiBlocksStoneAddress = _address;
  }

  function setWeiBlocksNFTAddress(address _address) public onlyOwner {
    weiBlocksNFT = _address;
  }

  function mintFinishedGame(uint256 _gameNo, address _to) public onlyOwner {
    require(gamesCounter.current() > _gameNo, "Game does not exists");

    FinishedGame memory game = games[_gameNo];

    require(!game.minted, "Game already minted");
    require(!WeiBlocksI(weiBlocksNFT).exists(_gameNo), "NFT already minted");

    WeiBlocksI(weiBlocksNFT).mint(_to, _gameNo);
    game.minted = true;
    games[_gameNo] = game;
    emit MintEndedGame(_gameNo, _to);
  }

  function transferOwnershipOfWeiBlocks(address _newOwner) public onlyOwner {
    require(_newOwner != address(0), "Can not transfer to 0 Address");
    WeiBlocksI(weiBlocksNFT).transferOwnership(_newOwner);
  }

  function transferOwnershipOfWeiBlocksStones(address _newOwner) public onlyOwner {
    require(_newOwner != address(0), "Can not transfer to 0 Address");
    WeiBlocksStoneI(weiBlocksStoneAddress).transferOwnership(_newOwner);
  }

  function resetGame() public onlyOwner {
    require(games.length > 0 && games[gamesCounter.current()].minted, "Last state not minted");
    stateCounter.reset();
    delete states;
  }

  /**
   * stone id is composed as 1001 is stone id 1 of color white... 2001 is the stone id 2 of color black
   */
  function getStoneTokenId(uint8 _color, uint8 _stoneNo) public pure returns (uint16) {
    return _color * 1000 + _stoneNo;
  }
}

abstract contract WeiBlocksStoneI is Ownable, IERC721Enumerable {
  function exists(uint256 _tokenId) public view virtual returns (bool);

  function mint(address _to, uint256 _tokenId) public virtual;
}

abstract contract WeiBlocksI is Ownable, IERC721Enumerable {
  function exists(uint256 _tokenId) public view virtual returns (bool);

  function mint(address _to, uint256 _tokenId) public virtual;
}

