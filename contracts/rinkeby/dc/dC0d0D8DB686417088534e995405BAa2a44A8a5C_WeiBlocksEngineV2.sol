contract Stack {
  uint256 public top;
  uint256 private height;
  struct Object {
    uint256 next;
    uint16[2] value;
  }
  mapping(uint256 => Object) public objects;

  constructor() {}

  function insert(uint16[2] calldata _number) public {
    Object memory newObj = Object(top, _number);
    objects[height] = newObj;
    top = height;
    height = height + 1;
  }

  function pop() public returns (Object memory) {
    require(height > 0);
    uint256 _top = top;
    Object memory lastObject = objects[top];
    top = objects[top].next;
    delete objects[_top];
    height = height - 1;
    return lastObject;
  }

  function getTop() public view returns (Object memory) {
    return objects[top];
  }

  function getHeight() public view returns (uint256) {
    return height;
  }

  function empty() public {
    for (uint256 i; i < height; i++) {
      delete objects[i];
    }
  }
}

