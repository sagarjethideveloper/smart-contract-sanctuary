import "./ABDKMath64x64.sol";



/**
 * @title SplytStaking
 * @dev A token holder contract that lets user stake their tokens
 */
contract SplytStaking is Ownable {
  using SafeMath for uint256;
  using SafeERC20 for IERC20;

  IERC20 private _token;
  uint256 private depositStartBlock;
  uint256 private depositEndBlock;

  struct Compound {
    uint blockStart; //block that starts interest
    uint blockEnd; // block that ends interest
    uint startTime; // time to start computing compound interest
    uint endTime; // time to stop compounding interest
    uint apyPerSec; //This is how often the interest compound. E.g: 31556952 secs/year, 8600 hours/year
    uint apyPerBlock; //this is reward per block. We assume 15s per block
  }

  Compound private compound;
  uint secondsPerYear = 31556952;
  uint blocksPerMonth = 175200; //this is the max amount of blocks to multiply by
  uint secondPerBlock = 15;    //Number of seconds between blocks
  uint totalPrincipal;
  uint numUsersWithDeposits;


  mapping (address => uint256) public _balances;

  event TokenStaked(address _userAddress, uint256 amount);
  event TokenWithdrawn(address _userAddress, uint256 amount);


  /**
   * @dev Creates a vesting contract that vests its balance of any ERC20 token to the
   * beneficiary, gradually in a linear fashion until start + duration. By then all
   * of the balance will have vested.
   * @param token address of the token which should be vested
   */
  constructor (address token, uint _apyPerSec, uint _depositStartBlock, uint _depositEndBlock) public {
    _token = IERC20(token);

    // Compute start and end blocks for interest compounding
    uint interestStartBlock = _depositEndBlock.add(1);
    uint interestEndBlock = interestStartBlock.add(blocksPerMonth);

    //compute star and end times for the same time period
    uint blocksUntilInterestStarts = interestStartBlock.sub(block.number);
    uint interestStartTime = block.timestamp.add(blocksUntilInterestStarts.mul(secondPerBlock));

    uint blocksUntilInterestEnd = interestEndBlock.sub(interestStartBlock);
    uint interestEndTime = block.timestamp.add(blocksUntilInterestEnd.mul(secondPerBlock));

    compound = Compound({
    blockStart: interestStartBlock, //When deposits end, we start interest on next block
    blockEnd: interestEndBlock,     //interestStartBlock + blocksPerMonth
    startTime: interestStartTime,
    endTime: interestEndTime,
    apyPerSec: _apyPerSec,
    apyPerBlock: _apyPerSec.mul(secondPerBlock)
    });
    depositStartBlock = _depositStartBlock;
    depositEndBlock = _depositEndBlock;
  }

  // -----------------------------------------------------------------------
  // Modifiers
  // -----------------------------------------------------------------------

  modifier depositWindow() {
    require (block.number > depositStartBlock && block.number <= depositEndBlock, "DepositWindow: Can be called only during deposit window");
    _;
  }


  // -----------------------------------------------------------------------
  // Helpers
  // -----------------------------------------------------------------------

  //Count how many blocks user has staked. Limit to blocksPerMonth=maxBlock
  function _blocksStaked(uint numBlocksPassed, uint maxBlock) public pure returns (uint) {
    if(numBlocksPassed >= maxBlock) {
      return maxBlock;
    } else {
      return numBlocksPassed;
    }
  }

  // -----------------------------------------------------------------------
  // COMPUTATIONS
  // -----------------------------------------------------------------------

  function pow (int128 x, uint n) public pure returns (int128 r) {
    r = ABDKMath64x64.fromUInt (1);  while (n > 0) {
    if (n % 2 == 1) {
      r = ABDKMath64x64.mul (r, x);
      n -= 1;
    } else {
      x = ABDKMath64x64.mul (x, x);
      n /= 2;
    }
  }
  }

  function compoundInterestByTime(uint principal, uint ratio, uint n) public pure returns (uint) {
    return ABDKMath64x64.mulu (
      pow (
        ABDKMath64x64.add (ABDKMath64x64.fromUInt (1), ABDKMath64x64.divu (ratio, 10**18)),
        n),
      principal);
  }

  function compoundWithPrincipalAndTime(uint principal, uint duration) public view returns (uint) {
    return compoundInterestByTime(principal, compound.apyPerSec, duration);
  }

  function compoundWithPrincipal(uint principal) public view returns (uint) {
    uint duration = block.timestamp - compound.startTime;
    return compoundInterestByTime(principal, compound.apyPerSec, duration);
  }

  function compoundWithPrincipalByUser() public view returns (uint) {
    return compoundWithPrincipal(_balances[msg.sender]);
  }

  // Internal Method To compute approx APY by blocks that have passed.
  function _compoundInterestByBlock(uint principal, uint blocksStaked) public view returns (uint) {
    uint reward = SafeMath.div(compound.apyPerBlock.mul(principal).mul(blocksStaked), 10**18);
    return reward.add(principal);
  }

  // Live production method
  function compoundInterestByBlock(uint principal) public view returns (uint) {
    uint numBlocksPassed = block.number.sub(compound.blockStart);
    uint blocksStaked = _blocksStaked(numBlocksPassed, blocksPerMonth);
    return _compoundInterestByBlock(principal, blocksStaked);
  }

  // -----------------------------------------------------------------------
  // GETTERS
  // -----------------------------------------------------------------------

  // Get User Token Staked: Javascript: SplyStakingContract._balances(userAddress);
  // Get Total Stake balance: Javascript: SplytERC20TokenContract.balanceOf(SplyStakingContractAddress));

  function getCompoundInfo() public view returns (
    uint _currentBlock,
    uint _interestStartBlock,
    uint _interestEndBlock,
    uint _interestStartTime,
    uint _interestEndTime,
    uint _interestApyPerSec,
    uint _interestApyPerBlock
  ) {
    return (
    block.number,
    compound.blockStart,
    compound.blockEnd,
    compound.startTime,
    compound.endTime,
    compound.apyPerSec,
    compound.apyPerBlock
    );
  }

  function getAdminStakingInfo() public view returns (uint _totalPrincipal, uint _numUsersWithDeposits) {
    return (totalPrincipal, numUsersWithDeposits);
  }

  // This methods is for the Frontend to read how much rewards there are for a specific user
  function getUserBalances(address userAddress) public view returns (uint _principal, uint _reward, uint _balance) {
    if(block.number <= compound.blockStart) {
      return (
      _balances[userAddress],
      0,
      _balances[userAddress]);
    } else {
      uint totalBalance = compoundInterestByBlock(_balances[userAddress]);
      uint reward = totalBalance.sub(_balances[userAddress]);
      return (
      _balances[userAddress],
      reward,
      totalBalance
      );
    }
  }

  //NOTE: this is an approximation so round up.
  function apy() public view returns (uint _apy) {
    return secondsPerYear * compound.apyPerSec * 100;
  }

  // -----------------------------------------------------------------------
  // SETTERS
  // -----------------------------------------------------------------------

  function changeAPY(uint _newApyRate) public onlyOwner {
    compound.apyPerSec = _newApyRate;
    compound.apyPerBlock = _newApyRate.mul(secondPerBlock);
  }

  function changeInterestTimes(uint _startTime, uint _endTime) public onlyOwner {
    compound.startTime = _startTime;
    compound.endTime = _endTime;
  }

  function changeInterestBlocks(uint _blockStart, uint _blockEnd) public onlyOwner {
    compound.blockStart = _blockStart;
    compound.blockEnd = _blockEnd;
  }


  function stakeTokens(uint _amount) public depositWindow {
    require(_token.allowance(msg.sender, address(this)) >= _amount, "TokenBalance: User has not allowed tokens to be used");
    require(_token.balanceOf(msg.sender) >= _amount, "TokenBalance: msg.sender can't stake more than they have!");

    if(_balances[msg.sender] == 0) {
      numUsersWithDeposits += 1;
    }
    _balances[msg.sender] += _amount;
    totalPrincipal += _amount;

    require(_token.transferFrom(msg.sender,address(this), _amount), "TokenTransfer: failed to transfer tokens from msg.sender here");
    emit TokenStaked(msg.sender, _amount);
  }

  function withdrawTokens() public {
    require(_balances[msg.sender] > 0, "TokenBalance: no tokens available to be withdrawn");

    uint totalBalance = compoundInterestByBlock(_balances[msg.sender]);
    totalPrincipal -= _balances[msg.sender];
    _balances[msg.sender] = 0;
    numUsersWithDeposits -= 1;

    require(_token.transfer(msg.sender, totalBalance));
    emit TokenWithdrawn(msg.sender, totalBalance);
  }

  function adminWithdrawRemaining() public onlyOwner {
    uint totalBalanceNeeded = compoundInterestByBlock(totalPrincipal);
    uint contractBalance = _token.balanceOf(address(this));

    // We deposit tokens and assume everyone gains for the full 30 days.
    //  There is a difference because some user will withdraw tokens before 30 days.
    if(contractBalance > totalBalanceNeeded) {
      uint extraTokenBalance = contractBalance.sub(totalBalanceNeeded);
      require(_token.transfer(msg.sender, extraTokenBalance));
      emit TokenWithdrawn(msg.sender, extraTokenBalance);
    }
  }

  //Only to be called in the worst case scenario.
  function withdrawExtraTokens(uint _amount) public onlyOwner {
    require(_token.transfer(msg.sender, _amount));
    emit TokenWithdrawn(msg.sender, _amount);
  }
}

