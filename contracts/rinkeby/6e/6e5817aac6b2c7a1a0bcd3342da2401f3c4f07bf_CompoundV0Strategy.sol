import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";

abstract contract BaseRhoStrategy is IRhoStrategy, AccessControlEnumerableUpgradeable {
    bytes32 public constant VAULT_ROLE = keccak256("VAULT_ROLE");
    bytes32 public constant SWEEPER_ROLE = keccak256("SWEEPER_ROLE");

    IERC20MetadataUpgradeable public override underlying;
    uint256 public override BLOCK_PER_YEAR;

    uint256 public override switchLockDuration;
    uint256 public override switchLockedUntil;
    uint256 public override switchingLockTarget;
    uint256 public slippage;

    //uint256 public override rewardConversionThreshold;

    function __initialize(address _underlyingAddr, uint256 _lockDurationInBlock) internal {
        AccessControlEnumerableUpgradeable.__AccessControlEnumerable_init();
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        underlying = IERC20MetadataUpgradeable(_underlyingAddr);
        _setSwitchLockDuration(_lockDurationInBlock);
    }

    function balanceOfUnderlying() public view virtual override returns (uint256);

    // function setRewardConversionThreshold(uint256 _threshold) external override onlyRole(VAULT_ROLE) {
    //     rewardConversionThreshold = _threshold;
    // }

    function setSlippage(uint256 _slippage) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        _setSlippage(_slippage);
    }

    function _setSlippage(uint256 _slippage) internal {
        require(_slippage <= 1000, "Input value must <= 1000");
        slippage = _slippage;
    }

    function isLocked() external view override returns (bool) {
        return _isLocked(balanceOfUnderlying());
    }

    function setSwitchLockDuration(uint256 numberOfBlocks) external virtual override onlyRole(DEFAULT_ADMIN_ROLE) {
        _setSwitchLockDuration(numberOfBlocks);
    }

    function switchingLock(uint256 target) external virtual override onlyRole(VAULT_ROLE) {
        switchingLockTarget = target;
        switchLockedUntil = block.number + switchLockDuration;
    }

    function _setSwitchLockDuration(uint256 numberOfBlocks) internal {
        switchLockDuration = numberOfBlocks;
    }

    function _isLocked(uint256 balance) internal view returns (bool) {
        return (balance < switchingLockTarget && block.number < switchLockedUntil);
    }

    function _sweepERC20Token(address token, address to) internal {
        IERC20Upgradeable tokenToSweep = IERC20Upgradeable(token);
        tokenToSweep.transfer(to, tokenToSweep.balanceOf(address(this)));
    }

    function setBlocksPerYear(uint256 blocksPerYear) external onlyRole(VAULT_ROLE) {
        BLOCK_PER_YEAR = blocksPerYear;
    }
}

