pragma solidity ^0.8.5;

import "@openzeppelin/contracts/access/Ownable.sol";

contract IsGame is Ownable {
    event GameAdded(address indexed game, uint256 timestamp);
    event GameDisable(address indexed game, uint256 timestamp);

    mapping(address => bool) private _games;

    function isGame(address _game) public view returns (bool) {
        return _games[_game];
    }

    function addGame(address _game) external onlyOwner {
        require(_games[_game]==false,"This game already Added");

        _games[_game] = true;
        emit GameAdded(_game, block.timestamp);
    }

    function disableGame(address _game) external onlyOwner {
        require(_games[_game]==true,"This game not found or already disable");

