pragma solidity ^0.8.5;

import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";
contract Timer is Ownable {
    // First time (block.timestamp) and cron time
    mapping(address => uint256[2]) public times;
    
    modifier isJobRegistered(address jobAddress) {
        require(times[jobAddress][0] != 0,"This job not registered");
          _;
      }
    
    function setTime(address jobAddress, uint256 period) external onlyOwner{
        require(times[jobAddress][0] == 0,"This job already registered");

        times[jobAddress][0] = block.timestamp;
        times[jobAddress][1] = period;
    }

    function deleteTime(address jobAddress) external onlyOwner isJobRegistered(jobAddress){
        delete times[jobAddress];
    }

    function updateTime(address jobAddress, uint256 newPeriod)
        external
        onlyOwner
        isJobRegistered(jobAddress)
    {
        times[jobAddress][0] = block.timestamp;
        times[jobAddress][1] = newPeriod;
    }

    function timeArrived(address jobAddress) internal returns (bool) {
        if (times[jobAddress][0] + times[jobAddress][1] <= block.timestamp && times[jobAddress][0] != 0) {
            times[jobAddress][0] = block.timestamp;
            return true;
        }
        return false;
    }
}

