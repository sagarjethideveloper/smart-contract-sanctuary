pragma solidity 0.8.6;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "../modifiers/IsGame.sol";
import "../oracle/IPriceFeed.sol";
import "../distribution/IDistribution.sol";
import "../token/IERC20List.sol";
import "../referral/IReferral.sol";
import "../statistics/IStatistics.sol";
import "../games/IGame.sol";
import "../formula/IMintingFormula.sol";
import "../structs/MintData.sol";
import "../modifiers/Timer.sol";
import "../utils/ITopPlayers.sol";
import "../collection/ICollection.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/governance/TimelockController.sol";
import "../library/EIP712.sol";
import "../library/EIP1271.sol";

contract MockController is TimelockController, IsGame, Timer, EIP712 {
    using SafeERC20 for IERC20;
    bytes4 internal constant EIP_1271_MAGICVALUE = 0x20c13b0b;
    bytes internal personalSignPrefix = "\x19Ethereum Signed Message:\n";

    event AddressSet(string name, address newAddress);
    event MinBetSet(uint256 amount);
    event MaxBetSet(uint256 amount);
    event Payout(
        address indexed winner,
        address indexed token,
        uint256 payout,
        address game
    );

    ICollection jack;
    ICollection king;

    IMintingFormula minting;
    IDistribution distributor;
    IReferral referral;
    IStatistics statistic;
    IPriceFeed priceFeed;
    IERC20List erc20List;
    ITopPlayers topPlayers;

    uint256 minBet;
    uint256 maxBet;
    uint256 housePerc;
    uint256 lotteryPerc;
    uint256 stakingPerc;

    // token address to TOKEN/USD price
    mapping(address => uint256) public tokenRates;
    // User address to bet count
    mapping(address => uint8) public betCounts;
    /* Order fill status, by maker address then by hash. */
    mapping(address => mapping(bytes32 => uint256)) public fills;

    mapping(address => mapping(bytes32 => bool)) public approved;

    constructor(
        uint256 minDelay,
        address[] memory proposers,
        address[] memory executors
    ) TimelockController(minDelay, proposers, executors) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function setNFTAddresses(address _jack, address _king) external {
        jack = ICollection(_jack);
        king = ICollection(_king);
    }

    function setMinBet(uint256 amount) external onlyRole(TIMELOCK_ADMIN_ROLE) {
        minBet = amount;
        emit MinBetSet(amount);
    }

    function setMaxBet(uint256 amount) external onlyRole(TIMELOCK_ADMIN_ROLE) {
        maxBet = amount;
        emit MaxBetSet(amount);
    }

    function setMintingFormula(address _minting)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        minting = IMintingFormula(_minting);
        emit AddressSet("Minting Formula", _minting);
    }

    function setDistributor(address _distributor)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        distributor = IDistribution(_distributor);
        emit AddressSet("Distribution", _distributor);
    }

    function setTopPlayers(address _topPlayers)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        topPlayers = ITopPlayers(_topPlayers);
        emit AddressSet("TopPlayers", _topPlayers);
    }

    function setReferral(address _referral)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        referral = IReferral(_referral);
        emit AddressSet("Referral", _referral);
    }

    function setStatistic(address _statistic)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        statistic = IStatistics(_statistic);
        emit AddressSet("Statistics", _statistic);
    }

    function setPriceFeed(address _priceFeed)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        priceFeed = IPriceFeed(_priceFeed);
        emit AddressSet("PriceFeed", _priceFeed);
    }

    function setERC20List(address _erc20list)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        erc20List = IERC20List(_erc20list);
        emit AddressSet("ERC20List", _erc20list);
    }

    /**
    @param _housePerc should be two digits
    @param _stakingPerc should be two digits
    @param _lotteryPerc should be two digits 
    */
    function setPercentages(
        uint256 _housePerc,
        uint256 _stakingPerc,
        uint256 _lotteryPerc
    ) external {
        housePerc = _housePerc;
        lotteryPerc = _lotteryPerc;
        stakingPerc = _stakingPerc;
    }

    function setTokenRate(address _token, uint256 _rate)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        tokenRates[_token] = _rate;
    }

    /**
     * @notice player can call bet function.
     * @param addresses[0] player
     * @param addresses[1] token
     * @param gameVar[0] is amountUSD
     * @param gameVar[1] tokenAmount
     */
    function bet(
        address _game,
        address[] memory addresses,
        uint256[] memory gameVar,
        bytes32[] memory hashes
    ) external {
        timerJobs();
        address _token = addresses[1];
        uint256 _amount = gameVar[1];
        uint256 _dec = erc20List.getDecimal(_token);
        require(
            addresses[0] == msg.sender,
            "controller: you can only bet with your own address!"
        );
        conditions(_game, _token, msg.sender, _amount, _dec);

        uint256 tokenUsdAmount = getUsdAmount(_token, _amount, _dec);

        gameVar[0] = (tokenRates[_token] * _amount);

        MintData memory data = prepareData(_game, _token, msg.sender, _amount);
        minting.calculate(data, addresses[0]);

        IERC20(_token).transferFrom(msg.sender, address(this), _amount);
        IGame(_game).setBet(addresses, gameVar, hashes);

        betCounts[msg.sender]++;

        topPlayers.update(msg.sender, tokenUsdAmount);
        referral.update(msg.sender, betCounts[msg.sender]);
    }

    /**
     * @notice player can call bet function.
     * @param addresses[0] player
     * @param addresses[1] token
     * @param gameVar[0] is amountUSD
     * @param gameVar[1] tokenAmount
     */
    function betWithSign(
        address _game,
        address[] memory addresses,
        uint256[] memory gameVar,
        bytes32[] memory hashes,
        bytes memory signature
    ) external onlyRole(TIMELOCK_ADMIN_ROLE) {
        timerJobs();
        address _token = addresses[1];
        address _user = addresses[0];
        uint256 _amount = gameVar[1];
        uint256 _dec = erc20List.getDecimal(_token);

        validateOrderAuthorization(hashes[0], _user, signature);

        conditions(_game, _token, _user, _amount, _dec);

        uint256 tokenUsdAmount = getUsdAmount(_token, _amount, _dec);

        gameVar[0] = (tokenRates[_token] * _amount);

        MintData memory data = prepareData(_game, _token, _user, _amount);
        minting.calculate(data, addresses[0]);

        IERC20(_token).transferFrom(_user, address(this), _amount);
        IGame(_game).setBet(addresses, gameVar, hashes);

        betCounts[_user]++;

        topPlayers.update(_user, tokenUsdAmount);
        referral.update(_user, betCounts[_user]);
    }

    /**@notice only can admin call play function. Because we send seed */
    function play(
        address _game,
        address[] memory playAddresses,
        uint256[] memory gameVar,
        bytes32[] memory hashes
    ) external onlyRole(TIMELOCK_ADMIN_ROLE) {
        IGame(_game).play(playAddresses, gameVar, hashes);
        // playeAddress[0] is user address
        // gameVar[0] is equivalent token USD amount
    }

    function payment(
        address[] memory winners,
        uint256[] memory amounts,
        address[] memory tokens,
        address _game
    ) external onlyRole(TIMELOCK_ADMIN_ROLE) {
        if (winners.length == 1) {
            IERC20(tokens[0]).transfer(winners[0], amounts[0]);
            emit Payout(winners[0], tokens[0], amounts[0], _game);
        } else if (winners.length > 1) {
            for (uint256 i = 0; i < winners.length; i++) {
                IERC20(tokens[i]).transfer(winners[i], amounts[i]);
                emit Payout(winners[i], tokens[i], amounts[i], _game);
            }
        }
    }

    /** Give unlimited approve to distributor contract for token distributing */
    function giveApprove(address _token, uint256 _amount)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        IERC20(_token).approve(address(distributor), _amount);
    }

    /////
    //// INTERNAL FUNCTIONS
    /////

    function timerJobs() public {
        bool dist = timeArrived(address(distributor));
        bool stat = timeArrived(address(statistic));
        bool priceFed = timeArrived(address(priceFeed));

        if (dist) {
            address[] memory _tokens = erc20List.getWhitelist();
            address[] memory tokens = new address[](_tokens.length);
            uint256[] memory amounts = new uint256[](_tokens.length);
            for (uint256 i = 0; i < _tokens.length; i++) {
                address token = _tokens[i];
                tokens[i] = token;
                amounts[i] = IERC20(token).balanceOf(address(this));
            }
            distributor.distribute(
                amounts,
                tokens,
                lotteryPerc,
                stakingPerc,
                housePerc
            );
        }
        if (priceFed) {
            //     updatePrices();
        }
    }

    function conditions(
        address _game,
        address _token,
        address _user,
        uint256 _amount,
        uint256 _dec
    ) private view {
        uint256 tokenUsdAmount = getUsdAmount(_token, _amount, _dec);

        require(isGame(_game), "This address is not a game address");
        require(_amount > 0, "Amount should be greater than 0");
        require(
            IERC20(_token).balanceOf(_user) >= _amount,
            "Controller: insufficient amount"
        );
        require(
            erc20List.isSupportedToken(_token),
            "This token is not supported yet"
        );
        require((tokenUsdAmount) >= minBet, "Minimum bet amount is 1 dollar");

        require((tokenUsdAmount) <= maxBet, "Maximum bet amount is 200 dollar");
    }

    function nftPromoPerc(address _user) private view returns (uint256) {
        uint256 jackPromo = jack.balanceOf(_user);
        uint256 kingPromo = king.balanceOf(_user);

        if (kingPromo > 0 && jackPromo > 0) {
            return king.getMintBon();
        } else if (kingPromo > 0) {
            return king.getMintBon();
        } else if (jackPromo > 0) {
            return jack.getMintBon();
        }
    }

    function prepareData(
        address _game,
        address _token,
        address _user,
        uint256 _amount
    ) private returns (MintData memory) {
        MintData memory md;
        // We have multiple currencies have different decimals. For calculation we need to convert them all to 18 decimals.
        _amount = (1e18 / _amount) * _amount;

        md.player = msg.sender;
        md.amount = _amount;
        md.referral = (referral.getMultiplier(md.player) * 1e16);
        md.promo = IGame(_game).promoPerc();
        md.factor = IGame(_game).gameFactor();
        md.topPlayer = statistic.getPlayerPromo(md.player);
        md.nft = nftPromoPerc(_user);

        return md;
    }

    function updatePrices() private {
        address[] memory supportedTokens = erc20List.getWhitelist();
        IERC20 token;

        for (uint256 i = 0; i < supportedTokens.length; i++) {
            address tokenAdd = supportedTokens[i];
            address tokenFeed = erc20List.getTokenFeed(tokenAdd);

            token = IERC20(tokenAdd);

            int256 priceUSD = IPriceFeed(priceFeed).getThePrice(
                tokenAdd,
                tokenFeed
            );
            uint256 uPriceUSD = uint256(priceUSD);

            tokenRates[tokenAdd] = uPriceUSD;
        }
    }

    function getUsdAmount(
        address token,
        uint256 amount,
        uint256 dec
    ) internal view returns (uint256) {
        return (tokenRates[token] * amount) / (10**(dec + 8));
    }

    //******  SIGN  ******//
    struct Order {
        /* Order maker address. */
        address maker;
        /* token address */
        address token;
        /* Order salt to prevent duplicate hashes. */
        uint256 salt;
    }
    /* Order typehash for EIP 712 compatibility. */
    bytes32 constant ORDER_TYPEHASH =
        keccak256("Order(address maker,address token,uint256 salt)");

    function hashOrder(Order memory order) public pure returns (bytes32 hash) {
        /* Per EIP 712. */
        return
            keccak256(
                abi.encode(ORDER_TYPEHASH, order.maker, order.token, order.salt)
            );
    }

    function hashToSign(bytes32 orderHash) public view returns (bytes32 hash) {
        /* Calculate the string a user must sign. */
        return
            keccak256(
                abi.encodePacked("\x19\x01", DOMAIN_SEPARATOR, orderHash)
            );
    }

    function exists(address what) public view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(what)
        }
        return size > 0;
    }

    // function validateOrderParameters(Order memory order, bytes32 hash_)
    //     internal
    //     view
    //     returns (bool)
    // {
    //     /* Order must be listed and not be expired. */
    //     if (
    //         order.listingTime > block.timestamp ||
    //         (order.expirationTime != 0 &&
    //             order.expirationTime <= block.timestamp)
    //     ) {
    //         return false;
    //     }

    //     return true;
    // }

    function validateOrderAuthorization(
        bytes32 hash,
        address maker,
        bytes memory signature
    ) public view returns (bool) {
        /* Memoized authentication. If order has already been partially filled, order must be authenticated. */
        if (fills[maker][hash] > 0) {
            return true;
        }

        /* Order authentication. Order must be either: */

        /* (a): sent by maker */
        if (maker == msg.sender) {
            return true;
        }

        /* (b): previously approved */
        if (approved[maker][hash]) {
            return true;
        }

        /* Calculate hash which must be signed. */
        bytes32 calculatedHashToSign = hashToSign(hash);

        (uint8 v, bytes32 r, bytes32 s) = abi.decode(
            signature,
            (uint8, bytes32, bytes32)
        );

        if (
            ecrecover(
                keccak256(
                    abi.encodePacked(
                        personalSignPrefix,
                        "32",
                        calculatedHashToSign
                    )
                ),
                v,
                r,
                s
            ) == maker
        ) {
            return true;
        }

        return false;
    }
}

