        if (geneIndex == uint(GenePairsIndexes.WEAPON_RIGHT)) item = weaponRightMap[itemIndex];
        if (geneIndex == uint(GenePairsIndexes.WEAPON_LEFT)) item = weaponLeftMap[itemIndex];

        return item;
    }

