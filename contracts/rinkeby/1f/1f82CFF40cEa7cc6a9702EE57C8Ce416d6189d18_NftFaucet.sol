// import "@openzeppelin/contracts/utils/Strings.sol";

contract NftFaucet is ERC721, Ownable {
    using Counters for Counters.Counter;
    uint private nextId = 0;
    uint private maxId = 1000000;

    struct Traits {
        bool div2;
        bool div3;
        bool div5;
        bool div7;
        bool div9;
        bool div10;
        bool div42;
        bool div69;
        bool div100;
        bool div360;
        bool div420;
        bool div1000;
        bool pow2;
    }

    constructor() ERC721("NFauceT", "NFauceT") {}

    function withdrawAllFunds() public onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function toString(address account) private pure returns(string memory) {
        return toString(abi.encodePacked(account));
    }

    function toString(uint256 value) private pure returns(string memory) {
        return toString(abi.encodePacked(value));
    }

    function toString(bytes32 value) private pure returns(string memory) {
        return toString(abi.encodePacked(value));
    }

    function toString(bytes memory data) private pure returns(string memory) {
        bytes memory alphabet = "0123456789abcdef";

        bytes memory str = new bytes(2 + data.length * 2);
        str[0] = "0";
        str[1] = "x";
        for (uint i = 0; i < data.length; i++) {
            str[2+i*2] = alphabet[uint(uint8(data[i] >> 4))];
            str[3+i*2] = alphabet[uint(uint8(data[i] & 0x0f))];
        }
        return string(str);
    }

    /**
     * @dev Converts a `uint256` to its ASCII `string` decimal representation.
     */
    //Copied from Mad Dog Jones' replicator
    function numberToString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT licence
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }

    function mint() public returns (uint256) {
        require(nextId < maxId, "We ran out of numbers");
        uint newItemId = nextId;
        nextId += 1;
        _mint(msg.sender, newItemId);
        return newItemId;
    }

    function tokenURI(uint256 itemId)
        public
        view
        override
        returns (string memory)
    {
        string memory strId = numberToString(itemId);

        Traits memory traits = Traits(
            itemId % 2 == 0,
            itemId % 3 == 0,
            itemId % 5 == 0,
            itemId % 7 == 0,
            itemId % 9 == 0,
            itemId % 10 == 0,
            itemId % 42 == 0,
            itemId % 69 == 0,
            itemId % 100 == 0,
            itemId % 360 == 0,
            itemId % 420 == 0,
            itemId % 1000 == 0,
            itemId != 0 && (itemId & (itemId - 1) == 0)
        );

        string memory classname = string(abi.encodePacked(
            string(abi.encodePacked(
                traits.div2 ? "d2 " : "",
                traits.div3 ? "d3 " : "",
                traits.div5 ? "d5 " : "",
                traits.div7 ? "d7 " : "",
                traits.div9 ? "d9 " : "",
                traits.div10 ? "d10 " : "",
                traits.div42 ? "d42 " : ""
            )),
            string(abi.encodePacked(
                traits.div69 ? "d69 " : "",
                traits.div100 ? "d100 " : "",
                traits.div360 ? "d360 " : "",
                traits.div420 ? "d420 " : "",
                traits.div1000 ? "d1000 " : "",
                traits.pow2 ? "p2 " : ""
            ))
        ));

        // string memory image = getImage(strId, getClassname(itemId));
        string memory image = string(abi.encodePacked(
            string(abi.encodePacked(
                "data:image/svg+xml;utf8,%3Csvg viewBox='0 0 640 640' xmlns='http://www.w3.org/2000/svg' class='", classname, "'%3E",
                    "%3Cstyle%3E",
                        "svg { background-color: white; height: 100%; width: 100%; }",
                        "text { font-weight: bold; font-size: 20vh; color: black; font-family: sans-serif; }",
                        ".d2 { background-color: green; }",
                        ".d3 > text { fill: orange; }",
                        ".d5 > text { font-style: italic; }",
                        ".d7 > text { font-weight: normal; }",
                        ".d9 > text { fill: red; }",
                        ".d10 > text { text-shadow: 2px 2px 2px #aaaaaa; }",
                        ".d42 { background-color: black; }",
                        ".d42 > text { fill: white; }",
                        ".d69 > text { fill: pink;}")),string(abi.encodePacked(
                        ".d100 > text { text-shadow: 4px 4px 4px #666666; }",
                        ".d360 { animation: spin 60s; }",
                        ".d420 > text { opacity: 0.5; }",
                        ".d1000 > text { text-shadow: 8px 8px 8px #333333; }",
                        ".p2 {",
                            "background-image: linear-gradient(45deg, #ccc 25%, transparent 25%),",
                                            "linear-gradient(-45deg, #ccc 25%, transparent 25%),",
                                            "linear-gradient(45deg, transparent 75%, #ccc 75%),",
                                            "linear-gradient(-45deg, transparent 75%, #ccc 75%);",
                            "background-size: 64px 64px;",
                            "background-position: 0 0, 0 32px, 32px -32px, -32px 0px;",
                        "}")),string(abi.encodePacked(
                        "@keyframes spin { 0% { transform: rotate(0deg); } 100% { transform: rotate(360deg); } }",
                    "%3C/style%3E",
                    "%3Ctext x='50%25' y='50%25' dominant-baseline='middle' text-anchor='middle'%3E",
                        strId,
                    "%3C/text%3E",
                "%3C/svg%3E"
            ))
        ));

        string memory strThisAddress = toString(address(this));


        return string(
            abi.encodePacked(
                string(abi.encodePacked(
                    'data:application/json;utf8,{',
                        '"name": "Number ', strId, '",',
                        '"description": "Mint a number",',
                        '"image": "', image, '",',
                        '"royaltyInfo":{"royaltyFeeByID": "10", "artistAddress":"',strThisAddress,'"},',
                        '"payout_address": "',strThisAddress, '",'
                )),
                string(abi.encodePacked(
                        '"traits": [ ',
                            '{"trait_type": "Divisible By 2", "value":', traits.div2 ? '"true"' : '"false"', '},',
                            '{"trait_type": "Divisible By 3", "value":', traits.div3 ? '"true"' : '"false"', '},',
                            '{"trait_type": "Divisible By 5", "value":', traits.div5 ? '"true"' : '"false"', '},',
                            '{"trait_type": "Divisible By 7", "value":', traits.div7 ? '"true"' : '"false"', '},'
                )),
                string(abi.encodePacked(
                            '{"trait_type": "Divisible By 9", "value":', traits.div9 ? '"true"' : '"false"', '},',
                            '{"trait_type": "Divisible By 10", "value":', traits.div10 ? '"true"' : '"false"', '},',
                            '{"trait_type": "Divisible By 42", "value":', traits.div42 ? '"true"' : '"false"', '},',
                            '{"trait_type": "Divisible By 69", "value":', traits.div69 ? '"true"' : '"false"', '},'
                )),
                string(abi.encodePacked(
                            '{"trait_type": "Divisible By 100", "value":', traits.div100 ? '"true"' : '"false"', '},',
                            '{"trait_type": "Divisible By 360", "value":', traits.div360 ? '"true"' : '"false"', '},',
                            '{"trait_type": "Divisible By 420", "value":', traits.div420 ? '"true"' : '"false"', '},',
                            '{"trait_type": "Divisible By 1000", "value":', traits.div1000 ? '"true"' : '"false"', '},'
                )),
                string(abi.encodePacked(
                           '{"trait_type": "Power Of 2", "value":', traits.pow2 ? '"true"' : '"false"', '},',
                           '{"trait_type": "Int", "value":"Integer"}',
                        '],'
                        // '"features": [',
                        //     traits.div2 ? '"Divisible By 2",' : '"Not Divisible By 2",',
                        //     traits.div3 ? '"Divisible By 3",' : '"Not Divisible By 3",',
                        //     traits.div5 ? '"Divisible By 5",' : '"Not Divisible By 5",',
                        //     traits.div7 ? '"Divisible By 7",' : '"Not Divisible By 7",',
                        //     traits.div9 ? '"Divisible By 9",' : '"Not Divisible By 9",'
                )),
                string(abi.encodePacked(
                        //     traits.div10 ? '"Divisible By 10",' : '"Not Divisible By 10",',
                        //     traits.div42 ? '"Divisible By 42",' : '"Not Divisible By 42",',
                        //     traits.div69 ? '"Divisible By 69",' : '"Not Divisible By 69",',
                        //     traits.div100 ? '"Divisible By 100",' : '"Not Divisible By 100",',
                        //     traits.div360 ? '"Divisible By 360",' : '"Not Divisible By 360",',
                        //     traits.div420 ? '"Divisible By 420",' : '"Not Divisible By 420",',
                        //     traits.div1000 ? '"Divisible By 1000",' : '"Not Divisible By 1000",',
                        //     traits.pow2 ? '"Power of 2"' : '"Not a Power of 2"'
                        // '],',
                        '"website": "nfaucet.eth"',
                    '}'
                ))
            )
        );
    }

    receive() external payable {}

    fallback () external payable {}

}

