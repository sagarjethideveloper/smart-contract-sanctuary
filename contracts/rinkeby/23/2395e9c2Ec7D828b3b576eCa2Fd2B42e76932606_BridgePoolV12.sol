/*
 * Bridge Pool Contract Version 1.2
 * @dev. Any action that adds tokens to the contract (swap, addLiquidity) is
 * to be called by the router. Any action that removes tokens from the contract
 * to be called by the end user (msg.sender)
 * We utilize the Group ID for signatures in a pecular way:
 * Group IDs below 256 are reserved for different governance groups.
 */
contract BridgePoolV12 is MultiSigCheckable, TokenReceivable, IBridgePool {
	struct TokenWithdrawConfig {
		uint64 feeX10000;
		uint16 groupId;
		bool noFee;
	}
    using SafeMath for uint256;
    event TransferBySignature(
        address receiver,
        address token,
        uint256 amount,
        uint256 fee);
    event BridgeLiquidityAdded(address actor, address token, uint256 amount);
    event BridgeLiquidityRemoved(address actor, address token, uint256 amountRemoved,
		uint256 owedNetwork, address owedToken, uint256 owedLiquidity);
    event BridgeSwap(address from,
        address indexed token,
        uint256 targetNetwork,
        address targetToken,
        address swapTargetTokenTo,
        address targetAddrdess,
        uint256 amount);

    string constant public NAME = "FERRUM_TOKEN_BRIDGE_POOL";
    string constant public VERSION = "001.200";
    uint64 constant public DEFAULT_FEE_X10000 = 50; // 0.5%
    mapping(address => mapping(address=>uint256)) private liquidities;
    mapping(address => TokenWithdrawConfig) public withdrawConfigs;
	mapping(address => mapping(uint256=>address)) public allowedTargets;
    address public feeDistributor;
    address public router;

    constructor () EIP712(NAME, VERSION) { }

	/*
	 *************** Owner only operations ***************
	 */
    function setFeeDistributor(address _feeDistributor) external onlyOwner() {
        feeDistributor = _feeDistributor;
    }

    function setRouter(address _router) external onlyOwner() {
        router = _router;
    }

	/*
	 *************** Admin operations ***************
	 */

    bytes32 constant SET_WITHDRAW_CONFIG_METHOD = keccak256("SetWithdrawConfig(address token,uint64 feeX10000,uint16 groupId,bool noFee)");
    function setWithdrawConfig(
			address token,
			uint64 feeX10000,
			uint16 groupId,
			bool noFee,
			uint16 expectedGroupId,
			bytes memory multiSignature) external onlyAdmin() {
        require(token != address(0), "Bad token");
		// console.logString("METHOD HASH");
		// console.logBytes32(SET_WITHDRAW_CONFIG_METHOD);
		require(expectedGroupId < 256, "BPV12: Requires governance signer"); // Signer must be of a governance group
		bytes32 message = keccak256(abi.encode(SET_WITHDRAW_CONFIG_METHOD, token, feeX10000, groupId, noFee));
		(, bool result) = tryVerify(message, expectedGroupId, multiSignature);
		require(result, "BP: Invalid signature");
		withdrawConfigs[token] = TokenWithdrawConfig({
			feeX10000: feeX10000,
			groupId: groupId,
			noFee: noFee
		});
    }

    bytes32 constant ALLOW_TARGET_METHOD = keccak256("AllowTarget(address token,uint256 chainId,address targetToken)");

	function allowTarget(
			address token,
			uint256 chainId,
			address targetToken,
			uint16  expectedGroupId,
			bytes memory multiSignature) external onlyAdmin() {
		bytes32 message = keccak256(abi.encode(ALLOW_TARGET_METHOD, token, chainId, targetToken));
		require(expectedGroupId < 256, "BPV12: Requires governance signer");
		(, bool result) = tryVerify(message, expectedGroupId, multiSignature);
		require(result, "BP: Invalid signature");
		allowedTargets[token][chainId] = targetToken;
	}

	/**
	 * We want to be able to remove a target faster than adding in case something is wrong with a target.
	 * No governance verification is required for removal.
	 */
	function disallowTarget(address token, uint256 chainId) external onlyAdmin() {
		delete allowedTargets[token][chainId];
	}

	/**
	 *************** Public operations ***************
	 */

    function swap(
		address to,
		address token,
        uint256 targetNetwork,
        address targetToken,
		address swapTargetTokenTo,
        address targetAddress)
    external override returns(uint256) {
        require(targetAddress != address(0), "BridgePool: targetAddress is required");
        return _swap(to, token, targetNetwork, targetToken, swapTargetTokenTo, targetAddress);
    }

    function _swap(address from, address token, uint256 targetNetwork,
        address targetToken, address swapTargetTokenTo, address targetAddress) internal returns(uint256) {
		require(targetNetwork != 0, "BP: targetNetwork is requried");
		require(allowedTargets[token][targetNetwork] == targetToken, "BP: target not allowed");
        uint256 amount = sync(token);
        emit BridgeSwap(from, token, targetNetwork, targetToken, swapTargetTokenTo, targetAddress, amount);
        return amount;
    }

    bytes32 constant WITHDRAW_SIGNED_METHOD =
        keccak256("WithdrawSigned(address token,address payee,uint256 amount,bytes32 swapTxId)");

    function withdrawSigned(
            address token,
            address payee,
            uint256 amount,
            bytes32 swapTxId,
            bytes memory multiSignature)
    external override returns(uint256) {
        bytes32 message = withdrawSignedMessage(token, payee, amount, swapTxId);
		TokenWithdrawConfig memory conf = withdrawConfigs[token];
        verifyUniqueSalt(message, swapTxId, conf.groupId, multiSignature);

		uint256 fee = 0;
        address _feeDistributor = feeDistributor;
        if (_feeDistributor != address(0)) {
			uint256 feeRatio = conf.noFee ? 0 : conf.feeX10000 != 0 ? conf.feeX10000 : DEFAULT_FEE_X10000;
            fee = amount.mul(feeRatio).div(10000);
            amount = amount.sub(fee);
            if (fee != 0) {
                sendToken(token, _feeDistributor, fee);
                IGeneralTaxDistributor(_feeDistributor).distributeTax(token, tx.origin); // tx.origin is only used for randomization
            }
        }
        sendToken(token, payee, amount);
        emit TransferBySignature(payee, token, amount, fee);
        return amount;
    }

    function withdrawSignedVerify(
            address token,
            address payee,
            uint256 amount,
            bytes32 swapTxId,
            bytes calldata multiSignature)
    external view returns (bytes32, bool) {
        bytes32 message = withdrawSignedMessage(token, payee, amount, swapTxId);
		TokenWithdrawConfig memory conf = withdrawConfigs[token];
        (bytes32 digest, bool result) = tryVerify(message, conf.groupId, multiSignature);
        return (digest, result);
    }

    function withdrawSignedMessage(
            address token,
            address payee,
            uint256 amount,
            bytes32 salt)
    internal pure returns (bytes32) {
        return keccak256(abi.encode(
          WITHDRAW_SIGNED_METHOD,
          token,
          payee,
          amount,
          salt
        ));
    }

    bytes32 constant REMOVE_LIQUIDITY_SIGNED_METHOD =
        keccak256("RemoveLiquiditySigned(address token,address payee,uint256 amount,bytes32 txId)");

    function removeLiquiditySigned(
            address token,
            address payee,
            uint256 amount,
            bytes32 txId,
            bytes memory multiSignature)
    external override returns(uint256) {
        bytes32 message = keccak256(abi.encode(REMOVE_LIQUIDITY_SIGNED_METHOD, token, payee, amount, txId));
		TokenWithdrawConfig memory conf = withdrawConfigs[token];
        verifyUniqueSalt(message, txId, conf.groupId, multiSignature);

        sendToken(token, payee, amount);
        emit TransferBySignature(payee, token, amount, 0);
        return amount;
    }

    function addLiquidity(address to, address token)
	external override {
        require(token != address(0), "Bad token");
        uint256 amount = sync(token);
        require(amount != 0, "Amount must be positive");
        liquidities[token][to] = liquidities[token][to].add(amount);
        emit BridgeLiquidityAdded(to, token, amount);
    }

	/**
	 * If there is not enough liquidity in this network, a log is issued for the bridge nodes to provide
	 * removeLiquiditySigned messages on the target network for the owed amount.
	 */
    function removeLiquidity(address to, address token, uint256 amount,
		uint256 targetNetwork, address targetToken)
	external override {
		// Using tx.origin in case we are calling this from router.
		// A malicious sc can call remove liquidity for the origin but
		// cannot steal their assets
		(uint256 actualLiq, uint256 owed) = _removeLiquidity(to, token, amount, true);
        if (actualLiq != 0) {
            sendToken(token, to, actualLiq);
        }
		if (owed != 0) {
			// We owe more liquidity. To be taken out of the target network
			require(allowedTargets[token][targetNetwork] == targetToken, "BP: target not allowed");
			emit BridgeLiquidityRemoved(to, token, amount, targetNetwork, targetToken, owed);
		} else {
			emit BridgeLiquidityRemoved(to, token, amount, 0, address(0), 0);
		}
    }

	/**
	 * Removes the liquidity only as much as is possible on this network
	 */
    function removeLiquidityIfPossible(address to, address token, uint256 amount)
	external override onlyRouter {
		(uint256 actualLiq,) = _removeLiquidity(to, token, amount, false);
        if (actualLiq != 0) {
            sendToken(token, to, actualLiq);
			emit BridgeLiquidityRemoved(to, token, amount, 0, address(0), 0);
        }
    }

    function _removeLiquidity(address to, address token, uint256 amount, bool force)
	internal returns (uint256 removed, uint256 owed) {
        require(amount != 0, "Amount must be positive");
        require(token != address(0), "Bad token");
        uint256 liq = liquidities[token][to];
        require(liq >= amount, "Not enough liquidity");
        uint256 balance = IERC20(token).balanceOf(address(this));
        removed = balance > amount ? amount : balance;
        liquidities[token][to] = liq.sub(force ? amount : removed);
		owed = amount.sub(removed);
    }

    function liquidity(address token, address liquidityAdder)
	external override view returns (uint256) {
        return liquidities[token][liquidityAdder];
    }

	modifier onlyRouter() {
		require(msg.sender == router, "RSRM: Only router method");
		_;
	}
}

