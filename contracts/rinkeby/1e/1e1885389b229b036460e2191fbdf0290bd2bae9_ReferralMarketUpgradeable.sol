pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

struct AccountToken {
  // Creation block time
  uint32 blockTime;
}

struct SingleTokenStore {
  // Primary tokens are allocated to accounts
  mapping(address => AccountToken) promotors;
  uint256 count;
}

// DAO strategy: contract becomes DAO owned

// This contract manages all minted referral tokens
contract ReferralTokenUpgradeable is Initializable, ContextUpgradeable, OwnableUpgradeable {

  // Market is authorized to mint tokens
  address public market;

  event ReferralTokenMinted(bytes32 refHash, address seller);

  function initialize() public initializer {
    __Context_init();
    __Ownable_init();
  }

  function setActiveMarket(address _market) public onlyOwner {
    market = _market;
  }

  // Mapping of minted tokens by referrable
  // refHash => SingleTokenStore
  mapping(bytes32 => SingleTokenStore) public tokens;

  function mintNewToken(bytes32 refHash, address promotor) public onlyMinters {
    require(tokens[refHash].promotors[promotor].blockTime == 0);
    tokens[refHash].promotors[promotor] = AccountToken({
      blockTime: uint32(block.timestamp)
    });
    tokens[refHash].count++;
    emit ReferralTokenMinted(refHash, promotor);
  }

  modifier onlyMinters {
    require(msg.sender == market);
    _;
  }
}

