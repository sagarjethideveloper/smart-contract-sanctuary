// Goals:
// -  List TimedReferrable Auction Status (detail on single auction: ok)
// -  Mint Referral Token (yes + expose status changes)

// -  List Referrables (nope: outside index)
// -  List marketplace

// This market behaves like a live state machine, it does not support history. 
// When payouts happen and referrables are closed, they are also removed from the market. Freeing up gas.
contract ReferralMarketUpgradeable is Initializable, ContextUpgradeable, OwnableUpgradeable {
  // The ATTR erc20 token address
  ATTRToken public attr;

  // Linked referral token contract
  ReferralTokenUpgradeable public referralTokenContract;

  // // Locked asset funder mapping (for returns, funder can be different from seller)
  // // To save gas, this is only set for tokens where the senderÂ differentiates from the funder.
  // // ERC20: token.address.amount / NFT: token.address.tokenids
  // mapping(address => mapping(address => uint256)) public fungibleTokenFunders;
  // mapping(address => mapping(address => uint256[])) public nonFungibleTokenFunders;

  // Registered referrables (which are guaranteed to have locked tokens in the vault)
  // They are looked up by identifier, which is a hash function.
  mapping(bytes32 => TimedReferrable) public referrables;

  // Testnet rescue switch for dev
  bool releaseChecksDisabled;

  // Events created
  event TimedReferrableCreated(bytes32 refHash);

  function initialize(
    ATTRToken attrErc20, 
    ReferralTokenUpgradeable rt) public initializer {
    __Context_init();
    __Ownable_init(); // For development purposes only

    attr = attrErc20;
    referralTokenContract = rt;
  }

  // TODO use metadata only and use an empty publish call for data security

  // --------------- REFERRABLES
  // ----------------------------

  // Register a referrable.
  function createTimedReferrable(
     TimedReferrable calldata referrable
  ) public returns (uint) {
    console.log("createReferrable");

    // We verify at entry point, so we don't have to do any additional checks later on
    verifyReferrable(referrable);

    // Referrables are content addressable (by funder)
    bytes32 refHash = calcRefHash(msg.sender, referrable);

    // Poor man's check if exists in storage
    // TODO evaluate if we can remove this check somehow with a parent structure (TimedReferrable can't be )
    TimedReferrable memory existing = referrables[refHash];
    bytes32 existsHash = calcRefHash(msg.sender, existing);
    require(refHash != existsHash);

    // TODO define utility fee in a market component
    // TODO: hit exchange contracts and calculate ATTR fee (5%)
    // For now we just take 0.1 ATTR
    uint utilityFee = 10**17;
    attr.transferFrom(msg.sender, address(this), utilityFee);

    // Lockup the assets
    bool hasAssets;
    if(referrable.primaryBounty.tokenContract != address(0x0)) {
      lockupAsset(referrable.primaryBounty, msg.sender);
      hasAssets = true;
    }
    if(referrable.secondaryBounty.tokenContract != address(0x0)) {
      lockupAsset(referrable.secondaryBounty, msg.sender);
      hasAssets = true;
    }
    if(!hasAssets) {
      revert("no assets");
    }

    // Save referrable
    referrables[refHash] = referrable;

    emit TimedReferrableCreated(refHash);
    console.log("createReferrable.done");

    return referrable.distEnd;
  }

  function lockupAsset(CryptoAsset calldata asset, address funder) private {
    require(funder != address(this) && funder != address(0x0));
    require(asset.kind > 0 && asset.kind < 3);

    // Fungible token (ERC20 / currencies)
    if (asset.kind == 1) {
      IERC20Upgradeable erc20 = IERC20Upgradeable(asset.tokenContract);
      erc20.transferFrom(funder, address(this), asset.value);
      
      // // Track ownership
      // fungibleTokenVault[asset.tokenContract][funder] += asset.asset;
      return;
    }

    // Non-fungible token (ERC721 / NFT)
    if(asset.kind == 2) {
      IERC721Upgradeable erc721 = IERC721Upgradeable(asset.tokenContract);
      erc721.safeTransferFrom(funder, address(this), asset.value);

      // // Track ownership
      // nonFungibleTokenVault[asset.tokenContract][funder].push(asset.asset);
      return;
    }
    
    revert("unsupported asset");
  }

  function getBountiesForReferrable(bytes32 refHash) public view returns (CryptoAsset memory primaryBounty, CryptoAsset memory secondaryBounty) {
    TimedReferrable storage tr = referrables[refHash];
    return (tr.primaryBounty, tr.secondaryBounty);
  }

  // TODO
  // function releaseAsset(CryptoAsset calldata asset, ) {
  //   Maintain marketplace here.
  // }

  // --------------- REFERRAL TOKENS
  // -------------------------------

  function mintReferralToken(bytes32 refHash) public {
    uint256 price = referrables[refHash].price;
    require(block.timestamp <= referrables[refHash].mintEnd);
    attr.transferFrom(msg.sender, address(this), price);
    referralTokenContract.mintNewToken(refHash, msg.sender);
  }

  // Dev function for rinkeny
  function flipReleaseChecks(bool state) public onlyOwner {
    require(block.chainid != 1); // Don't allow this on mainnet
    releaseChecksDisabled = state;
  }
}

// --------------- HELPERS
// ----------------------------

// Calculate a unique hash for a single referrable, which can be used for indexing
function calcRefHash(address sender, TimedReferrable memory referrable) pure returns (bytes32) {
  return sha256(abi.encode(sender, referrable)); // no encodePacked here
}

// Verify is a referrable has a good structure
function verifyReferrable(TimedReferrable memory referrable) pure {
  require(referrable.convertable.ERC721.contractAddress != address(0x0));
}

