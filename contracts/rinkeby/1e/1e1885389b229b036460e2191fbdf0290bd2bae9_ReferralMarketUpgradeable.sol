pragma solidity ^0.8.0;

struct CryptoAsset {
  // Asset contract
  address tokenContract;

  // what kind of contract is this (1 for erc20, 2 for erc721)
  uint8 kind;

  // The currency value / TokenId
  uint256 value;
}

// All convertables have to implement this interface
struct Convertable {
  // NFT convertable
  ERC721Convertable ERC721;
}

// NFT support
struct ERC721Convertable {
  address contractAddress;
  uint256 tokenId;

    // Conversion validation fields
  address seller;

  // // TODO, maybe we can avoid this by putting more trust in miners or move it to a publish step
  // bytes ownershipSourceTransaction; 
}

// Structure to track state by referrable
struct TimedReferrable {
  // What is being sold
  Convertable convertable;

  // The primary bounty (eg: commission bounty)
  CryptoAsset primaryBounty;

  // The secondary bounty (eg: cashback bounty)
  CryptoAsset secondaryBounty;

  // IPFS metadata content address
  // 46 bytes
  bytes contentHash;

  // Minting period for this referrable
  // Times in here are relevant to block timing
  uint64 mintStart;
  uint64 mintEnd;

  // Distribution period for this referrable
  // Times in here are relevant to block timing
  uint64 distStart;
  uint64 distEnd; // when 0: unlimited?

  // Price in ATTR this token
  uint256 price;

  // // We send this info through the transaction log
  // // The resource to refer to, this can be without protocol prefix
  // // eg: target.com/1234...
  // // TODO: maybe remove this and add it to the external content
  // string targetURI;
}


