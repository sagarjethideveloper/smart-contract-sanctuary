pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@uniswap/v3-core/contracts/interfaces/IUniswapV3Pool.sol";
import "./libraries/OracleLibrary.sol";
import "./IYoinkTokenFactory.sol";
import "hardhat/console.sol";

error AlreadyInitialized();
error IncorrectNFT();
error InsufficentVotes();
error InsufficentOracleData();
error InsufficentLiquidity();
error AlreadyRedeemed();
error NotRedeemed();
error Overflow();

contract YoinkToken is ERC20, IERC721Receiver {
  ERC721 public immutable nft;
  uint256 public immutable tokenId;

  uint256 public immutable redemptionThreshold;
  ERC20 public immutable paymentToken;
  IUniswapV3Pool public immutable uniswapPool;

  mapping(address => address) public votes;
  mapping(address => uint256) public totalVotes;

  uint16 private constant MIN_OBSERVATION_CARDINALITY = 2;

  bool public hasBeenRedeemed;

  event Initialized(address indexed initializer);
  event VoteSet(address indexed voter, address indexed recipient);
  event Redeemed(address indexed recipient, uint256 tokensBurned, uint256 paymentAmount);
  event PaymentClaimed(address indexed holder, uint256 amount);

  constructor(
    address _nft,
    uint256 _tokenId,
    address _paymentToken,
    uint256 threshold
  ) ERC20(
    string(abi.encodePacked("Yoink - ", ERC721(_nft).name(), " #", uint2str(_tokenId))),
    string(abi.encodePacked("YNK-", ERC721(_nft).symbol()))
  ) {
    tokenId = _tokenId;
    nft = ERC721(_nft);
    paymentToken = ERC20(_paymentToken);
    redemptionThreshold = threshold;

    IYoinkTokenFactory _factory = IYoinkTokenFactory(msg.sender);
    IUniswapV3Pool _uniswapPool = IUniswapV3Pool(_factory.createUniswapPool(address(this), _paymentToken));
    uniswapPool = _uniswapPool;
  }

  function initialize(address recipient) external {
    if (totalSupply() != 0) {
      revert AlreadyInitialized();
    }

    _mint(recipient, 1e18);

    // This allows a "router" to send the token directly, without approvals
    if (nft.ownerOf(tokenId) != address(this)) {
      nft.transferFrom(msg.sender, address(this), tokenId);
    }

    uniswapPool.increaseObservationCardinalityNext(MIN_OBSERVATION_CARDINALITY);

    emit Initialized(msg.sender);
  }

  function onERC721Received(
    address /*operator*/,
    address from,
    uint256 _tokenId,
    bytes calldata data
  ) external override returns (bytes4) {
    if (totalSupply() != 0) {
      revert AlreadyInitialized();
    }
    if (msg.sender != address(nft) || _tokenId != tokenId) {
      revert IncorrectNFT();
    }

    address recipient = from;
    if (data.length > 0) {
      (recipient) = abi.decode(data, (address));
    }

    _mint(from, 1e18);
    emit Initialized(from);

    return IERC721Receiver.onERC721Received.selector;
  }

  function redeem(address recipient) external {
    if (hasBeenRedeemed) {
      revert AlreadyRedeemed();
    }

    if (totalVotes[recipient] < redemptionThreshold) {
      revert InsufficentVotes();
    }

    (,,, uint16 observationCardinality,,,) = uniswapPool.slot0();
    if (observationCardinality < MIN_OBSERVATION_CARDINALITY) {
      revert InsufficentOracleData();
    }
console.log("1");

    uint32[] memory secondsAgos = new uint32[](2);
    secondsAgos[0] = 0;
    secondsAgos[1] = 1 hours;
    (int56[] memory tickCumulatives, uint160[] memory secPerLiqX128s) = uniswapPool.observe(secondsAgos);

    uint256 secondsPerLiquidity = (secPerLiqX128s[0] - secPerLiqX128s[1]) / 1 hours;
    if (secondsPerLiquidity < 100e18) { // TODO: figure out the actual amount of liquidity needed
      revert InsufficentLiquidity();
    }
console.log("2");
    uint256 supply = totalSupply();
    uint256 balance = balanceOf(msg.sender);
    uint256 yoinkTokensToBuyOut = supply - balance;
    if (yoinkTokensToBuyOut > type(uint128).max) {
      revert Overflow();
    }

    // TODO: Use data above
    int24 averageTick = OracleLibrary.consult(address(uniswapPool), 1 hours);
    
    uint256 payment = OracleLibrary.getQuoteAtTick(
      averageTick,
      uint128(yoinkTokensToBuyOut),
      address(this),
      address(paymentToken)
    );

    _burn(msg.sender, balance);
    hasBeenRedeemed = true;

    paymentToken.transferFrom(msg.sender, address(this), payment);

    nft.safeTransferFrom(address(this), recipient, tokenId);

    emit Redeemed(recipient, balance, payment);
  }

  function claimPayment(address recipient) external returns (uint256 payment) {
    if (!hasBeenRedeemed) {
      revert NotRedeemed();
    }

    uint256 balance = balanceOf(msg.sender);
    uint256 paymentBalance = paymentToken.balanceOf(address(this));

    payment = paymentBalance * balance / totalSupply();

    _burn(msg.sender, balance);
    paymentToken.transfer(recipient, payment);

    emit PaymentClaimed(msg.sender, payment);
  }

  function vote(address recipient) external {
    uint256 balance = balanceOf(msg.sender);

    address currentVote = votes[msg.sender];
    if (currentVote != address(0)) {
      totalVotes[currentVote] -= balance;
    }

    votes[msg.sender] = recipient;

    if (recipient != address(0)) {
      totalVotes[recipient] += balance;
    }

    emit VoteSet(msg.sender, recipient);
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 amount
  ) internal override {
    address fromVote = votes[from];
    address toVote = votes[to];

    if (fromVote != address(0)) {
      totalVotes[fromVote] -= amount;
    }
    if (toVote != address(0)) {
      totalVotes[toVote] += amount;
    }
  }

  function uint2str(uint _i) internal pure returns (string memory _uintAsString) {
    if (_i == 0) {
        return "0";
    }
    uint j = _i;
    uint len;
    while (j != 0) {
        len++;
        j /= 10;
    }
    bytes memory bstr = new bytes(len);
    uint k = len;
    while (_i != 0) {
        k = k-1;
        uint8 temp = (48 + uint8(_i - _i / 10 * 10));
        bytes1 b1 = bytes1(temp);
        bstr[k] = b1;
        _i /= 10;
    }
    return string(bstr);
  }
}

