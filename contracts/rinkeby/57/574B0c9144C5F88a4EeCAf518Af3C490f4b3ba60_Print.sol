pragma solidity ^0.7.0;
import "./libraries/Ownable.sol";

contract Print is Ownable {
    uint256 private variable;

    constructor() {
        variable = 20;
    }

    function setVariable(uint256 _variable) public onlyOwner {
        variable = _variable;
    }

    function gerVariable() public onlyOwner returns (uint256) {
        require(msg.sender == owner);
        return variable;
    }
}

