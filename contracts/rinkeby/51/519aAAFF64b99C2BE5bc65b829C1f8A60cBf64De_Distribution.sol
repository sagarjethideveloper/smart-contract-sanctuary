pragma solidity ^0.8.5;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "../token/IERC20List.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "../staking/IStaking.sol";
import "hardhat/console.sol";

contract Distribution is AccessControl {
    event TotalDistributed(address[] tokens, uint256[] amounts);
    event AddressSet(
        address lotteryContract,
        address stakingContract,
        address HouseContract,
        address erc20List
    );

    uint256 prevTime;
    address lotteryContract;
    address stakingContract;
    address houseContract;
    IERC20List private erc20List;

    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function setDistAddress(
        address _lotteryCont,
        address _stakingCont,
        address _houseCont,
        address _erc20List
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        lotteryContract = _lotteryCont;
        stakingContract = _stakingCont;
        houseContract = _houseCont;
        erc20List = IERC20List(_erc20List);

        emit AddressSet(
            lotteryContract,
            stakingContract,
            houseContract,
            _erc20List
        );
    }

    /**
     *@param lotteryPerc should be 1-100
     *@param stakingPerc should be 1-100
     *@param housePerc should be 1-100
     */
    function distribute(
        uint256[] memory amounts,
        address[] memory tokens,
        uint256 lotteryPerc,
        uint256 stakingPerc,
        uint256 housePerc
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(amounts.length == tokens.length, "Length's should be same");
        uint256[] memory stakingAmount = new uint256[](amounts.length);

        for (uint256 i = 0; i < amounts.length; i++) {
            uint256 decimal = erc20List.getDecimal(tokens[i]);

            // %16 Lottery
            IERC20 token = IERC20(tokens[i]);
            token.transferFrom(
                msg.sender,
                lotteryContract,
                (amounts[i] * (lotteryPerc * (10**(decimal - 2)))) /
                    (10**decimal)
            );
            // %64 Staking
            stakingAmount[i] =
                (amounts[i] * (stakingPerc * (10**(decimal - 2)))) /
                (10**decimal);
            token.transferFrom(
                msg.sender,
                stakingContract,
                (amounts[i] * (stakingPerc * (10**(decimal - 2)))) /
                    (10**decimal)
            );
            // %20 House
            token.transferFrom(
                msg.sender,
                houseContract,
                (amounts[i] * (housePerc * (10**(decimal - 2)))) / (10**decimal)
            );
        }
        console.log(stakingAmount[0]);
        console.log(stakingAmount[1]);
        console.log(stakingAmount[2]);
        console.log(stakingAmount[3]);

        IStaking(stakingContract).fund(
            stakingAmount[0], //Should be matic
            stakingAmount[1], // Should be Wbtc
            stakingAmount[2], // Should be Weth
            stakingAmount[3] // Should be Usdt
        );

        emit TotalDistributed(tokens, amounts);
    }
}

