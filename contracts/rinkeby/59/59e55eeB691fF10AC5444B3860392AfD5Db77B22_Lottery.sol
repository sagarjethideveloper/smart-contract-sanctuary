pragma solidity 0.8.6;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@chainlink/contracts/src/v0.8/dev/VRFConsumerBase.sol";
import "../token/IERC20List.sol";
import "../collection/ICollection.sol";
import "../library/HeapSort.sol";
import "./ITickets.sol";
import "hardhat/console.sol";

// Represents the status of the lottery
enum Status {
    NotStarted, // The lottery has not started yet
    Open, // The lottery is open for ticket purchases
    Closed, // The lottery is no longer open for ticket purchases
    Completed // The lottery has been closed and the numbers drawn
}

// All the needed info around a lottery
struct LottoInfo {
    uint256 lotteryID; // ID for lotto
    Status lotteryStatus; // Status for lotto
    uint256 costPerTicket; // Cost per ticket in $winr
    uint256 startingTimestamp; // Block timestamp for star of lotto
    uint256 closingTimestamp; // Block timestamp for end of entries
    uint256 randomNumber;
    uint16[] winningNumbers; // The winning numbers
}

struct VRF {
    bytes32 keyHash;
    uint256 fee;
    address token;
    address coordinator;
}

contract Lottery is AccessControl, VRFConsumerBase {
    bytes32 public constant CONTRACT_ROLE = bytes32(keccak256("CONTRACT_ROLE"));

    //using Counters for uint256;

    event LotteryFunded(address[] tokens, uint256[] amounts);
    event LotteryOpen(
        uint256 lotteryId,
        uint256 startingTimestamp,
        uint256 closingTimestamp,
        uint256 circulatingAmount,
        uint256 costPerTicket
    );
    event LotteryNewTime(uint256 startTime, uint256 endTime);
    event LotteryClose(uint256 lotteryId);
    event LotteryCompleted(uint256 lotteryId, uint256[] wnngNumbs);
    event ClaimedReward(
        uint256 lotteryId,
        uint256 tokenId,
        address[] tokens,
        address sender,
        uint256[] amounts
    );
    event ClaimedWithNFT(
        uint256 lotteryId,
        address collection,
        address sender,
        uint256 amount
    );
    event TicketCostNew(uint256 ticketCost);
    event NoOfMatching(uint256 noOfMatching);
    event RandomNumber(uint256 number, uint256 lotteryId);

    // Instance of WINR token (collateral currency for lotto)
    IERC20 public immutable winr;
    // Instance of for all tokens to be added
    IERC20 private token;
    VRF private _vrf;

    // State variables
    // Counter for lottery IDs
    uint256 public lotteryIdCounter_;
    // Random generator address
    // NFT ticket address
    ITickets private ticket;
    // If there is no winner of the lottery, the prize will be distributed to this collection owners
    ICollection private collection;

    IERC20List private erc20List;
    // Random number
    uint256 private rnd;
    // Lottery number max
    uint256 maxBoundry = 20;
    // Lottery number min
    uint256 minBoundry = 1;
    // How many numbers will be winning the lottery
    uint256 winningTicketNumberLength = 5;
    // 1/1,000,000,000 * Total number of tokens in circulation 1e18
    uint256 public constant ticketPriceMul = 1_000_000_000;
    // Lottery ID's to info
    mapping(uint256 => LottoInfo) internal allLotteries_;
    // Lottery claim %10 percent info
    mapping(address => mapping(uint256 => bool)) public claimWithNFT;
    // Lottery prize distribution if user has nft on this collection
    mapping(uint256 => mapping(uint256 => bool)) private winngNumb;
    mapping(uint256 => uint256[]) public getWinnerNumber;
    //Lotto prize amount for all different token
    mapping(address => uint256) public prizeAmounts;
    mapping(bytes32 => uint256) public requestAndLottryId;

    constructor(
        address _winr,
        address _ticket,
        VRF memory vrf
    )
        VRFConsumerBase(
            vrf.coordinator, // VRF Coordinator
            vrf.token // LINK Token
        )
    {
        lotteryIdCounter_ = 0;
        winr = IERC20(_winr);
        ticket = ITickets(_ticket);
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _vrf = vrf;
    }

    function setLuckyNftCollec(address _collecAdd)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        collection = ICollection(_collecAdd);
    }

    function setERC20List(address _erc20List)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        erc20List = IERC20List(_erc20List);
    }

    function getRandomNumber(uint256 _lotteryId)
        internal
        returns (bytes32 requestId)
    {
        require(
            LINK.balanceOf(address(this)) >= _vrf.fee,
            "Not enough LINK - fill contract with faucet"
        );
        bytes32 requestId = requestRandomness(_vrf.keyHash, _vrf.fee);
        requestAndLottryId[requestId] = _lotteryId;
        return requestId;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness)
        internal
        override
    {
        uint256 lotteryId = requestAndLottryId[requestId];
        allLotteries_[lotteryId].randomNumber = randomness;

        emit RandomNumber(randomness, lotteryId);
    }

    function withdrawLink() external onlyRole(DEFAULT_ADMIN_ROLE) {
        LINK.transfer(msg.sender, LINK.balanceOf(address(this)));
    }

    function _setPrizeAmount() internal {
        address[] memory supportedTokens = erc20List.getWhitelist();

        for (uint256 i = 0; i < supportedTokens.length; i++) {
            address tokenAddress = supportedTokens[i];

            token = IERC20(tokenAddress);
            prizeAmounts[tokenAddress] = token.balanceOf(address(this));
        }
    }

    function createLottery(
        uint256 _startingTimestamp,
        uint256 _closingTimestamp,
        uint256 _circulatingAmount
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        if (lotteryIdCounter_ >= 1) {
            require(
                allLotteries_[lotteryIdCounter_].lotteryStatus ==
                    Status.Completed,
                "You can't create multiple lotterys at the same time"
            );
        }

        require(
            _startingTimestamp >= block.timestamp,
            "Start time should be bigger than the current time"
        );

        require(
            _startingTimestamp < _closingTimestamp,
            "End time should be greater than the start time"
        );

        lotteryIdCounter_++;
        uint256 costPerTic = _circulatingAmount * ticketPriceMul;

        allLotteries_[lotteryIdCounter_].lotteryID = lotteryIdCounter_;
        allLotteries_[lotteryIdCounter_].lotteryStatus = Status.Open;
        allLotteries_[lotteryIdCounter_].startingTimestamp = _startingTimestamp;
        allLotteries_[lotteryIdCounter_].closingTimestamp = _closingTimestamp;
        allLotteries_[lotteryIdCounter_].costPerTicket = costPerTic;

        _setPrizeAmount();

        emit LotteryOpen(
            lotteryIdCounter_,
            _startingTimestamp,
            _closingTimestamp,
            _circulatingAmount,
            costPerTic
        );
    }

    function changeLotteryTime(
        uint256 _lotteryId,
        uint256 _startingTimestamp,
        uint256 _closingTimestamp
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            _startingTimestamp >= block.timestamp,
            "Lottery start time should be greater than current time"
        );

        require(
            _startingTimestamp < _closingTimestamp,
            "end time should be greater than start time"
        );

        allLotteries_[_lotteryId].startingTimestamp = _startingTimestamp;
        allLotteries_[_lotteryId].closingTimestamp = _closingTimestamp;

        emit LotteryNewTime(_startingTimestamp, _closingTimestamp);
    }

    function costToBuyTickets(uint256 circulatingAmount, uint256 _lotteryId)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (uint256 totalCost)
    {
        uint256 ticketPrice = circulatingAmount * ticketPriceMul;
        allLotteries_[_lotteryId].costPerTicket = ticketPrice;

        emit TicketCostNew(ticketPrice);

        return ticketPrice;
    }

    function getBasicLottoInfo(uint256 _lotteryId)
        external
        view
        returns (LottoInfo memory)
    {
        return (allLotteries_[_lotteryId]);
    }

    /**
     * @notice The funding @param _amounts order must be in the order of the tokens listed in the erc20List contract.
     * @dev this function should be called after sending token to lottery.
     */
    function fund(uint256[] memory _amounts)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        address[] memory supportedTokens = erc20List.getWhitelist();
        require(
            _amounts.length == supportedTokens.length,
            "The amount you entered does not match the number of tokens in the list."
        );

        for (uint256 i = 0; i < _amounts.length; i++) {
            address tokenAddress = supportedTokens[i];

            token = IERC20(tokenAddress);

            //token.transferFrom(msg.sender, address(this), _amounts[i]);

            prizeAmounts[tokenAddress] += token.balanceOf(address(this));
        }
        emit LotteryFunded(supportedTokens, _amounts);
    }

    function drawWinningNumbers(uint256 _lotteryId)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        // Checks that the lottery is past the closing block
        require(
            allLotteries_[_lotteryId].closingTimestamp <= block.timestamp,
            "Cannot set winning numbers during lottery"
        );
        // Checks lottery numbers have not already been drawn
        require(
            allLotteries_[_lotteryId].lotteryStatus == Status.Open,
            "Lottery State incorrect for draw"
        );
        // Sets lottery status to closed
        allLotteries_[_lotteryId].lotteryStatus = Status.Closed;
        // Requests a random number from the generator

        //##############################################
        //THIS LINE SHOULD BE COMMEND OUT FOR LOCAL TESTING !!!!!
        getRandomNumber(_lotteryId);
        //##############################################

        // Emits that random number has been requested
        emit LotteryClose(_lotteryId);
    }

    function withdrawWINR(uint256 _amount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        winr.transfer(msg.sender, _amount);
    }

    function claimReward(uint256 _lotteryId, uint256 _tokenId) external {
        // Checking the lottery is in a valid time for claiming
        require(
            allLotteries_[_lotteryId].closingTimestamp <= block.timestamp,
            "Wait till end to claim"
        );
        // Checks the lottery winning numbers are available
        require(
            allLotteries_[_lotteryId].lotteryStatus == Status.Completed,
            "Winning Numbers not chosen yet"
        );
        require(
            ticket.getOwnerOfTicket(_tokenId, msg.sender),
            "Only the owner can claim"
        );
        // Sets the claim of the ticket to true (if claimed, will revert)
        require(!ticket.isClaimed(_tokenId, _lotteryId), "You already claimed");
        // Getting the number of matching tickets
        allLotteries_[_lotteryId].winningNumbers = HeapSort.heapSort(
            allLotteries_[_lotteryId].winningNumbers
        );

        uint8 matchingNumbers = _getNumberOfMatching(
            ticket.getTicketNumber(_tokenId, _lotteryId),
            allLotteries_[_lotteryId].winningNumbers
        );

        address[] memory supportedTokens = erc20List.getWhitelist();
        uint256[] memory playerPrizeAmounts = _prizeForMatching(
            matchingNumbers,
            _lotteryId
        );

        for (uint256 i = 0; i < playerPrizeAmounts.length; i++) {
            address tokenAddress = supportedTokens[i];

            token = IERC20(tokenAddress);

            // Removing the prize amount from the pool
            //prizeAmounts[tokenAddress] -= playerPrizeAmounts[i];

            // Transfering the user their winnings
            token.transfer(address(msg.sender), playerPrizeAmounts[i]);
        }
        emit ClaimedReward(
            _lotteryId,
            _tokenId,
            supportedTokens,
            msg.sender,
            playerPrizeAmounts
        );

        ticket.claimTicket(_tokenId, _lotteryId);
    }

    function _getNumberOfMatching(
        uint16[] memory _usersNumber,
        uint16[] memory _winningNumbers
    ) internal returns (uint8 noOfMatching) {
        bytes32 userHash = keccak256(abi.encode(_usersNumber));
        bytes32 winningHash = keccak256(abi.encode(_winningNumbers));

        if (userHash == winningHash) {
            noOfMatching = 5;
        }

        require(noOfMatching == 5, "You didn't win the lottery");

        emit NoOfMatching(noOfMatching);

        return noOfMatching;
    }

    /**
     * @param   _noOfMatching The number of matching numbers the user has
     * @param   _lotteryId The ID of the lottery the user is claiming on
     * @return  The prize amount in cake the user is entitled to
     */
    function _prizeForMatching(uint8 _noOfMatching, uint256 _lotteryId)
        internal
        view
        returns (uint256[] memory)
    {
        // If user has no matching numbers their prize is 0
        require(_noOfMatching == 5, "You didn't win the lottery");

        // Getting the percentage of the pool the user has won
        uint256 getSellCount = ticket.getSellCount(
            _lotteryId,
            allLotteries_[_lotteryId].winningNumbers
        );
        // Timesing the percentage one by the pool
        address[] memory supportedTokens = erc20List.getWhitelist();
        uint256[] memory prizes = new uint256[](supportedTokens.length);

        for (uint256 i = 0; i < supportedTokens.length; i++) {
            address tokenAddress = supportedTokens[i];

            uint256 decimal = erc20List.getDecimal(tokenAddress);

            prizes[i] =
                (prizeAmounts[tokenAddress] * (10**decimal)) /
                (getSellCount * (10**decimal));
        }

        return (prizes);
    }

    function split(uint256 _lotteryId)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (uint256[] memory)
    {
        bool _continue = true;
        uint256[] memory winningNumbers = new uint256[](5);
        uint256 numberLen = 5;
        uint256 index = 0;
        uint256 hashIndex = 0;
        uint256 randomNumber = allLotteries_[_lotteryId].randomNumber;
        while (_continue == true) {
            bytes32 hashOfRandom = keccak256(
                abi.encodePacked(randomNumber, hashIndex)
            );
            uint256 numberRepresentation = uint256(hashOfRandom);
            uint256 pickedNumber = ((numberRepresentation) % 20) + 1;

            if (winngNumb[_lotteryId][pickedNumber] == false) {
                winngNumb[_lotteryId][pickedNumber] = true;
                winningNumbers[index] = pickedNumber;
                allLotteries_[_lotteryId].winningNumbers.push(
                    uint16(pickedNumber)
                );
                index++;
            }
            hashIndex++;

            if (index == numberLen) {
                _continue = false;
            }
        }

        allLotteries_[_lotteryId].lotteryStatus = Status.Completed;

        getWinnerNumber[_lotteryId] = winningNumbers;

        emit LotteryCompleted(_lotteryId, winningNumbers);

        return winningNumbers;
    }

    function getLotterWinnerNumb(uint256 id)
        external
        view
        returns (uint256[] memory)
    {
        return getWinnerNumber[id];
    }

    /**
     *@notice It doesn't matter how many NFTs a person has. Can only get (prize % 10 / nftholders)
     *@notice This function only valid if no ones win the lottery
     */
    function distPrzNftHolders(uint256 _lotteryId) external {
        uint256 amount = collection.balanceOf(msg.sender);
        uint256 collecSold = collection.getSold();
        uint256 getSellCount = ticket.getSellCount(
            _lotteryId,
            allLotteries_[_lotteryId].winningNumbers
        );

        require(
            allLotteries_[_lotteryId].lotteryStatus == Status.Completed,
            "Winning Numbers not chosen yet"
        );

        require(getSellCount < 1, "There is a winner of the lottery");

        require(amount > 0, "You don't have nft");

        require(
            claimWithNFT[msg.sender][_lotteryId] == false,
            "You have already claimed the prize"
        );

        address[] memory supportedTokens = erc20List.getWhitelist();
        for (uint256 i = 0; i < supportedTokens.length; i++) {
            address tokenAddress = supportedTokens[i];

            token = IERC20(tokenAddress);
            uint256 tokenDecimal = erc20List.getDecimal(tokenAddress);

            uint256 tenPercent = (prizeAmounts[tokenAddress] *
                (10**(tokenDecimal - 1))) / (10**tokenDecimal);

            uint256 prizeAmount = (tenPercent * (10**tokenDecimal)) /
                (collecSold * (10**(tokenDecimal)));

            token.transfer(address(msg.sender), prizeAmount);

            emit ClaimedWithNFT(
                _lotteryId,
                address(collection),
                msg.sender,
                prizeAmount
            );
        }

        claimWithNFT[msg.sender][_lotteryId] = true;
    }
}

