pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./KiraAccessControl.sol";
import "./RewardDistributor.sol";

import "hardhat/console.sol";

contract NFTStaking is Context, ERC1155Holder {
    using SafeMath for uint256;

    IERC1155 nftToken;

    KiraAccessControl accessControl;
    RewardDistributor distributor;

    uint256 rewardPerNFT = 25 * 10**18;

    struct STAKE {
        uint256 initTime;
        uint256 lastClaimedAt;
        uint256 amount;
    }

    struct STAKE_DETAIL {
        STAKE[] stakes;
        uint256 rewardSoFar;
        uint256 firstStakedAt;
        uint256 total;
    }

    mapping(uint256 => mapping(address => STAKE_DETAIL)) stakingBalance;

    mapping(uint256 => uint256) totalStakes;

    event Stake(address indexed _staker, uint256 nftTokenId, uint256 amount);
    event Unstake(address indexed _staker, uint256 nftTokenId, uint256 amount);
    event Withdraw(address indexed _staker, uint256 nftTokenId, uint256 amount);

    constructor(
        KiraAccessControl _accessControl,
        RewardDistributor _distributor,
        IERC1155 _nftToken
    ) {
        accessControl = _accessControl;
        distributor = _distributor;
        nftToken = _nftToken;
    }

    modifier onlyManager {
        require(
            accessControl.hasManagerRole(_msgSender()),
            "Need manager role"
        );
        _;
    }

    /**
     * @notice change access control contract
     * @param _accessControl is new access control contract
     */
    function updateAccessControl(KiraAccessControl _accessControl) public {
        require(
            accessControl.hasAdminRole(_msgSender()),
            "updateAccessControl: Sender must be admin"
        );

        require(
            address(_accessControl) != address(0),
            "updateAccessControl: New access controls cannot be ZERO address"
        );

        accessControl = _accessControl;
    }

    /**
     * @notice change distributor contract
     * @param _distributor is new distributor contract
     */
    function updateDistributor(RewardDistributor _distributor) external {
        require(
            accessControl.hasAdminRole(_msgSender()),
            "updateDistributor: Sender must be admin"
        );

        require(
            address(_distributor) != address(0),
            "updateDistributor: New Distributor cannot be ZERO address"
        );

        distributor = _distributor;
    }

    /**
     * @notice calculate total stakes of staker
     * @param _staker is the address of staker
     * @return _total
     */
    function totalStakeOf(uint256 nftTokenId, address _staker)
        public
        view
        returns (uint256)
    {
        return stakingBalance[nftTokenId][_staker].total;
    }

    /**
     * @notice calculate entire stake amount
     * @return _total
     */
    function getTotalStakes(uint256 nftTokenId) public view returns (uint256) {
        return totalStakes[nftTokenId];
    }

    /**
     * @notice get the first staked time
     * @return firstStakedAt
     */
    function getFirstStakedAtOf(uint256 nftTokenId, address _staker)
        public
        view
        returns (uint256)
    {
        return stakingBalance[nftTokenId][_staker].firstStakedAt;
    }

    /**
     * @notice get total claimed reward of staker
     * @return rewardSoFar
     */
    function getRewardSoFarOf(uint256 nftTokenId, address _staker)
        public
        view
        returns (uint256)
    {
        return stakingBalance[nftTokenId][_staker].rewardSoFar;
    }

    /**
     * @notice calculate reward of staker
     * @return reward is the reward amount of the staker
     */
    function rewardOf(uint256 nftTokenId, address _staker)
        public
        view
        returns (uint256)
    {
        STAKE_DETAIL memory _stakeDetail = stakingBalance[nftTokenId][_staker];

        if (_stakeDetail.total == 0) return 0;

        uint256 _totalReward;

        for (uint256 i = 0; i < _stakeDetail.stakes.length; i = i.add(1)) {
            STAKE memory _stake = _stakeDetail.stakes[i];

            uint256 timePassed;
            uint256 timeNow = block.timestamp;

            timePassed = timeNow - _stake.initTime;

            if (_stake.lastClaimedAt != _stake.initTime) {
                timePassed = timePassed.sub(
                    _stake.lastClaimedAt - _stake.initTime
                );
            }
            uint256 _reward =
                _stake.amount.mul(rewardPerNFT).mul(timePassed).div(30 days);

            _totalReward = _totalReward.add(_reward);
        }

        if (_totalReward > distributor.totalRewards())
            return distributor.totalRewards();
        return _totalReward;
    }

    function claimReward(uint256 nftTokenId) public {
        uint256 reward = rewardOf(nftTokenId, _msgSender());
        STAKE_DETAIL storage _stake = stakingBalance[nftTokenId][_msgSender()];

        distributor.distribute(_msgSender(), reward);
        for (uint256 i = 0; i < _stake.stakes.length; i = i.add(1)) {
            _stake.stakes[i].lastClaimedAt = block.timestamp;
        }

        _stake.rewardSoFar = _stake.rewardSoFar.add(reward);

        emit Withdraw(_msgSender(), nftTokenId, reward);
    }

    /**
     * @notice stake KEX
     * @param _amount is the kex amount to stake
     */
    function stake(uint256 nftTokenId, uint256 _amount) public {
        nftToken.safeTransferFrom(
            _msgSender(),
            address(this),
            nftTokenId,
            _amount,
            ""
        );

        STAKE_DETAIL storage _stake = stakingBalance[nftTokenId][_msgSender()];

        if (_stake.total == 0) {
            _stake.firstStakedAt = block.timestamp;
        }

        _stake.total = _stake.total.add(_amount);
        _stake.stakes.push(STAKE(block.timestamp, block.timestamp, _amount));
        totalStakes[nftTokenId] = totalStakes[nftTokenId].add(_amount);

        emit Stake(_msgSender(), nftTokenId, _amount);
    }

    /**
     * @notice unstake current staking
     */
    function unstake(uint256 nftTokenId) public {
        require(
            stakingBalance[nftTokenId][_msgSender()].total > 0,
            "Not staking"
        );

        STAKE_DETAIL storage _stake = stakingBalance[nftTokenId][_msgSender()];
        uint256 reward = rewardOf(nftTokenId, _msgSender());

        distributor.distribute(_msgSender(), reward);
        nftToken.safeTransferFrom(
            address(this),
            _msgSender(),
            nftTokenId,
            _stake.total,
            ""
        );

        totalStakes[nftTokenId] = totalStakes[nftTokenId].sub(_stake.total);
        _stake.total = 0;
        _stake.rewardSoFar = _stake.rewardSoFar.add(reward);
        _stake.firstStakedAt = 0;
        delete _stake.stakes;

        emit Unstake(_msgSender(), nftTokenId, _stake.total);
    }
}

