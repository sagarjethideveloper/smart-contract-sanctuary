pragma solidity =0.7.6;

import '../interfaces/IPositionNFT.sol';

import '@openzeppelin/contracts/token/ERC721/ERC721.sol';


abstract contract PositionNFT is ERC721, IPositionNFT {

    constructor(string memory name, string memory symbol) ERC721(name, symbol) {}

    
    uint64 public override nextPositionID;

    
    
    function positionIDs(address account) external view override returns (uint64[] memory IDs) {
        uint balance =  balanceOf(account);
        IDs = new uint64[](balance);
        for(uint i = 0; i < balance; i++) IDs[i] = uint64(tokenOfOwnerByIndex(account, i));
    }

    
    function isApprovedOrOwner(address account, uint tokenID) external view override returns (bool) {
        return _isApprovedOrOwner(account, tokenID);
    }

    
    
    function mintTo(address dest) external override returns (uint64 id) {
        require(_canMint(msg.sender), 'Not Authorized');
        require(nextPositionID < type(uint64).max, 'Overflow');

        id = nextPositionID;
        nextPositionID++;

        _mint(dest, id);

        return id;
    }

    
    function burn(uint64 tokenID) external override {
        require(_canBurn(msg.sender), 'Not Authorized');

        _burn(tokenID);
    }

    
    function _canMint(address) internal virtual view returns (bool);

    
    function _canBurn(address) internal virtual view returns (bool);
}

