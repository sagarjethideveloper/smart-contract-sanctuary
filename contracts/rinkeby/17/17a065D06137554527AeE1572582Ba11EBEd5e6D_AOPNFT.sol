pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "./IAOPManager.sol";

contract AOPNFT is Initializable, ERC721Upgradeable, PausableUpgradeable, AccessControlUpgradeable {
    using SafeMathUpgradeable for uint256;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    CountersUpgradeable.Counter private _tokenIdCounter;

    enum Race {
        A,
        B,
        C,
        D,
        E
    }

    struct Pet {
        Race race;
        uint256 exp;
        uint256 dna;
        uint256 bornTime;
        bool isCreation;
    }

    struct Sale {
        address seller;
        uint256 aopPrice;
        uint256 bnbPrice;
    }

    Pet[] public pets;
    IERC20Upgradeable public aopToken;
    IAOPManager public manager;

    uint256 public MIN_MARKET_AOP_PRICE;
    uint256 private _marketSize;

    mapping(uint256 => Sale) public market;
    mapping(address => EnumerableSetUpgradeable.UintSet) private _ownerPets;
    mapping(address => EnumerableSetUpgradeable.UintSet) private _ownerSales;
    mapping(uint256 => bool) public isDNAUpdated;

    event MarketSell(uint256 indexed tokenId, address indexed seller);
    event MarketAOPBuy(uint256 indexed tokenId, address indexed buyer, address indexed seller);
    event MarketBNBBuy(uint256 indexed tokenId, address indexed buyer, address indexed seller);
    event MarketDown(uint256 indexed tokenId, address indexed seller);
    event MarketUpdateAOPPrice(uint256 indexed tokenId, address seller, uint256 newPrice);
    event MarketUpdateBNBPrice(uint256 indexed tokenId, address seller, uint256 newPrice);
    event AddExp(uint256 indexed tokenId, uint256 exp);
    event UpdateDNA(uint256 indexed tokenId, uint256 dna);
    event UpdateRace(uint256 indexed tokenId, Race race);

    function initialize() initializer public {
        __ERC721_init("AttackOnPet NFT", "AOPN");
        __Pausable_init();
        __AccessControl_init();

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(PAUSER_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);

        MIN_MARKET_AOP_PRICE = 1 ether;
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
    internal
    whenNotPaused
    override
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
    public
    view
    override(ERC721Upgradeable, AccessControlUpgradeable)
    returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function initMarket(uint256 dna, Race race, uint256 amount, uint256 bnbPrice) public onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint256 i = 0; i < amount; i ++) {
            uint256 tokenId = _petMint(msg.sender, dna, race, true);
            market[tokenId] = Sale(msg.sender, 0, bnbPrice);
            _ownerSales[msg.sender].add(tokenId);
            _marketSize.add(1);
        }
    }

    function petMint(Race race, address account) public onlyRole(DEFAULT_ADMIN_ROLE) returns (uint256)  {
        return _petMint(account, manager.randomDNA(), race, false);
    }

    function _petMint(address to, uint256 dna, Race race, bool isCreation) private returns (uint256) {
        uint256 tokenId = _tokenIdCounter.current();
        pets.push(Pet(race, 0, dna, block.timestamp, isCreation));
        _ownerPets[to].add(tokenId);
        _safeMint(to, tokenId);
        _tokenIdCounter.increment();
        return tokenId;
    }

    function addExp(uint256 tokenId, uint256 exp) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(ownerOf(tokenId) != address(0), "tokenId: not exist");
        require(exp > 0, "exp: invalid value");
        Pet storage pet = pets[tokenId];
        pet.exp = pet.exp.add(exp);
        emit AddExp(tokenId, exp);
    }

    function updateDNA(uint256 tokenId, uint256 dna) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(ownerOf(tokenId) != address(0), "tokenId: not exist");
        require(!isDNAUpdated[tokenId], "require: not evolved");
        Pet storage pet = pets[tokenId];
        pet.bornTime = block.timestamp;
        pet.dna = dna;
        isDNAUpdated[tokenId] = true;
        emit UpdateDNA(tokenId, dna);
    }

    function updateRace(uint256 tokenId, Race race) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(ownerOf(tokenId) != address(0), "tokenId: not exist");
        Pet storage pet = pets[tokenId];
        pet.race = race;
        emit UpdateRace(tokenId, race);
    }

    function ownerPets(address account) public view returns (Pet[] memory) {
        uint256 count = _ownerPets[account].length();
        Pet[] memory myPets = new Pet[](count);
        for (uint256 i = 1; i <= count; i++) {
            uint256 tokenId = _ownerPets[account].at(i);
            myPets[i] = pets[tokenId];
        }
        return myPets;
    }

    function marketSell(uint256 tokenId, uint256 price) public {
        require(ownerOf(tokenId) == _msgSender(), "pet not yours");
        require(price >= MIN_MARKET_AOP_PRICE, 'price: invalid value');
        market[tokenId] = Sale(_msgSender(), price, 0);
        _ownerSales[_msgSender()].add(tokenId);
        _marketSize.add(1);
        emit MarketSell(tokenId, _msgSender());
    }

    function marketAOPBuy(uint256 tokenId) public {
        require(market[tokenId].seller != address(0), "tokenId: not on market");
        uint256 price = market[tokenId].aopPrice;
        aopToken.transfer(market[tokenId].seller, price);
        safeTransferFrom(market[tokenId].seller, _msgSender(), tokenId);
        delete market[tokenId];
        _ownerSales[_msgSender()].remove(tokenId);
        _marketSize -= 1;
        emit MarketAOPBuy(tokenId, _msgSender(), market[tokenId].seller);
    }

    function marketBNBBuy(uint256 tokenId) public payable {
        require(market[tokenId].seller != address(0), "tokenId: not on market");
        uint256 price = market[tokenId].bnbPrice;
        require(msg.value >= price, "not enough money");
        safeTransferFrom(market[tokenId].seller, _msgSender(), tokenId);
        delete market[tokenId];
        _ownerSales[_msgSender()].remove(tokenId);
        _marketSize -= 1;
        emit MarketBNBBuy(tokenId, _msgSender(), market[tokenId].seller);
    }

    function marketDown(uint256 tokenId) public {
        require(ownerOf(tokenId) == _msgSender(), "pet not yours");
        require(market[tokenId].seller != address(0), "tokenId: not on market");
        delete market[tokenId];
        _ownerSales[_msgSender()].remove(tokenId);
        _marketSize -= 1;
        emit MarketDown(tokenId, _msgSender());
    }

    function updateAOPPrice(uint256 tokenId, uint256 price) public {
        require(ownerOf(tokenId) == _msgSender(), "pet not yours");
        require(market[tokenId].seller != address(0), "tokenId: not on market");
        require(price >= MIN_MARKET_AOP_PRICE, "price: invalid value");
        market[tokenId].aopPrice = price;
        emit MarketUpdateAOPPrice(tokenId, market[tokenId].seller, price);
    }

    function updateBNBPrice(uint256 tokenId, uint256 price) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(market[tokenId].seller != address(0), "tokenId: not on market");
        require(price > 0, "price: invalid value");
        market[tokenId].bnbPrice = price;
        emit MarketUpdateBNBPrice(tokenId, market[tokenId].seller, price);
    }

    function saleCount(address) public view returns (uint256) {
        return _ownerSales[_msgSender()].length();
    }

    function marketSize() public view returns (uint256) {
        return _marketSize;
    }

    function mysteryBoxesPrice() public view returns (uint256) {
        return manager.mysteryBoxesPrice();
    }

    function setManager(address account) public onlyRole(DEFAULT_ADMIN_ROLE) {
        manager = IAOPManager(account);
    }

    function setAOPToken(address account) public onlyRole(DEFAULT_ADMIN_ROLE) {
        aopToken = IERC20Upgradeable(account);
    }

    function buyMysteryBoxes(Race[] memory races) public returns (uint256[] memory){
        uint256[] memory tokenIds = new uint256[](races.length);
        uint256 allReceipt = mysteryBoxesPrice() * races.length;
        uint256 teamReceipt = allReceipt * manager.teamReceipt() / 10000;
        uint256 marketReceipt = allReceipt - teamReceipt;
        aopToken.safeApprove(msg.sender, allReceipt);
        aopToken.safeTransferFrom(msg.sender, manager.marketAddress(), marketReceipt);
        aopToken.safeTransferFrom(msg.sender, manager.teamReceiptAddress(), teamReceipt);
        for (uint i = 0; i < races.length; i++) {
            tokenIds[i] = _petMint(msg.sender, manager.randomDNA(), races[i], false);
        }
        return tokenIds;
    }
}

