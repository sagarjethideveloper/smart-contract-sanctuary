pragma solidity ^0.8.0;

interface IAOPManager {
    function mysteryBoxesPrice() external view returns (uint256);

    function teamReceipt() external view returns (uint256);

    function teamReceiptAddress() external view returns (address);

    function randomDNA() external view returns (uint256);

    function isMarketAddress(address account) external view returns (bool);

    function marketAddress() external view returns (address);
}

