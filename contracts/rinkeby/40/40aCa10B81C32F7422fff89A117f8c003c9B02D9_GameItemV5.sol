// contracts/GameItem.sol
pragma solidity ^0.7.0;

import "./contracts/token/ERC721/ERC721.sol";
import "./contracts/access/Ownable.sol";
import "./contracts/utils/Counters.sol";
contract GameItemV5 is ERC721, Ownable { //ERC721Auctionable, Ownable {
    using SafeMath for uint256;
    using Address for address;
    using Strings for uint256;
    using Counters for Counters.Counter;
    Counters.Counter private _pixelIds;
    Counters.Counter private _packIds;

    constructor() public ERC721("Banana", "BNN", 0.0001 ether) {}

    function buyPixel(uint16 posX, uint16 posY) payable public {
        require(msg.value >= ERC721.buyPrice(), "Buy Pixel: Value less than minimum");
        payable(owner()).transfer(msg.value);
        _buyPixel(posX, posY, 0);
    }
    function buyPixelAndPack(uint16 posX, uint16 posY, uint256 packId) payable public {
        require(msg.value >= ERC721.buyPrice(), "Buy Pixel and pack: Value less than minimum");
        payable(owner()).transfer(msg.value);
        _buyPixel(posX, posY, packId);
    }

    function buyPack(string memory uri, string memory url, string memory name, uint16 x, uint16 y, uint16 w, uint16 h) payable public {
        require(msg.value >= ERC721.buyPrice() * w * h, "Buy Pack: Value less than minimum");
        payable(owner()).transfer(msg.value);
        _packIds.increment();
        uint256 newPack = _packIds.current();
        _createPack(newPack, uri, url, name, msg.sender);
         for (uint16 posX = x; posX < x + w; posX++) {
            for (uint16 posY = y; posY < y + h; posY++) {
                if(!_existsPos(posX, posY)) {
                   _pixelIds.increment();
                   uint256 newPixel = _pixelIds.current();
                   _mintAndPack(msg.sender, newPixel, posX, posY, newPack);
                }
            }
        }
    }
    function _buyPixel(uint16 posX, uint16 posY, uint256 packId) internal virtual returns (uint256){
        _pixelIds.increment();
        uint256 newPixel = _pixelIds.current();
        _mint(msg.sender, newPixel, posX, posY);
        if(packId != 0) {
            ERC721.packPixel(newPixel, packId);
        }
        return newPixel;
    }
    function setBuyPrice(uint price) public onlyOwner {
        _setBuyPrice(price);
    }


}

