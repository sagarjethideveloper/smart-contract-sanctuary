pragma solidity >=0.6.0 <0.8.0;

import "../../utils/Context.sol";
import "../../math/SafeMath.sol";
import "../../utils/Address.sol";
import "../../utils/EnumerableSet.sol";
import "../../utils/EnumerableMap.sol";
import "../../utils/Strings.sol";

/*
 * @title ERC721 Non-Fungible Pixel Standard basic implementation
 * @dev see https://eips.ethereum.org/EIPS/eip-721
 */
contract ERC721 {
    using SafeMath for uint256;
    using Address for address;
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableMap for EnumerableMap.UintToAddressMap;
    using Strings for uint256;

    //User mapping
    // Map user to pixels
    mapping (address => EnumerableSet.UintSet) private _holderPixels;
    // Map pixels to user
    EnumerableMap.UintToAddressMap private _pixelOwners;

    // Map user to pack
    mapping (address => EnumerableSet.UintSet) private _holderPacks;
    // Map pack to user
    EnumerableMap.UintToAddressMap private _packOwners;


    // Pixel name
    string private _name;
    // Pixel symbol
    string private _symbol;
    // Pixel price
    uint256 private _price;
    event ChangeBuyPrice(uint indexed price);
    /* Pack Mappings and events */
    //Pack mapping
    mapping(uint256 => string) private _packNames;
    mapping(uint256 => string) private _packURLs;
    mapping(uint256 => string) private _packURIs;
    // Map pack to pixel
    mapping (uint256 => EnumerableSet.UintSet) private _packPixels;
    // Map pixel to pack
    mapping(uint256 => uint256) private _pixelPacks;

    //Pack events
    event ChangePackName(uint256 indexed pack, string indexed name);
    event ChangePackURL(uint256 indexed pack, string indexed url);
    event ChangePackURI(uint256 indexed pack, string indexed uri);

    /* Pixel Mappings and events */
    //Pixel Mapping
    mapping(uint16 => mapping (uint16 => uint256)) private _pixelByPoss;
    mapping(uint256 => uint16) private _posXByPixels;
    mapping(uint256 => uint16) private _posYByPixels;
    //Pixel events 
    event CreatePackedPixel(uint256 indexed pixelId, uint256 indexed packId);
    event PackedPixel(uint256 indexed pixelId, uint256 indexed packId);
    event UnpackedPixel(uint256 indexed pixelId, uint256 indexed packId);
    event Transfer(address indexed from, address indexed to, uint256 indexed pixelId);
   

    /* Constructor  */
    constructor (string memory name_, string memory symbol_, uint price_) public {
        _name = name_;
        _symbol = symbol_;
        _price = price_;
    }

    /* Contract info methods */
    //Return contract name
    function name() public view virtual returns (string memory) {
        return _name;
    }
    //Return Symbol
    function symbol() public view virtual returns (string memory) {
        return _symbol;
    }
    //Return price
    function buyPrice() public view virtual returns (uint) {
        return _price;
    }
    function _setBuyPrice(uint price) internal virtual {
        _price = price;
        emit ChangeBuyPrice(price);
    }
    //Return Supply
    function totalSupply() public view virtual returns (uint256) {
        return 1000000;
    }

    /* User info methods */
    function balanceOf(address owner) public view virtual returns (uint256) {
        require(owner != address(0), "Pixel and packs balance query for the zero address");
        return _holderPixels[owner].length().add(_holderPacks[owner].length());
    }
    function pixelsBalanceOf(address owner) public view virtual returns (uint256) {
        require(owner != address(0), "Pixel balance query for the zero address");
        return _holderPixels[owner].length();
    }
    function packsBalanceOf(address owner) public view virtual returns (uint256) {
        require(owner != address(0), "Pack balance query for the zero address");
        return _holderPacks[owner].length();
    }

    /* Pack Methods */
    //Read methods
    function getPackName(uint256 packId) public view virtual returns (string memory) {
        require(_packExists(packId), "Pack name: No pack");
        string memory _packName = _packNames[packId];
        return _packName;
    }
    function getPackURI(uint256 packId) public view virtual returns (string memory) {
        require(_packExists(packId), "Pack URI: No pack");
        string memory _packURI = _packURIs[packId];
        return _packURI;
    }
    function getPackURL(uint256 packId) public view virtual returns (string memory) {
        require(_packExists(packId), "Pack URL: No pack");
        string memory _packURL = _packURLs[packId];
        return _packURL;
    }
    function getPackPixelsCount(uint256 packId) public view virtual returns (uint256) {
        require(_packExists(packId), "Pack Pixel Count: No pack");
        return _packPixels[packId].length();
    }
    function getPackPixelByIndex(uint256 packId, uint256 pixelId) public view virtual returns (uint256) {
        return _packPixels[packId].at(pixelId);
    }
    function getPackByPixel(uint256 pixelId) public view virtual returns (uint256) {
        return _pixelPacks[pixelId];
    }
    //Write methods
    function setPackName(uint256 packId, string memory name) public {
        require(_packExists(packId), "Set pack name: No pack");
        //require(_pack)
        _packNames[packId] =  name;
        emit ChangePackName(packId, name);
    }
    function setPackURI(uint256 packId, string memory uri) public {
        require(_packExists(packId), "Set pack URI: No pack");
        _packURIs[packId] =  uri;
        emit ChangePackURI(packId, uri);
    }
    function setPackURL(uint256 packId, string memory url) public {
        require(_packExists(packId), "Set pack URL: No pack");
        _packURLs[packId] =  url;
        emit ChangePackURL(packId, url);
    }
    function getPackOfOwnerByIndex(address owner, uint256 index) public view virtual returns (uint256) {
        return _holderPacks[owner].at(index);
    }
    function _createPack(uint256 packId, string memory uri, string memory url, string memory name, address owner) internal virtual {
        require(owner != address(0), "Creating a pack to zero address");
        require(!_packExists(packId), "Pack already created");
        _holderPacks[owner].add(packId);
        _packOwners.set(packId, owner);
        _packNames[packId] =  name;
        _packURIs[packId] =  uri;
        _packURLs[packId] =  url;
        emit Transfer(address(this), owner, packId);
    }
    function transferPack(address to, uint256 packId) public virtual {
        _transferPack(msg.sender, to, packId);
    }
    //Test methods
    function ownerOfPack(uint256 packId) public view virtual returns (address) {
        return _packOwners.get(packId, "Pack owner: No pack");
    }
    function _packExists(uint256 packId) internal view virtual returns (bool) {
        return _packOwners.contains(packId);
    }

    /* Pixel Methods */
    //Read Methods
    function getPixelByIndex(uint256 index) public view virtual returns (uint256) {
        (uint256 pixelId, ) = _pixelOwners.at(index);
        return pixelId;
    }
    function getPixelByPos(uint16 x, uint16 y) public view virtual returns (uint256) {
        require(x <= 1000, "Pixel By Pos: exceeds maximum x pos");
        require(x >= 1, "Pixel By Pos: lower than minimum x pos");
        require(y <= 1000, "Pixel By Pos: exceeds maximum y pos");
        require(y >= 1, "Pixel By Pos: lower than minimum y pos");
        uint256 pixelId = _pixelByPoss[x][y];
        return pixelId;
    }
    function getPosByPixel(uint256 pixelId) public view virtual returns (uint256, uint256) {
        require(_pixelExists(pixelId), "Pos by pixel: No pixel");
        return (_posXByPixels[pixelId], _posYByPixels[pixelId]);
    }
    function getPixelOfOwnerByIndex(address owner, uint256 index) public view virtual returns (uint256) {
        return _holderPixels[owner].at(index);
    }

    //Write Methods
    function packPixel(uint256 pixelId, uint256 packId) public {
        require(_pixelExists(pixelId), "Pack Pixel: No pixel");
        require(_packExists(packId), "Pack Pixel: No pack");
        _packPixels[packId].add(pixelId);
        _pixelPacks[pixelId] = packId;
        address owner = _pixelOwners.get(pixelId);
        _holderPixels[owner].remove(pixelId);
        _pixelOwners.remove(pixelId);
        emit PackedPixel(pixelId, packId);
    }
    function UnpackPixel(uint256 pixelId) public {
        uint256 pack = _pixelPacks[pixelId];
        _packPixels[pack].remove(pixelId);
        address owner = ERC721.ownerOfPack(pack);
        delete _pixelPacks[pixelId];
        emit UnpackedPixel(pixelId, pack);
        _holderPixels[owner].add(pixelId);
        _pixelOwners.set(pixelId, owner);
        emit Transfer(address(0), owner, pixelId);
    }
    function _mintAndPack(address to, uint256 pixelId, uint16 posX, uint16 posY, uint256 packId) internal virtual {
        require(to != address(0), "Create Pixel: mint to the zero address");
        require(!_pixelExists(pixelId), "Create Pixel: pixel already minted");
        require(posX <= 1000, "Create Pixel: exceeds maximum x pos");
        require(posX >= 1, "Create Pixel: lower than minimum x pos");
        require(posY <= 1000, "Create Pixel: exceeds maximum y pos");
        require(posY >= 1, "Create Pixel: lower than minimum y pos");
        require(!_existsPos(posX, posY), "Create Pixel: position already minted");
        require(_packExists(packId), "Create Pixel: No pack");
        _pixelByPoss[posX][posY] = pixelId;
        _posXByPixels[pixelId] = posX;
        _posYByPixels[pixelId] = posY;
        _packPixels[packId].add(pixelId);
        _pixelPacks[pixelId] = packId;
        emit CreatePackedPixel(pixelId, packId);
    }
    function _mint(address to, uint256 pixelId, uint16 posX, uint16 posY) internal virtual {
        require(to != address(0), "Create Pixel: mint to the zero address");
        require(!_pixelExists(pixelId), "Create Pixel: pixel already minted");
        require(posX <= 1000, "Create Pixel: exceeds maximum x pos");
        require(posX >= 1, "Create Pixel: lower than minimum x pos");
        require(posY <= 1000, "Create Pixel: exceeds maximum y pos");
        require(posY >= 1, "Create Pixel: lower than minimum y pos");
        require(!_existsPos(posX, posY), "Create Pixel: position already minted");
        _pixelByPoss[posX][posY] = pixelId;
        _posXByPixels[pixelId] = posX;
        _posYByPixels[pixelId] = posY;
        _holderPixels[to].add(pixelId);
        _pixelOwners.set(pixelId, to);
        emit Transfer(address(0), to, pixelId);
    }

    //Test Methods
    function _pixelExists(uint256 pixelId) internal view virtual returns (bool) {
        if(ERC721.isPacked(pixelId)){
            return true;
        }
        else {
            return _pixelOwners.contains(pixelId);
        }
        
    }
    function _existsPos(uint16 x, uint16 y) public view virtual returns (bool) {
        return _pixelByPoss[x][y] != 0;
    }
    function ownerOfPixel(uint256 pixelId) public view virtual returns (address) {
        if(ERC721.isPacked(pixelId)) {
            return ERC721.ownerOfPack(_pixelPacks[pixelId]);
        }
        else {
            return _pixelOwners.get(pixelId, "Owner of: No pixel");
        }
        
    }
    function isPacked(uint256 pixelId) public view virtual returns (bool) {
        return _pixelPacks[pixelId] != 0;
    }
    function transfer(address to, uint256 pixelId) public virtual {
        _transfer(msg.sender, to, pixelId);
    }
    /* Utility Methods */
    function _transfer(address from, address to, uint256 pixelId) internal virtual {
        require(_pixelExists(pixelId), "Transfer: No pixel");
        require(ERC721.ownerOfPixel(pixelId) == from, "Transfer: transfer of pixel that is not own");
        require(to != address(0), "Transfer: transfer to the zero address");
        require(!ERC721.isPacked(pixelId), "Transfer: Pixel Transfer have to be unpacked");
        _holderPixels[from].remove(pixelId);
        _holderPixels[to].add(pixelId);
        _pixelOwners.set(pixelId, to);
        emit Transfer(from, to, pixelId);
    }
    function _transferPack(address from, address to, uint256 packId) internal virtual {
        require(_packExists(packId), "Transfer Pack: No pixel");
        require(ERC721.ownerOfPack(packId) == from, "Transfer Pack: transfer of pixel that is not own");
        require(to != address(0), "Transfer Pack: transfer to the zero address");
        _holderPacks[from].remove(packId);
        _holderPacks[to].add(packId);
        _packOwners.set(packId, to);
        emit Transfer(from, to, packId);
    }
}

