pragma solidity 0.8.6;

import "../token/IERC20List.sol";
import "../controller/IController.sol";
import "@openzeppelin/contracts/governance/TimelockController.sol";

contract CoreGame is TimelockController {
    uint256 public gameId;
    uint256 public promo;
    uint256 public waitPerGame;
    uint256 public houseEdge;
    IERC20List erc20List;
    IController controller;

    constructor(
        uint256 minDelay,
        address[] memory proposers,
        address[] memory executors
    ) TimelockController(minDelay, proposers, executors) {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    function setERC20List(address _erc20list)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        erc20List = IERC20List(_erc20list);
    }

    function setController(address payable _controller)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        controller = IController(_controller);
    }

    /**@param _wait the value given must be in seconds */
    function setWaitPerGame(uint256 _wait)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        waitPerGame = _wait;
    }

    /**@param _houseEdge must be within the given value range 1-100 */
    function setHouseEdge(uint256 _houseEdge)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        houseEdge = _houseEdge;
    }

    /**@param _promo the value given must be 18 decimals */
    function setPromo(uint256 _promo) external onlyRole(TIMELOCK_ADMIN_ROLE) {
        promo = _promo;
    }

    function setGameId(uint256 _gameId) external onlyRole(TIMELOCK_ADMIN_ROLE) {
        gameId = _gameId;
    }

    function gameFactor() external view returns (uint256) {
        return (waitPerGame * (1e36)) / (60 * (1e18));
    }

    function promoPerc() external view returns (uint256) {
        return promo;
    }

    function getGameId() external view returns (uint256) {
        return gameId;
    }
}

