struct VRF {
    bytes32 keyHash;
    uint256 fee;
    address token;
    address coordinator;
}

contract MockRouletteV2 is CoreGame, MockLocalRandom, VRFConsumerBase {
    enum BetStatus {
        NOTSTARTED,
        STARTED,
        FINISHED
    }

    enum BetTypes {
        COLOR,
        COLUMN,
        DOZEN,
        EIGHTEEN,
        MODULUS,
        NUMBER
    }

    struct Room {
        uint256 playerCount;
        uint256 randomNumber;
        bool isGameStarted;
    }

    struct Bet {
        address player;
        uint256 tokenAmount;
        uint256 amountUSD;
        uint256 timestamp;
        uint256 option;
        uint256 payout;
        address token;
        BetStatus status;
        BetTypes betType;
        bool win;
    }

    uint8[6] payouts;
    uint8[6] numberRange;
    uint256 public randomNumber;
    uint256 public roomLimit;
    VRF private _vrf;
    Bet[] public bets;
    Bet[] empty;

    mapping(address => Bet) public playerBet;
    mapping(address => uint256) winnings;
    mapping(uint256 => Bet[]) public rooms;
    mapping(uint256 => Room) public roomInfo;
    mapping(bytes32 => uint256) public requestAndRoomId;

    /*
      BetTypes are as follow:
        0: color
        1: column
        2: dozen
        3: eighteen
        4: modulus
        5: number
        
      Depending on the BetType, number will be:
        color: 0 for black, 1 for red
        column: 0 for left, 1 for middle, 2 for right
        dozen: 0 for first, 1 for second, 2 for third
        eighteen: 0 for low, 1 for high
        modulus: 0 for even, 1 for odd
        number: number
    */

    constructor(
        VRF memory vrf,
        uint256 minDelay,
        address[] memory proposers,
        address[] memory executors
    )
        VRFConsumerBase(
            vrf.coordinator, // VRF Coordinator
            vrf.token // LINK Token
        )
        CoreGame(minDelay, proposers, executors)
    {
        _vrf = vrf;
        payouts = [2, 3, 3, 2, 2, 36];
        numberRange = [1, 2, 2, 1, 1, 36];
        waitPerGame = 180;
        roomLimit = 6;
    }

    event NewBet(
        address indexed game,
        address indexed player,
        address token,
        uint256 tokenAmount,
        uint256 setBetTime,
        uint256 amountUSD,
        BetTypes betType,
        uint256 option
    );

    event Result(
        address indexed game,
        uint256 indexed gameId,
        uint256 randomNumber,
        Bet[] bets
    );

    event RandomNumber(uint256 number, uint256 roomId);

    modifier isBetOpen(address _player) {
        require(
            block.timestamp - playerBet[_player].timestamp >= waitPerGame,
            "You have to wait before play again"
        );
        _;
    }

    modifier isBetSet(address _player) {
        require(
            playerBet[_player].status == BetStatus.STARTED,
            "You have to choose Bet before play"
        );
        _;
    }

    modifier isBetNotSet(address _player) {
        require(
            playerBet[_player].status != BetStatus.STARTED,
            "you already have a Bet now"
        );
        _;
    }

    modifier IsGameStart(uint256 roomId) {
        require(
            roomInfo[roomId].isGameStarted == false,
            "Game has started. you can't bet"
        );
        _;
    }

    function payoutAmount(address _player) public view returns (uint256) {
        return (playerBet[_player].tokenAmount * winnings[_player]);
    }

    function getRandomNumber() internal returns (bytes32 requestId) {
        require(
            LINK.balanceOf(address(this)) >= _vrf.fee,
            "Not enough LINK - fill contract with faucet"
        );
        bytes32 requestId = requestRandomness(_vrf.keyHash, _vrf.fee);
        return requestId;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness)
        internal
        override
    {
        randomNumber = randomness;
        uint8 roomId = uint8(requestAndRoomId[requestId]);
        roomInfo[roomId].randomNumber = randomness;
        emit RandomNumber(randomness, roomId);
    }

    function startGame(uint256 roomId, uint256 randomNumber)
        public
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        roomInfo[roomId].isGameStarted = true;
        roomInfo[roomId].randomNumber = randomNumber;
    }

    function withdrawLink() external onlyRole(TIMELOCK_ADMIN_ROLE) {
        LINK.transfer(msg.sender, LINK.balanceOf(address(this)));
    }

    function setRoomLimit(uint256 _newRoomLimit) external {
        roomLimit = _newRoomLimit;
    }

    /**
     *@param playVar[0] is roomId
     */

    function play(address[] memory playAddresses, uint256[] memory playVar)
        public
        onlyRole(TIMELOCK_ADMIN_ROLE)
    {
        /* are there any bets? */

        bets = rooms[playVar[0]];

        require(bets.length > 0, "No bets");
        require(
            roomInfo[playVar[0]].isGameStarted == true,
            "Game is not started"
        );

        address[] memory winners = new address[](bets.length);
        uint256[] memory amounts = new uint256[](bets.length);
        address[] memory tokens = new address[](bets.length);
        /*We need allBets for front*/
        Bet[] memory allBets = bets;

        uint256 index;
        uint256 number = ((roomInfo[playVar[0]].randomNumber) % 37);
        /* check every bet for this number */
        bool isOneWin = false;
        for (uint256 i = 0; i < bets.length; i++) {
            bool won = false;
            Bet memory b = bets[i];
            if (number == 0) {
                won = (b.betType == BetTypes.NUMBER && b.option == 0); /* bet on 0 */
            } else {
                if (b.betType == BetTypes.NUMBER) {
                    won = (b.option == number); /* bet on number */
                } else if (b.betType == BetTypes.MODULUS) {
                    if (b.option == 0) won = (number % 2 == 0); /* bet on even */
                    if (b.option == 1) won = (number % 2 == 1); /* bet on odd */
                } else if (b.betType == BetTypes.EIGHTEEN) {
                    if (b.option == 0) won = (number <= 18); /* bet on low 18s */
                    if (b.option == 1) won = (number >= 19); /* bet on high 18s */
                } else if (b.betType == BetTypes.DOZEN) {
                    if (b.option == 0) won = (number <= 12); /* bet on 1st dozen */
                    if (b.option == 1) won = (number > 12 && number <= 24); /* bet on 2nd dozen */
                    if (b.option == 2) won = (number > 24); /* bet on 3rd dozen */
                } else if (b.betType == BetTypes.COLUMN) {
                    if (b.option == 0) won = (number % 3 == 1); /* bet on left column */
                    if (b.option == 1) won = (number % 3 == 2); /* bet on middle column */
                    if (b.option == 2) won = (number % 3 == 0); /* bet on right column */
                } else if (b.betType == BetTypes.COLOR) {
                    if (b.option == 0) {
                        /* bet on black */
                        if (number <= 10 || (number >= 20 && number <= 28)) {
                            won = (number % 2 == 0);
                        } else {
                            won = (number % 2 == 1);
                        }
                    } else {
                        /* bet on red */
                        if (number <= 10 || (number >= 20 && number <= 28)) {
                            won = (number % 2 == 1);
                        } else {
                            won = (number % 2 == 0);
                        }
                    }
                }
            }
            /* if winning bet, add to player winnings balance */
            if (won) {
                winnings[b.player] = uint256(payouts[uint256(b.betType)]);
                playerBet[b.player].win = true;
                //playerBet[b.player].payout = payoutAmount(b.player);

                winners[index] = b.player;
                amounts[index] = playerBet[b.player].payout;
                tokens[index] = playerBet[b.player].token;

                allBets[i].payout = amounts[index];
                allBets[i].win = true;

                isOneWin = true;
                index++;
            } else {
                allBets[i].win = false;
                playerBet[b.player].win = false;
            }
            playerBet[b.player].status = BetStatus.FINISHED;
        }
        /* delete all bets */
        roomInfo[playVar[0]].playerCount = 0;
        roomInfo[playVar[0]].isGameStarted = false;

        bets = empty;
        rooms[playVar[0]] = empty;
        if (isOneWin == true) {
            //controller.payment(winners, amounts, tokens, address(this));
        }
        emit Result(address(this), gameId, number, allBets);
    }

    /**
    @param adresses[0] is player
    @param adresses[1] is token
    @param gameVar[0] is amountUSD
    @param gameVar[1] is tokenAmount
    @param gameVar[2] is option
    @param gameVar[3] is betType
    @param gameVar[4] is roomId
    */
    function setBet(address[] memory adresses, uint256[] memory gameVar)
        external
        onlyRole(TIMELOCK_ADMIN_ROLE)
        isBetOpen(adresses[0])
        isBetNotSet(adresses[0])
        IsGameStart(gameVar[4])
    {
        require(
            rooms[gameVar[4]].length < roomLimit,
            "Maximum room limit exceeded"
        );

        address player = adresses[0];
        address token = adresses[1];

        uint256 amountUSD = gameVar[0];
        uint256 tokenAmount = gameVar[1];
        uint256 betOption = gameVar[2];
        uint8 betType = uint8(gameVar[3]);

        require(
            betType >= 0 && betType <= 5,
            "Bet type should be this range 1-5"
        );
        require(
            betOption >= 0 && betOption <= numberRange[betType],
            "Bet option is wrong"
        );

        rooms[gameVar[4]].push(
            Bet({
                player: player,
                tokenAmount: tokenAmount,
                amountUSD: amountUSD,
                timestamp: block.timestamp,
                option: betOption,
                payout: 0,
                token: token,
                status: BetStatus.STARTED,
                betType: BetTypes(betType),
                win: false
            })
        );

        playerBet[player].tokenAmount = tokenAmount;
        playerBet[player].amountUSD = amountUSD;
        playerBet[player].option = betOption;
        playerBet[player].timestamp = block.timestamp;
        playerBet[player].status = BetStatus.STARTED;
        playerBet[player].token = token;
        playerBet[player].payout = 0;
        playerBet[player].betType = BetTypes(betType);
        roomInfo[gameVar[4]].playerCount++;

        emit NewBet(
            address(this),
            player,
            token,
            tokenAmount,
            playerBet[player].timestamp,
            amountUSD,
            BetTypes(betType),
            betOption
        );
    }
}

