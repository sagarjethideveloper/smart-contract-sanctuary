pragma solidity ^0.7.0;
pragma experimental ABIEncoderV2;

import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '../NFT/ImmortalNFT.sol';
import './AuctionSignature.sol';

import 'hardhat/console.sol';

contract Exchange is Ownable, AuctionSignature {
  using SafeMath for uint256;

  ImmortalNFT public nft;

  IERC20 public constant weth =
    IERC20(0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2);

  struct Item {
    uint256 price;
    bool isAuction;
  }

  mapping(uint256 => Item) listing;
  mapping(bytes32 => bool) public cancelledBid;

  constructor(address _nft) {
    nft = ImmortalNFT(_nft);
  }

  function isSoldOut(uint256 _tokenId) internal view returns (bool) {
    return nft.totalSupply(_tokenId) == nft.maxSupply(_tokenId);
  }

  function isListed(uint256 _tokenId) internal view returns (bool) {
    return listing[_tokenId].price > 0;
  }

  function isCreated(uint256 _tokenId) internal view returns (bool) {
    return nft.exists(_tokenId);
  }

  function listItem(
    uint256 _tokenId,
    uint256 _price,
    bool _isAuction
  ) external onlyOwner {
    require(isCreated(_tokenId), 'exchange:token id not created');
    require(!isListed(_tokenId), 'exchange:token id already listed');
    listing[_tokenId] = Item(_price, _isAuction);
  }

  function buy(uint256 _tokenId) external payable {
    require(isCreated(_tokenId), 'exchange:token id not created');
    require(isListed(_tokenId), 'exchange:token id not listed');
    require(msg.value >= listing[_tokenId].price, 'exchange:price too low');
    require(!listing[_tokenId].isAuction, 'exchange:no direct buy available');

    nft.mint(msg.sender, _tokenId, 1, '');
    msg.sender.call{value: msg.value.sub(listing[_tokenId].price)}('');
  }

  function cancelAuctionBid(
    address _userAddress,
    uint256 _tokenId,
    uint256 _price,
    bytes memory sig
  ) external {
    require(
      verifyAuctionSignature(_userAddress, _price, _tokenId, sig),
      'exchange:user cannot cancel order'
    );
    require(isListed(_tokenId), 'exchange:token id not listed');
    require(listing[_tokenId].isAuction, 'exchange:not an auction');

    bytes32 h = getAuctionHashToSign(_price, _tokenId);
    cancelledBid[h] = true;
  }

  function executeAuctionBid(
    address _userAddress,
    uint256 _tokenId,
    uint256 _price,
    bytes memory _sig
  ) external onlyOwner {
    require(
      verifyAuctionSignature(_userAddress, _price, _tokenId, _sig),
      'exchange:user is not the signer'
    );

    bytes32 h = getAuctionHashToSign(_price, _tokenId);
    require(!cancelledBid[h], 'Bid cancelled');
    require(isListed(_tokenId), 'exchange:token id not listed');
    require(listing[_tokenId].isAuction, 'exchange:not an auction');

    nft.mint(_userAddress, _tokenId, 1, '');
    weth.transferFrom(_userAddress, address(this), _price);
  }
}

