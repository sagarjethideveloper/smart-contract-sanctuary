/**
 * Follows a remote asset as the stake and provides
 * rewards.
 * One reward token per stake token.
 */
abstract contract RemoteStakeRewardManager is TokenReceivable, IRemoteStake {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    event Staked(
		address token,
		address staker_,
		uint256 stakedAmount_);
	event PaidOut(
		address token,
		address rewardToken,
		address staker_,
		uint256 rewardAmount);
	address immutable public router;
	address immutable public reflectionContract;

    mapping (address => mapping(address => uint256)) public stakes;
    mapping (address => mapping(address => uint256)) public fakeRewards;
    mapping (address => uint256) public stakedBalance;
    mapping (address => address) public rewardTokens;
    mapping (address => uint256) public rewardsTotal;
    mapping (address => uint256) public fakeRewardsTotal;

    constructor() {
		console.logString("CONSTRUCTOR %s");
		console.logBytes(IFerrumDeployer(msg.sender).initData());
		(address a, address b) = abi.decode(
			IFerrumDeployer(msg.sender).initData(), (address, address));
		console.log("CONSTRUCTOR %s : %s", a, b);

		(router, reflectionContract) = abi.decode(
			IFerrumDeployer(msg.sender).initData(), (address, address));
		// require(1==2, "WTF??");
    }

    function addReward(address baseToken, address rewardToken)
    external override onlyRouter returns (uint256) {
        return _addMarginalReward(baseToken, rewardToken);
    }

    /**
     * First send the rewards to this contract, then call this method.
     * Designed to be called by smart contracts.
     */
    function addMarginalReward(address baseToken)
    external returns (uint256) {
        return _addMarginalReward(baseToken, address(0));
    }

    function _addMarginalReward(address baseToken, address _rewardToken)
    internal virtual returns (uint256) {
		address rewardToken = rewardTokens[baseToken];
		if (rewardToken == address(0) && _rewardToken != address(0)) {
			rewardTokens[baseToken] = _rewardToken;
			rewardToken = _rewardToken;
		}
		require(rewardToken != address(0), "RSRM: No reward token");
        uint256 amount = sync(rewardToken);
        if (amount == 0) {
            return 0; // No reward to add. Its ok. No need to fail callers.
        }
        rewardsTotal[baseToken] = rewardsTotal[baseToken].add(amount);
        fakeRewardsTotal[baseToken] = fakeRewardsTotal[baseToken].add(amount);
        return amount;
    }

    function fakeRewardOf(address staker, address baseToken)
	external view returns (uint256) {
        return fakeRewards[baseToken][staker];
    }

    function rewardOf(address staker, address token)
    external virtual view returns (uint256) {
        uint256 stake = stakes[token][staker];
		// TODO: run a sync logic here
        return _calcRewardOf(staker, stakedBalance[token], stake, token);
    }

    function _calcRewardOf(address staker, uint256 totalStaked_, uint256 stake,
		address token)
    internal view returns (uint256) {
        if (stake == 0) {
            return 0;
        }
        uint256 fr = fakeRewards[token][staker];
        uint256 rew = _calcReward(totalStaked_, fakeRewardsTotal[token], stake);
        return rew > fr ? rew.sub(fr) : 0; // Ignoring the overflow problem
    }

    function withdrawRewardsFor(address to, address baseToken)
	external override onlyRouter returns (uint256) {
        require(to != address(0), "RSRM: Bad address");
        return _withdrawRewards(to, baseToken);
    }

    function withdrawRewards(address baseToken)
	external returns (uint256) {
        require(msg.sender != address(0), "RSRM: Bad address");
        return _withdrawRewards(msg.sender, baseToken);
    }

	/*
	* Chek the remote source, and monito
	*/
	function syncStake(address to, address token) public override returns (uint256){
		uint256 currentStake = stakes[token][to];
		uint256 newStake = getStake(reflectionContract, to, token);
		if (newStake > currentStake) {
			uint256 amount = newStake.sub(currentStake);
			_stake(to, token, currentStake, amount);
			return amount;
		}
		if (newStake < currentStake) {
			uint256 amount = currentStake.sub(newStake);
			_withdraw(to, token, currentStake, amount);
			return amount;
		}
		// Nothing to sync
		return 0;
	}

	/**
	 * @dev override this
	 */
	function getStake(address source, address to, address token) internal virtual returns(uint256);

    function _withdraw(address _staker, address token, uint256 current, uint256 amount)
    internal virtual returns (bool) {
        if (amount == 0) {
            return true;
        }
        _withdrawOnlyUpdateStateNoPayout(_staker, token, current, amount);
        return true;
    }

	/*
	 * @dev: Formula:
	 * Calc total rewards: balance * fake_total / stake_balance
	 * Calc faked rewards: amount  * fake_total / stake_balance
	 * Calc pay ratio: (total rewards - debt) / total rewards [ total rewards should NEVER be less than debt ]
	 * Pay: pay ratio * faked rewards
	 * Debt: Reduce by (fake rewards - pay)
	 * total fake: reduce by fake rewards
	 * Return the pay amount as rewards
	 */
    function _withdrawOnlyUpdateStateNoPayout(
		address _staker, address token, uint256 userStake, uint256 amount)
    internal virtual returns (uint256) {
        require(amount <= userStake, "RSRM: Not enough balance");
        uint256 userFake = fakeRewards[token][_staker];
        uint256 fakeTotal = fakeRewardsTotal[token];
        uint256 _stakedBalance = stakedBalance[token];
        uint256 fakeRewAmount = _calculateFakeRewardAmount(amount, fakeTotal, _stakedBalance);

		uint256 actualPay = 0;
		if (fakeRewAmount > userFake) {
			// We have some rew to return. But we don't so add it back
			actualPay = fakeRewAmount.sub(userFake);
			userFake = actualPay;
        	fakeRewardsTotal[token] = fakeTotal.sub(fakeRewAmount).add(actualPay);
		} else {
			userFake = userFake.sub(fakeRewAmount);
        	fakeRewardsTotal[token] = fakeTotal.sub(fakeRewAmount);
		}

        fakeRewards[token][_staker] = userFake;
        stakes[token][_staker] = userStake.sub(amount);
        stakedBalance[token] = _stakedBalance.sub(amount);
        return actualPay;
    }

    function _stake(address staker, address token, uint256 userStake, uint256 amount)
    internal {
        _stakeNoPreAction(staker, token, userStake, amount);
    }

    function _stakeNoPreAction(address staker, address token, uint256 userStake, uint256 amount)
    internal {
		_stakeUpdateStateOnly(staker, token, userStake, amount);
        // To ensure total is only updated here. Not when simulating the stake.
        emit Staked(token, staker, amount);
    }

    function _stakeUpdateStateOnly(address staker, address token, uint256 userStake, uint256 amount)
    internal {
        uint256 _stakedBalance = stakedBalance[token];
        uint256 _fakeTotal = fakeRewardsTotal[token];
        bool isNotNew = _stakedBalance != 0;
        uint256 curRew = isNotNew ?
            _calculateFakeRewardAmount(amount, _fakeTotal, _stakedBalance) :
            _fakeTotal;

        _stakedBalance = _stakedBalance.add(amount);
        stakes[token][staker] = userStake.add(amount);
        fakeRewards[token][staker] = fakeRewards[token][staker].add(curRew);

        stakedBalance[token] = _stakedBalance;
        if (isNotNew) {
            fakeRewardsTotal[token] = _fakeTotal.add(curRew);
        }
    }

    function _calculateFakeRewardAmount(
        uint256 amount, uint256 baseFakeTotal, uint256 baseStakeTotal
    ) internal pure returns (uint256) {
        return amount.mul(baseFakeTotal).div(baseStakeTotal);
    }

    function _withdrawRewards(address _staker, address token) internal returns (uint256) {
		syncStake(_staker, token);
		uint256 userStake = stakes[token][_staker];
        uint256 _stakedBalance = stakedBalance[token];
        uint256 totalFake = fakeRewardsTotal[token];
        uint256 userFake = fakeRewards[token][_staker];
        uint256 actualPay = _calcWithdrawRewards(userStake, userFake, _stakedBalance, totalFake);
		console.log("User stake is %s", userStake, userFake, actualPay);
        rewardsTotal[token] = rewardsTotal[token].sub(actualPay);
        fakeRewards[token][_staker] = userFake.add(actualPay);
        if (actualPay != 0) {
			address rewTok = rewardTokens[token];
            sendToken(rewTok, _staker, actualPay);
        	emit PaidOut(token, rewTok, _staker, actualPay);
        }
        return actualPay;
    }

    function _calcWithdrawRewards(
        uint256 _stakedAmount,
        uint256 _userFakeRewards,
        uint256 _totalStaked,
        uint256 _totalFakeRewards)
    internal pure returns (uint256) {
        uint256 toPay = _calcReward(_totalStaked, _totalFakeRewards, _stakedAmount);
        return toPay > _userFakeRewards ? toPay.sub(_userFakeRewards) : 0; // Ignore rounding issue
    }

    function _calcReward(uint256 total, uint256 fakeTotal, uint256 staked)
    internal pure returns (uint256) {
        return fakeTotal.mul(staked).div(total);
    }

	modifier onlyRouter() {
		require(msg.sender == router, "RSRM: Only router method");
		_;
	}
}

