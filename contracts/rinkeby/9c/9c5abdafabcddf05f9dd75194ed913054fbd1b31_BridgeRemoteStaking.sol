pragma solidity >=0.6.0 <=0.8.0;
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

abstract contract TokenReceivable {
  using SafeERC20 for IERC20;
  using SafeMath for uint256;
  mapping(address => uint256) inventory; // Amount of received tokens that are accounted for

  function sync(address token) internal returns (uint256 amount) {
    uint256 inv = inventory[token];
    amount = IERC20(token).balanceOf(address(this)).sub(inv);
    inventory[token] = inv.add(amount);
  }

  function sendToken(address token, address payee, uint256 amount) internal {
    IERC20(token).transfer(payee, amount);
    inventory[token] = inventory[token].sub(amount);
  }
}

