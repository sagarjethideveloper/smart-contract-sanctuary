import "../library/ABDKMath64x64.sol";
import "../structs/MintData.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
struct Stage {
    uint256 maxBet;
    uint256 minBet;
    uint256 maxWINR;
    uint256 minWINR;
    uint256 mintAmount;
}

contract MintingFormula is AccessControl {
    event Minted(
        uint256 betAmount,
        uint256 currentStage,
        uint256 amount,
        uint256 timestamp
    );
    event StageChanged(uint256 currentStage, uint256 timestamp);

    IMintableERC20 token;
    mapping(uint256 => Stage) _stages;
    uint256 _currentStage = 1;

    constructor(Stage[] memory stages) {
        for (uint256 i = 0; i < stages.length; i++) {
            _stages[i + 1] = stages[i];
        }
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    /**
     * @notice attention function has stage modifier
     */
    function calculate(MintData calldata data, address mintTo)
        external
        stage
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        uint256 nominal = calculateNominal(data.amount);
        uint256 logCurve = calculateLogCurve(data.amount);
        uint256 result = (nominal * logCurve) / 1 ether;
        if (data.factor > 1) {
            result = ((result * data.factor)) / 1 ether;
        }
        if (data.promo > 1) {
            result = ((result * data.promo)) / 1 ether;
        }
        if (data.referral > 1) {
            result = ((result * data.referral)) / 1 ether;
        }
        if (data.nft > 1) {
            result = ((result * data.nft)) / 1 ether;
        }
        if (data.topPlayer > 1) {
            result = ((result * data.topPlayer)) / 1 ether;
        }

        token.mint(mintTo, result);
        //console.log("resss", result);
        if (result > _stages[_currentStage].mintAmount) {
            result -= _stages[_currentStage].mintAmount;
            _stages[_currentStage].mintAmount = 0;
            _currentStage++;
        }
        _stages[_currentStage].mintAmount -= result;
        emit Minted(data.amount, _currentStage, result, block.timestamp);
    }

    function setTokenAddress(address _token)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        token = IMintableERC20(_token);
    }

    // function calculateNominal(uint256 amount) public view returns (uint256) {
    //     Stage memory _stage = _stages[_currentStage];
    //     int128 iMaxBet = ABDKMath64x64.fromUInt(_stage.maxBet);
    //     int128 iMinBet = ABDKMath64x64.fromUInt(_stage.minBet);
    //     int128 iMaxWinr = ABDKMath64x64.fromUInt(_stage.maxWINR);
    //     int128 iMinWinr = ABDKMath64x64.fromUInt(_stage.minWINR);
    //     int128 iAmount = ABDKMath64x64.fromUInt(amount);
    //     uint256 dec = 10**18;

    //     int128 betDiff = ABDKMath64x64.sub(iMaxBet, iMinBet);
    //     int128 winrDiff = ABDKMath64x64.sub(iMaxWinr, iMinWinr);
    //     int128 iSub = ABDKMath64x64.sub(iAmount, iMinBet);
    //     int128 iMul = ABDKMath64x64.mul(betDiff, winrDiff);
    //     int128 iDiv = ABDKMath64x64.div(iSub, iMul);

    //     int128 iAdd = ABDKMath64x64.add(iDiv, iMinWinr);
    //     uint256 result = ABDKMath64x64.mulu(iAdd, dec);

    //     return result;
    // }

    /**
     * @param amount must be 18 decimal
     */
    function calculateNominal(uint256 amount) internal view returns (uint256) {
        Stage memory _stage = _stages[_currentStage];
        uint256 betDiff = _stage.maxBet - _stage.minBet;
        uint256 winrDiff = _stage.maxWINR - _stage.minWINR;
        uint256 sub = (amount - _stage.minBet);
        uint256 result = (sub / betDiff) * winrDiff;
        result = result + _stage.minWINR;

        return result;
    }

    /**
     * @param amount must be 18 decimal
     */
    // sorun burada decimaller
    function calculateLogCurve(uint256 amount) internal pure returns (uint256) {
        uint256 form = (11 * (10**16) * ((5 * (10**16)) * (amount / 10**18))) /
            10**34;
        //uint256 numbFix = form * 1e17;
        uint256 numbOne = 1;
        uint256 dec = 10**18;
        uint256 mulDec = 10**17;
        int128 iNumbFix = ABDKMath64x64.fromUInt(form);
        int128 iMulDec = ABDKMath64x64.fromUInt(mulDec);
        int256 mulNumb = ABDKMath64x64.muli(iNumbFix, iMulDec);
        int256 iDec = ABDKMath64x64.fromUInt(dec);
        int256 iDiv = ABDKMath64x64.divi(mulNumb, iDec);
        int128 expR = ABDKMath64x64.exp(int128(-iDiv));
        int128 iOne = ABDKMath64x64.fromUInt(numbOne);
        expR = ABDKMath64x64.add(iOne, expR);
        expR = ABDKMath64x64.div(iOne, expR);
        uint256 result = ABDKMath64x64.mulu(expR, dec);

        return result;
    }

    modifier stage() {
        if (_stages[_currentStage].mintAmount == 0) {
            _currentStage += 1;
            emit StageChanged(_currentStage, block.timestamp);
        }
        _;
    }
}

