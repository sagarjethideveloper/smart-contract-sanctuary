pragma solidity ^0.8.0;
import "hardhat/console.sol";

import "@openzeppelin/contracts/utils/Context.sol";
import "./stabledex/interfaces/ISwap.sol";
import "./symbdex/interfaces/ISymbiosisV2Router02Restricted.sol";
import "./synth-contracts/interfaces/ISynthesis.sol";
import "./synth-contracts/interfaces/IBridge.sol";

import "./symbdex/interfaces/IERC20.sol";
import "./symbdex/libraries/TransferHelper.sol";
import "./MetaBurnTransaction.sol";
import "./MetaRouteTransaction.sol";

contract MetaRouterUSU is Context {
  struct routerInfo {
    address in_router;
    address out_router;
    address synthesis;
  }

  routerInfo router;

  receive() external payable {}

  constructor(
    address _out_router, // uni-like dex
    address _synthesis,
    address _in_router // stable dex
  ) public {
    router = routerInfo(_in_router, _out_router, _synthesis);
  }

  function metaRoute(
    MetaRouteTransaction.metaRouteTransaction memory metarouteTransaction
  ) external payable // returns (uint256 third_swap_return_amount)
  {
    uint256 bridging_fee = ISynthesis(router.synthesis).getBridgingFee();

    TransferHelper.safeTransferFrom(
      metarouteTransaction.out_path[0],
      _msgSender(),
      address(this),
      metarouteTransaction.amount
    );

    IERC20(metarouteTransaction.out_path[0]).approve(
      router.out_router,
      metarouteTransaction.amount
    );

    uint256 token1_balance_before_swap = IERC20(
      metarouteTransaction.out_path[1]
    ).balanceOf(address(this));

    ISymbiosisV2Router02Restricted(router.out_router).swapExactTokensForTokens(
      metarouteTransaction.amount,
      metarouteTransaction.out_slippage,
      metarouteTransaction.out_path,
      address(this),
      metarouteTransaction.first_deadline
    );

    uint256 first_swap_return_amount = IERC20(metarouteTransaction.out_path[1])
      .balanceOf(address(this)) - token1_balance_before_swap;

    IERC20(metarouteTransaction.out_path[1]).approve(
      router.in_router,
      first_swap_return_amount
    );

    uint256 token2_balance_before_swap = IERC20(metarouteTransaction.in_path[1])
      .balanceOf(address(this));

    ISwap(router.in_router).swap(
      ISwap(router.in_router).getTokenIndex(metarouteTransaction.in_path[0]),
      ISwap(router.in_router).getTokenIndex(metarouteTransaction.in_path[1]),
      first_swap_return_amount,
      metarouteTransaction.in_slippage,
      metarouteTransaction.first_deadline
    );

    uint256 second_swap_return_amount = IERC20(metarouteTransaction.in_path[1])
      .balanceOf(address(this)) - token2_balance_before_swap;

    address real_representation_address = metarouteTransaction.final_path[0];
    metarouteTransaction.final_path[0] = metarouteTransaction.in_path[1];

    address[] memory swap_path = new address[](2);
    swap_path[0] = real_representation_address;
    swap_path[1] = metarouteTransaction.final_path[1];

    MetaBurnTransaction.metaBurnTransaction memory _metaBurnTransaction = MetaBurnTransaction
      .metaBurnTransaction(
        _msgSender(),
        metarouteTransaction.final_router,
        metarouteTransaction.final_path, // path here
        second_swap_return_amount,
        metarouteTransaction.to,
        metarouteTransaction.portal,
        metarouteTransaction.bridge,
        metarouteTransaction.chainID,
        0,
        metarouteTransaction.first_deadline,
        // swap calldata
        abi.encodeWithSelector(
          bytes4(
            keccak256(
              bytes(
                "swapExactTokensForTokens(uint256,uint256,address[],address,uint256)"
              )
            )
          ),
          second_swap_return_amount,
          metarouteTransaction.final_slippage,
          swap_path,
          metarouteTransaction.portal,
          metarouteTransaction.final_deadline
        )
      );

    IERC20(real_representation_address).approve(
      metarouteTransaction.portal,
      second_swap_return_amount
    );

    ISynthesis(router.synthesis).metaBurnSyntheticToken{ value: bridging_fee }(
      _metaBurnTransaction
    );
  }
}

