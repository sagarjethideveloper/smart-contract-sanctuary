interface ISwapExecutor {
    function swapSingle(
        address to,
        address token,
        uint256 minAmount
    ) external payable returns (uint256 amountPurchased);

    function swapMulti(
        address to,
        uint256 value,
        Common.Holding[] memory estimatedHoldings,
        Common.Allocation[] memory allocations,
        address[] memory supportedTokens
    ) external payable returns (Common.Holding[] memory holdings);

    function topUp(
        address to,
        uint256 value,
        Common.Allocation[] memory allocations,
        Common.Holding[] memory estimatedHoldings,
        Common.Holding[] memory currentHoldings,
        address[] memory supportedTokens
    ) external payable returns (Common.Holding[] memory);

    function liquidate(
        address to,
        address[] memory supportedTokens,
        Common.Holding[] memory holdings
    ) external returns (uint256 amtEth);

    function liquidateOne(
        address token,
        uint256 tokenOwnership,
        address vault
    ) external returns (uint256 ethAmt);

    function estimate(
        Common.Allocation[] memory allocations,
        uint256 ethToSpend,
        address[] memory supportedTokens
    ) external view returns (uint256[2][] memory estimates);

    function getPendingEstimates(
        uint8[] memory tokensToBuy,
        uint256[] memory tokenamtsToBuy,
        address[] memory supportedTokens
    ) external view returns (uint256[] memory estimates);
}

