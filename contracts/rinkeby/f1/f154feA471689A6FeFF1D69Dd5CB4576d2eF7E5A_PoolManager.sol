contract PoolManager {
    mapping(address => bool) private _executors;
    address[] private poolRequests;

    uint256 private MAX_ALLOCATIONS;
    uint256 private pendingEth;
    bool private locked;

    IBasketManager public basketmanager;
    ISwapExecutor public swapper;
    IVault public vault;

    event Withdrawal(address indexed withdrawer, uint256 amt);
    event PendingFunding(address indexed funder, uint256 amt);
    event PendingExecution(address indexed executor);

    modifier onlyExecutor() {
        require(_executors[msg.sender] == true);
        _;
    }

    modifier onlyUnlocked() {
        require(locked == false);
        _;
    }

    constructor(IBasketManager basketAddr, ISwapExecutor swapAddr) {
        _executors[msg.sender] = true;
        swapper = swapAddr;
        basketmanager = basketAddr;
        locked = false;
        MAX_ALLOCATIONS = 10;
    }

    //Admin functions

    function setVault(IVault addr) external onlyExecutor {
        vault = addr;
    }

    function setLock(bool lock) external onlyExecutor {
        locked = lock;
    }

    function setExecutor(bool status, address executor) external onlyExecutor {
        _executors[executor] = status;
    }

    function fundPending(Common.Allocation[] memory allocations)
        external
        payable
        onlyUnlocked
    {
        require(
            Validator.validate(
                allocations,
                basketmanager.getSupportedTokens(),
                basketmanager.maxAllocations()
            )
        );
        address requester = msg.sender;
        vault.createAllocations(requester, allocations);
        pendingAdmin(requester, msg.value);
    }

    function fundPendingTopUp() external payable onlyUnlocked {
        address requester = msg.sender;
        require(vault.hasHoldings(requester), "NO HOLDINGS");
        vault.setTopUp(msg.sender, true);
        pendingAdmin(requester, msg.value);
    }

    function pendingAdmin(address requester, uint256 value) internal {
        vault.setAsPending(requester, value);
        pendingEth += value;
        poolRequests.push(requester);
        emit PendingFunding(requester, value);
    }

    function removePendingAllocation() public {
        require(vault.isPending(msg.sender) == true);
        address requester = msg.sender;
        uint256 amtPending = vault.destroyPendingAllocations(requester);
        SafeTransferHelper.safeTransferETH(requester, amtPending);
    }

    function getPendingPortfolios()
        public
        view
        onlyExecutor
        returns (address[] memory pendingAddrs, Common.Portfolio[] memory)
    {
        Common.Portfolio[] memory pendingPortfolios = new Common.Portfolio[](
            poolRequests.length
        );
        for (uint256 i = 0; i < poolRequests.length; i++) {
            address customer = poolRequests[i];
            pendingPortfolios[i] = vault.getPortfolio(customer);
        }
        return (poolRequests, pendingPortfolios);
    }

    function getPendingEstimates(
        uint8[] memory tokensToBuy,
        uint256[] memory tokenAmtsToBuy
    ) public view onlyExecutor returns (uint256[] memory) {
        require(tokensToBuy.length == tokenAmtsToBuy.length, "LENGTHS INVALID");
        return
            swapper.getPendingEstimates(
                tokensToBuy,
                tokenAmtsToBuy,
                basketmanager.getSupportedTokens()
            );
    }

    function executePendingAllocations(
        uint8[] memory tokens,
        uint256[] memory amts,
        uint256[] memory minAmts,
        address[] memory customers,
        Common.Holding[][] memory pending,
        uint8 index
    ) public onlyExecutor {
        uint256[] memory amtBought = swapPendingEth(tokens, amts, minAmts);
        distribute(tokens, amtBought, customers, pending, index);
        updatePoolRequests(index);
        emit PendingExecution(msg.sender);
    }

    function swapPendingEth(
        uint8[] memory toBuy,
        uint256[] memory amts,
        uint256[] memory minAmts
    ) internal returns (uint256[] memory) {
        uint256[] memory amtBought = new uint256[](toBuy.length);
        address[] memory supportedTokens = basketmanager.getSupportedTokens();
        for (uint256 i = 0; i < toBuy.length; i++) {
            address tokenAddr = supportedTokens[toBuy[i]];
            amtBought[i] = swapper.swapSingle{value: amts[i]}(
                address(vault),
                tokenAddr,
                minAmts[i]
            );
        }
        return amtBought;
    }

    function distribute(
        uint8[] memory tokens,
        uint256[] memory amount,
        address[] memory customers,
        Common.Holding[][] memory pending,
        uint8 index
    ) internal {
        for (uint256 i = 0; i <= index; i++) {
            Common.Holding[] memory claims = pending[i];
            for (uint256 j = 0; j < claims.length; j++) {
                uint8 token = claims[j].token;
                for (uint256 k = 0; k < tokens.length; k++) {
                    if (token == tokens[k]) {
                        distributeToken(customers[i], claims[j], amount[k], j);
                    }
                }
            }
            vault.resetPendingAllocations(customers[i]);
        }
    }

    function distributeToken(
        address owner,
        Common.Holding memory claim,
        uint256 amt,
        uint256 index
    ) internal {
        uint256 owned = (claim.amtInLocalDecimals * amt) / 10**18;
        claim.amtInLocalDecimals = owned;
        if (vault.isToppingUp(owner)) {
            vault.mergeHolding(owner, owned, index);
        } else {
            vault.addHolding(owner, claim);
        }
    }

    function updatePoolRequests(uint8 index) internal {
        if (index == poolRequests.length - 1) {
            delete poolRequests;
            return;
        }
        for (uint256 i = 0; i <= index; i++) {
            delete poolRequests[i];
        }
        removeEmptyPoolRequests();
    }

    function removeEmptyPoolRequests() internal {
        for (uint256 i = 0; i < poolRequests.length; i++) {
            if (poolRequests[i] == address(0)) {
                poolRequests[i] = poolRequests[poolRequests.length - 1];
                poolRequests.pop();
            }
        }
    }

    receive() external payable {}

    fallback() external payable {}
}

