    if (game.koCounter >= 2) {
      resetKo();
      game.koCounter = 0;
    } else if (game.koCounter == 1) {
      game.koCounter += 1;
    }

    resetVisited();

    if (identifyKo(coordinates, currentplayer) == true) {
      //this executes when it is a suicidal move that results in a capture we need to check if the adjacent groups contain a ko mark

      resetVisited();
      bool ko = false;

      identifyKoCapture([coordinates[0] + 1, coordinates[1]], opponent) ? (ko = true) : (ko = ko);
      identifyKoCapture([coordinates[0] - 1, coordinates[1]], opponent) ? (ko = true) : (ko = ko);
      identifyKoCapture([coordinates[0], coordinates[1] + 1], opponent) ? (ko = true) : (ko = ko);
      identifyKoCapture([coordinates[0], coordinates[1] - 1], opponent) ? (ko = true) : (ko = ko);

      if (ko == true) {
        console.logString("an invalid move 1");
        game.boardModel[coordinates[0]][coordinates[1]].color = 0;
        // return setState({
        //   boardRender: updateRender(),
        // });
        return false;
      } else {
        // console.log("a valid move");
        console.logString("an valid move 1");
      }
    } else {
      //this has checked and confirmed that it is a sucicidal move
      // console.log("an invalid move");
      console.logString("an invalid move 2");

      game.boardModel[coordinates[0]][coordinates[1]].color = 0;
      // return setState({
      //   boardRender: updateRender(),
      // });
      return false;
    }

    resetVisited();
    // process axis rules
    processOrthagonalLogic([coordinates[0] + 1, coordinates[1]], opponent);
    processOrthagonalLogic([coordinates[0] - 1, coordinates[1]], opponent);
    processOrthagonalLogic([coordinates[0], coordinates[1] + 1], opponent);
    processOrthagonalLogic([coordinates[0], coordinates[1] - 1], opponent);

    // console.log("player 1's score", player1);
