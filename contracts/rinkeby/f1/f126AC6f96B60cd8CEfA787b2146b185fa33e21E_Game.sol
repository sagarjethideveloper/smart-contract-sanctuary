library Game {
  struct Cell {
    uint8 color;
    uint16 stoneNo;
    uint8 groupId;
    uint8 koMark;
  }

  struct GameState {
    uint16 openVertices;
    uint16 koCounter;
    uint16 capturesOnTurn;
    uint16 p1Score;
    uint16 p2Score;
    
    //true means black, false means white
    bool turn;
    Cell[19][19] boardModel;
  }

  struct Object {
    uint256 next;
    uint16[2] value;
  }

  struct Stack {
    uint256 top;
    uint256 height;
    mapping(uint256 => Object) objects;
  }

  function insert(uint16[2] memory _number, Stack storage target) public {
    Object memory newObj = Object(target.top, _number);
    target.objects[target.height] = newObj;
    target.top = target.height;
    target.height = target.height + 1;
  }

  function pop(Stack storage target) public returns (Object memory) {
    require(target.height > 0);
    uint256 _top = target.top;
    Object memory lastObject = target.objects[target.top];
    target.top = target.objects[target.top].next;
    delete target.objects[_top];
    target.height = target.height - 1;
    return lastObject;
  }

  function getTop(Stack  storage target) public view returns (Object memory) {
    return target.objects[target.top];
  }

  function getHeight(Stack storage target) public view returns (uint256) {
    return target.height;
  }

  function empty(Stack storage target) public {
    for (uint256 i; i < target.height; i++) {
      delete target.objects[i];
    }
  }
}

contract WeiBlocksCore {
  using Game for Game.GameState;
  Game.GameState private game;

    Game.Stack emptyVerticesToReset;
    Game.Stack positionsToCapture;
    Game.Stack visitedPositions;
    Game.Stack koStack;
  constructor() {
    generateFirstModel();
  }

  function generateFirstModel() internal {
    game.openVertices = 0;
    game.koCounter = 0;
    game.capturesOnTurn = 0;
    game.p1Score = 0;
    game.p2Score = 0;
    emptyVerticesToReset;
    positionsToCapture;
    visitedPositions;
    koStack;
    game.turn = true;
    for (uint8 x = 0; x < 19; x++) {
      for (uint8 y = 0; y < 19; y++) {
        game.boardModel[x][y] = Game.Cell(0, 0, 0, 0);
      }
    }
  }

  function resetVisited() private {
    while (Game.getHeight(visitedPositions) > 0) {
      game.boardModel[Game.getTop(visitedPositions).value[0]][Game.getTop(visitedPositions).value[1]].groupId = 0;
      Game.pop(visitedPositions);
    }
  }

  function resetemptyVisited() private {
    while (Game.getHeight(emptyVerticesToReset) > 0) {
      game.boardModel[Game.getTop(emptyVerticesToReset).value[0]][Game.getTop(emptyVerticesToReset).value[1]].groupId = 0;
      Game.pop(emptyVerticesToReset);
    }
  }

  function removeCaptured() private {
    while (Game.getHeight(positionsToCapture) > 0) {
      game.boardModel[Game.getTop(positionsToCapture).value[0]][Game.getTop(positionsToCapture).value[1]].color = 0;
      Game.pop(positionsToCapture);
    }
  }

  function determineOutOfBounds(uint16[2] memory coordinates) private pure returns (bool indexOutbound) {
    //inbounds means 0 < X < 20
    bool output = true;
    if (coordinates[0] == 0) {
      output = false;
    } else if (coordinates[1] == 0) {
      output = false;
    } else if (coordinates[0] > 19) {
      output = false;
    } else if (coordinates[1] > 19) {
      output = false;
    }
    return output;
  }

  function addAndMarkeOpenAdjacentVerticies(uint16[2] memory coordinates) private returns (uint16) {
    uint16 number = 0;
    //if it is imbounds
    if (determineOutOfBounds([coordinates[0] + 1, coordinates[1]]) == true) {
      //if it is empty
      if (game.boardModel[coordinates[0] + 1][coordinates[1]].color == 0) {
        //add to the total
        //check to see if it has been visited
        if (game.boardModel[coordinates[0] + 1][coordinates[1]].groupId == 0) {
          game.boardModel[coordinates[0] + 1][coordinates[1]].groupId = 1;
          Game.insert([coordinates[0] + 1, coordinates[1]],emptyVerticesToReset);
          number += 1;
        }
      }
    }

    if (determineOutOfBounds([coordinates[0] - 1, coordinates[1]]) == true) {
      //if it is empty
      if (game.boardModel[coordinates[0] - 1][coordinates[1]].groupId == 0) {
        //add to the total
        if (game.boardModel[coordinates[0] - 1][coordinates[1]].groupId == 0) {
          game.boardModel[coordinates[0] - 1][coordinates[1]].groupId = 1;
          Game.insert([coordinates[0] - 1, coordinates[1]],emptyVerticesToReset);
          number += 1;
        }
      }
    }

    if (determineOutOfBounds([coordinates[0], coordinates[1] + 1]) == true) {
      //if it is empty
      if (game.boardModel[coordinates[0]][coordinates[1] + 1].color == 0) {
        //add to the total
        if (game.boardModel[coordinates[0]][coordinates[1] + 1].groupId == 0) {
          game.boardModel[coordinates[0]][coordinates[1] + 1].groupId = 1;
          Game.insert([coordinates[0], coordinates[1] + 1],emptyVerticesToReset);
          number += 1;
        }
      }
    }

    if (determineOutOfBounds([coordinates[0], coordinates[1] - 1]) == true) {
      //if it is empty
      if (game.boardModel[coordinates[0]][coordinates[1] - 1].color == 0) {
        //add to the total
        if (game.boardModel[coordinates[0]][coordinates[1] - 1].groupId == 0) {
          game.boardModel[coordinates[0]][coordinates[1] - 1].groupId = 1;
          Game.insert([coordinates[0], coordinates[1] - 1],emptyVerticesToReset);
          number += 1;
        }
      }
    }

    return number;
  }

  function identifyGroup(uint16[2] memory coordinates, uint8 groupid) private returns (uint16) {
    //this should give me the total number of connected soldiers in an orthogonal pattern

    //check if the position is in bounds
    if (determineOutOfBounds(coordinates) == true) {
      // game.boardModel[coordinates[0]][coordinates[1]][1] = 1
      // check the position has not been visited
      if (game.boardModel[coordinates[0]][coordinates[1]].groupId == 0) {
        //mark it as visited
        //check if the position belongs to the group id
        if (game.boardModel[coordinates[0]][coordinates[1]].color == groupid) {
          game.boardModel[coordinates[0]][coordinates[1]].groupId = 1;
          Game.insert(coordinates,visitedPositions);
          Game.insert(coordinates,positionsToCapture);

          game.openVertices += addAndMarkeOpenAdjacentVerticies(coordinates);
          //if the current position belongs to the group id, add one to the total count
          return (1 +
            identifyGroup([coordinates[0] + 1, coordinates[1]], groupid) +
            identifyGroup([coordinates[0] - 1, coordinates[1]], groupid) +
            identifyGroup([coordinates[0], coordinates[1] + 1], groupid) +
            identifyGroup([coordinates[0], coordinates[1] - 1], groupid));
        } else {
          //if the current position belongs to anything other than the group id add nothing
          return 0;
        }
      } else {
        //if the position is empty add nothing
        return 0;
      }
    } else {
      //if the current position is out of bounds, add nothing.
      return 0;
    }
  }

  function determineCapture(uint16[2] memory coordinates, uint8 groupid) private returns (bool captured) {
    // console.logUint(coordinates[0]);
    // console.logUint(coordinates[1]);
    // console.log(groupid);
    game.openVertices = 0;
    uint16 groupPopulation = 0;
    // console.log("Determine capture");
    if (determineOutOfBounds(coordinates)) {
      // console.log("Determine capture 1");

      groupPopulation = identifyGroup(coordinates, groupid);
      // if(determineOutOfBounds([coordinates[0], coordinates[1] - 1])) console.log("verticies",determineopenVertices([coordinates[0], coordinates[1] -1], opponent))
      if (game.openVertices == 0) {
        // console.log("Determine capture 2");
        if (groupPopulation > 0) {
          // console.log("Determine capture 3");

          Game.empty(positionsToCapture);
          // console.log("Determine capture 4");

          resetemptyVisited();
          return true;
        } else {
          // console.log("Determine capture 5");

          resetemptyVisited();
          Game.empty(positionsToCapture);
          return false;
        }
      } else {
        // console.log("Determine capture 6");

        resetemptyVisited();
        Game.empty(positionsToCapture);
        return false;
      }
    }
  }

  function identifyKo(uint16[2] memory coordinates, uint8 groupid) private returns (bool ko) {
    // console.log("Identify ko");
    game.openVertices = 0;
    uint16 groupPopulation = 0;

    if (determineOutOfBounds(coordinates)) {
      groupPopulation = identifyGroup(coordinates, groupid);
      // console.log("Identify ko 6");
      // console.log(groupPopulation);

      // if(determineOutOfBounds([coordinates[0], coordinates[1] - 1])) console.log("verticies",determineopenVertices([coordinates[0], coordinates[1] -1], opponent))
      if (game.openVertices == 0) {
        if (groupPopulation > 0) {
          // console.log("Identify ko 5");

          // console.log(game.turn);
          bool capture = false;
          if (determineCapture([coordinates[0] + 1, coordinates[1]], game.turn ? 2 : 1)) capture = true;
          // console.logString("capture bool 1");
          // console.logBool(capture);

          if (determineCapture([coordinates[0] - 1, coordinates[1]], game.turn ? 2 : 1)) capture = true;
          // console.logString("capture bool 2");
          // console.logBool(capture);
          if (determineCapture([coordinates[0], coordinates[1] + 1], game.turn ? 2 : 1)) capture = true;
          // console.logString("capture bool 3");
          // console.logBool(capture);
          if (determineCapture([coordinates[0], coordinates[1] - 1], game.turn ? 2 : 1)) capture = true;
          // console.logString("capture bool 4");
          // console.logBool(capture);

          // determineCapture([coordinates[0] - 1, coordinates[1]], game.turn ? 2 : 1) ? (capture = true) : (capture = capture);
          // determineCapture([coordinates[0], coordinates[1] + 1], game.turn ? 2 : 1) ? (capture = true) : (capture = capture);
          // determineCapture([coordinates[0], coordinates[1] - 1], game.turn ? 2 : 1) ? (capture = true) : (capture = capture);
          // console.log("Identify ko 51");

          if (capture == false) {
            //this code block executes if it is a suicide move that does not result in a capture
            // console.log("identify ko 4");

            return false;
          } else {
            //this code block only executes if the move is suicidal, but results in a capture therefore is marked ko.
            //at this point we need to see if the captured groups are marked ko.
            game.boardModel[coordinates[0]][coordinates[1]].koMark = 1;
            Game.insert(coordinates,koStack);
            game.koCounter = 1;
            // console.log("identify ko 1");

            return true;
          }
        } else {
          resetemptyVisited();
          Game.empty(positionsToCapture);
          // console.log("identify ko 2");

          return true;
        }
      } else {
        resetemptyVisited();
        Game.empty(positionsToCapture);
        // console.log("identify ko 3");

        return true;
      }
    }
    // console.log("Identify ko 7");
  }

  function findKoMark(uint16[2] memory coordinates, uint8 groupid) private returns (uint8) {
    if (determineOutOfBounds(coordinates) == true) {
      if (game.boardModel[coordinates[0]][coordinates[1]].groupId == 0) {
        if (game.boardModel[coordinates[0]][coordinates[1]].color == groupid) {
          if (game.boardModel[coordinates[0]][coordinates[1]].koMark == 1) {
            return 1;
          } else {
            game.boardModel[coordinates[0]][coordinates[1]].groupId = 1;
            Game.insert(coordinates,visitedPositions);
            return (0 +
              findKoMark([coordinates[0] + 1, coordinates[1]], groupid) +
              findKoMark([coordinates[0] - 1, coordinates[1]], groupid) +
              findKoMark([coordinates[0], coordinates[1] + 1], groupid) +
              findKoMark([coordinates[0], coordinates[1] - 1], groupid));
          }
        } else {
          return 0;
        }
      } else {
        return 0;
      }
    } else {
      return 0;
    }
  }

  function identifyKoCapture(uint16[2] memory coordinates, uint8 groupid) private returns (bool koCapture) {
    game.openVertices = 0;
    uint16 groupPopulation = 0;

    if (determineOutOfBounds(coordinates)) {
      groupPopulation = identifyGroup(coordinates, groupid);
      // if(determineOutOfBounds([coordinates[0], coordinates[1] - 1])) console.log("verticies",determineopenVertices([coordinates[0], coordinates[1] -1], opponent))
      if (game.openVertices == 0) {
        if (groupPopulation > 0) {
          //this block executes when a capture is taking place during a suicidal move.
          //we need to return false if the capture is going to capture a ko marked group
          //and return true if the capture is suicide, and not captureing a ko marked group
          resetVisited();
          bool koMark = false;
          findKoMark(coordinates, game.turn ? 2 : 1) > 0 ? (koMark = true) : (koMark = false);
          if (koMark == true) {
            Game.empty(positionsToCapture);
            resetemptyVisited();
            return true;
          } else {
            Game.empty(positionsToCapture);
            resetemptyVisited();
            return false;
          }
        } else {
          resetemptyVisited();
          Game.empty(positionsToCapture);
          return false;
        }
      } else {
        resetemptyVisited();
        Game.empty(positionsToCapture);
        return false;
      }
    }
  }

  function resetKo() private {
    while (Game.getHeight(koStack) > 0) {
      game.boardModel[Game.getTop(koStack).value[0]][Game.getTop(koStack).value[1]].koMark = 0;
      Game.pop(koStack);
    }
    game.koCounter = 0;
  }

  // Core rule-checking on stone drop
