    return true;
  }

  function processOrthagonalLogic(uint16[2] memory coordinates, uint8 groupid) private {
    game.openVertices = 0;
    uint16 groupPopulation = 0;

    if (determineOutOfBounds(coordinates)) {
      groupPopulation = identifyGroup(coordinates, groupid);
      // if(determineOutOfBounds([coordinates[0], coordinates[1] - 1])) console.log("verticies",determineopenVertices([coordinates[0], coordinates[1] -1], opponent))
      if (game.openVertices == 0) {
        if (groupPopulation > 0) {
          removeCaptured();
          resetemptyVisited();
          game.turn ? (game.p1Score += groupPopulation) : (game.p2Score += groupPopulation);
        } else {
          resetemptyVisited();
          Game.empty(positionsToCapture);
        }
      } else {
        resetemptyVisited();
        Game.empty(positionsToCapture);
      }
    }
  }

  function getCurrentGame() public view returns (Game.GameState memory) {
    return game;
  }
}

