    */

    function getStatus()
        external
        view
        returns (SharedStates.BconContractState)
    {
        return status;
    }
    
    /*function getStatusOfBillingUnit(string memory billingUnitId)
        external
        view
        returns (BillingUnitItemState billingUnitState)
    {
        billingUnitState = SharedStates.BillingUnitItemState.Open;
