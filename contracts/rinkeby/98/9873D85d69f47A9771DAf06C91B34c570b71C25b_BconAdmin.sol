// BconContract represents a single legal contractual relation between "client" (Auftraggeber) and "contractor" (Auftragnehmer)
// with multiple BillingUnits (Abrechnungseinheiten) which consists of multiple BillingUnitItems (LV-Positionen)
contract BconContract {    
    string public id; // ID of the bconContract
    string public projectId; // project ID for the bconContract
    address public client; // Client of the bconContract, also called 'Auftraggeber'
    address public contractor; // Contractor of the bconContract, also called 'Auftragnehmer'/'Generalunternehmer'

    // required legal documents as the base for this contract
    string public billOfQuantitiesDocument;
    string public billingPlan;
    string public bimModel;
    string public paperContract;

    // global configuration items for the whole contract
    SharedDataStructure.ConfigurationItem[] public configurationItems;

    SharedStates.BconContractState public status; // status of bconContract

    // flat mapping of all billing units and their items
    string[] public billingUnitIds;
    mapping(string => SharedDataStructure.BillingUnit) public billingUnits; // key isSharedDataStructure.BillingUnitId
    string[] public billingUnitItemIds;
    mapping(string => SharedDataStructure.BillingUnitItem) public billingUnitItems; // key is BillingUnitItemId
    string[] public messageIds;
    mapping(string => SharedDataStructure.Message) messages;

    SingleConfirmationHandler defaultConfirmationHandler;
    BasicReportProgressHandler defaultReportProgressHandler;

    uint256 paymentInterval = 0;

    /*
     ---- Events ----
    */

    event ContractSigned();

    event BillingUnitItemReported(
        string indexed reportId, 
        address indexed sender,
        SharedDataStructure.MessageType indexed reportType,
        uint256 timestamp,
        uint256 completionQuantity,
        uint256 completionPrice,
        uint256 completionRate,
        string[] fileIds);

    event BillingUnitItemCompleted(
        string indexed reportId, 
        address indexed sender,
        SharedDataStructure.MessageType indexed reportType,
        uint256 timestamp,
        uint256 completionQuantity,
        uint256 completionPrice,
        uint256 completionRate,
        string[] fileIds);

    event BillingUnitItemConfirmed(
        string reportId, 
        address indexed sender,
        SharedDataStructure.MessageType indexed reportType,
        uint256 timestamp,
        string[] fileIds);

    event BillingUnitItemRejected(
        string reportId, 
        address indexed sender,
        SharedDataStructure.MessageType indexed reportType,
        uint256 timestamp,
        string[] fileIds); 

    event BillingUnitItemPaymentTriggered(
        string reportId,
        string indexed billingUnitItemId,
        address indexed sender,
        SharedDataStructure.MessageType indexed reportType,
        uint256 timestamp,
        uint256 paymentTriggerTimestamp,
        uint256 paymentQuantity,
        uint256 paymentPrice,
        string[] fileIds); 
        
    event BillingUnitItemPaymentConfirmed(
        string reportId, 
        string billingUnitItemId,
        address indexed sender,
        SharedDataStructure.MessageType indexed reportType,
        uint256 timestamp,
        uint256 paymentQuantity,
        uint256 paymentPrice,
        string[] fileIds); 

    /*
     ---- Modifier ----
    */
    modifier onlyClient() {
        require(msg.sender == client);
        _;
    }

    modifier onlyContractor() {
        require(msg.sender == contractor);
        _;
    }

    modifier onlyHandler(string memory _billingUnitItemId) {
        // TODO: check for custom handler and add additional handlers
        require(msg.sender == address(defaultConfirmationHandler) || msg.sender == address(defaultReportProgressHandler));
        _;
    }

    /*
     ---- Public functions ----
    */
    constructor(address _clientAddr, address _contractorAddr) {
        status = SharedStates.BconContractState.Created;
        client = _clientAddr;
        contractor = _contractorAddr;
    }

    function initContract(string memory _id, string memory _projectId, address _client, address _contractor,string calldata _billOfQuantitiesDocument, string calldata _billingPlan, string calldata _bimModel,string memory _paperContract,SharedDataStructure.ConfigurationItem[] memory _configurationItems) external onlyClient() {
        id = _id;
        projectId = _projectId;
        client = _client;
        contractor = _contractor;
        billOfQuantitiesDocument = _billOfQuantitiesDocument;
        billingPlan = _billingPlan;
        bimModel = _bimModel;
        paperContract = _paperContract;

        defaultConfirmationHandler = new SingleConfirmationHandler(_id);
        defaultReportProgressHandler = new BasicReportProgressHandler(_id);

        for(uint256 i = 0; i < _configurationItems.length; i++) {
            configurationItems.push(_configurationItems[i]);

            bytes32 typeKey = keccak256(abi.encodePacked(_configurationItems[i].configType));
            bytes32 paymentIntervalKey = keccak256(abi.encodePacked("paymentInterval"));

            if(typeKey == paymentIntervalKey) {
                paymentInterval = parseInt(_configurationItems[i].configValue);
            }
            
            // TODO choose handler based on configuration
        }
    }

    function initBillingUnits(SharedDataStructure.BillingUnitInit[] memory _billingUnits) public onlyClient() {
        for(uint256 i = 0; i < _billingUnits.length; i++) {
            SharedDataStructure.BillingUnitInit memory billingUnit = _billingUnits[i];
            
            string memory _id = billingUnit.id; // billingUnitId

            billingUnits[_id].id = _id;
            billingUnits[_id].state = SharedStates.BillingUnitState.Open;

            for(uint256 j = 0; j < billingUnit.configurationItems.length; j++) {
                billingUnits[_id].configurationItems.push(billingUnit.configurationItems[j]);
            }

            billingUnitIds.push(_id);

            initBillingUnitItems(billingUnit.items);
        }
    }

    // confirmation method to be called by the contractor
    function signContract() external onlyContractor(){
        // can only be called by the contractor
        // can only be called after contract was created
        require(status == SharedStates.BconContractState.Created);

        // update status
        status = SharedStates.BconContractState.Signed;
        emit ContractSigned();
    }

    function reportProgress(SharedDataStructure.Message[] memory _reports) external onlyContractor(){
        // can only be called by the contractor
        
        // TODO
        // deal with handlers of each BillingUnitItem in order to update the state machine
        // e.g. item.handlerReportProgress(item, parentBillingUnit) --> item.state will be updated based on configurationItems of billingUnit

        for (uint256 i = 0; i < _reports.length; i++) {
            SharedDataStructure.Message memory report = _reports[i];

            SharedDataStructure.BillingUnitItem storage billingUnitItem = billingUnitItems[report.billingUnitItemId];
            SharedDataStructure.Message memory relatedMessage = messages[report.relatedMessageId];

            defaultReportProgressHandler.reportProgress(billingUnitItem.id, relatedMessage.completionQuantity, relatedMessage.completionRate, report.msgType);

            /*
             #### 2. EFFECTS
            */
            
            billingUnitItem.messages.push(report);
            messages[report.id] = report;
            messageIds.push(report.id);
            
            // emit event
            emit BillingUnitItemReported(report.id, msg.sender, SharedDataStructure.MessageType.ReportDone, block.timestamp, relatedMessage.completionQuantity, relatedMessage.completionPrice, relatedMessage.completionRate, report.fileIds);

            // check if state update is possible
            //console.log("### BconContract.sol | updated completion rate is",(billingUnitItem.completionRateTotal));
            if(billingUnitItem.state == SharedStates.BillingUnitItemState.CompletionClaimed) {
                //billingUnitItem.state = SharedStates.BillingUnitItemState.CompletionClaimed;
                //console.log(">>> completion claimed");
                emit BillingUnitItemCompleted(report.id, msg.sender, SharedDataStructure.MessageType.CompletionClaimed, block.timestamp, relatedMessage.completionQuantity, relatedMessage.completionPrice, relatedMessage.completionRate, report.fileIds);
            }
        }
    }

    // ---- Confirm Progress (client) ----
    function confirmProgress(SharedDataStructure.Message[] memory _reports) external onlyClient(){
        // can only be called by the client

        for (uint256 i = 0; i < _reports.length; i++) {
            SharedDataStructure.Message memory report = _reports[i];

            SharedDataStructure.Message memory relatedMessage = messages[report.relatedMessageId];
            //BillingUnitItem storage billingUnitItem = billingUnitItems[relatedMessage.billingUnitItemId];

            defaultConfirmationHandler.confirmProgress(billingUnitItems[relatedMessage.billingUnitItemId].id, relatedMessage.completionQuantity, relatedMessage.completionRate, report.msgType);

            billingUnitItems[relatedMessage.billingUnitItemId].messages.push(report);

            string[] memory fileIds = report.fileIds;
            if(report.msgType == SharedDataStructure.MessageType.ConfirmationOkay) {
                // positive confirmation

                emit BillingUnitItemConfirmed(report.id, msg.sender, SharedDataStructure.MessageType.ConfirmationOkay, block.timestamp, fileIds);

                // TODO Abrechnung nach Zeit-Intervall oder Mindestbetrag // Teilzahlung?
                // emit payment triggered
                
                // v0: wenn item zu 100% bestätigt wurde > volle Auszahlung veranlassen
                if(billingUnitItems[report.billingUnitItemId].state == SharedStates.BillingUnitItemState.CompletionConfirmed) {
                    billingUnitItems[relatedMessage.billingUnitItemId].state = SharedStates.BillingUnitItemState.FPApproved;
                    
                    emit BillingUnitItemPaymentTriggered(
                        report.id, 
                        report.billingUnitItemId, 
                        msg.sender, 
                        SharedDataStructure.MessageType.PaymentRequest, 
                        block.timestamp, 
                        (block.timestamp + paymentInterval * 1 days), 
                        billingUnitItems[relatedMessage.billingUnitItemId].quantity, 
                        billingUnitItems[relatedMessage.billingUnitItemId].price, 
                        fileIds);
                }
            } else {
                // negative confirmation

                emit BillingUnitItemRejected(report.id, msg.sender, SharedDataStructure.MessageType.ConfirmationNotOkay, block.timestamp, fileIds);   
            }
        }
    }

    function confirmPayment(SharedDataStructure.Message[] memory _reports) external onlyClient(){
        for (uint256 i = 0; i < _reports.length; i++) {
            SharedDataStructure.Message memory report = _reports[i];

            // Annahme: confirmPayment findet für jedes Item statt
            if(billingUnitItems[report.billingUnitItemId].state == SharedStates.BillingUnitItemState.FPApproved) {
                billingUnitItems[report.billingUnitItemId].state = SharedStates.BillingUnitItemState.PaymentConfirmed;

                //string[] memory fileIds = ;
                emit BillingUnitItemPaymentConfirmed(
                    report.id, 
                    report.billingUnitItemId, 
                    msg.sender, 
                    SharedDataStructure.MessageType.PaymentConfirmation, 
                    block.timestamp, 
                    billingUnitItems[report.billingUnitItemId].quantity, 
                    billingUnitItems[report.billingUnitItemId].price, 
                    report.fileIds);
            }
        }
    }

    function updateBillingUnitItem(string memory _billingUnitItemId, uint256 _newQuantityTotal, uint256 _newCompletionRateTotal, uint256 _newOutstandingReports, uint256 _completionRateClaimed, SharedStates.BillingUnitItemState _newState) public onlyHandler(_billingUnitItemId) {
        //BillingUnitItem memory billingUnitItem = billingUnitItems[billingUnitItemId];
        //console.log("[%s] prevState: %s", _billingUnitItemId,uint(billingUnitItems[_billingUnitItemId].state));

        billingUnitItems[_billingUnitItemId].quantityTotal = _newQuantityTotal;
        billingUnitItems[_billingUnitItemId].completionRateTotal = _newCompletionRateTotal;
        billingUnitItems[_billingUnitItemId].outstandingReports = _newOutstandingReports;
        billingUnitItems[_billingUnitItemId].completionRateClaimed = _completionRateClaimed;
        billingUnitItems[_billingUnitItemId].state = _newState;

        //console.log("[%s] new state is: %s", _billingUnitItemId,uint(billingUnitItems[_billingUnitItemId].state));

        //console.log("id: %s, var: %s", billingUnitItems[billingUnitItemId].id, uint(billingUnitItems[billingUnitItemId].completionRateTotal));
        //console.log("new quantity %s, new completion rate %s, new reports %s, new claimed rate %s", newQuantityTotal, newCompletionRateTotal, newOutstandingReports, completionRateClaimed);
    }

    function updateBillingUnitItemState(string memory _billingUnitItemId, SharedStates.BillingUnitItemState _newState) public onlyHandler(_billingUnitItemId){
        //BillingUnitItem memory billingUnitItem = billingUnitItems[billingUnitItemId];
        billingUnitItems[_billingUnitItemId].state = _newState;
    }

    function getBillingUnitItem(string memory _billingUnitItemId) public view returns(string memory,string memory, string memory,uint256,uint256,uint256,uint256,uint256,uint256,uint256,SharedStates.BillingUnitItemState){
        SharedDataStructure.BillingUnitItem memory item = billingUnitItems[_billingUnitItemId];       
        return (_billingUnitItemId, item.billingUnitId, id, item.price, item.quantity, item.quantityTotal, item.completionRateTotal, item.completionRateClaimed, item.outstandingReports, item.payoutTotal, item.state);
    }


    // ---- Create BillingUnits ----
    // function createBillingUnit(string id, string[] configurationItems, BillingUnitItem[] items) {
        // TODO select handlers for each BillingUnitItem based on configurationItems..
        // TODO e.g. select a certain "partial payment" handler if partial payment is given via configurationItems
    // }

    // ---- Report message about anything that should be documented regarding a BillingUnitItem ----






    /*
     ---- Internal functions ----
    */

    function initBillingUnitItems(SharedDataStructure.BillingUnitItemInit[] memory _billingUnitItems) internal {

        for(uint256 i = 0; i < _billingUnitItems.length; i++) {
            //BillingUnitItem memory billingUnitItem = _billingUnitItems[i];
            
            // fetch values
            string memory _id = _billingUnitItems[i].id; // billingUnitId     
            string memory _billingUnitId = _billingUnitItems[i].billingUnitId; // billingUnitId     

            // set values
            billingUnitItems[_id].id = _id;
