pragma solidity ^0.7.0;
pragma abicoder v2;

import "./SharedDataStructure.sol";
import "./SharedStates.sol";
import "./IBconContract.sol";
import "hardhat/console.sol";

contract ConfirmationHandler {

    mapping(string => address) contractWhitelist;
    mapping(bytes32 => address[]) confirmers;
    mapping(bytes32 => mapping(address => uint8)) confirmations;

    function getConfirmers(string memory contractId, string memory itemId) public view returns (address[] memory){
        return confirmers[keccak256(abi.encodePacked(contractId, itemId))];
    }

    function getOutstandingConfirmers(string memory contractId, string memory itemId) public view returns (address[] memory){
        // use keccak256 to identify billingUnitItem of contract
        bytes32 entryKey = keccak256(abi.encodePacked(contractId,itemId));

        address[] memory contractConfirmers = confirmers[entryKey];
        address[] memory outstandingConfirmers;
        
        uint256 counter;

        for(uint256 i = 0; i < contractConfirmers.length; i++) {
            address confirmer = contractConfirmers[i];    
            uint8 hasConfirmed = confirmations[entryKey][confirmer];

            if(hasConfirmed == 0) {
                outstandingConfirmers[counter++]=confirmer;
            }
        }

        return outstandingConfirmers;
    }
}

contract SingleConfirmationHandler is ConfirmationHandler {
    constructor(string memory contractId) {
        contractWhitelist[contractId] = msg.sender;
    }

    function setConfirmer(string memory contractId, string memory itemId, address newConfirmer) public {
        // use keccak256 to identify billingUnitItem of contract
        bytes32 entryKey = keccak256(abi.encodePacked(contractId,itemId));

        confirmers[entryKey].pop();
        confirmers[entryKey].push(newConfirmer);
    }

    // updates values based on the configuration and returns the updated values
    function confirmProgress(string memory billingUnitItemId,uint256 quantityReported, uint256 completionRateReported, SharedDataStructure.MessageType reportType) public {
        //bytes32 entryKey = keccak256(abi.encodePacked(contractId, billingUnitItemId));

        //item.id, item.billingUnitId, id, item.price, item.quantity, item.quantityTotal, item.completionRateTotal, item.completionRateClaimed, item.outstandingReports, item.payoutTotal, item.state
        (,,string memory contractId,,,uint256 quantityTotal, uint256 completionRateTotal, uint256 completionRateClaimed,uint256 outstandingReports,,SharedStates.BillingUnitItemState state) = IBconContract(msg.sender).getBillingUnitItem(billingUnitItemId);

        console.log("sender is", msg.sender);
        // inline because of stack-too-deep
        require(contractWhitelist[contractId] == msg.sender, "sender must be BconContract");
        console.log("sender is allowed :)");

        /* #### 1. CHECKS #### */

        require(state == SharedStates.BillingUnitItemState.CompletionStarted || state == SharedStates.BillingUnitItemState.CompletionClaimed || state == SharedStates.BillingUnitItemState.CompletionPartiallyConfirmed, "invalid item state");
        require(reportType == SharedDataStructure.MessageType.ConfirmationOkay || reportType == SharedDataStructure.MessageType.ConfirmationNotOkay, "invalid confirmation type");

          /* #### 2. EFFECTS #### */

        if(reportType == SharedDataStructure.MessageType.ConfirmationOkay) {
            // positive confirmation

            quantityTotal += quantityReported;
            completionRateTotal += completionRateReported;
            outstandingReports -= 1;
            completionRateClaimed -= completionRateReported;

            SharedStates.BillingUnitItemState newState;

            // 1000000000 = 100%
            if(outstandingReports == 0 && completionRateTotal == 1000000000) {
                newState = SharedStates.BillingUnitItemState.CompletionConfirmed;
            } else {
                newState = SharedStates.BillingUnitItemState.CompletionPartiallyConfirmed;                    
            }

            //console.log("[Item] %s", billingUnitItemId);
            IBconContract(msg.sender).updateBillingUnitItem(billingUnitItemId, quantityTotal, completionRateTotal, outstandingReports, completionRateClaimed, newState);
        } else {
            // negative confirmation
            SharedStates.BillingUnitItemState newState = SharedStates.BillingUnitItemState.RectificationProcess;
            IBconContract(msg.sender).updateBillingUnitItemState(billingUnitItemId, newState);
        }
    }
}

