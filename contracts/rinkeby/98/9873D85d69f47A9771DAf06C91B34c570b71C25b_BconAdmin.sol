pragma solidity ^0.7.0;
pragma abicoder v2;

import "./SharedStates.sol";

library SharedDataStructure {
       /*
     ---- Structs ----
    */
    

    struct ConfigurationItem {
        string configType;
        string configValue;
    }

    struct BillingUnit {
        // provided by smart contract config
        string id; // BillingUnitId
        ConfigurationItem[] configurationItems;
        BillingUnitItem[] items; // refers to all BillingUnitItems of this BillingUnit

        // provided during lifecycle (BillingUnitConsensus)
        string[] fileIds;
        SharedStates.BillingUnitState state;

        // action handler
       
        // TODO?
        // address handlerReportProgress;
        // address confirmationHandler;
        // address handlerRectification;
        // address handlerPayment;
    }

    struct BillingUnitInit {
        // provided by smart contract config
        string id; // BillingUnitId
        ConfigurationItem[] configurationItems;
        BillingUnitItemInit[] items; // refers to all BillingUnitItems of this BillingUnit
    }

    struct BillingUnitItem {
        string id; // BillingUnitItemId
        string billingUnitId; // ID of parent BilligUnit

        // provided during initialization / update
        uint256 price;
        uint256 quantity;

        // provided during lifecycle (BillingUnitConsensusItem)
        uint256 quantityTotal; // quantityCompleted
        uint256 payoutTotal; // paid
        uint256 completionRateTotal; // completionRate
        uint256 completionRateClaimed; // claimed completionRate = reported but not yet confirmed
        uint256 outstandingReports;

        string[] fileIds;

        SharedStates.BillingUnitItemState state;

        // Messages ("Meldungen" or events) for this item
        Message[] messages;

        //ConfirmationHandler confirmationHandler;
        
        
        /*string[] messageIds;
        mapping(string => Message) messages; */
    }

    struct BillingUnitItemInit {
        string id; // BillingUnitItemId
        string billingUnitId; // ID of parent BilligUnit

        // provided during initialization / update
        uint256 price;
        uint256 quantity;
    }

    struct Message {
        string id;
        string billingUnitItemId;
        string relatedMessageId;

        // different types of messages, which lead to different state machine changes:
        // - "reportDone" == "Leistungsmeldung", usually from the contractor ("Auftragnehmer")
        // - "confirmationOkay" / "confirmationNotOkay" == "Leistungsbestätigung", usually from the client ("Auftraggeber"/"Generalunternehmer")
        // - "issue" == "Mängelmeldung", usually from the client
        // - "notice" == "Info/Notiz", only for documentation purposes
        // - "paymentRequest" == "Zahlungsanweisung"
        // - "paymentCancelled" == "Zahlungsanweisung" cancelled
        // - "paymentConfirmation" == "Zahlungsbestätigung"
        MessageType msgType;

        address sender; // who is sending the message?
        uint256 timestamp;

        // merge of all attributes of all message types

        // reportDone / confirmationOkay / confirmationNotOkay / paymentRequest
        uint256 completionQuantity;
        uint256 completionPrice;
        uint256 completionRate;

        // issue
        uint256 issueReductionAmount; // "Mängeleinbehalt" which will be considered during payment handling

        string[] fileIds; // file references (e.g. for photos, notes, etc.)
    }

    struct BillingUnitItemConfirmation {
        string id;
        string[] fileIds;
    }

    /*
     ---- Enums ----
    */

    enum MessageType {
        Notice,
        ReportDone,
        CompletionClaimed,
        ConfirmationOkay,
        ConfirmationNotOkay,
        Issue,
        PaymentRequest,
        PaymentCancelled,
        PaymentConfirmation
    }
}

