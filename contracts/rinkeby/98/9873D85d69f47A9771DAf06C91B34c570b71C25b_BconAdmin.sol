            billingUnitItems[_id].price = _billingUnitItems[i].price;
            billingUnitItems[_id].quantity = _billingUnitItems[i].quantity;

            billingUnitItems[_id].state = SharedStates.BillingUnitItemState.Open;

            //billingUnitItems[_id].confirmationHandler = defaultConfirmationHandler;

            // link billingUnitItem and billingUnit
            billingUnitItemIds.push(_id);
            billingUnits[_billingUnitId].items.push(billingUnitItems[_id]);
            //console.log("### BconContract.sol | Added new billing unit item: unit %s >> item %s",_billingUnitId,_id);
        }
    }

    /*
