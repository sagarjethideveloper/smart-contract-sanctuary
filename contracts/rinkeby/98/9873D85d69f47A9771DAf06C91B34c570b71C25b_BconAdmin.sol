    }

    // updates values based on the configuration and returns the updated values
    function reportProgress(string memory billingUnitItemId,uint256 reportedQuantity, uint256 reportedCompletionRate, SharedDataStructure.MessageType reportType) public {
        //bytes32 entryKey = keccak256(abi.encodePacked(contractId, billingUnitItemId));

        //item.id, item.billingUnitId, id, item.price, item.quantity, item.quantityTotal, item.completionRateTotal, item.completionRateClaimed, item.outstandingReports, item.payoutTotal, item.state
        (,,string memory contractId,,uint256 contractualQuantity,uint256 currentQuantity, uint256 currentCompletionRate, uint256 claimedCompletionRate,uint256 outstandingReports,,SharedStates.BillingUnitItemState state) = IBconContract(msg.sender).getBillingUnitItem(billingUnitItemId);

        console.log("sender is", msg.sender);
        // inline because of stack-too-deep
        require(contractWhitelist[contractId] == msg.sender, "sender must be BconContract");
        console.log("sender is allowed :)");

        require(reportType == SharedDataStructure.MessageType.ReportDone, "invalid report type");

        /*
            #### 1. CHECKS
        */

        // check if reported quantity is within + - 10% boundary (90% < x < 110%%)
        // TODO check for over-/underflow
        uint256 upper10PercentBoundary = mulScale(contractualQuantity,110);
        uint256 lower10PercentBoundary = mulScale(contractualQuantity,90);
        //console.log("### BconContract.sol | upper boundary", upper10PercentBoundary);
        //console.log("l### BconContract.sol | lower boundary", lower10PercentBoundary);

        if((currentQuantity + reportedQuantity) > upper10PercentBoundary) {
            //console.log("[BconContract.sol] quantity higher than contractual agreement");
            // TODO: split item and pay out prev reports

        } else if((currentQuantity + reportedQuantity) < lower10PercentBoundary) {
            //console.log("[BconContract.sol] quantity lower than contractual agreement");
            // TODO:  recalc all items

        } else {
            //console.log("[BconContract.sol] within boundary; normal handling :)");

        }

        // check if summed up completion rate is lower or equals 100%
        require((currentCompletionRate + claimedCompletionRate + reportedCompletionRate) <= 1000000000, "invalid completion rate; must be lower than 100%");

        /*
            #### 2. EFFECTS
        */
        
        state = SharedStates.BillingUnitItemState.CompletionStarted;
        outstandingReports += 1;
        claimedCompletionRate += reportedCompletionRate;

        if((currentCompletionRate + claimedCompletionRate + reportedCompletionRate) == 1000000000) {
            state = SharedStates.BillingUnitItemState.CompletionClaimed;
        }

        IBconContract(msg.sender).updateBillingUnitItem(billingUnitItemId, currentQuantity, currentCompletionRate, outstandingReports, claimedCompletionRate, state);
    }

    // util to calc percent values
    // x = number the percent value should be calculated for
    // y = percentage (e.g. 90 = 90%)
    function mulScale (uint x, uint y) internal pure returns (uint) {
        // Assumption: 100% = 100 scale
        uint128 scale = 100;

        uint a = x / scale;
        uint b = x % scale;
        uint c = y / scale;
        uint d = y % scale;

        return a * c * scale + a * d + b * c + b * d / scale;
    }
}

