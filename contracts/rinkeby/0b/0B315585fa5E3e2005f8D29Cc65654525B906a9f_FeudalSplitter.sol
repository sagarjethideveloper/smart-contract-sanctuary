pragma solidity ^0.7.0;

import "@openzeppelin/[emailÂ protected]/access/Ownable.sol";

abstract contract PaymentSplitter {
    function pay(uint256 id) external payable virtual;
}

contract FeudalSplitter is Ownable{
    PaymentSplitter _splitter = PaymentSplitter(0xDC7130df63647f891dCC8B0f99bdEfFEE600bF81);
    uint256 public splitterId;
    
    function setSplitterId(uint _splitterId) external onlyOwner {
        splitterId = _splitterId;   
    }
    
    function withdrawAll(address[] memory addresses) external payable onlyOwner {
        require(addresses.length == 61, "wrong addresses length");
        
        _splitter.pay{value: address(this).balance / 5}(splitterId);
        
        uint remainingBalance = address(this).balance;
        
        payable(addresses[0]).transfer(remainingBalance / 5);
        
        uint i;
        for (i = 1; i < 21; i++) {
            payable(addresses[i]).transfer(remainingBalance / 50);
        }
        for (i = 21; i < 61; i++) {
            payable(addresses[i]).transfer(remainingBalance / 100);
        }
    }
    
    receive() external payable {}
}

