pragma solidity ^0.8.0;

import "hardhat/console.sol";
import {PausableUpgradeable} from "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import {SafeMathUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import {IERC20Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";


contract Project is PausableUpgradeable {

    using SafeMathUpgradeable for uint;

    address public owner;

    /// project metadata
    string public name;
    string public symbol;
    uint256 public startTime;
    uint256 public endTime;
    uint256 public softCap;
    uint256 public hardCap;
    uint256 public totalSupply;

    address public token;

    uint256 public maxParticipants;

    uint256 private DEV_FEE;

    event ProjectCreated(
        string name,
        string symbol,
        address indexed projectDev,
        address indexed projectAddress
    );
    event DepositEvent(address indexed _from, uint256 _amount);
    event ClaimRewardEvent(address indexed to, uint256 _amount);
    event WithdrawFundsEvent(address indexed to, uint256 _amount);

    /// project state
    modifier onlyOwner() {
        require(msg.sender == owner, "Only project owner can call this function");
        _;
    }

    function setupProject(
        uint256 _startTime,
        uint256 _endTime,
        uint256 _softCap,
        uint256 _hardCap,
        uint256 _totalSupply,
        string calldata _name,
        string calldata _symbol,
        uint256 _maxParticipants,
        address _owner
    ) public {
        require(_endTime > _startTime, "Project must end after start");
        require(_startTime > block.timestamp, "Project must start in the future");
        require(_hardCap >= _softCap, "Hard cap must be greater than or equal to soft cap");
        require(_softCap >= 10 ether, "Soft cap must be at least 10 ether");
        require(_totalSupply >= 1000 * (10**18), "Total Supply must be at least 1000 tokens");

        name = _name;
        symbol = _symbol;
        startTime = _startTime;
        endTime = _endTime;
        softCap = _softCap;
        hardCap = _hardCap;
        totalSupply = _totalSupply;
        maxParticipants = _maxParticipants;

        owner = _owner;
        // DEV_FEE = devFee;
        emit ProjectCreated(name, symbol, msg.sender, address(this));
    }

    function setToken(address _token) public onlyOwner {
        token = _token;
    }

    function setStartTime(uint256 _startTime) public onlyOwner {
        startTime = _startTime;
    }

    function setEndTime(uint256 _endTime) public onlyOwner {
        endTime = _endTime;
    }

    function deposit() public payable whenNotPaused {
        require(msg.sender != owner, "Project owner cannot deposit funds.");
        require(
            startTime > block.timestamp,
            "Project has not started"
        );
        IERC20Upgradeable(token).transferFrom(msg.sender, address(this), msg.value);
        emit DepositEvent(msg.sender, msg.value);
    }

    function claimReward() public whenNotPaused {
        require(
            startTime > block.timestamp && endTime < block.timestamp,
            "Project is no longer accepting funds."
        );
        IERC20Upgradeable(token).transferFrom(address(this), msg.sender, totalSupply);
        emit ClaimRewardEvent(msg.sender, totalSupply);
    }

    function withdrawFunds(uint256 _amount, address _to) public onlyOwner {
        require(
            startTime > block.timestamp && endTime < block.timestamp,
            "Project is no longer accepting funds."
        );
        IERC20Upgradeable(token).transferFrom(address(this), _to, _amount);
        emit WithdrawFundsEvent(_to, _amount);
    }

    receive() external payable { }
}

