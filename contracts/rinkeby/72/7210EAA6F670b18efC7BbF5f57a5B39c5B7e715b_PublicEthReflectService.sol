pragma solidity ^0.8.4;

import "./Context.sol";
import "./SafeMath.sol";
import "./Ownable.sol";

interface RewardClient2 {
    function getInfo(address _contract) external view returns (uint256, uint256, uint256, uint256, uint256);
    function withdraw(address _contract, uint256 amount) external;
}

contract PublicEthReflectService is Ownable2 {

    RewardClient2 private ethReflectService;

    constructor(address _ethReflectService) {
        ethReflectService = RewardClient2(_ethReflectService);
    }

    receive() external payable {}

    function getInfo(address _contract) external view returns (uint256, uint256, uint256, uint256, uint256) {
        return ethReflectService.getInfo(_contract);
    }

    function withdraw(address _contract, uint256 amount) external {
        ethReflectService.withdraw(_contract, amount);
    }

    function setEthReflectService(address _ethReflectService) external onlyOwner() {
        ethReflectService = RewardClient2(_ethReflectService);
    }

}

