pragma solidity 0.7.3;
//pragma experimental ABIEncoderV2;

import "./SolidifiedStorage.sol";
import "hardhat/console.sol";

contract SolidifiedBugBounty is SolidifiedStorage {
    using SafeMath for uint256;
    using SafeMath for uint32;

    modifier onlyInitialized() {
        require(initialized, "not yet initialized");
        _;
    }

    modifier onlyAdmin() {
        require(msg.sender == admin, "not allowed");
        _;
    }


    /**
        @dev whitelist token is a admin controlled token that serve as a credit for whitelisted projects that don't want to put the money upfront. This parties will have off-chain legal agreements with Solidified
    */
    function initialize(
        address _admin,
        address _dai,
        address _usdc,
        address _whitelistToken
    ) external {
        require(!initialized);
        admin = _admin;
        tokens[0] = _dai;
        tokens[1] = _usdc;
        tokens[2] = _whitelistToken;
        projectCount++;
        initialized = true;
    }

    // Setting who is allowed to vote
    function setJury(address[] memory add, bool[] memory isJury)
        external
        onlyAdmin
    {
        for (uint256 i = 0; i < add.length; i++) {
            jury[add[i]] = isJury[i];
        }
    }

    /**
            User Balance Functions
    */
    function deposit(uint256 _amount, address _token) external onlyInitialized {
        require(
            _token == tokens[0] || _token == tokens[1] || _token == tokens[2],
            "token not accepted"
        );
        require(
            IERC20(_token).transferFrom(msg.sender, address(this), _amount),
            "Transfer from failed"
        );
        balances[msg.sender][_token] = balances[msg.sender][_token].add(
            _amount
        );
        emit Deposit(msg.sender, _amount);
    }

    function withdraw(uint256 _amount, address _token)
        external
        onlyInitialized
    {
        require(balances[msg.sender][_token] >= _amount);
        require(IERC20(_token).transfer(msg.sender, _amount));
        balances[msg.sender][_token] = balances[msg.sender][_token].sub(
            _amount
        );
        emit Withdraw(msg.sender, _amount);
    }

    function __transfer(
        address _from,
        address _to,
        address _token,
        uint256 _amount
    ) internal {
        balances[_from][_token] = balances[_from][_token].sub(_amount);
        balances[_to][_token] = balances[_to][_token].add(_amount);
    }

    function __transfer(
        bytes32 _from,
        bytes32 _to,
        address _token,
        uint256 _amount
    ) internal {
        objectBalances[_from][_token] = objectBalances[_from][_token].sub(
            _amount
        );
        objectBalances[_to][_token] = objectBalances[_to][_token].add(_amount);
    }

    function __transferAll(bytes32 _from, bytes32 _to) internal {
        __transfer(_from, _to, tokens[0], objectBalances[_from][tokens[0]]);
        __transfer(_from, _to, tokens[1], objectBalances[_from][tokens[1]]);
        __transfer(_from, _to, tokens[2], objectBalances[_from][tokens[2]]);
    }

    function __transfer(
        bytes32 _from,
        bytes32 _to,
        uint256[3] memory _amounts
    ) internal {
        __transfer(_from, _to, tokens[0], _amounts[0]);
        __transfer(_from, _to, tokens[1], _amounts[1]);
        __transfer(_from, _to, tokens[2], _amounts[2]);
    }

    // //Move funds between users and objetcs(Pool, Bug, Arbitration, etc)
    function sendToObject(
        address origin,
        bytes32 object,
        address _token,
        uint256 _amount
    ) internal {
        balances[origin][_token] = balances[origin][_token].sub(_amount);
        objectBalances[object][_token] = objectBalances[object][_token].add(
            _amount
        );
    }

    function sendToAddress(
        bytes32 origin,
        address dest,
        address _token,
        uint256 _amount
    ) internal {
        objectBalances[origin][_token] = objectBalances[origin][_token].sub(
            _amount
        );
        balances[dest][_token] = balances[dest][_token].add(_amount);
    }

    /**
            Project Posting Functions
    **/
    function postProject(
        bytes32 ipfsHash,
        uint256 totalPool,
        address _token,
        uint256[5] memory _rewardsValue
    ) external onlyInitialized returns (bytes32 projectId) {
        require(isOrdered(_rewardsValue));
        require(totalPool >= _rewardsValue[0]);
        uint256 projectNumber = projectCount;
        projectCount++;
        projectId = keccak256(abi.encodePacked(msg.sender, projectNumber));
        projects[projectId] = Project(msg.sender, ProjectStatus.active);
        for (uint256 i = 0; i < _rewardsValue.length; i++) {
            projectRewards[projectId][i] = _rewardsValue[i];
        }
        sendToObject(msg.sender, projectId, _token, totalPool);
        emit ProjectPosted(
            projectId,
            projectNumber,
            msg.sender,
            ipfsHash,
            totalPool
        );
    }

    function pullProject(bytes32 projectId) external onlyInitialized {
        require(msg.sender == projects[projectId].owner);

        for (uint256 i = 0; i < tokens.length; i++) {
            sendToAddress(
                projectId,
                msg.sender,
                tokens[i],
                objectBalances[projectId][tokens[i]]
            );
        }
        projects[projectId].status = ProjectStatus.closed;
        emit ProjectPulled(projectId, msg.sender, block.timestamp);
    }

    function increasePool(
        bytes32 projectId,
        uint256 _amount,
        address _token
    ) external onlyInitialized {
        require(msg.sender == projects[projectId].owner);
        require(
            projects[projectId].status != ProjectStatus.closed,
            "Project alredy closed"
        );
        sendToObject(msg.sender, projectId, _token, _amount);
        uint256 bal = 0;
        for (uint256 i = 0; i < tokens.length; i++) {
            bal += objectBalances[projectId][tokens[i]];
        }
        if (bal >= projectRewards[projectId][0])
            projects[projectId].status = ProjectStatus.active;
        emit PoolIncreased(
            projectId,
            msg.sender,
            objectBalances[projectId][_token]
        );
    }

    /**
            Bug Functions
    **/
    function postBug(
        bytes32 bugDescription,
        bytes32 projectId,
        Severity severity,
        address _token,
        uint256[3] calldata amounts
    ) external onlyInitialized returns (bytes32 bugId) {
        require(_token == tokens[0] || _token == tokens[1], "invalid token");
        require(amounts[2] == 0, "can't have whitelisted token");
        require(projects[projectId].status == ProjectStatus.active);
        uint256 bugNumber = bugCount[projectId];
        uint256 bugValue = projectRewards[projectId][uint256(severity)];

        require(
            bugValue == amounts[0] + amounts[1] + amounts[2],
            "wrong amounts passed"
        );
        bugId = keccak256(abi.encodePacked(projectId, bugNumber));
        bugs[bugId] = Bug(
            msg.sender,
            BugStatus.pending,
            severity,
            uint32(block.timestamp),
            projectId
        );
        sendToObject(msg.sender, bugId, _token, BUG_STAKE);
        __transfer(projectId, bugId, amounts);
        bugCount[projectId] = bugCount[projectId].add(1);
        if (objectBalances[projectId][_token] < projectRewards[projectId][0]) {
            projects[projectId].status = ProjectStatus.unfunded;
        }
        emit BugPosted(
            projectId,
            bugId,
            bugNumber,
            bugDescription,
            msg.sender,
            block.timestamp
        );
    }

    function acceptBug(
        bytes32 projectId,
        bytes32 bugId,
        uint256[3] calldata amounts
    ) external onlyInitialized {
        require(
            msg.sender == projects[projectId].owner ||
                block.timestamp.sub(bugs[bugId].timestamp) >= INTERIM
        );
        require(bugs[bugId].status == BugStatus.pending);
        _resolveBug(bugId, amounts);
    }

    function rejectBug(
        bytes32 projectId,
        bytes32 bugId,
        bytes32 justification,
        Severity severity
    ) external onlyInitialized {
        require(msg.sender == projects[projectId].owner);
        require(uint256(bugs[bugId].severity) <= uint256(severity));
        bugs[bugId].status = BugStatus.negotiation;
        proposalCount[bugId]++;
        proposals[bugId][proposalCount[bugId]] = Proposal(
            severity,
            uint32(block.timestamp)
        );
        emit ProposalMade(
            projectId,
            bugId,
            justification,
            proposalCount[bugId],
            msg.sender
        );
    }

    function _resolveBug(bytes32 _bugId, uint256[3] memory amounts) internal {
        Bug storage bug = bugs[_bugId];
        uint256 bugValue = projectRewards[bug.projectId][uint256(bug.severity)];
        //console.log("bug balance");
        //console.log(objectBalances[_bugId][tokens[0]]);
        //console.log("am0");
        //console.log(amounts[0]);
        //console.log("bv");
        //console.log(bugValue);
        //console.log("sum");
        //console.log(bugValue.add(BUG_STAKE));
        if (bugValue > 0) {
            //acept Bug
            bug.status = BugStatus.accepted;
            require(
                bugValue.add(BUG_STAKE) == amounts[0] + amounts[1] + amounts[2],
                "wrong amounts passed as arguments"
            );
            // sendToAddress(_bugId, bug.hunter, tokens[i], BUG_STAKE);
            for (uint256 i = 0; i < tokens.length; i++) {
                uint fee = amounts[i].mul(PAYOUT_FEE).div(100);
                sendToAddress(_bugId, admin, tokens[i], fee);
                sendToAddress(_bugId, bug.hunter, tokens[i], amounts[i].sub(fee));
            }
            emit BugAccepted(bug.projectId, _bugId, bug.hunter, msg.sender);
        } else {
            //reject Bug
            bug.status = BugStatus.rejected;
            __transferAll(_bugId, bug.projectId);
            emit BugRejected(bug.projectId, _bugId, bug.hunter, msg.sender);
        }
    }

    function acceptProposal(
        bytes32 projectId,
        bytes32 bugId,
        uint256[3] calldata amounts,
        address stakeToken
    ) external onlyInitialized {
        require(
            stakeToken == tokens[0] || stakeToken == tokens[1],
            "token not accepted"
        );
        (address turn, ) = inTurn(projectId, bugId);
        require(
            msg.sender == turn ||
                (block.timestamp.sub(
                    proposals[bugId][proposalCount[bugId]].timestamp
                ) > INTERIM)
        );
        Proposal storage proposal = proposals[bugId][proposalCount[bugId]];
        Bug storage bug = bugs[bugId];
        uint256 bugValue = projectRewards[projectId][uint256(bug.severity)];
        bug.severity = proposal.severity;
        _resolveBug(bugId, amounts);
        __transferAll(bugId, bug.projectId);
    }

    function counterProposal(
        bytes32 projectId,
        bytes32 bugId,
        bytes32 justification,
        Severity severity
    ) external onlyInitialized {
        (address proposer, ) = inTurn(projectId, bugId);
        require(msg.sender == proposer);
        require(uint256(bugs[bugId].severity) <= uint256(severity));
        proposalCount[bugId]++;
        proposals[bugId][proposalCount[bugId]] = Proposal(
            severity,
            uint32(block.timestamp)
        );
        emit ProposalMade(
            projectId,
            bugId,
            justification,
            proposalCount[bugId],
            msg.sender
        );
    }

    /**
            Arbitration Functions
    **/
    function sendToArbitration(
        bytes32 projectId,
        bytes32 bugId,
        address _token
    ) external onlyInitialized returns (bytes32 arbitrationId) {
        // Since arbitration can use only a single token, it can't b ethe whitelisted one
        require(
            _token == tokens[0] || _token == tokens[1],
            "token not accepted"
        );
        uint256 proposalNumber = proposalCount[bugId];
        require(proposalNumber > 1);
        (address plaintiff, address defendant) = inTurn(projectId, bugId);
        require(msg.sender == plaintiff);
        arbitrationId = keccak256(abi.encodePacked(projectId, bugId));
        sendToObject(msg.sender, arbitrationId, _token, ARBITRATION_FEE);
        // __transferAll(bugId, arbitrationId);
        arbitrations[arbitrationId] = Arbitration(
            msg.sender,
            defendant,
            _token,
            0,
            uint32(0),
            uint32(block.timestamp),
            bugId
        );
        emit ArbitrationRequested(
            projectId,
            bugId,
            arbitrationId,
            plaintiff,
            defendant,
            block.timestamp
        );
    }

    function acceptArbitration(bytes32 arbitrationId) external onlyInitialized {
        Arbitration storage arb = arbitrations[arbitrationId];
        require(msg.sender == arb.defendant);
        sendToObject(msg.sender, arbitrationId, arb.token, ARBITRATION_FEE);
        arbitrations[arbitrationId].commitPeriod = uint32(
            block.timestamp.add(INTERIM)
        );
        emit ArbitrationAccepted(
            bugs[arb.bugId].projectId,
            arb.bugId,
            arbitrationId,
            arb.plaintiff,
            arb.defendant,
            block.timestamp
        );
    }

    function rejectArbitration(
        bytes32 arbitrationId,
        uint256[3] calldata amounts
    ) external onlyInitialized {
        Arbitration storage arb = arbitrations[arbitrationId];
        require(
            msg.sender == arb.defendant ||
                arb.requestTime.add(INTERIM) < block.timestamp
        );
        sendToAddress(arbitrationId, arb.plaintiff, arb.token, ARBITRATION_FEE);
        __transferAll(arbitrationId, arb.bugId);
        //get last proposal and update bug
        bugs[arb.bugId].severity = proposals[arb.bugId][
            proposalCount[arb.bugId]
        ]
            .severity;
        _resolveBug(arb.bugId, amounts);
        emit ArbitrationRejected(
            bugs[arb.bugId].projectId,
            arb.bugId,
            arbitrationId,
            arb.plaintiff,
            arb.defendant,
            block.timestamp
        );
    }

    function commitVote(bytes32 arbitrationId, bytes32 commit) external {
        Arbitration storage arbitration = arbitrations[arbitrationId];
        require(
            msg.sender != arbitration.plaintiff &&
                msg.sender != arbitration.defendant
        );
        require(jury[msg.sender], "not part of the jury");
        // require(
        //     arbitration.commitPeriod > 0 &&
        //         block.timestamp <= arbitration.commitPeriod
        // );
        require(commits[arbitrationId][msg.sender] == bytes32(0), "Already doo doo doo doo doo doo");

        arbitrations[arbitrationId].votersCount = uint32(
            arbitrations[arbitrationId].votersCount.add(1)
        );
        sendToObject(msg.sender, arbitrationId, arbitration.token, VOTING_FEE);
        commits[arbitrationId][msg.sender] = commit;
    }

    function revealCommit(
        bytes32 arbitrationId,
        Ruling vote,
        bytes32 salt
    ) external {
        Arbitration storage arb = arbitrations[arbitrationId];
        require(arb.votersCount >= MINIMUN_QUORUM);
        bool validVote =
            keccak256(abi.encodePacked(uint256(vote), salt)) ==
                commits[arbitrationId][msg.sender];
        if (validVote) {
            votes[arbitrationId][msg.sender] = vote;
            commits[arbitrationId][msg.sender] = bytes32(0);
            uint256 index = 0;
            while (voters[arbitrationId][index] != address(0)) {
                index++;
            }
            voters[arbitrationId][index] = msg.sender;
        }
    }

    function slashCommit(
        bytes32 arbitrationId,
        uint256 vote,
        bytes32 salt,
        address voter
    ) external {
        require(block.timestamp < arbitrations[arbitrationId].commitPeriod);
        bool validVote =
            keccak256(abi.encodePacked(vote, salt)) ==
                commits[arbitrationId][voter];
        if (validVote) {
            arbitrations[arbitrationId].votersCount = uint32(arbitrations[arbitrationId].votersCount.sub(1));
            commits[arbitrationId][voter] = bytes32(0);
            sendToAddress(
                arbitrationId,
                msg.sender,
                arbitrations[arbitrationId].token,
                VOTING_FEE
            );
        }
    }

    function tallyVotes(bytes32 arbitrationId)
        public
        view
        returns (
            uint256 plaintiffVotes,
            uint256 defendantVotes,
            Ruling winner
        )
    {
        address[5] storage vs = voters[arbitrationId];

        for (uint256 i = 0; i < vs.length; i++) {
            // require(vs[i] != address(0), "invalid voter");
            if (votes[arbitrationId][vs[i]] == Ruling.plaintiff)
                plaintiffVotes++;
            if (votes[arbitrationId][vs[i]] == Ruling.defendant)
                defendantVotes++;
        }
        winner = plaintiffVotes > defendantVotes
            ? Ruling.plaintiff
            : Ruling.defendant;
    }

    function updateVotersStakes(bytes32 arbitrationId) internal {
        uint256 totalPool = VOTING_FEE.mul(5).add(ARBITRATION_FEE); //loosing part get split between voters
        (uint256 plaintiffVotes, uint256 defendantVotes, Ruling ruling) =
            tallyVotes(arbitrationId);
        uint256 winningVotes =
            ruling == Ruling.plaintiff ? plaintiffVotes : defendantVotes;
        for (uint256 i = 0; i < 5; i++) {
            if (votes[arbitrationId][voters[arbitrationId][i]] == ruling) {
                uint am = totalPool.div(winningVotes);
                //Voter voted according to the jury
                sendToAddress(
                    arbitrationId,
                    voters[arbitrationId][i],
                    arbitrations[arbitrationId].token,
                    totalPool.div(winningVotes)
                );
            }
        }
    }

    function resolveArbitration(
        bytes32 arbitrationId,
        uint256[3] calldata amounts
    ) external {
        //console.log("start");
        require(
            block.timestamp >
                arbitrations[arbitrationId].commitPeriod.add(INTERIM.mul(2))
        );
        Arbitration storage arb = arbitrations[arbitrationId];
        (, , Ruling winner) = tallyVotes(arbitrationId);
        uint256 lastProposal = proposalCount[arb.bugId];
        uint256 proposalNumber =
            winner == Ruling.plaintiff ? lastProposal -1: lastProposal;
        Proposal storage prop = proposals[arb.bugId][proposalNumber];
        bugs[arb.bugId].severity = prop.severity;
        updateVotersStakes(arbitrationId);
        address winnerAddress =
            winner == Ruling.plaintiff ? arb.plaintiff : arb.defendant;
        sendToAddress(arbitrationId, winnerAddress, arb.token, ARBITRATION_FEE);
        _resolveBug(arb.bugId, amounts);
    }

    //getters
    function getProjectDetails(bytes32 projectId)
        external
        view
        returns (
            address owner,
            ProjectStatus status,
            uint256[5] memory rewards,
            uint256 totalPool
        )
    {
        Project memory p = projects[projectId];
        owner = p.owner;
        status = p.status;
        rewards = [
            projectRewards[projectId][0],
            projectRewards[projectId][1],
            projectRewards[projectId][2],
            projectRewards[projectId][3],
            projectRewards[projectId][4]
        ];
        totalPool =
            objectBalances[projectId][tokens[0]] +
            objectBalances[projectId][tokens[1]];
    }

    function getBugDetails(bytes32 bugId)
        external
        view
        returns (
            address hunter,
            BugStatus status,
            Severity severity,
            uint256 timestamp
        )
    {
        Bug memory b = bugs[bugId];
        hunter = b.hunter;
        severity = b.severity;
        status = b.status;
        timestamp = b.timestamp;
    }

    function getLatestProposal(bytes32 bugId)
        external
        view
        returns (Severity severity, uint256 timestamp)
    {
        Proposal memory p = proposals[bugId][proposalCount[bugId]];
        timestamp = p.timestamp;
        severity = p.severity;
    }

    function getArbitrationDetails(bytes32 arbitrationId)
        external
        view
        returns (
            //        returns(address plaintiff , address defendant, uint256 commitPeriod, uint32 votersCount) {
            address plaintiff,
            address defendant,
            uint256 timestamp,
            uint256 commitPeriod,
            uint32 votersCount
        )
    {
        Arbitration memory a = arbitrations[arbitrationId];
        plaintiff = a.plaintiff;
        defendant = a.defendant;
        votersCount = a.votersCount;
        commitPeriod = a.commitPeriod;
    }

    //Helper Functions
    function isOrdered(uint256[5] memory _arr) internal pure returns (bool) {
        return
            _arr[0] > _arr[1] &&
            _arr[1] > _arr[2] &&
            _arr[2] > _arr[3] &&
            _arr[3] > _arr[4];
    }

    function inTurn(bytes32 _projectId, bytes32 _bugId)
        internal
        view
        returns (address first, address second)
    {
        first = proposalCount[_bugId] % 2 == 0
            ? projects[_projectId].owner
            : bugs[_bugId].hunter;
        second = proposalCount[_bugId] % 2 == 1
            ? projects[_projectId].owner
            : bugs[_bugId].hunter;
    }
}

pragma solidity 0.7.3;

import "openzeppelin-solidity/contracts/math/SafeMath.sol";
import "openzeppelin-solidity/contracts/token/ERC20/IERC20.sol";

contract SolidifiedStorage {
    enum Severity {Critical, Major, Medium, Minor, Note, NoBug}
    enum BugStatus {pending, accepted, rejected, negotiation, arbitration}
    enum ProjectStatus {active, unfunded, closed}
    enum Ruling {noVote, plaintiff, defendant}

    // mapping projectId to bug severity / possibly reward?
    mapping(bytes32 => mapping(uint256 => uint256)) public projectRewards;

    struct Project {
        address owner;
        ProjectStatus status;
    }

    struct Bug {
        address hunter;
        BugStatus status;
        Severity severity;
        uint32 timestamp;
        bytes32 projectId;
    }

    struct Proposal {
        Severity severity;
        uint32 timestamp;
    }

    struct Arbitration {
        address plaintiff;
        address defendant;
        address token;
        uint32 votersCount;
        uint32 commitPeriod;
        uint32 requestTime;
        bytes32 bugId;
    }

    address public admin;
    address[3] public tokens; //Dai, USDC and the whitelist

    mapping(address => bool) public jury;
    mapping(address => mapping(address => uint256)) public balances; //address -> token -> balance
    mapping(bytes32 => mapping(address => uint256)) public objectBalances; //object -> token -> balance

    mapping(bytes32 => Project) public projects;
    mapping(bytes32 => Bug) public bugs;
    mapping(bytes32 => Arbitration) public arbitrations;
    mapping(bytes32 => mapping(uint256 => Proposal)) public proposals;

    mapping(bytes32 => uint256) public bugCount;
    mapping(bytes32 => uint256) public proposalCount;
    uint256 internal projectCount;

    mapping(bytes32 => mapping(address => bytes32)) public commits;
    mapping(bytes32 => mapping(address => Ruling)) public votes;
    mapping(bytes32 => address[5]) public voters;

    bool initialized;
    uint256 public constant INTERIM = 1 seconds;
    uint256 public constant ARBITRATION_FEE = 10 ether;
    uint256 public constant VOTING_FEE = 10 ether;
    uint256 public constant BUG_STAKE = 10 ether;
    uint256 public constant MINIMUN_QUORUM = 1;
    uint256 public constant PAYOUT_FEE = 20; //20%

    event Deposit(address holder, uint256 amount);
    event Withdraw(address holder, uint256 amount);
    event ProjectPosted(
        bytes32 Id,
        uint256 projectNumber,
        address indexed Owner,
        bytes32 ipfsInfo,
        uint256 totalPool
    );
    event ProjectPulled(bytes32 Id, address indexed Owner, uint256 time);
    event PoolIncreased(bytes32 Id, address indexed Owner, uint256 totalPool);
    event BugPosted(
        bytes32 projectId,
        bytes32 bugId,
        uint256 bugNumber,
        bytes32 bugInfo,
        address hunter,
        uint256 time
    );
    event BugAccepted(
        bytes32 projectId,
        bytes32 bugId,
        address hunter,
        address indexed sender
    );
    event BugRejected(
        bytes32 projectId,
        bytes32 bugId,
        address hunter,
        address indexed sender
    );
    event ProposalMade(
        bytes32 projectId,
        bytes32 bugId,
        bytes32 justification,
        uint256 proposalNumber,
        address proposer
    );
    event ArbitrationRequested(
        bytes32 projectId,
        bytes32 bugId,
        bytes32 arbitrationId,
        address indexed plaintiff,
        address indexed defendant,
        uint256 time
    );
    event ArbitrationAccepted(
        bytes32 projectId,
        bytes32 bugId,
        bytes32 arbitrationId,
        address indexed plaintiff,
        address indexed defendant,
        uint256 time
    );
    event ArbitrationRejected(
        bytes32 projectId,
        bytes32 bugId,
        bytes32 arbitrationId,
        address indexed plaintiff,
        address indexed defendant,
        uint256 time
    );
}

