pragma solidity ^0.6.12;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./curves/BancorBondingCurve.sol";
import "./lib/Strings.sol";

import "hardhat/console.sol";

/**
 * @title Xavier
 * @author xToken
 *
 * Xavier allows the minting and burning of artwork tokens with prices according to a bonding curve (using Bancor's
 * formula). These tokens contain metadata detailing the artwork's creator, collection, name, and the visual artwork
 * itself. Each Xavier identifier corresponds to an art piece, and is used as the token id for fungible ERC1155 tokens.
 */
contract Xavier is ERC1155, BancorBondingCurve, Ownable {
    using Strings for string;

    // Enum used for legibility, but used as uint256 token identifiers from 0 to 3
    enum XavierIdentifier {
        NINE_TO_FIVE,
        FRIDAY_NIGHT,
        SATURDAY_NIGHT,
        SUNDAY
    }

    /* ============ State Variables ============ */

    // addresses are locked from transfer after minting or burning
    uint256 private constant BLOCK_LOCK_COUNT = 6;
    // mapping of token art id to the amount of tokens minted
    mapping(XavierIdentifier => uint256) public tokenBalances;
    // mapping of token art id to the reserve balances (XTK is the reserve token)
    mapping(XavierIdentifier => uint256) public reserveBalances;
    // mapping of token art id to its initializer state
    mapping(XavierIdentifier => bool) public isPoolInitialized;
    // last block for which this address is timelocked
    mapping(address => uint256) public lastLockedBlock;

    // XTK token which is used as the reserve token in the liquidity pools
    IERC20 private xtkToken;

    string private baseURI;

    /* ============ Events ============ */

    event Minted(XavierIdentifier xavierId, uint256 mintAmount, address indexed to, uint256 supplyAfterMint);
    event Burned(XavierIdentifier xavierId, uint256 burnAmount, address indexed owner, uint256 supplyAfterBurn);

    /* ============ Modifiers ============ */

    modifier initialized(XavierIdentifier _xavierId) {
        require(isPoolInitialized[_xavierId], "Liquidity pool has not been initialized");
        _;
    }

    modifier notLocked(address lockedAddress) {
        require(lastLockedBlock[lockedAddress] <= block.number, "Function is temporarily locked for this address");
        _;
    }

    /* ============ Functions ============ */

    constructor(
        string memory _uri,
        uint32 _reserveRatio,
        address _xtkTokenAddress
    ) public ERC1155(_uri) BancorBondingCurve(_reserveRatio) {
        // uri stored in OpenZeppelin's ERC1155 will not be used
        setURI(baseURI);
        xtkToken = IERC20(_xtkTokenAddress);
    }

    /**
     * @dev Initialize the pool for trading
     * @param _xavierId The xavier id
     * @param _initXavierAmount The amount of Xavier token to mint
     * @param _initXtkAmount The amount of XTK to be transferred into the pool
     */
    function initializePool(
        XavierIdentifier _xavierId,
        uint256 _initXavierAmount,
        uint256 _initXtkAmount
    ) external onlyOwner {
        // transfer xtk
        xtkToken.transferFrom(msg.sender, address(this), _initXtkAmount);
        reserveBalances[_xavierId] = _initXtkAmount;

        // mint the initial xavier amount
        _mint(msg.sender, uint256(_xavierId), _initXavierAmount, "");
        tokenBalances[_xavierId] = tokenBalances[_xavierId].add(_initXavierAmount);

        isPoolInitialized[_xavierId] = true;
    }

    /**
     * @dev Gets the price to buy Xavier tokens
     * @param _xavierId The Xavier artwork id
     * @param _amount The amount to buy
     * @return The price to buy token amount
     */
    function getXavierBuyPrice(XavierIdentifier _xavierId, uint256 _amount) public view returns (uint256) {
        return getBuyPrice(tokenBalances[_xavierId], reserveBalances[_xavierId], _amount);
    }

    /**
     * @dev Gets the price to sell Xavier tokens
     * @param _xavierId The Xavier artwork id
     * @param _amount The amount to sell
     * @return The price to sell token amount
     */
    function getXavierSellPrice(XavierIdentifier _xavierId, uint256 _amount) public view returns (uint256) {
        return getSellPrice(tokenBalances[_xavierId], reserveBalances[_xavierId], _amount);
    }

    /**
     * @dev Mint the Xavier token
     * @param _xavierId The Xavier artwork id
     * @param _amount The amount of tokens to purchase
     * @param _xtkMaxAmount The max amount of XTK tokens approve to make purchase
     */
    function mint(
        XavierIdentifier _xavierId,
        uint256 _amount,
        uint256 _xtkMaxAmount
    ) public notLocked(msg.sender) initialized(_xavierId) {
        uint256 buyPrice = getXavierBuyPrice(_xavierId, _amount);
        require(_xtkMaxAmount >= buyPrice, "Not enough XTK to mint");

        xtkToken.transferFrom(msg.sender, address(this), buyPrice);
        reserveBalances[_xavierId] = reserveBalances[_xavierId].add(buyPrice);

        _mint(msg.sender, uint256(_xavierId), _amount, "");
        tokenBalances[_xavierId] = tokenBalances[_xavierId].add(_amount);

        // block lock
        lock(msg.sender);

        emit Minted(_xavierId, _amount, msg.sender, tokenBalances[_xavierId]);
    }

    /**
     * @dev Burn the Xavier token
     * @param _amount The amount of tokens to burn
     */
    function burn(XavierIdentifier _xavierId, uint256 _amount)
        public
        notLocked(msg.sender)
        initialized(_xavierId)
        returns (uint256)
    {
        require(balanceOf(msg.sender, uint256(_xavierId)) >= _amount, "Balance of burner is less than burn amount");

        uint256 salePrice = getXavierSellPrice(_xavierId, _amount);
        xtkToken.transfer(msg.sender, salePrice);
        reserveBalances[_xavierId] = reserveBalances[_xavierId].sub(salePrice);

        _burn(msg.sender, uint256(_xavierId), _amount);
        tokenBalances[_xavierId] = tokenBalances[_xavierId].sub(_amount);

        // block lock
        lock(msg.sender);

        emit Burned(_xavierId, _amount, msg.sender, tokenBalances[_xavierId]);
    }

    /**
     * @dev See {IERC1155-safeTransferFrom}.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public override notLocked(msg.sender) {
        super.safeTransferFrom(from, to, id, amount, data);
    }

    /**
     * @dev See {IERC1155-safeBatchTransferFrom}.
     */
    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public override notLocked(msg.sender) {
        super.safeBatchTransferFrom(from, to, ids, amounts, data);
    }

    /**
     * @dev Sets the token URI which is the same for all minted tokens
     * @notice Will fail if method is not called by the owner
     * @param _uri The token URI which will be used as the base URI
     */
    function setURI(string memory _uri) public onlyOwner {
        baseURI = _uri;
    }

    function uri(uint256 _id) external view override returns (string memory) {
        string memory result = Strings.strConcat(baseURI, Strings.uint2str(_id), ".json");
        console.log(result);
        return result;
    }

    /**
     * @dev Lock mint, burn, transfer and transferFrom functions
     *      for _address for BLOCK_LOCK_COUNT blocks
     */
    function lock(address _address) private {
        lastLockedBlock[_address] = block.number + BLOCK_LOCK_COUNT;
    }
}

