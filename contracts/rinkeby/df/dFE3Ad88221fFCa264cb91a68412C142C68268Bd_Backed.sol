pragma abicoder v2;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "./interfaces/IPriceProvider.sol";
import "hardhat/console.sol";

interface IERC20Extented is IERC20 {
    function decimals() external view returns (uint8);
}

contract Backed is Ownable {
    using SafeMath for uint256;
    
    uint256 constant BASE_PERCENT = 10000;
    uint256 constant BASE_PRICE = 10 * 10**18;
    address constant BurnerAddress = 0x000000000000000000000000000000000000dEaD;
    // Status of the policy. Policy in the status "Created" is an offer.
    enum Status {
        Created,
        Accepted,
        Canceled,
        Terminated
    }

    enum ParticipantType {
        Insurer,
        Insured
    }

    struct InsuranceTerms {
        address asset; //Asset to insure. It must be ERC-20 compatible token
        address baseCurrency; //Base currency - Currency in which asset is priced. Usually it is an ERC-20 stablecoin , e.g. USDC
        ParticipantType participantType; // Participant that created an offer
        uint256 insuredAssetAmount; //Amount of funds in the asset currency insured for the loss
        uint256 lossCoverage; // Maximum amount of funds payable by the insurer to the insured to cover losses, in the base currency
        uint256 premiumPercent; //Mutually agreed amount the Insured pays weekly to Insurer in order to maintain the policy.
        //Premium is denominated as a percentage of Coverage amount and is paid in the asset being covered.
        address creator;
        uint256 period; //Policy period - Period, in weeks
    }

    struct Policy {
        uint256 acceptedTime;
        InsuranceTerms insuranceTerms;
        uint256 depositPremium;
        Status status;
        address acceptor;
        uint256 depositFee;
        uint256 depositWithdrawn;
        uint256 originalPrice;
    }

    address private priceProvider;
    uint256 private currentPolicyId;
    uint256 private feeBalance;
    // policyId => Policy
    mapping(uint256 => Policy) public policies;
    // _asset =>baseERC20=>uniswapPair
    mapping(address => mapping(address => address)) public pairs;
    // uniswapPair => price provider

    // events
    event PlacedOffer(uint256 policyId);
    event CanceledOffer(uint256 policyId);
    event AcceptedOffer(uint256 policyId, address secondParticipant);
    event TerminatedPolicy(
        uint256 policyId,
        address initiator,
        ParticipantType initiatorType
    );
    event PremiumDeposit(
        uint256 policyId,
        address sourceAddress,
        uint256 amount
    );
    event PremiumWithdraval(uint256 policyId);
    event SetBurnPercent(uint256 percent);

    address public agioToken;
    uint256 public platformFeePercent;
    uint256 public BurnPercent;
    

    constructor(
        address _agioToken,
        uint256 _platformFeePercent,
        address _priceProvider
    ) {
        agioToken = _agioToken;
        platformFeePercent = _platformFeePercent;
        priceProvider = _priceProvider;
        setBurnPercent(100); //;
    }

    function addPair(
        address _uniswapPair,
        address _asset,
        address _baseERC20
    ) external onlyOwner {
        pairs[_asset][_baseERC20] = _uniswapPair;
    }

    function setBurnPercent(uint256 percent) public onlyOwner {
        BurnPercent = percent;
        require(percent / BASE_PERCENT < 100 && percent>=0, 'setBurnPercent: wrong value');
        emit SetBurnPercent(percent);
    }

    // external API
    function placeOfferByInsurer(
        address _asset, //Asset to insure. It must be ERC-20 compatible token
        address _baseCurrency, //Base currency - Currency in which asset is priced. Usually it is stablecoin ERC-20, e.g. USDC
        uint256 _insuredAssetAmount, //amount of funds in Asset currency insured for the loss.
        uint256 _lossCoverage, //Maximum loss coverage - Maximum amount of funds payable by the insurer to the insured to cover losses, in base currency
        uint256 _premiumPercent, //Premium is denominated as a percentage of Coverage amount and is paid in the asset being covered.
        uint256 _period // Policy period - Period, in weeks
    ) external returns (uint256) {
        require(pairs[_asset][_baseCurrency] != address(0), "pair not allowed");

        InsuranceTerms memory insuranceTerms;
        insuranceTerms.asset = _asset;
        insuranceTerms.baseCurrency = _baseCurrency;
        insuranceTerms.insuredAssetAmount = _insuredAssetAmount;
        insuranceTerms.premiumPercent = _premiumPercent;
        insuranceTerms.lossCoverage = _lossCoverage;
        insuranceTerms.participantType = ParticipantType.Insurer;
        insuranceTerms.creator = msg.sender;
        insuranceTerms.period = _period;

        _insurerOfferLogic(insuranceTerms);

        uint256 offerId = _getPolicyId();

        Policy memory policy;
        policy.acceptedTime = block.timestamp;
        policy.insuranceTerms = insuranceTerms;
        policy.status = Status.Created;
        policies[offerId] = policy;

        emit PlacedOffer(offerId);

        return offerId;
    }

    function acceptOfferByInsurer(uint256 _policyId) external {
        Policy memory policy = policies[_policyId];
        require(
            policy.insuranceTerms.participantType == ParticipantType.Insured,
            'acceptOfferByInsurer: wrong type of offer'
        );
        require(
            policy.insuranceTerms.creator != msg.sender,
            'acceptOfferByInsurer: creator == acceptor'
        );
        
        _insurerOfferLogic(policy.insuranceTerms);

        policy.status = Status.Accepted;
        policy.acceptor = msg.sender;
        policy.acceptedTime = block.timestamp;
        policy.originalPrice = assetPrice(policy);
        policies[_policyId] = policy;
        _acceptFee(policy);

        emit AcceptedOffer(_policyId, msg.sender);
    }

    function placeOfferByInsured(
        address _asset, //Asset to insure. It must be ERC-20 compatible token
        address _baseCurrency, //Base currency - Currency in which asset is priced. Usually it is stablecoin ERC-20, e.g. USDC
        uint256 _insuredAssetAmount, //amount of funds in Asset currency insured for the loss.
        uint256 _lossCoverage, //Maximum loss coverage - Maximum amount of funds payable by the insurer to the insured to cover losses, in base currency
        uint256 _premiumPercent, //Premium is denominated as a percentage of Coverage amount and is paid in the asset being covered.
        uint256 _period, //Policy period - Period, in weeks
        uint256 _premiumAmount
    ) external {
        require(pairs[_asset][_baseCurrency] != address(0), "placeOfferByInsured: pair not allowed");

        InsuranceTerms memory insuranceTerms;
        insuranceTerms.asset = _asset;
        insuranceTerms.baseCurrency = _baseCurrency;
        insuranceTerms.insuredAssetAmount = _insuredAssetAmount;
        insuranceTerms.premiumPercent = _premiumPercent;
        insuranceTerms.creator = msg.sender;
        insuranceTerms.lossCoverage = _lossCoverage;
        insuranceTerms.participantType = ParticipantType.Insured;
        insuranceTerms.period = _period;

        uint256 policyId = _getPolicyId();
        uint256 fee = _insuredOfferLogic(
            policyId,
            _premiumAmount,
            0,
            insuranceTerms
        );

        Policy memory policy;
        policy.acceptedTime = block.timestamp;
        policy.insuranceTerms = insuranceTerms;
        policy.status = Status.Created;
        policy.depositFee = fee;
        policy.depositPremium = _premiumAmount;
        policies[policyId] = policy;

        emit PlacedOffer(policyId);
    }

    function acceptOfferByInsured(uint256 _policyId, uint256 _premiumAmount)
        public
    {
        Policy memory policy = policies[_policyId];
        require(
            policy.insuranceTerms.participantType == ParticipantType.Insurer,
            'acceptOfferByInsured: wrong participantType'
        );
        require(
            policy.insuranceTerms.creator != msg.sender,
            'acceptOfferByInsured: creator == acceptor'
        );
        uint256 fee = _insuredOfferLogic(
            _policyId,
            _premiumAmount,
            policy.depositPremium,
            policy.insuranceTerms
        );

        policy = policies[_policyId];
        policy.depositFee = fee;
        policy.depositPremium = policy.depositPremium.add(_premiumAmount);
        policy.status = Status.Accepted;
        policy.acceptor = msg.sender;
        policy.acceptedTime = block.timestamp;
        policy.originalPrice = assetPrice(policy);
        policies[_policyId] = policy;
        _acceptFee(policy);

        emit AcceptedOffer(_policyId, msg.sender);
    }

    function premiumDeposit(uint256 _policyId, uint256 _premiumAmount) external {
        Policy memory policy = policies[_policyId];
        require(enoughPremium(policy), 'premiumDeposit: not enoughPremium');

        _premiumDepositLogic(
            _policyId,
            _premiumAmount,
            policy.depositPremium,
            policy.insuranceTerms
        );

        policy.depositPremium = policy.depositPremium.add(_premiumAmount);
        policies[_policyId] = policy;
    }

    function cancelOffer(uint256 _policyId) external {
        Policy memory policy = policies[_policyId];
        if (policy.insuranceTerms.participantType == ParticipantType.Insurer) {
           cancelOfferByInsurer(_policyId);
        } else {
            cancelOfferByInsured(_policyId);
        }
    }
    
    function cancelOfferByInsurer(uint256 _policyId) public {
        Policy memory policy = policies[_policyId];
        require(policy.status == Status.Created, 'cancelOfferByInsurer: wrong status');
        require(
            policy.insuranceTerms.participantType == ParticipantType.Insurer, 'cancelOfferByInsurer: wrong participant type'
        );
        require(policy.insuranceTerms.creator == msg.sender, 'cancelOfferByInsurer: not creator');
        
        IERC20 token = IERC20(policy.insuranceTerms.baseCurrency);
        require(token.transfer(address(this), policy.insuranceTerms.lossCoverage));

        policy.status = Status.Canceled;
        policies[_policyId] = policy;
        emit CanceledOffer(_policyId);
    }

    function cancelOfferByInsured(uint256 _policyId) public {
        Policy memory policy = policies[_policyId];
        require(policy.status == Status.Created, 'cancelOfferByInsured: wrong status');
        require(policy.insuranceTerms.creator == msg.sender, 'cancelOfferByInsured: not creator');
        require(
            policy.insuranceTerms.participantType == ParticipantType.Insured, 'cancelOfferByInsured: wrong participant type'
        );

        IERC20 token = IERC20(agioToken);
        token.transfer(msg.sender, policy.depositFee);

        IERC20 assetToken = IERC20(policy.insuranceTerms.asset);
        assetToken.transfer(msg.sender, policy.depositPremium);

        policy.status = Status.Canceled;
        policies[_policyId] = policy;
        emit CanceledOffer(_policyId);
    }

    function premiumWithdraval(uint256 _policyId) external {
        Policy memory policy = policies[_policyId];

        address insurer;
        require(policy.status == Status.Accepted, 'premiumWithdraval: wrong status');

        if (policy.insuranceTerms.participantType == ParticipantType.Insurer) {
            insurer = policy.insuranceTerms.creator;
        } else {
            insurer = policy.acceptor;
        }


        uint256 premium_available_to_insurer_for_withdrawal
         = premiumAvailableInsurerWithdrawal(policy);
        IERC20 assetToken = IERC20(policy.insuranceTerms.asset);
        require(
            assetToken.transfer(
                insurer,
                premium_available_to_insurer_for_withdrawal
            )
        );
        policy.depositWithdrawn = premium_available_to_insurer_for_withdrawal
        .add(policy.depositWithdrawn);
        policies[_policyId] = policy;

        emit PremiumWithdraval(_policyId);
    }

    function transferFee(address _to) external onlyOwner {
        IERC20 token = IERC20(agioToken);
        require(token.transfer(_to, feeBalance));
        feeBalance = 0;
    }

    function terminateionResult(uint256 _policyId, address terminater)
        public
        view
        returns (
            uint256 lossCoverageInsuredWithdraw,
            uint256 lossCoverageInsurerWithdraw,
            uint256 premium_available_to_insured_for_withdrawal,
            uint256 premium_available_to_insurer_for_withdrawal,
            address insurer,
            address insured,
            ParticipantType initiator
        )
    {
        Policy memory policy = policies[_policyId];
        require(policy.status == Status.Accepted, 'terminateionResult: incorrect status');

        if (policy.insuranceTerms.participantType == ParticipantType.Insurer) {
            insured = policy.acceptor;
            insurer = policy.insuranceTerms.creator;
        } else {
            insured = policy.insuranceTerms.creator;
            insurer = policy.acceptor;
        }

        require(terminater == insurer || terminater == insured, 'terminateionResult: this user cant terminate');

        if (terminater == insurer) initiator = ParticipantType.Insurer;
        else initiator = ParticipantType.Insured;

        //if Not enough premium loss_coverage_for_insured_to_withdraw = 0
        //loss_coverage_for_insured_to_withdraw = (original_price - current_price) * asset_amount
        //loss_coverage_for_insurer_to_withdraw = maximum_loss_coverage - loss_coverage_for_insured_to_withdraw
        lossCoverageInsuredWithdraw = (
            policy.originalPrice.sub(assetPrice(policy))
        )
        .mul(policy.insuranceTerms.insuredAssetAmount)
        .div(BASE_PRICE);

        if (!enoughPremium(policy)) {
            lossCoverageInsuredWithdraw = 0;
        }
        if (lossCoverageInsuredWithdraw > policy.insuranceTerms.lossCoverage) {
            lossCoverageInsuredWithdraw = policy.insuranceTerms.lossCoverage;
        }

        lossCoverageInsurerWithdraw = policy.insuranceTerms.lossCoverage.sub(
            lossCoverageInsuredWithdraw
        );

        uint256 premiumPerWeekL = premiumPerWeek(policy);

        //Terminated by Insurer or Expired
        if (expire(policy) || initiator == ParticipantType.Insurer) {
            premium_available_to_insured_for_withdrawal = policy
            .depositPremium
            .sub(premiumPerWeekL.mul(weekElapsedRoundDown(policy)));
            premium_available_to_insurer_for_withdrawal = premiumPerWeekL
            .mul(weekElapsedRoundDown(policy))
            .sub(policy.depositWithdrawn);
        } else {
            premium_available_to_insured_for_withdrawal = policy
            .depositPremium
            .sub(premiumPerWeekL.mul(weekElapsedRoundUp(policy)));
            premium_available_to_insurer_for_withdrawal = premiumPerWeekL
            .mul(weekElapsedRoundUp(policy))
            .sub(policy.depositWithdrawn);
        }
        if (!enoughPremium(policy)) {
            premium_available_to_insurer_for_withdrawal = policy.depositPremium;
            premium_available_to_insured_for_withdrawal = 0;
        }
    }

    function terminate(uint256 _policyId) external {
        uint256 lossCoverageInsuredWithdraw;
        uint256 lossCoverageInsurerWithdraw;
        uint256 premium_available_to_insured_for_withdrawal;
        uint256 premium_available_to_insurer_for_withdrawal;
        address insurer;
        address insured;
        ParticipantType initiator;

        (
            lossCoverageInsuredWithdraw,
            lossCoverageInsurerWithdraw,
            premium_available_to_insured_for_withdrawal,
            premium_available_to_insurer_for_withdrawal,
            insurer,
            insured,
            initiator
        ) = terminateionResult(_policyId, msg.sender);

        Policy memory policy = policies[_policyId];
        IERC20 baseToken = IERC20(policy.insuranceTerms.baseCurrency);
        if(lossCoverageInsuredWithdraw != 0)
            require(baseToken.transfer(insured, lossCoverageInsuredWithdraw));
        if(lossCoverageInsurerWithdraw != 0)
            require(baseToken.transfer(insurer, lossCoverageInsurerWithdraw));

        IERC20 assetToken = IERC20(policy.insuranceTerms.asset);
        if(premium_available_to_insured_for_withdrawal != 0)
            require(assetToken.transfer(
                insured,
                premium_available_to_insured_for_withdrawal
            ));
        if(premium_available_to_insurer_for_withdrawal != 0)
            require(assetToken.transfer(
                insurer,
                premium_available_to_insurer_for_withdrawal
            )); 

        policy.status = Status.Terminated;
        policies[_policyId] = policy;
        emit TerminatedPolicy(_policyId, msg.sender, initiator);
    }

    function platformFee(
        address _asset, //Asset to insure. It must be an ERC-20 compatible token
        address _baseCurrency,
        uint256 _premiumPercent,
        uint256 _insuredAssetAmount, //amount of funds in the Asset currency insured for the loss.
        uint256 _period
    ) public view returns (uint256) {
        address assetUniswapPair = pairs[_asset][_baseCurrency];
        address agioUniswapPair = pairs[agioToken][_baseCurrency];

        uint256 plaformFeeAsset = _plaformFeeAsset(
            _premiumPercent,
            _insuredAssetAmount,
            _period
        );

        IPriceProvider oracle = IPriceProvider(priceProvider);
        uint256 feeBase = plaformFeeAsset.mul(
            oracle.getPairPrice(assetUniswapPair, _baseCurrency)
        );
        uint256 result = feeBase.div(
            oracle.getPairPrice(agioUniswapPair, _baseCurrency)
        );
        return result.div(10**(18 - IERC20Extented(_asset).decimals()));
    }

    function _plaformFeeAsset(
        uint256 _premiumPercent,
        uint256 _insuredAssetAmount,
        uint256 _period
    ) private view returns (uint256) {
        uint256 premiumPerWeekL = _insuredAssetAmount.mul(_premiumPercent).div(
            BASE_PERCENT
        );
        uint256 totalPremium = premiumPerWeekL.mul(_period);
        uint256 plaformFeeAsset = totalPremium.mul(platformFeePercent).div(
            BASE_PERCENT
        );
        return plaformFeeAsset;
    }

    function getCurrentPolicyId() external view returns (uint256) {
        return currentPolicyId;
    }

    function assetPrice(Policy memory policy) public view returns (uint256) {
        address assetUniswapPair = pairs[policy.insuranceTerms.asset][
            policy.insuranceTerms.baseCurrency
        ];
        IPriceProvider oracle = IPriceProvider(priceProvider);
        return
            oracle.getPairPrice(
                assetUniswapPair,
                policy.insuranceTerms.baseCurrency
            );
    }

    function weekElapsed(Policy memory policy) public view returns (uint256) {
        return ((block.timestamp - policy.acceptedTime) / 1 weeks);
    }

    function weekElapsedRoundUp(Policy memory policy)
        public
        view
        returns (uint256)
    {
        uint256 weeksElapsed = weekElapsed(policy) + 1;
        if (weeksElapsed >= policy.insuranceTerms.period) {
            weeksElapsed = policy.insuranceTerms.period;
        }
        return weeksElapsed;
    }

    function premiumAvailableInsurerWithdrawal(Policy memory policy)
        public
        view
        returns (uint256)
    {
        return
            premiumPerWeek(policy).mul(weekElapsedRoundDown(policy)).sub(
                policy.depositWithdrawn
            );
    }

    function premiumPerWeek(Policy memory policy)
        public
        view
        returns (uint256)
    {
        return
            policy
                .insuranceTerms
                .insuredAssetAmount
                .mul(policy.insuranceTerms.premiumPercent)
                .div(BASE_PERCENT);
    }

    function weekElapsedRoundDown(Policy memory policy)
        public
        view
        returns (uint256)
    {
        uint256 weeksElapsed = weekElapsed(policy);
        if (weeksElapsed >= policy.insuranceTerms.period) {
            weeksElapsed = policy.insuranceTerms.period;
        }
        return weeksElapsed;
    }

    function enoughPremium(Policy memory policy) public view returns (bool) {
        uint256 weeksElapsed = weekElapsedRoundUp(policy);
        uint256 premiumRequired = policy
        .insuranceTerms
        .premiumPercent
        .mul(policy.insuranceTerms.insuredAssetAmount)
        .mul(weeksElapsed)
        .div(BASE_PERCENT);
        return policy.depositPremium >= premiumRequired;
    }

    function expire(Policy memory policy) public view returns (bool) {
        uint256 weeksElapsed = weekElapsed(policy);
        return weeksElapsed >= policy.insuranceTerms.period;
    }

    function getPolicy(uint256 policyId)
        public
        view
        returns (Policy memory policy)
    {
        return policies[policyId];
    }

    function _insurerOfferLogic(InsuranceTerms memory _terms) private {
        IERC20 token = IERC20(_terms.baseCurrency);
        require(
            token.transferFrom(msg.sender, address(this), _terms.lossCoverage)
        );
    }

    function _acceptFee(Policy memory policy) private {
        uint256 burnAmount = policy.depositFee.mul(BurnPercent).div(
            BASE_PERCENT
        );
        feeBalance = feeBalance.add(policy.depositFee.sub(burnAmount));
        IERC20 token = IERC20(agioToken);
        if(burnAmount > 0)
            token.transfer(BurnerAddress, burnAmount);
    }

    function _insuredOfferLogic(
        uint256 policyId,
        uint256 premiumAmount,
        uint256 currentDepositBalance,
        InsuranceTerms memory _terms
    ) private returns (uint256 fee) {
        fee = platformFee(
            _terms.asset,
            _terms.baseCurrency,
            _terms.premiumPercent,
            _terms.insuredAssetAmount,
            _terms.period
        );
        IERC20 token = IERC20(agioToken);
        require(token.transferFrom(msg.sender, address(this), fee));

        _premiumDepositLogic(
            policyId,
            premiumAmount,
            currentDepositBalance,
            _terms
        );
    }

    //return asset token amount
    function _premiumDepositLogic(
        uint256 policyId,
        uint256 premiumAmount,
        uint256 currentDepositBalance,
        InsuranceTerms memory terms
    ) private {
        uint256 premium = terms
        .premiumPercent
        .mul(terms.insuredAssetAmount)
        .div(BASE_PERCENT);

        currentDepositBalance = currentDepositBalance.add(premiumAmount);
        require(currentDepositBalance >= premium, '_premiumDepositLogic: should be deposit more then min');

        IERC20 assetToken = IERC20(terms.asset);
        require(
            assetToken.transferFrom(msg.sender, address(this), premiumAmount)
        );
        emit PremiumDeposit(policyId, msg.sender, premiumAmount);
    }

    function _getPolicyId() internal returns (uint256) {
        currentPolicyId = currentPolicyId.add(1);
        return currentPolicyId;
    }
}

