    uint256 opaqueError = noError + 11; // an arbitrary, opaque error code

    /*** Assets You Are In ***/

    function enterMarkets(address[] calldata _cTokens) external override returns (uint256[] memory) {
        _cTokens;
        uint256[] memory ret;
        return ret;
