        _cToken;
        _minter;
        _mintAmount;
        return allowMint ? noError : opaqueError;
    }

    function mintVerify(
        address _cToken,
        address _minter,
        uint256 _mintAmount,
        uint256 _mintTokens
    ) external override {
        _cToken;
        _minter;
        _mintAmount;
        _mintTokens;
