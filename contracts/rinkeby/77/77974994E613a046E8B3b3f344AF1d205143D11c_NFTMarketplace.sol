pragma solidity ^0.5.0;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/token/ERC721/ERC721Full.sol";
import "@openzeppelin/contracts/ownership/Ownable.sol";

contract NFTFactory is ERC721Full, Ownable {

    struct Item {
        string name;
        uint256 tokenId;
        string baseURI;
        address owner;
    }
    uint public lastTokenId = 0;
    string public nftName = "Barbie SuperDriver Collection";
    string public uri = "https://ipfs.io/ipfs/QmWBbfLq4TbTSaugHvbWewvvzb5hnzjki73jk9kQKn73Ab/";

    constructor() public ERC721Full(nftName, "BDC") {}

    function safeMint(address to, uint256 tokenId, string memory uri) public onlyOwner {
        if (tokenId > lastTokenId) {
            lastTokenId = tokenId;
        }
        _safeMint(to, tokenId);
        _setTokenURI(tokenId, uri);
    }

    // function _baseURI() internal pure returns (string memory) {
    //     return "https://ipfs.io/ipfs/QmWBbfLq4TbTSaugHvbWewvvzb5hnzjki73jk9kQKn73Ab/";
    // }

    /// @notice Returns all NFTs and owners
    /// @param pagination Number of nfts per page
    /// @param page Page number
    function allNFTs(uint pagination, uint page) public view 
        returns (Item[] memory) {
        Item[] memory items = new Item[](pagination);
        uint start =  ((page - 1) * pagination) + 1;
        uint end = start + pagination - 1;
        for (uint256 i = start; i <= end; i++) {
            if (i <= lastTokenId) {
                Item memory it = Item({
                    name: nftName,
                    tokenId: i,
                    baseURI: uri,
                    owner: ownerOf(i)
                });
                items[i - 1] = it;
            }
        }
        return items;
    }

}

