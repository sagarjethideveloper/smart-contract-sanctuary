pragma solidity ^0.6.2;

import '@openzeppelin/contracts/math/Math.sol';
import '@openzeppelin/contracts/math/SafeMath.sol';
import "@openzeppelin/contracts/access/Ownable.sol";
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/utils/Address.sol';
import '@openzeppelin/contracts/token/ERC20/SafeERC20.sol';
import "@openzeppelin/contracts/utils/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/EnumerableMap.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import './token/LPTokenWrapper.sol';
import "./interface/IBUFactoryV2.sol";
import "./interface/INFT.sol";

contract SoviBeeBattleGround is Ownable, LPTokenWrapper, IERC721Receiver
{
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableMap for EnumerableMap.UintToAddressMap;

    struct UserInfo {
        uint256 heroId;
        uint256 extraWeight;
        uint256 minSurvivalBlock;
    }

    struct BUInfo {
        address owner;
        uint256 id;
        uint256 ruleId;
        uint256 category;
        uint256 nftType;
        uint256 poc;
        uint256 extra0;
        uint256 extra1;
        uint256 extra2;
        uint256 enterBlock;
        uint256 hpPerBlock;
        uint256 endBlock;
        uint256 deadDay;
    }

    uint256 constant BU_SOLDIER = 1;
    uint256 constant BU_IGNITER = 3;
    uint256 constant BU_DEFIER = 2;
    uint256 constant BU_GENERAL = 4;
    uint256 constant DAILY_BLOCKS = 28800;
    uint256 public constant DURATION = DAILY_BLOCKS * 30;
    uint256 public constant LIMIT_NFT_TYPE_MIN = 12;

    IERC20 public SOVI;
    INFT public _battleUnit;
    IBUFactoryV2 public _buFactory;
    address public _devAddr;

    uint256 public initReward;
    uint256 public startTime;
    uint256 public periodFinish;
    uint256 public rewardRate;
    uint256 public lastUpdateTime;
    uint256 public rewardPerTokenStored;
    uint256 public totalExtraWeight;
    mapping(address => uint256) public userRewardPerTokenPaid;
    mapping(address => uint256) public rewards;
    uint256 public totalHeroes;
    uint256 public totalSoldiers;

    // battle ground info
    uint256 _ruleId;
    uint256 _difficult;
    uint256 _soldierMaxLimit = 5;

    mapping(address => UserInfo) public _userInfo;
    mapping(uint256 => BUInfo) public _buInfo;
    mapping(address => EnumerableSet.UintSet) private _usersBU;
    mapping(uint256 => EnumerableSet.UintSet) private _diedBU;
    EnumerableSet.UintSet private _cardsBlackList;

    event RewardAdded(uint256 reward);
    event Staked(address indexed user, uint256 tokenId, uint256 poc);
    event Withdrawn(address indexed user, uint256 tokenId, uint256 poc);
    event RewardPaid(address indexed user, uint256 reward);
    event NFTReceived(address operator, address from, uint256 tokenId, bytes data);
    event BattleGroundCleaned(address operator, uint256 deadDay, uint256 length);

    constructor(
        address SOVI_,
        address battleUnit_,
        address buFactory_,
        uint256 ruleId_,
        uint256 difficult_,
        uint256 startTime_
    ) public {
        SOVI = IERC20(SOVI_);
        _battleUnit = INFT(battleUnit_);
        _buFactory = IBUFactoryV2(buFactory_);
        _ruleId = ruleId_;
        startTime = startTime_;
        periodFinish = startTime_.add(DURATION);
        lastUpdateTime = startTime_;
        _devAddr = msg.sender;
        _difficult = difficult_;
    }

    modifier updateReward(address account) {
        rewardPerTokenStored = rewardPerToken();
        lastUpdateTime = lastTimeRewardApplicable();
        if (account != address(0)) {
            rewards[account] = earned(account);
            userRewardPerTokenPaid[account] = rewardPerTokenStored;
        }
        _;
    }

    function checkRule(uint256 ruleId_) public view returns (bool){
        return _ruleId == 0 || ruleId_ == _ruleId;
    }

    function checkBanned(uint256 tokenId_) external view returns (bool){
        return _cardsBlackList.contains(tokenId_);
    }

    function lastTimeRewardApplicable() public view returns (uint256) {
        return block.number < startTime ? startTime : Math.min(block.number, periodFinish);
    }

    function rewardPerToken() public view returns (uint256) {
        if (totalSupply() == 0) {
            return rewardPerTokenStored;
        }
        return
        rewardPerTokenStored.add(
            lastTimeRewardApplicable()
            .sub(lastUpdateTime)
            .mul(rewardRate)
            .mul(1e18)
            .div(totalSupply().add(totalExtraWeight))
        );
    }

    function earned(address account) public view returns (uint256) {
        if (block.number < startTime) {
            return 0;
        }
        return
        balanceOf(account).add(_userInfo[account].extraWeight)
        .mul(rewardPerToken().sub(userRewardPerTokenPaid[account]))
        .div(1e18)
        .add(rewards[account]);
    }

    function stakedHero(address account_) external view returns (uint256) {
        return _userInfo[account_].heroId;
    }

    function stakedSoldiers(address account_) external view returns (uint256[] memory) {
        uint256 length = _usersBU[account_].length();
        uint256[] memory tokenIds = new uint256[](length);
        if (length > 0) {
            for (uint idx = 0; idx < length; idx ++) {
                tokenIds[idx] = _usersBU[account_].at(idx);
            }
        }
        return tokenIds;
    }

    function totalPoc() public view returns (uint256){
        return totalSupply().add(totalExtraWeight);
    }

    function userPoc(address account_) external view returns (uint256){
        return balanceOf(account_).add(_userInfo[account_].extraWeight);
    }

    function rewardPerPoc() external view returns (uint256){
        if (totalPoc() == 0) {
            return 0;
        }
        return rewardRate.mul(DAILY_BLOCKS).div(totalPoc());
    }

    function soldiersHP(uint256[] calldata tokenIds) external view returns (uint256[] memory){
        uint256[] memory soldierHP = new uint256[](tokenIds.length);

        uint256 left;
        uint256 consume;
        uint256 finishBlock = Math.min(block.number, periodFinish);
        for (uint256 idx; idx < tokenIds.length; idx ++) {
            BUInfo storage bu = _buInfo[tokenIds[idx]];

            if (block.number < startTime) {
                soldierHP[idx] = bu.extra1;
                continue;
            }

            if (bu.endBlock < finishBlock) {
                left = 0;
            } else {
                consume = finishBlock.sub(bu.enterBlock).mul(bu.hpPerBlock);
                left = consume >= bu.extra1 ? 0 : bu.extra1.sub(consume);
            }
            soldierHP[idx] = left;
        }
        return soldierHP;
    }

    function stake(uint256 tokenId_) public checkBUSurvival updateReward(msg.sender) {
        require(block.number < periodFinish, "SOVI: ended");
        require(tokenId_ > 0, "SOVI: tokenId error");
        require(!_cardsBlackList.contains(tokenId_), "SOVI: banned!");
        BUInfo memory bu = getBU(tokenId_);

        require(checkRule(bu.ruleId), "SOVI: deny");
        require(bu.category == BU_SOLDIER || bu.category == BU_DEFIER, "SOVI: !category");

        _battleUnit.safeTransferFrom(msg.sender, address(this), tokenId_);

        uint256 minBlock = Math.max(block.number, startTime);
        if (bu.category == BU_DEFIER) {
            require(_userInfo[msg.sender].heroId == 0, "SOVI: only one hero");
            // only new heroes
            require(bu.nftType > LIMIT_NFT_TYPE_MIN, "SOVI: only new heroes");
            _userInfo[msg.sender].heroId = tokenId_;
            totalHeroes = totalHeroes.add(1);
        } else {
            require(_usersBU[msg.sender].length() < _soldierMaxLimit, "SOVI: max limit exceeded");
            bu.enterBlock = minBlock;
            bu.hpPerBlock = hpPerBlock(bu.extra0);
            bu.endBlock = bu.extra1.div(bu.hpPerBlock).add(minBlock);
            require(bu.endBlock > block.number, "SOVI: soldier too weak");
            _usersBU[msg.sender].add(tokenId_);
            uint256 deadDay = bu.endBlock.sub(startTime).div(DAILY_BLOCKS).add(1);
            bu.deadDay = deadDay;
            _diedBU[deadDay].add(tokenId_);
            totalSoldiers = totalSoldiers.add(1);
        }

        bu.owner = msg.sender;
        _buInfo[tokenId_] = bu;
        if (bu.poc > 0) {
            super._stake(bu.poc);
        }
        updateExtraWeight(msg.sender);
        emit Staked(msg.sender, tokenId_, bu.poc);
    }

    function withdraw(uint256 tokenId_) public checkBUSurvival {
        if (_buInfo[tokenId_].id == 0) {
            return;
        }

        require(
            _usersBU[msg.sender].contains(tokenId_) ||
            _userInfo[msg.sender].heroId == tokenId_,
            'SOVI: tokenId error'
        );

        retreat(msg.sender, tokenId_);
    }

    function getReward() public checkBUSurvival updateReward(msg.sender) checkStart {
        uint256 reward = earned(msg.sender);
        if (reward > 0) {
            rewards[msg.sender] = 0;
            SOVI.safeTransfer(msg.sender, reward);
            if (_devAddr != address(0)) {
                SOVI.safeTransfer(_devAddr, reward.div(10));
            }
            emit RewardPaid(msg.sender, reward);
        }
    }

    function retreat(address account_, uint256 tokenId_) internal updateReward(account_) {
        BUInfo memory bu = _buInfo[tokenId_];
        if (bu.poc > 0) {
            super._withdrawFrom(account_, bu.poc);
        }
        if (bu.category == BU_DEFIER) {
            _userInfo[account_].heroId = 0;
            totalHeroes = totalHeroes.sub(1);
        } else {
            uint256 finishBlock = Math.min(block.number, periodFinish);
            if (block.number >= startTime && bu.enterBlock < finishBlock) {
                if (bu.endBlock < finishBlock) {
                    _buFactory.decreaseHP(tokenId_, bu.extra1);
                } else {
                    uint256 sub = Math.max(startTime, bu.enterBlock);
                    _buFactory.decreaseHP(tokenId_, finishBlock.sub(sub).mul(bu.hpPerBlock));
                }
            }

            totalSoldiers = totalSoldiers.sub(1);
            _diedBU[bu.deadDay].remove(tokenId_);
        }

        withdrawNFT(account_, tokenId_);
        updateExtraWeight(account_);
        emit Withdrawn(account_, tokenId_, bu.poc);
    }

    function withdrawNFT(address account_, uint256 tokenId_) internal {
        _usersBU[account_].remove(tokenId_);
        _battleUnit.transferFrom(address(this), account_, tokenId_);
        delete _buInfo[tokenId_];
    }

    function updateExtraWeight(address account) internal {
        uint256 preExtraWeight = _userInfo[account].extraWeight;
        uint256 newExtraWeight = 0;
        uint256 heroId = _userInfo[account].heroId;
        if (heroId > 0) {
            newExtraWeight = super.balanceOf(account).mul(_buInfo[heroId].extra0).div(100);
        }
        _userInfo[account].extraWeight = newExtraWeight;
        totalExtraWeight = totalExtraWeight.sub(preExtraWeight).add(newExtraWeight);
    }

    function getBU(uint256 tokenId_) internal view returns (BUInfo memory bu){
        uint256 ruleId;
        uint256 category;
        uint256 nftType;
        uint256 poc;
        uint256[] memory extras;
        (,, ruleId, category, nftType, poc, extras) = _buFactory.getBattleUnit(tokenId_);

        bu.id = tokenId_;
        bu.ruleId = ruleId;
        bu.category = category;
        bu.nftType = nftType;
        bu.poc = poc;
        bu.extra0 = extras[0];
        bu.extra1 = extras[1];
        bu.extra2 = extras[2];
    }

    modifier checkBUSurvival() {
        if (isOpening(block.number)) {
            uint256 num = block.number.sub(startTime).div(DAILY_BLOCKS);
            cleanup(num.add(1), 5);
            cleanup(num, 5);
        }
        _;
    }

    modifier checkStart() {
        require(block.number >= startTime, 'SOVI: not start');
        _;
    }

    function isOpening(uint256 blockNum) public view returns (bool){
        return blockNum >= startTime && blockNum < periodFinish;
    }

    function hpPerBlock(uint256 pob_) public view returns (uint256){
        return pob_ >= _difficult ? 1 : _difficult.sub(pob_);
    }

    function cleanup(uint256 deadDay_, uint256 max_) public {
        uint256 length = Math.min(max_, _diedBU[deadDay_].length());
        uint256 tokenId;
        uint256[] memory tokenIds = new uint256[](length);
        for (uint256 idx; idx < length; idx ++) {
            tokenId = _diedBU[deadDay_].at(idx);
            if (tokenId > 0 && _buInfo[tokenId].endBlock < block.number) {
                tokenIds[idx] = tokenId;
            }
        }
        cleanup2(tokenIds);

        emit BattleGroundCleaned(msg.sender, deadDay_, length);
    }

    function fullCleanup(uint256 deadDay_, uint256 max_) external {
        for (uint256 idx; idx < deadDay_; idx ++) {
            cleanup(deadDay_.sub(idx), max_);
        }
    }

    function forceQuit(uint256[] memory tokenIds_) public onlyOwner {
        for (uint256 i = 0; i < tokenIds_.length; i ++) {
            retreat(_buInfo[tokenIds_[i]].owner, tokenIds_[i]);
        }
    }

    function setFinishBlock(uint256 newPeriodFinish_) external onlyOwner {
        periodFinish = newPeriodFinish_;
    }

    function setBannedCards(uint256[] calldata tokenIds_) external onlyOwner {
        for (uint256 i = 0; i < tokenIds_.length; i ++) {
            _cardsBlackList.add(tokenIds_[i]);
        }
    }

    function removeBannedCards(uint256[] calldata tokenIds_) external onlyOwner {
        for (uint256 i = 0; i < tokenIds_.length; i ++) {
            _cardsBlackList.remove(tokenIds_[i]);
        }
    }

    function dev(address newAddr_) external onlyOwner {
        _devAddr = newAddr_;
    }

    function checkClean(uint256 deadDay_, uint256 max_) external view returns (uint256[] memory){
        uint256 length = _diedBU[deadDay_].length();
        uint256[] memory tokenIds = new uint256[](Math.min(max_, length));
        uint256 i = 0;
        uint256 tokenId;
        for (uint256 idx; idx < length; idx ++) {
            tokenId = _diedBU[deadDay_].at(idx);
            if (_buInfo[tokenId].endBlock < block.number) {
                tokenIds[i] = tokenId;
                i ++;
            }
            if (i >= max_) {
                break;
            }
        }
        return tokenIds;
    }

    function cleanup2(uint256[] memory tokenIds_) public {
        uint256 tokenId;
        for (uint256 i = 0; i < tokenIds_.length; i ++) {
            tokenId = tokenIds_[i];
            if (tokenId > 0 && _buInfo[tokenId].endBlock > 0 && _buInfo[tokenId].endBlock < block.number) {
                retreat(_buInfo[tokenId].owner, tokenId);
            }
        }
    }

    function notifyRewardAmount(uint256 reward) external onlyOwner updateReward(address(0)) {
        if (block.number > startTime) {
            if (block.number >= periodFinish) {
                rewardRate = reward.div(DURATION);
            } else {
                uint256 remaining = periodFinish.sub(block.number);
                uint256 leftover = remaining.mul(rewardRate);
                rewardRate = reward.add(leftover).div(DURATION);
            }
            lastUpdateTime = block.number;
            periodFinish = block.number.add(DURATION);
            emit RewardAdded(reward);
        } else {
            if (initReward == 0) {
                initReward = reward;
            }
            if (startTime == 0) {
                startTime = block.number;
            }
            rewardRate = initReward.div(DURATION);
            lastUpdateTime = startTime;
            periodFinish = startTime.add(DURATION);
            emit RewardAdded(reward);
        }
    }

    function onERC721Received(address operator, address from, uint256 tokenId, bytes memory data) override public returns (bytes4) {
        if (address(this) != operator) {
            return 0;
        }
        emit NFTReceived(operator, from, tokenId, data);
        return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    }
}

pragma solidity ^0.6.2;


interface IBUFactory {

    function getSoldier(uint256 tokenId)
    external view
    returns (
        uint256 id,
        uint256 blockNum,
        uint256 ruleId,
        uint256 category,
        uint256 nftType,

        uint256 poc,
        uint256 pob,
        uint256 hp,
        uint256 reg
    );

    function getIgniter(uint256 tokenId)
    external view
    returns (
        uint256 id,
        uint256 blockNum,
        uint256 ruleId,
        uint256 category,
        uint256 nftType,

        uint256 poc,
        uint256 magic
    );

    function getDefier(uint256 tokenId)
    external view
    returns (
        uint256 id,
        uint256 blockNum,
        uint256 ruleId,
        uint256 category,
        uint256 nftType,

        uint256 poc,
        uint256 extra1,
        uint256 extra2,
        uint256 extra3,
        uint256 extra4
    );

    function getGeneral(uint256 tokenId)
    external view
    returns (
        uint256 id,
        uint256 blockNum,
        uint256 ruleId,
        uint256 category,
        uint256 nftType,

        uint256 poc,
        uint256 extra1,
        uint256 extra2,
        uint256 extra3,
        uint256 extra4,
        uint256 extra5,
        uint256 extra6
    );

    function getBattleUnit(uint256 tokenId) external view
    returns (
        uint256 id,
        uint256 blockNum,
        uint256 ruleId,
        uint256 category,
        uint256 nftType,

        uint256 poc,
        uint256[] memory extras
    );

    function mint(address receiver, uint256 ruleId, uint256 category, uint256 nftType, uint256 level) external returns (uint256);

    function upgrade(uint256 tokenId) external;

    function burn(uint256 tokenId) external;
}

