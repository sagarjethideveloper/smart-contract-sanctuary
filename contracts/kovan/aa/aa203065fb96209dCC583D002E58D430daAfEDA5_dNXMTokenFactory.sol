pragma solidity ^0.7.4;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/math/Math.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/TransparentUpgradeableProxy.sol";
import "./interfaces/IdNXMToken.sol";
import "./interfaces/IdNXMTokenFactory.sol";
import "./interfaces/INXMaster.sol";

contract dNXMTokenFactory is IdNXMTokenFactory, Ownable, AccessControl {
    using Math for uint256;
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;

    bytes32 constant public FACTORY_ADMIN_ROLE = keccak256("FACTORY_ADMIN_ROLE");

    INXMaster public nxMaster;

    address private _tokenImplementation;
    address private _wNXM;
    EnumerableSet.AddressSet internal _tokens;

    event TokenCreated(
        address _proxyInstance,
        string _symbol,
        address _contractAddress);

    modifier onlyAdmin() {
        require(hasRole(FACTORY_ADMIN_ROLE, msg.sender), "dNXMTokenFactory: Caller is not an admin");
        _;
    }

    constructor(address _tokenImplementationAddress, address _nxMasterAddress, address _wNXMAddress) {
        _setupRole(FACTORY_ADMIN_ROLE, owner());
        _setRoleAdmin(FACTORY_ADMIN_ROLE, FACTORY_ADMIN_ROLE);
        require(_tokenImplementationAddress != address(0), "dNXMTokenFactory: Empty token implementation address");
        require(_nxMasterAddress != address(0), "dNXMTokenFactory: Empty NX Master address");
        require(_wNXMAddress != address(0), "dNXMTokenFactory: Empty wNXM address");

        _tokenImplementation = _tokenImplementationAddress;
        _wNXM = _wNXMAddress;
        nxMaster = INXMaster(_nxMasterAddress);
    }

    function upgradeTokenImplementation(address _tokenImplementationAddress) external onlyAdmin {
        require(_tokenImplementationAddress != address(0), "dNXMTokenFactory: Empty address");
        _tokenImplementation = _tokenImplementationAddress;
    }

    function upgradeTokens() external onlyAdmin {
        //TODO upgrade the token implementations to the new `_tokenImplementation`
    }

    function initializeTokens() external onlyAdmin {
        //TODO upgrade dependencies (`setDependencies`) for the tokens
    }

    /**
    * @dev This will be a permissionless function in the end.
    **/
    function createToken(string calldata _symbol, string calldata _description, address _contractAddress) external onlyAdmin {
        require(_contractAddress != address(0), "dNXMTokenFactory: Null address");
        require(bytes(_description).length <= 20, "dNXMTokenFactory: Token description is too long");

        //TODO validate if _contractAddress is correct

        TransparentUpgradeableProxy _proxy = new TransparentUpgradeableProxy(
            _tokenImplementation, _msgSender(), ""
        );

        IdNXMToken(address(_proxy)).__dNXMToken_init(
            _description,
            _symbol,
            _contractAddress
        );

        IdNXMToken(address(_proxy)).setDependencies(address(nxMaster), _wNXM);

        _tokens.add(address(_proxy));

        emit TokenCreated(address(_proxy), _symbol, _contractAddress);
    }

    function isToken(address _token) public view returns (bool) {
        return _tokens.contains(_token);
    }

    function count() external view returns (uint256) {
        return _tokens.length();
    }

    /// @notice use with count()
    function list(uint256 offset, uint256 limit)
        public
        view
        returns (address[] memory _tokensArr)
    {
        return _list(offset, limit, _tokens);
    }

    function _list(
        uint256 offset,
        uint256 limit,
        EnumerableSet.AddressSet storage set
    ) internal view returns (address[] memory _tokensArr) {
        uint256 to = (offset.add(limit)).min(set.length()).max(offset);

        _tokensArr = new address[](to - offset);

        for (uint256 i = offset; i < to; i++) {
            _tokensArr[i - offset] = set.at(i);
        }
    }

}

