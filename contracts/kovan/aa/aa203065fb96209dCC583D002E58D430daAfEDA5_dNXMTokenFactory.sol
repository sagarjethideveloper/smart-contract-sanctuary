pragma solidity ^0.7.4;
pragma experimental ABIEncoderV2;


interface IdNXMToken {

    struct WithdrawalInfo {
        uint256 coolDownTimeEnd;
        uint256 amountDNXMRequested;
    }

    event Stake (
        address indexed sender,
        uint nxmAmount,
        uint dnxmAmount
    );
    event CallWithdraw (
        address indexed sender,
        uint dnxmAmount
    );

    // solhint-disable-next-line func-name-mixedcase
    function __dNXMToken_init(
        string calldata _description,
        string calldata _contractSymbol,
        address _contractAddress
    ) external;

    function setDependencies(address _nxMaster, address _wNXMTokenAddress) external;

    function stake(uint _amount) external;

    function callWithdrawal(uint _amountDNXM) external;

    function maxDNXMCallWithdrawal(address _address) external view returns (uint256);

    function minNXMAmount() external view returns (uint256);

    function minDNXMAmount() external view returns (uint256);

    function convertDNXMToNXM(uint256 _amount) external view returns (uint256);

    function convertNXMToDNXM(uint256 _amount) external view returns (uint256);

    function getWithdrawalInfo(address _userAddr) external view
    returns (
        uint256 _amountDNXMRequested,
        uint256 _amountNXMSRequested,
        uint256 _unlockPeriod
    );
}

