pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "./interfaces/IUniswapV2Router02.sol";
import "./interfaces/IUniswapV2Factory.sol";
import "./interfaces/ILendingPool.sol";
import "./interfaces/IERC20.sol";
import "./interfaces/IWETH.sol";
import "./ChiGasSaver.sol";

contract Execution is Initializable, OwnableUpgradeable, AccessControlUpgradeable, ChiGasSaver {
  using SafeMathUpgradeable for uint256;

  address public WETH;
  address public uniswapV2Router;
  address public uniswapV2Factory;
  address public AAVE_LENDING_POOL;

  bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");

  function initialize() external virtual initializer {
    __Ownable_init();
    __AccessControl_init();

    // uniswapV2Router = 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D; // mainnet
    // AAVE_LENDING_POOL = 0x7d2768dE32b0b80b7a3454c06BdAc94A69DDc7A9; // mainnet

    WETH = 0xd0A1E359811322d97991E03f863a0C30C2cF029C; // kovan
    uniswapV2Router = 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D; // kovan
    uniswapV2Factory = IUniswapV2Router02(uniswapV2Router).factory();
    AAVE_LENDING_POOL = 0xE0fBa4Fc209b4948668006B2bE61711b7f465bAe; // kovan
    _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
  }

  function addOperator(address operator) external onlyOwner {
    grantRole(OPERATOR_ROLE, operator);
  }

  function revokeOperator(address operator) external onlyOwner {
    revokeRole(OPERATOR_ROLE, operator);
  }

  function investWithETH(address token, uint256 tokenAmt, bool isETHSelling, bool gasSaverOn) external payable saveGas(gasSaverOn) {
    require(msg.value > 0, "Zero ETH transfer");
    require(hasRole(OPERATOR_ROLE, _msgSender()));
    require(IERC20(token).transferFrom(_msgSender(), address(this), tokenAmt), "Token0 transfer failed");
    IWETH(WETH).deposit{value: msg.value}();
    if (isETHSelling) {
      _invest(WETH, token, msg.value, tokenAmt);
    } else {
      _invest(token, WETH, tokenAmt, msg.value);
    }    
  }

  function invest(address token0, address token1, uint256 token0Amt, uint256 token1Amt, bool gasSaverOn) external saveGas(gasSaverOn) {
    require(hasRole(OPERATOR_ROLE, _msgSender()));
    require(IERC20(token0).transferFrom(_msgSender(), address(this), token0Amt), "Token0 transfer failed");
    require(IERC20(token1).transferFrom(_msgSender(), address(this), token1Amt), "Token1 transfer failed");
    _invest(token0, token1, token0Amt, token1Amt);
  }

  function _invest(address token0, address token1, uint256 token0Amt, uint256 token1Amt) private {    
    address lpToken = IUniswapV2Factory(uniswapV2Factory).getPair(token0, token1);
    require(lpToken != address(0), "No pair existing");
    
    address[] memory path = new address[](2);
    path[0] = token0;
    path[1] = token1;

    _depositAndBorrow(token0, token1, lpToken, token0Amt, token1Amt);
    IERC20(token0).approve(uniswapV2Router, token0Amt.div(2));
    // IUniswapV2Router02(uniswapV2Router).swapExactTokensForTokensSupportingFeeOnTransferTokens(token0Amt.div(2), 0, path, address(this), block.timestamp);

    // _depositAndBorrow(token0, token1, lpToken, token0Amt.div(2), token1Amt.div(2)); // Remain little token0
    // uint256 newToken0Amt = IERC20(token0).balanceOf(address(this));
    // IERC20(token0).approve(uniswapV2Router, newToken0Amt);
    // IUniswapV2Router02(uniswapV2Router).swapExactTokensForTokensSupportingFeeOnTransferTokens(newToken0Amt, 0, path, address(this), block.timestamp);

    // uint256 newToken1Amt = IERC20(token1).balanceOf(address(this));
    // IERC20(token1).approve(AAVE_LENDING_POOL, newToken1Amt);
    // ILendingPool(AAVE_LENDING_POOL).deposit(token1, newToken1Amt, _msgSender(), 0); // TBC; onBehalfOf sender or contract
  }

  function borrow(address token, uint256 amount, uint256 interestRateMode, address onBehalfOf) external {
    ILendingPool(AAVE_LENDING_POOL).borrow(token, amount, interestRateMode, 0, onBehalfOf);
  }

  function deposit(address token, uint256 amount, address onBehalfOf) external {
    ILendingPool(AAVE_LENDING_POOL).deposit(token, amount, onBehalfOf, 0);
  }

  function withdraw(address token, uint256 amount, address to) external {
    ILendingPool(AAVE_LENDING_POOL).withdraw(token, amount, to);
  }

  function repay(address token, uint256 amount, uint256 rateMode, address onBehalfOf) external payable {
    ILendingPool(AAVE_LENDING_POOL).repay(token, amount, rateMode, onBehalfOf);
  }

  function _depositAndBorrow(address token0, address token1, address lpToken, uint256 token0Amt, uint256 token1Amt) internal {
    IERC20(token0).approve(uniswapV2Router, token0Amt);
    IERC20(token1).approve(uniswapV2Router, token1Amt);
    IUniswapV2Router02(uniswapV2Router).addLiquidity(token0, token1, token0Amt, token1Amt, 0, 0, address(this), block.timestamp);
    uint256 lpAmount = IERC20(lpToken).balanceOf(address(this));
    IERC20(lpToken).approve(AAVE_LENDING_POOL, lpAmount);
    ILendingPool(AAVE_LENDING_POOL).deposit(lpToken, lpAmount, _msgSender(), 0); // sender is the ccf hot wallet where the funds move from
    // ILendingPool(AAVE_LENDING_POOL).borrow(token0, token0Amt, 2, 0, address(this)); // TBC; InterestRate Mode, onBehalfOf sender or contract
  }

  receive() external payable {}
}

