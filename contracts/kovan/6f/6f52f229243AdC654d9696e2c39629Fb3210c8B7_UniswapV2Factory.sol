pragma solidity =0.8.3;
pragma experimental ABIEncoderV2;
import './UniswapV2Pair.sol';
import 'hardhat/console.sol';

contract ContangoPair is UniswapV2Pair {
	struct Position {
		uint112 blockOpen;
		uint112 borrowed;
		uint112 debtShares;
		uint112 vault; //collateral + swapped for borrowed
		address owner;
		bool isOpen;
		bool isToken0; //is the collateral token
	}

	mapping(uint256 => Position) public positions;

	uint256 public nextPositionId;

	event PositionOpened(
		uint256 indexed positionId,
		address indexed owner,
		address indexed collateralToken,
		uint256 collateral,
		uint112 amountBorrowed //in other token
	);

	event PositionClosed(
		uint256 indexed positionId,
		address indexed owner,
		address indexed collateralToken,
		uint256 debt, //in other token
		uint256 amountRepayed, //out of debt - actuall repayed in other token (can be less than borrowed in case of default)
		uint256 interestPayed, //out of amountRepayed
		uint256 amountEarned //in collateral token
	);

	event PositionsClosed(uint256[] ids, address liquidator, uint256 debt0Repayed, uint256 debt1Repayed);

	function open(
		uint256 _collateral0,
		uint256 _collateral1,
		address to
	) external {
		require(_collateral0 == 0 || _collateral1 == 0, 'Contango: one token per position');
		(uint112 r0, uint112 r1, ) = getReserves();
		require(sidePool0 >= _collateral0 && sidePool1 >= _collateral1, 'Contango: not enough to cover exposure');

		//now we just virtually transfer tokens between pools

		//calculate how much collateral is worth in borrowed token without fee
		uint256 borrow1 = getValue(_collateral0, r0, r1);
		uint256 borrow0 = getValue(_collateral1, r1, r0);
		//lending pool - remove borrowed amount
		sidePool0 -= uint112(borrow0);
		sidePool1 -= uint112(borrow1);

		//calculate exposure of the short position
		uint256 swap1 = getAmountOut(borrow0, r0, r1, true);
		uint256 swap0 = getAmountOut(borrow1, r1, r0, true);
		require(
			borrow0 <= type(uint112).max &&
				borrow1 <= type(uint112).max &&
				swap0 <= type(uint112).max &&
				swap1 <= type(uint112).max,
			'Contango: OVERFLOW'
		);

		//lock collateral + exposure in vault. exposure (swap) value is received when swapping the borrowed token to the collateral token
		sidePool0Vault += uint112(_collateral0 + swap0);
		sidePool1Vault += uint112(_collateral1 + swap1);

		//virtual swap to = this
		swap(swap0, swap1, address(this), '');

		(uint112 debtSharesBorrow0, uint112 debtSharesBorrow1) = _mintInterestDebt(to, borrow0, borrow1);

		Position memory pos;

		pos.isToken0 = _collateral0 > _collateral1;
		pos.blockOpen = uint112(block.number);
		pos.debtShares = debtSharesBorrow0 > debtSharesBorrow1 ? debtSharesBorrow0 : debtSharesBorrow1;
		pos.borrowed = uint112(borrow0 > borrow1 ? borrow0 : borrow1);
		pos.vault = uint112(_collateral0 > _collateral1 ? _collateral0 + swap0 : _collateral1 + swap1);
		pos.owner = to;
		pos.isOpen = true;

		positions[nextPositionId] = pos;

		uint256 collateral = _collateral0 > _collateral1 ? _collateral0 : _collateral1;
		emit PositionOpened(nextPositionId, to, pos.isToken0 ? token0 : token1, collateral, pos.borrowed);

		nextPositionId += 1;
	}

	function close(uint256 positionId, address to) external {
		Position memory pos = positions[positionId];
		require(pos.isOpen && msg.sender == pos.owner, 'Contango: position already closed or not owner');

		bool isToken0 = pos.isToken0;

		(, uint112 token0DebtValue, uint112 token1DebtValue, uint256 borrowedRepayed, , uint256 toUser, ) = _closeData(pos);

		_repayInterestDebt(pos.owner, isToken0 ? 0 : pos.debtShares, isToken0 ? pos.debtShares : 0);

		uint256 fees = borrowedRepayed > pos.borrowed ? borrowedRepayed - pos.borrowed : 0;

		//lending pool - return actuall repayed amount
		sidePool0 += isToken0 ? 0 : uint112(borrowedRepayed - fees); //fees accrue in regular pool
		sidePool1 += isToken0 ? uint112(borrowedRepayed - fees) : 0;

		//update total debt  - we deduct everything (ie in case of default) not just what was repayed
		sidePool0Debt -= token0DebtValue;
		sidePool1Debt -= token1DebtValue;

		//unlock collateral + exposure in vault. (we exchanged it to debtToken and sent the rest to user)
		sidePool0Vault -= isToken0 ? pos.vault : 0;
		sidePool1Vault -= isToken0 ? 0 : pos.vault;

		//6. send to user.
		//this MUST happen before swap otherwise _update in swap will causes reserves to be incorrect
		if (toUser > 0) _safeTransfer(isToken0 ? token0 : token1, to, toUser);
		//7. swap
		swap(isToken0 ? 0 : borrowedRepayed, isToken0 ? borrowedRepayed : 0, address(this), '');

		//78. in case of default? debt is lost.

		delete positions[positionId];

		emit PositionClosed(
			positionId,
			msg.sender,
			isToken0 ? token0 : token1,
			isToken0 ? token1DebtValue : token0DebtValue,
			borrowedRepayed,
			fees,
			toUser
		);
	}

	function liquidate(uint256[] calldata ids) public {
		uint256 sidePool0Repayed;
		uint256 sidePool1Repayed;
		uint112 sidePool0VaultReduced;
		uint112 sidePool1VaultReduced;

		for (uint256 i = 0; i < ids.length; i++) {
			Position memory pos = positions[ids[i]];
			if (!pos.isOpen) continue;
			{
				(, uint256 token0DebtValue, uint256 token1DebtValue, , bool hasDefaulted, , ) = _closeData(pos);
				if (hasDefaulted == false) continue;

				_repayInterestDebt(pos.owner, pos.isToken0 ? 0 : pos.debtShares, pos.isToken0 ? pos.debtShares : 0);

				if (pos.isToken0) {
					//unlock collateral + exposure in vault. (we exchanged it to debtToken and sent the rest to user)
					sidePool0VaultReduced += pos.vault;
				} else {
					sidePool1VaultReduced += pos.vault;
				}

				//update total debt  - we deduct everything (ie in case of default) not just what was repayed
				//we can update globals here as it has no effect on swap results
				sidePool0Debt -= uint112(token0DebtValue);
				sidePool1Debt -= uint112(token1DebtValue);
			}
			delete positions[ids[i]];
		}

		uint256 _reserve0;
		uint256 _reserve1;
		if (sidePool1VaultReduced > 0) {
			sidePool1Vault -= sidePool1VaultReduced;
			(_reserve0, _reserve1, ) = getReserves();
			sidePool0Repayed = getAmountOut(sidePool1VaultReduced, _reserve1, _reserve0, true);
			sidePool0 += uint112(sidePool0Repayed);
			swap(sidePool0Repayed, 0, address(this), '');
		}
		if (sidePool0VaultReduced > 0) {
			sidePool0Vault -= sidePool0VaultReduced;
			(_reserve0, _reserve1, ) = getReserves();
			sidePool1Repayed = getAmountOut(sidePool0VaultReduced, _reserve0, _reserve1, true);
			sidePool1 += uint112(sidePool1Repayed);
			swap(0, sidePool1Repayed, address(this), '');
		}

		//78. in case of default? debt is lost.
		emit PositionsClosed(ids, msg.sender, sidePool0Repayed, sidePool1Repayed);
	}

	/**
	 @dev Liquiadate given position ids if they are already defaulted and it sends liquidatation of msg.sender back
	 */
	function liquidateAndBurn(address _to, uint256[] calldata _positionIds)
		external
		returns (uint256 amount0, uint256 amount1)
	{
		liquidate(_positionIds);
		(uint256 amount0, uint256 amount1) = burn(_to);
	}

	function _closeData(Position memory pos)
		internal
		view
		returns (
			uint256 toSwap,
			uint112 token0DebtValue,
			uint112 token1DebtValue,
			uint256 borrowedRepayed,
			bool hasDefaulted,
			uint256 toUser,
			uint256 debtValueInCollateralToken
		)
	{
		(uint112 r0, uint112 r1, ) = getReserves();
		(token0DebtValue, token1DebtValue) = _debtValue(
			pos.isToken0 ? 0 : pos.debtShares,
			pos.isToken0 ? pos.debtShares : 0
		);
		//1. calculate how much other token (collateral token) is required to cover tokenXDebtValue
		debtValueInCollateralToken = getValue(
			pos.isToken0 ? token1DebtValue : token0DebtValue,
			pos.isToken0 ? r1 : r0,
			pos.isToken0 ? r0 : r1
		);

		//2. if <= collateral + exposure then swap back some of collateral token to cover borrowed tokenXDebtvalue
		toSwap = debtValueInCollateralToken;
		toUser = 0;
		//case of deault
		if (debtValueInCollateralToken >= pos.vault) {
			//3. if > collateral swap everything
			hasDefaulted = true;
			toSwap = pos.vault;
		} else {
			//4. send remaining to user
			toUser = pos.vault - debtValueInCollateralToken;
		}

		//5. update pools - return borrowed to sidepool, update debtshares and debt balances, update vault
		borrowedRepayed = getAmountOut(toSwap, uint256(pos.isToken0 ? r0 : r1), uint256(pos.isToken0 ? r1 : r0), true);
	}

	/**
	 * update borrower share of the debt + interest
	 */
	function _mintInterestDebt(
		address user,
		uint256 borrow0,
		uint256 borrow1
	) internal returns (uint112, uint112) {
		uint256 debtShareValue0 = debtShares0 > 0 ? (uint256(sidePool0Debt) * 1e18) / uint256(debtShares0) : 1e18;
		uint256 debtSharesToMint0 = (borrow0 * 1e18) / debtShareValue0;
		uint256 debtShareValue1 = debtShares1 > 0 ? (uint256(sidePool1Debt) * 1e18) / uint256(debtShares1) : 1e18;
		uint256 debtSharesToMint1 = (borrow1 * 1e18) / debtShareValue1;

		uint112 debtSharesToMint0In112 = uint112(debtSharesToMint0);
		uint112 debtSharesToMint1In112 = uint112(debtSharesToMint1);
		debtBalances[user][0] += debtSharesToMint0In112;
		debtBalances[user][1] += debtSharesToMint1In112;
		debtShares0 += debtSharesToMint0In112;
		debtShares1 += debtSharesToMint1In112;

		//update total debt of borrowed token
		sidePool0Debt += uint112(borrow0);
		sidePool1Debt += uint112(borrow1);

		return (debtSharesToMint0In112, debtSharesToMint1In112);
	}

	function _debtValue(uint112 debtSharesOwned0, uint112 debtSharesOwned1) internal view returns (uint112, uint112) {
		uint256 debtShareValue0 = debtSharesOwned0 > 0 ? (uint256(sidePool0Debt) * 1e18) / uint256(debtShares0) : 1e18;
		uint256 debtOwnedValue0 = (debtShareValue0 * uint256(debtSharesOwned0)) / 1e18;

		uint256 debtShareValue1 = debtSharesOwned1 > 0 ? (uint256(sidePool1Debt) * 1e18) / uint256(debtShares1) : 1e18;
		uint256 debtOwnedValue1 = (debtShareValue1 * uint256(debtSharesOwned1)) / 1e18;
		return (uint112(debtOwnedValue0), uint112(debtOwnedValue1));
	}

	/**
	 * update borrower share of the debt + interest and global debt
	 */
	function _repayInterestDebt(
		address user,
		uint112 debtSharesOwned0,
		uint112 debtSharesOwned1
	) internal {
		debtBalances[user][0] -= debtSharesOwned0;
		debtBalances[user][1] -= debtSharesOwned1;
		debtShares0 -= debtSharesOwned0;
		debtShares1 -= debtSharesOwned1;
	}

	function isDefaulted(uint256 positionId) public view returns (bool) {
		Position memory pos = positions[positionId];
		require(pos.isOpen, 'Contango: position already closed');

		(, , , , bool hasDefaulted, , ) = _closeData(pos);

		//case of deault
		return hasDefaulted;
	}

	/**
    *@dev helper function that returns worth of current withdrawable liquidity for particular user for token0 and token1
    * what is locked in the sidepool vaults is not withdrawable
	*/
	function liquidityBalance(address _user) public view returns (uint256, uint256) {
		uint256 liquidity = balanceOf[_user];
		uint256 balance0 = IERC20(token0).balanceOf(address(this));
		uint256 balance1 = IERC20(token1).balanceOf(address(this));
		uint256 _totalSupply = totalSupply; // gas savings
		uint256 amount0 = (liquidity * (balance0 - sidePool0Vault)) / _totalSupply; // using balances ensures pro-rata distribution
		uint256 amount1 = (liquidity * (balance1 - sidePool1Vault)) / _totalSupply; // using balances ensures pro-rata distribution
		return (amount0, amount1);
	}

	/**
	 *@dev helper function that returns worth of liquidity for particular user for token0 and token1 after liquidate given positions
	 */
	function liquidityBalanceAfterLiquidate(address _user, uint256[] memory _positionIds)
		public
		view
		returns (uint256, uint256)
	{
		uint256 initialSidePool0Vault = sidePool0Vault;
		uint256 initialSidePool1Vault = sidePool1Vault;
		uint112 sidePool0VaultReduced;
		uint112 sidePool1VaultReduced;
		for (uint256 i; i < _positionIds.length; i++) {
			Position memory pos = positions[_positionIds[i]];
			if (!pos.isOpen) continue;

			(, , , , bool hasDefaulted, , ) = _closeData(pos);

			if (hasDefaulted == false) continue;
			if (pos.isToken0) {
				//unlock collateral + exposure in vault. (we exchanged it to debtToken and sent the rest to user)
				sidePool0VaultReduced += pos.vault;
			} else {
				sidePool1VaultReduced += pos.vault;
			}
		}
		initialSidePool0Vault -= sidePool0VaultReduced;
		initialSidePool1Vault -= sidePool1VaultReduced;
		uint256 liquidity = balanceOf[_user];
		uint256 balance0 = IERC20(token0).balanceOf(address(this));
		uint256 balance1 = IERC20(token1).balanceOf(address(this));
		uint256 _totalSupply = totalSupply; // gas savings, must be defined here since totalSupply can update in _mintFee
		uint256 amount0 = (liquidity * (balance0 - initialSidePool0Vault)) / _totalSupply; // using balances ensures pro-rata distribution
		uint256 amount1 = (liquidity * (balance1 - initialSidePool1Vault)) / _totalSupply; // using balances ensures pro-rata distribution
		return (amount0, amount1);
	}

	/**
	 * uniswap amountOut helper
	 */
	function getAmountOut(
		uint256 _amountIn,
		uint256 _reserveIn,
		uint256 _reserveOut,
		bool withFee
	) public pure returns (uint256 amountOut) {
		uint256 amountInWithFee = _amountIn * (withFee ? 997 : 1000);
		uint256 numerator = amountInWithFee * _reserveOut;
		uint256 denominator = _reserveIn * 1000 + amountInWithFee;
		amountOut = numerator / denominator;
	}

	function getValue(
		uint256 _amountIn,
		uint256 _reserveIn,
		uint256 _reserveOut
	) public pure returns (uint256 amountOut) {
		return (_amountIn * _reserveOut) / _reserveIn;
	}

	//TODO: make this a helper for tests
	function calcFee() public view returns (uint256) {
		uint256 _kLast = kLast;
		(uint256 _reserve0, uint256 _reserve1, ) = getReserves();
		uint256 rootK = Math.sqrt(uint256(_reserve0) * _reserve1);
		uint256 rootKLast = Math.sqrt(_kLast);
		if (rootK > rootKLast) {
			//TODO: contango, in uniswap this is calculated based on reserves,
			//do we need it all? do we want to change it relative to whole growth no just reserves?
			uint256 numerator = totalSupply * (rootK - rootKLast);
			uint256 denominator = rootK * 5 + rootKLast;
			uint256 liquidity = numerator / denominator;
			return liquidity;
		}
		return 0;
	}
}

