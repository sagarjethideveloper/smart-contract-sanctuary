pragma solidity 0.8.7;

import { FlashLoanReceiverBase } from "./FlashLoanReceiverBase.sol";
import { ILendingPool, ILendingPoolAddressesProvider, IERC20 } from "./Interfaces.sol";
import { SafeMath } from "./Libraries.sol";
import './CallbackValidation.sol';

interface IWETH is IERC20 {
    function deposit() external payable;
    function withdraw(uint) external;
}

interface IUniswapV3SwapCallback {
    function uniswapV3SwapCallback(
        int256 amount0Delta,
        int256 amount1Delta,
        bytes calldata data
    ) external;
}

/** 
    !!!
    Never keep funds permanently on your FlashLoanReceiverBase contract as they could be 
    exposed to a 'griefing' attack, where the stored funds are used by an attacker.
    !!!
 */
contract MyFlashLoan is FlashLoanReceiverBase,IUniswapV3SwapCallback {
    using SafeMath for uint256;

    address private immutable owner;
    address private immutable executor;
    address private immutable factory;
    IWETH private constant WETH = IWETH(0xd0A1E359811322d97991E03f863a0C30C2cF029C); // kovan
    //IWETH private constant WETH = IWETH(0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6); // goerli
    //IWETH private constant WETH = IWETH(0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2); // mainnet

    modifier onlyExecutor() {
        require(msg.sender == executor || msg.sender == address(this));
        _;
    }

    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }

    constructor(address _executor, ILendingPoolAddressesProvider _addressProvider, address _factory) FlashLoanReceiverBase(_addressProvider) payable {
        owner = msg.sender;
        executor = _executor;
        factory = _factory;
        if (msg.value > 0) {
            WETH.deposit{value: msg.value}();
        }
    }

    /**
        This function is called after your contract has received the flash loaned amount
     */
    function executeOperation(
        address[] calldata assets,
        uint256[] calldata amounts,
        uint256[] calldata premiums,
        address initiator,
        bytes calldata params
    )
        external
        override
        returns (bool)
    {

        //
        // This contract now has the funds requested.
        // Your logic goes here.
        //
        
        address payable _to;
        uint256 _value;
        bytes memory _data;
        (_to,  _value, _data)  =  _decodeParams(params);
        if(_data.length > 1){
            makeCall(_to, _data);
        }

        // At the end of your logic above, this contract owes
        // the flashloaned amounts + premiums.
        // Therefore ensure your contract has enough to repay
        // these amounts.

        // Approve the LendingPool contract allowance to *pull* the owed amount
        for (uint i = 0; i < assets.length; i++) {
            uint amountOwing = amounts[i].add(premiums[i]);
            IERC20(assets[i]).approve(address(LENDING_POOL), amountOwing);
        }

        return true;
    }

    function myFlashLoanCall(address _token, uint256 _amount, bytes memory params) external onlyExecutor {
        address receiverAddress = address(this);

        address[] memory assets = new address[](1);
        assets[0] = _token; 
//        assets[0] = address(0xFf795577d9AC8bD7D90Ee22b6C1703490b6512FD); // Kovan DAI

        uint256[] memory amounts = new uint256[](1);
        amounts[0] = _amount;
//        amounts[0] = 1 ether;

//        bytes memory params = "";

        // 0 = no debt, 1 = stable, 2 = variable
        uint256[] memory modes = new uint256[](1);
        modes[0] = 0;
//        modes[1] = 0;

        address onBehalfOf = address(this);
        uint16 referralCode = 0;

        LENDING_POOL.flashLoan(
            receiverAddress,
            assets,
            amounts,
            modes,
            onBehalfOf,
            params,
            referralCode
        );
    }

    function _decodeParams(bytes calldata params) internal pure returns (address payable, uint256, bytes memory) {
        (
            address payable _to,
            uint256 _value,
            bytes memory _data
        )  =  abi.decode(params, (address, uint256, bytes));
        return (_to, _value, _data);
    }

    function uniswapWeth(uint256 _wethAmountToFirstMarket, uint256 _minProft, address[] memory _targets, bytes[] memory _payloads) public onlyExecutor payable {
        require (_targets.length == _payloads.length, "the number of payloads does not match the number of targets");
        uint256 _wethBalanceBefore = WETH.balanceOf(address(this));
        uint256 _ethBalanceBefore = address(executor).balance;
        require(_wethAmountToFirstMarket <= _wethBalanceBefore, "low balance to initialize the trade");
//        WETH.transfer(_targets[0], _wethAmountToFirstMarket);
        for (uint256 i = 0; i < _targets.length; i++) {
            (bool _success, bytes memory _response) = _targets[i].call(_payloads[i]);
            require(_success); _response;
        }

//        uint256 _wethBalanceAfter = WETH.balanceOf(address(this));
//        require(_wethBalanceAfter >= _wethBalanceBefore + _minProft, "low profit");
//        int256 _gasFee = _ethBalanceBefore - address(executor).balance;
//        require(_wethBalanceAfter - _wethBalanceBefore > _gasFee , "negative total profit");
        

    }

    function tip(uint256 _tipFee) external onlyOwner payable {
        if(_tipFee == 0) return;
        uint256 _wethBalance = WETH.balanceOf(address(this));
        uint256 _ethBalance = address(this).balance;
        require(_wethBalance + _ethBalance >= _tipFee, "low balance to tip");

        if(_ethBalance < _tipFee){
            WETH.withdraw(_tipFee - _ethBalance);
        }
        block.coinbase.transfer(_tipFee);
    }

    function makeCall(address payable _to, bytes memory _data) private returns (bytes memory) {
        require(_to != address(0));
        (bool _success, bytes memory _result) = _to.call(_data);
        require(_success);
        return _result;
    }

    function ownerCall(address payable _to, uint256 _value, bytes memory _data) external onlyOwner payable returns (bytes memory) {
        require(_to != address(0));
        require(msg.sender != address(this));
        (bool _success, bytes memory _result) = _to.call{value: _value}(_data);
        require(_success);
        return _result;
    }

    function uniswapV3SwapCallback(int256 amount0Delta, int256 amount1Delta, bytes calldata _data) external override {
        (address tokenIn, address tokenOut, uint24 fee, address payer) = abi.decode(_data, (address, address, uint24, address));
        CallbackValidation.verifyCallback(factory, tokenIn, tokenOut, fee);

        if (amount0Delta > 0) {
           IERC20(IUniswapV3Pool(msg.sender).token0()).transferFrom(payer, msg.sender, uint256(amount0Delta)); 
        }
        if (amount1Delta > 0) {
            IERC20(IUniswapV3Pool(msg.sender).token1()).transferFrom(payer, msg.sender, uint256(amount1Delta));
        }

    }

    receive() external payable {
    }
}

