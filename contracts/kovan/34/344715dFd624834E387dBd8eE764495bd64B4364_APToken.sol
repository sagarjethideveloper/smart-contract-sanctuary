pragma solidity ^0.7.0;

import "./Manager.sol";
import "./Recorder.sol";
import "./Allocator.sol";
import "./Regulator.sol";


contract APToken is Manager, Recorder, Allocator, Regulator {

    bytes32 constant TREASURY_ROLE = bytes32(keccak256("TREASURY_ROLE"));

    constructor(
        address admin,
        address recorder,
        address treasury,
        address regulator,
        string memory name,
        string memory symbol
    )
        Manager(admin)
        Recorder(recorder)
        Allocator(name, symbol)
        Regulator(regulator)
    {
        _setupRole(TREASURY_ROLE, treasury);
    }

    /**
     * @notice Overrides the parent class token transfer function to enforce transfer restrictions.
     */
    function transfer(address to, uint256 value)
        public
        override
        notTransferRestricted(msg.sender, to, value)
        returns (bool success)
    {
        success = super.transfer(to, value);
    }

    /**
     * @notice Overrides the parent class token transferFrom function to enforce transfer restrictions.
     */
    function transferFrom(address from, address to, uint256 value)
        public
        override
        notTransferRestricted(from, to, value)
        returns (bool success)
    {
        success = super.transferFrom(from, to, value);
    }

    /**
     * @notice Exposes the ability to mint new APToken for a given account. Caller has to have the TREASURY role.
     */
    function mint(address account, uint256 amount) public returns (bool) {
        require(hasRole(TREASURY_ROLE, msg.sender), "APToken: Sender does not have required role");

        _mint(account, amount);
        return true;
    }

    /**
     * @notice Exposes the ability to burn exisiting APToken for a given account. Caller has to have the TREASURY role.
     */
    function burn(address account, uint256 amount) public returns (bool) {
        require(hasRole(TREASURY_ROLE, msg.sender), "APToken: Sender does not have required role");

        _burn(account, amount);
        return true;
    }
}

