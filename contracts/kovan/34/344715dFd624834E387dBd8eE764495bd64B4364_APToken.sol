pragma solidity ^0.7.0;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/ReentrancyGuard.sol";

import "hardhat/console.sol";


contract Checkpoints {

    /** 
     * @dev `Checkpoint` is the structure that attaches a timestamp to a 
     * given value, the timestamp attached is the one that last changed the value
     */
    struct Checkpoint {
        // `timestamp` is the timestamp that the value was generated from
        uint128 timestamp;
        // `value` is the amount of tokens at a specific timestamp
        uint256 value;
    }

    /**
     * @notice Queries a value at a defined checkpoint
     * @param checkpoints array of Checkpoint objects
     * @param timestamp timestamp to retrieve the value at
     * @return uint256
     */
    function getValueAt(Checkpoint[] storage checkpoints, uint256 timestamp) 
        internal
        view 
        returns (uint256)
    {
        // initially return 0
        if (checkpoints.length == 0) return 0;

        // Shortcut for the actual value
        if (timestamp >= checkpoints[checkpoints.length - 1].timestamp)
            return checkpoints[checkpoints.length - 1].value;
        if (timestamp < checkpoints[0].timestamp) return 0;

        // Binary search of the value in the array
        uint256 min = 0;
        uint256 max = checkpoints.length - 1;
        while (max > min) {
            uint256 mid = (max + min + 1) / 2;
            if (checkpoints[mid].timestamp <= timestamp) {
                min = mid;
            } else {
                max = mid - 1;
            }
        }
        return checkpoints[min].value;
    }

    /**
     * @notice Create a new checkpoint for a value if
     * there does not exist a checkpoint for the current block timestamp,
     * otherwise updates the value of the current checkpoint.
     * @param checkpoints Checkpointed values
     * @param value Value to be updated
     */ 
    function updateValueAtNow(Checkpoint[] storage checkpoints, uint value) internal {
        // create a new checkpoint if:
        // - there are no checkpoints
        // - the current block has a greater timestamp than the last checkpoint
        // otherwise update value at current checkpoint
        if (checkpoints.length == 0 || (block.timestamp > checkpoints[checkpoints.length - 1].timestamp)) {
            // create checkpoint with value
            checkpoints.push(Checkpoint({ timestamp: uint128(block.timestamp), value: value }));
        } else {
            // update value at current checkpoint
            Checkpoint storage oldCheckPoint = checkpoints[checkpoints.length - 1];
            oldCheckPoint.value = value;
        }
    }
}

contract CheckpointedToken is ERC20, Checkpoints {

    using SafeMath for uint256;

    // Checkpointed total supply
    Checkpoint[] public checkpointTotalSupply;
    // Map each holder to a series of checkpoints
    mapping(address => Checkpoint[]) public checkpointBalances;

    constructor(string memory name, string memory symbol) ERC20(name, symbol) {}

    /**
     * @notice Queries the balances of a holder at a specific timestamp
     * @param holder Holder to query balance for
     * @param timestamp Timestamp of the balance checkpoint
     */
    function balanceOfAt(address holder, uint256 timestamp) public view returns(uint256) {
        return getValueAt(checkpointBalances[holder], timestamp);
    }

    /**
     * @notice Queries totalSupply at a specific timestamp
     * @param timestamp Timestamp of the totalSupply checkpoint
     * @return uint256
     */
    function totalSupplyAt(uint256 timestamp) public view returns(uint256) {
        return getValueAt(checkpointTotalSupply, timestamp);
    }

    /**
     * @notice Internal - adjusts totalSupply at checkpoint before a token transfer
     */
    function _adjustTotalSupplyCheckpoints() internal {
        updateValueAtNow(checkpointTotalSupply, totalSupply());
    }

    /**
     * @notice Internal - adjusts token holder balance at checkpoint before a token transfer
     * @param holder address of the token holder affected
     */
    function _adjustBalanceCheckpoints(address holder) internal {
        updateValueAtNow(checkpointBalances[holder], balanceOf(holder));
    }

    /**
     * @notice Updates internal variables when performing a transfer
     * @param from sender of transfer
     * @param to receiver of transfer
     */
    function _updateTransfer(address from, address to) internal {
        _adjustTotalSupplyCheckpoints();
        _adjustBalanceCheckpoints(from);
        _adjustBalanceCheckpoints(to);
    }

    function _mint(
        address tokenHolder,
        uint256 value
    )
        internal
        override
    {
        super._mint(tokenHolder, value);
        _updateTransfer(address(0), tokenHolder);
    }

    function _burn(
        address tokenHolder,
        uint256 value
    )
        internal
        override
    {
        super._burn(tokenHolder, value);
        _updateTransfer(tokenHolder, address(0));
    }

    function _transfer(
        address from,
        address to,
        uint256 value
    )
        internal
        virtual
        override
    {
        super._transfer(from, to, value);
        _updateTransfer(from, to);
    }
}

/**
 * @title Logic for distributing funds based on checkpointing
 * @dev abstract contract
 */
contract Allocator is CheckpointedToken, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    event CreatedDeposit(bytes32 depositId);

    struct Deposit {
        // Time at which the deposit is scheduled for
        uint256 scheduledFor;
        // Time until which holders can signal for a deposit
        uint256 signalingCutoff;
        // Deposit amount in WEI
        uint256 amount;
        // Amount of funds claimed so far
        uint256 claimedAmount;
        // Sum of the signaled tokens of whitelisted token holders (only used if isWhitelisted == true)
        uint256 totalAmountSignaled;
        // Address of the token in which the deposit is made
        address token;
        // Indicates whether holders have to signal in advance to claim their share of the deposit
        bool onlySignaled;
        // List of addresses which have withdrawn their share of funds of the deposit
        mapping (address => bool) claimed;
        // Subset of holders which can claim their share of funds of the deposit
        mapping (address => uint256) signaledAmounts;
    }

    // depositId => Deposit
    mapping(bytes32 => Deposit) public deposits;
    // holder => amount signaled
    mapping(address => uint256) public totalAmountSignaledByHolder;

    constructor(string memory name, string memory symbol) CheckpointedToken(name, symbol) {}

    function createDeposit(
        bytes32 depositId,
        uint256 scheduledFor,
        uint256 signalingCutoff,
        bool onlySignaled,
        address token,
        uint256 amount
    ) public {
        Deposit storage deposit = deposits[depositId];

        require(
            deposit.scheduledFor == uint256(0),
            "Allocator: Deposit already exists"
        );

        deposit.scheduledFor = scheduledFor;
        deposit.signalingCutoff = signalingCutoff;
        deposit.onlySignaled = onlySignaled;
        deposit.token = token;
        deposit.amount = amount;

        emit CreatedDeposit(depositId);
    }

    function createDepositAndPay(
        bytes32 depositId,
        uint256 scheduledFor,
        uint256 signalingCutoff,
        bool onlySignaled,
        address token,
        uint256 amount
    ) public {
        Deposit storage deposit = deposits[depositId];

        require(deposit.scheduledFor == uint256(0), "Allocator: Deposit already exists");

        deposit.scheduledFor = scheduledFor;
        deposit.signalingCutoff = signalingCutoff;
        deposit.onlySignaled = onlySignaled;
        deposit.token = token;
        deposit.amount = amount;
        
        emit CreatedDeposit(depositId);

        IERC20(token).safeTransferFrom(msg.sender, address(this), amount);
    }

    function pay(bytes32 depositId) public {
        Deposit storage deposit = deposits[depositId];

        require(deposit.scheduledFor != uint256(0), "Allocator: Deposit does not exist");

        IERC20(deposit.token).safeTransferFrom(msg.sender, address(this), deposit.amount);
    }

    function signalAmountForDeposit(bytes32 depositId, uint256 signalAmount) public {
        Deposit storage deposit = deposits[depositId];

        require(deposit.scheduledFor != uint256(0), "Allocator: Deposit does not exist");
        require(deposit.onlySignaled == true, "Allocator: Signaling not enabled");
        require(deposit.signalingCutoff > block.timestamp, "Allocator: Signaling ended");
        require(
            totalAmountSignaledByHolder[msg.sender] <= balanceOfAt(msg.sender, block.timestamp),
            "Allocator: Signal amount exceeds balance"
        );

        // increment total amount of signaled by the holder comprising all deposits
        if (signalAmount == 0) {
            totalAmountSignaledByHolder[msg.sender] = totalAmountSignaledByHolder[msg.sender].sub(deposit.signaledAmounts[msg.sender]);
        } else if (signalAmount < deposit.signaledAmounts[msg.sender]) {
            uint256 deltaAmountSignaled = deposit.signaledAmounts[msg.sender].sub(signalAmount);
            totalAmountSignaledByHolder[msg.sender] = totalAmountSignaledByHolder[msg.sender].sub(deltaAmountSignaled);
        } else {
            uint256 deltaAmountSignaled = signalAmount.sub(deposit.signaledAmounts[msg.sender]);
            totalAmountSignaledByHolder[msg.sender] = totalAmountSignaledByHolder[msg.sender].add(deltaAmountSignaled);
        }

        // update total amount signaled for deposit
        deposit.totalAmountSignaled = deposit.totalAmountSignaled.sub(deposit.signaledAmounts[msg.sender]);
        deposit.totalAmountSignaled = deposit.totalAmountSignaled.add(signalAmount);
        // update the signaled amount of holder
        deposit.signaledAmounts[msg.sender] = signalAmount;
    }

    /**
     * @notice Issuer can push funds to provided addresses
     * @param depositId Id of the deposit
     * @param payees Addresses to which to push the funds
     */
    function pushFundsToAddresses(bytes32 depositId, address payable[] memory payees) external {
        Deposit storage deposit = deposits[depositId];

        for (uint256 i = 0; i < payees.length; i++) {
            if (deposit.claimed[payees[i]] == false) {
                transferDeposit(payees[i], deposit, depositId);
            }
        }
    }

    /**
     * @notice Withdraws the holders share of funds of the deposit
     * @param depositId Id of the deposit
     */
    function claimDeposit(bytes32 depositId) public {
        Deposit storage deposit = deposits[depositId];

        require(deposit.claimed[msg.sender] == false, "Allocator: Deposit already claimed");

        transferDeposit(msg.sender, deposit, depositId);
    }

    /**
     * @notice Internal function for transferring deposits
     * @param payee Address of holder
     * @param deposit Pointer to deposit in storage
     */
    function transferDeposit(address payee, Deposit storage deposit, bytes32 depositId)
        internal
        virtual
        nonReentrant()
    {
        uint256 claim = calculateClaimOnDeposit(payee, depositId);
        require(claim > 0, "Allocator: Nothing to claim");

        deposit.claimed[payee] = true;
        deposit.claimedAmount = claim.add(deposit.claimedAmount);

        // decrease total amount signaled by holder for all deposits by the holders signaled amount of the deposit
        totalAmountSignaledByHolder[payee] = totalAmountSignaledByHolder[payee].sub(deposit.signaledAmounts[payee]);
     
        IERC20(deposit.token).safeTransfer(payee, claim);
    }

    /**
     * @notice Calculate claimable amount of a deposit for a given address
     * @param payee Address of holder
     * @param depositId Id of the deposit
     * @return withdrawable amount
     */
    function calculateClaimOnDeposit(address payee, bytes32 depositId) public view returns(uint256) {
        Deposit storage deposit = deposits[depositId];

        if (deposit.claimed[payee]) return 0;

        uint256 totalSupply = totalSupplyAt(deposit.scheduledFor);

        // if deposit is marked as `onlySignaled` use the holders signaled amount
        // instead of the holders checkpointed balance
        uint256 balance = (deposit.onlySignaled)
            ? deposit.signaledAmounts[payee]
            : balanceOfAt(payee, deposit.scheduledFor);

        // if deposit is marked as `onlySignaled` use the total amount signaled instead of the checkpointed total supply
        if ((deposit.onlySignaled && deposit.totalAmountSignaled == 0) || totalSupply == 0) return 0;
        uint256 claim = balance.mul(deposit.amount).div(
            (deposit.onlySignaled) ? deposit.totalAmountSignaled : totalSupply
        );

        return claim;
    }

    /**
     * @notice Checks whether an address has withdrawn funds for a deposit
     * @param depositId Id of the deposit
     * @return bool whether the address has claimed
     */
    function hasClaimedDeposit(address holder, bytes32 depositId) public view returns (bool) {
        return deposits[depositId].claimed[holder];
    }

    /**
     * @notice Returns params of a deposit
     * @return scheduledFor
     * @return amount
     * @return claimedAmount
     * @return totalAmountSignaled
     * @return onlySignaled
     * @return token
     */
    function getDeposit(bytes32 depositId)
        public
        view
        returns (
            uint256 scheduledFor,
            uint256 amount,
            uint256 claimedAmount,
            uint256 totalAmountSignaled,
            bool onlySignaled,
            address token
        )
    {
        Deposit storage deposit = deposits[depositId];
        scheduledFor = deposit.scheduledFor;
        amount = deposit.amount;
        claimedAmount = deposit.claimedAmount;
        totalAmountSignaled = deposit.totalAmountSignaled;
        onlySignaled = deposit.onlySignaled;
        token = deposit.token;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.7.0;

import "@openzeppelin/contracts/access/AccessControl.sol";

import "./interfaces/IERC1404.sol";


contract Regulator is AccessControl, IERC1404 {

    bytes32 public constant REGULATOR_ROLE = bytes32(keccak256("REGULATOR_ROLE"));

    /* ERC1404 Error codes and messages */
    uint8 public constant SUCCESS_CODE = 0;
    uint8 public constant FAILURE_NON_SENDER_LIST = 1;
    string public constant SUCCESS_MESSAGE = "Success";
    string public constant FAILURE_NON_SENDER_LIST_MESSAGE = "Transfer restricted due to sender list configuration";
    string public constant UNKNOWN_ERROR = "Unknown Error Code";

    /* Sender List options */
    bool public senderListRestrictionEnabled = true;
    // Zero is reserved for indicating it is not on a senderList (the default for all addresses)
    uint8 constant internal NO_SENDER_LIST = 0;
    // address => SenderList
    mapping(address => uint8) public addressSenderLists;
    // SenderList => SenderList => outbound transfer enabled
    mapping(uint8 => mapping(uint8 => bool)) public outboundSenderListsEnabled;

    event AddressAddedToSenderList(address indexed addedAddress, uint8 indexed senderList);
    event AddressRemovedFromSenderList(address indexed removedAddress, uint8 indexed senderList);
    event OutboundSenderListUpdated(uint8 indexed srcSenderList, uint8 indexed destSenderList, bool from, bool to);

    /**
     * @notice Evaluates whether a transfer should be allowed or not.
     */
    modifier notTransferRestricted(address from, address to, uint256 value) {
        uint8 restrictionCode = detectTransferRestriction(from, to, value);
        require(
            restrictionCode == SUCCESS_CODE || hasRole(REGULATOR_ROLE, from),
            messageForTransferRestriction(restrictionCode)
        );
        _;
    }

    constructor(address regulator) {
        _setupRole(REGULATOR_ROLE, regulator);
    }

    function enableSenderLists() external {
        require(hasRole(REGULATOR_ROLE, msg.sender), "Regulator: Sender does not have required role");
        senderListRestrictionEnabled = true;
    }

    function disableSenderLists() external {
        require(hasRole(REGULATOR_ROLE, msg.sender), "Regulator: Sender does not have required role");
        senderListRestrictionEnabled = false;
    }

    /**
     * @notice Sets an address's white list ID.  Only administrators should be allowed to update this.
     * If an address is on an existing senderList, it will just get updated to the new value (removed from previous).
     */
    function addToSenderList(address addressToAdd, uint8 senderList) external {
        require(hasRole(REGULATOR_ROLE, msg.sender), "Regulator: Sender does not have required role");

        // Verify the senderList is valid
        require(senderList != NO_SENDER_LIST, "Invalid senderList ID supplied");
        // Save off the previous white list
        uint8 previousSenderList = addressSenderLists[addressToAdd];
        // Set the address's white list ID
        addressSenderLists[addressToAdd] = senderList;
        
        // If the previous senderList existed then we want to indicate it has been removed
        if (previousSenderList != NO_SENDER_LIST) {
            // Emit the event for tracking
            emit AddressRemovedFromSenderList(addressToAdd, previousSenderList);
        }

        // Emit the event for new senderList
        emit AddressAddedToSenderList(addressToAdd, senderList);
    }

    /**
     * @notice Clears out an address's white list ID.  Only administrators should be allowed to update this.
     */
    function removeFromSenderList(address addressToRemove) external {
        require(hasRole(REGULATOR_ROLE, msg.sender), "Regulator: Sender does not have required role");

        // Save off the previous white list
        uint8 previousSenderList = addressSenderLists[addressToRemove];
        // Zero out the previous white list
        addressSenderLists[addressToRemove] = NO_SENDER_LIST;

        // Emit the event for tracking
        emit AddressRemovedFromSenderList(addressToRemove, previousSenderList);
    }

    /**
     * @notice Sets the flag to indicate whether source senderList is allowed to send to destination senderList.
     */
    function updateOutboundSenderListEnabled(uint8 srcSenderList, uint8 destSenderList, bool newEnabledValue)
        external
    {
        require(hasRole(REGULATOR_ROLE, msg.sender), "Regulator: Sender does not have required role");

        // Get the old enabled flag
        bool oldEnabledValue = outboundSenderListsEnabled[srcSenderList][destSenderList];
        // Update to the new value
        outboundSenderListsEnabled[srcSenderList][destSenderList] = newEnabledValue;

        // Emit event for tracking
        emit OutboundSenderListUpdated(srcSenderList, destSenderList, oldEnabledValue, newEnabledValue);
    }

    /**
     * @notice Determine if the a sender is allowed to send to the receiver.
     * The source senderList must be enabled to send to the senderList where the receive exists.
     */
    function checkSenderListAllowed(address sender, address receiver) public view returns (bool) {
        // First get each address white list
        uint8 senderSenderList = addressSenderLists[sender];
        uint8 receiverSenderList = addressSenderLists[receiver];
        // If either address is not on a white list then the check should fail
        if (senderSenderList == NO_SENDER_LIST || receiverSenderList == NO_SENDER_LIST) { return false; }
        // Determine if the sending senderList is allowed to send to the destination senderList
        return outboundSenderListsEnabled[senderSenderList][receiverSenderList];
    }

    /**
     * @notice Detects if a transfer will be reverted and if so returns an appropriate reference code
     * @dev Overwrite with your custom transfer restriction logic
     * @param from Sending address
     * @param to Receiving address
     * param value Amount of tokens being transferred
     * @return Code by which to reference message for rejection reasoning
     */
    function detectTransferRestriction(address from, address to, uint256 /* value */)
        public
        view
        override
        returns (uint8)
    {
        // If the restrictions have been disabled by the owner, then just return success
        // Logic defined in Restrictable parent class
        if (!senderListRestrictionEnabled) return SUCCESS_CODE;
        // Restrictions are enabled, so verify the senderList config allows the transfer.
        // Logic defined in SenderListable parent class
        if (!checkSenderListAllowed(from, to)) return FAILURE_NON_SENDER_LIST;
        // If no restrictions were triggered return success
        return SUCCESS_CODE;
    }

    /**
     * @notice Returns a human-readable message for a given restriction code
     * @dev Overwrite with your custom message and restrictionCode handling
     * @param restrictionCode Identifier for looking up a message
     * @return Text showing the restriction's reasoning
     */
    function messageForTransferRestriction(uint8 restrictionCode)
        public
        pure
        override
        returns (string memory)
    {
        if (restrictionCode == SUCCESS_CODE) return SUCCESS_MESSAGE;
        if (restrictionCode == FAILURE_NON_SENDER_LIST) return FAILURE_NON_SENDER_LIST_MESSAGE;
        // An unknown error code was passed in.
        return UNKNOWN_ERROR;
    }
}

