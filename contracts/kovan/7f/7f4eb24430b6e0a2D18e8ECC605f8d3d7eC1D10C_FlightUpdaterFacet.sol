pragma solidity ^0.8.4;

import {LibOracle} from "../libraries/LibOracle.sol";

contract Modifiers {
    modifier onlyOracle() {
        LibOracle.enforeIsOracle();
        _;
    }

    modifier oracleCallback(bytes32 requestId) {
        LibOracle.handleOracleCallback(requestId);
        _;
