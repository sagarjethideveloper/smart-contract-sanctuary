pragma solidity ^0.8.4;

import {Errors} from "../libraries/Errors.sol";

struct Flight {
    bool departed;
    uint8 checkInterval;
    uint64 nextCheck;
    uint64 departureTime;
    uint64 arrivalTime;
    bytes32[] policies;
}

struct FlightInitArgs {
    uint64 departureTime;
    uint64 arrivalTime;
}

library LibFlight {
    function initialize(
        Flight storage self,
        uint64 departureTime,
        uint64 arrivalTime
    ) internal {
        require(
            departureTime > block.timestamp && arrivalTime > block.timestamp,
            Errors.FL_INVALID_FLIGHT_TIME
        );

        self.nextCheck = arrivalTime;
        self.departureTime = departureTime;
        self.arrivalTime = arrivalTime;
    }

    function isEmpty(Flight storage self) internal view returns (bool) {
        return self.departureTime == 0;
    }

    function addPolicy(
        Flight storage self,
        bytes32 policyId,
        uint8 delayInterval
    ) internal {
        require(!self.departed, Errors.FL_ALREADY_DEPARTED);
        require(self.nextCheck == self.arrivalTime, Errors.FL_ALREADY_CHECKED);

        if (self.checkInterval == 0 || delayInterval < self.checkInterval) {
            self.checkInterval = delayInterval;
        }

        self.policies.push(policyId);
    }

    function rescheduleNextCheck(Flight storage self) internal {
        self.nextCheck += self.checkInterval;
    }
}

