pragma solidity ^0.8.4;

import {Modifiers} from "../base/Modifiers.sol";
import {Flight, LibFlight} from "../models/Flight.sol";
import {InsurancePackage, LibInsurancePackage} from "../models/InsurancePackage.sol";

import {Errors} from "../libraries/Errors.sol";
import {AppStorage, LibAppStorage} from "../libraries/LibAppStorage.sol";
import {FlightQueue, LibFlightQueue} from "../libraries/LibFlightQueue.sol";
import {LibOracle} from "../libraries/LibOracle.sol";

contract FlightUpdaterFacet is Modifiers {
    using LibInsurancePackage for InsurancePackage;
    using LibFlight for Flight;
    using LibFlightQueue for FlightQueue;
    using LibAppStorage for AppStorage;

    AppStorage s;

    event FlightStatusUpdate(
        bytes32 requestId,
        bytes32 indexed flightId,
        bool departed,
        address updater
    );

    function updateFlightStatus(
        bytes32 requestId,
        bytes32 flightId,
        bool departed,
        uint256 actualDepartureTime
    ) external onlyOracle oracleCallback(requestId) {
        Flight storage flight = s.getFlight(flightId);

        require(!flight.departed, Errors.FLUP_FLIGHT_ALREADY_DEPARTED);

        if (departed) {
            _evaluateFlightDeparture(flightId, flight, actualDepartureTime);
            flight.departed = true;
        } else {
            flight.rescheduleNextCheck();
            s.flightQueue.addFlightToQueue(flightId);
        }

        emit FlightStatusUpdate(requestId, flightId, departed, msg.sender);
    }

    function _evaluateFlightDeparture(
        bytes32 flightId,
        Flight memory flight,
        uint256 actualDepartureTime
    ) private {
        uint256 hourDifference = (actualDepartureTime - flight.departureTime) /
            1 hours;

        if (hourDifference != 0) {
            for (uint256 i = 0; i < flight.policies.length; i++) {
                uint256 indemnity = s
                    .getInsurancePackage(flight.policies[i])
                    .calculateIndemnity(hourDifference);

                LibOracle.requestPayout(flightId, indemnity);
            }
        }
    }
}

