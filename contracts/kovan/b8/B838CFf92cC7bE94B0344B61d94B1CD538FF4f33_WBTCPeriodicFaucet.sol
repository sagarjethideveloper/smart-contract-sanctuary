pragma solidity ^0.8.3;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "hardhat/console.sol";

contract MockBTC is ERC20, AccessControl {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    constructor(
        address admin,
        string memory name,
        string memory symbol
    ) ERC20(name, symbol) {
        _setupRole(DEFAULT_ADMIN_ROLE, admin);

        grantRole(MINTER_ROLE, admin);
    }

    function mint(address account, uint256 amount) public {
        require(hasRole(MINTER_ROLE, _msgSender()), "require minter role");

        _mint(account, amount);
    }
}

contract WBTC is MockBTC {
    constructor(address admin) MockBTC(admin, "Wrapped Bitcoin", "WBTC") {}

    function decimals() public pure override returns (uint8) {
        return 8;
    }
}

contract HBTC is MockBTC {
    constructor(address admin) MockBTC(admin, "Huobi BTC", "HBTC") {}
}

contract OtherCoin is MockBTC {
    constructor(address admin) MockBTC(admin, "Other ERC20", "OTHER") {}
}

contract WBTCTransferable is WBTC {
    bytes32 public constant RECEIVER_ROLE = keccak256("RECEIVER_ROLE");

    constructor(address admin) WBTC(admin) {}

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        super._beforeTokenTransfer(from, to, amount);

        require((address(0) == from) || hasRole(RECEIVER_ROLE, to), "require receiver role");
    }
}

