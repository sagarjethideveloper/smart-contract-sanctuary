pragma solidity ^0.8.3;

import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./MockBTC.sol";

contract PeriodicFaucet is Ownable, EIP712 {
    bytes32 private immutable _VERIFY_TYPEHASH = keccak256("Verify(uint256 nonce,bytes32 ip)");

    MockBTC public token;
    uint256 public unitAmount;
    uint256 public period = 4 hours;

    uint256 startTime;
    address signer;

    mapping(address => uint256) private _nextUserClaim;
    mapping(bytes32 => uint256) private _nextIPClaim;
    mapping(address => uint256) private _nonces;

    event Claim(address indexed user, bytes32 indexed ip, uint256 amount);

    constructor(
        address _signer,
        MockBTC _token,
        uint256 _unitAmount,
        uint256 _startTime
    ) EIP712("PeriodicFaucet", "1") {
        require(address(_token) != address(0), "zero address");

        signer = _signer;
        token = _token;
        unitAmount = _unitAmount;
        startTime = _startTime;
    }

    function claim(
        uint256 nonce,
        bytes32 ip,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public {
        require(currentTime() >= startTime, "not started");

        // verify Signature
        require(nonce > _nonces[msg.sender], "invalid nonce");
        _verifySignature(nonce, ip, v, r, s);
        _nonces[msg.sender] = nonce;

        // check period
        uint256 _currPeriod = currentPeriod();
        require(_nextUserClaim[msg.sender] <= _currPeriod, "user too frequent");
        _nextUserClaim[msg.sender] = _currPeriod + 1;

        // check ip
        require(_nextIPClaim[ip] <= _currPeriod, "ip too frequent");
        _nextIPClaim[ip] = _currPeriod + 1;

        // mint
        token.mint(msg.sender, unitAmount);

        emit Claim(msg.sender, ip, unitAmount);
    }

    function changeSigner(address _signer) external onlyOwner {
        signer = _signer;
    }

    function nonces(address user) public view returns (uint256) {
        return _nonces[user];
    }

    function _verifySignature(
        uint256 nonce,
        bytes32 ip,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) private view {
        bytes32 digest = _hashTypedDataV4(keccak256(abi.encode(_VERIFY_TYPEHASH, nonce, ip)));

        require(ECDSA.recover(digest, v, r, s) == signer, "invalid signature");
    }

    function currentTime() private view returns (uint256) {
        return block.timestamp;
    }

    function currentPeriod() private view returns (uint256) {
        return block.timestamp / period;
    }
}

contract WBTCPeriodicFaucet is PeriodicFaucet {
    constructor(
        address signer,
        MockBTC token,
        uint256 startTime
    ) PeriodicFaucet(signer, token, 2 * (uint256(10)**7), startTime) {}
}

