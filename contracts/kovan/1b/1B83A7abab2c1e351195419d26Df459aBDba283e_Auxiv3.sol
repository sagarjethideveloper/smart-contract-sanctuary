pragma solidity 0.7.6;
pragma abicoder v2;

contract Auxiv3 {
    uint256 avgDifferenceForRounds = 50;
    using SafeMath for uint256;
    struct ASSET_DETAILS {
        address Asset;
        address Oracle;
        string[] vp;
    }

    mapping(string => ASSET_DETAILS) public asset_id;

    address owner;

    constructor() public {
        owner = msg.sender;
    }

    modifier _ownerOnly() {
        require(msg.sender == owner);
        _;
    }

    function getHistoricalPriceCurrency(uint80 roundId, address oracle)
        public
        view
        returns (int256)
    {
        AggregatorV2V3Interface priceFeedCurrency = AggregatorV2V3Interface(
            oracle
        );
        (
            uint80 id,
            int256 price,
            uint256 startedAt,
            uint256 timeStamp,
            uint80 answeredInRound
        ) = priceFeedCurrency.getRoundData(roundId);
        require(timeStamp > 0, "Round not complete");
        return price;
    }

    function getlatestAnswerCurrency(string memory assetID)
        public
        view
        returns (int256)
    {
        AggregatorV2V3Interface priceFeedCurrency = AggregatorV2V3Interface(
            getOracle(assetID)
        );
        int256 price = priceFeedCurrency.latestAnswer();
        return price;
    }

    function getCurrencyTimeStampForRoundId(uint80 roundId, address oracle)
        public
        view
        returns (uint256)
    {
        AggregatorV2V3Interface priceFeedCurrency = AggregatorV2V3Interface(
            oracle
        );
        (
            uint80 id,
            int256 price,
            uint256 startedAt,
            uint256 timeStamp,
            uint80 answeredInRound
        ) = priceFeedCurrency.getRoundData(roundId);
        return timeStamp;
    }

    function getCurrencyAverageDifferenceInTimeStamps(uint256 n, address oracle)
        public
        view
        returns (uint256)
    {
        AggregatorV2V3Interface priceFeedCurrency = AggregatorV2V3Interface(
            oracle
        );
        uint256 _roundId = priceFeedCurrency.latestRound();
        uint256 currTimeStamp = getCurrencyTimeStampForRoundId(
            uint80(_roundId),
            oracle
        );
        uint256 _avgtimestamp = 0;
        for (uint256 i = _roundId; i > _roundId - n; i--) {
            uint256 _timestamp = priceFeedCurrency.getTimestamp(i);
            _avgtimestamp += (currTimeStamp.sub(_timestamp));
            currTimeStamp = _timestamp;
        }
        return _avgtimestamp / n;
    }

    function getCurrencyPriceForTimeStamp(
        uint256 _timestamp,
        string memory assetID
    ) public view returns (int256) {
        uint256 diff = getCurrencyAverageDifferenceInTimeStamps(
            avgDifferenceForRounds,
            getOracle(assetID)
        );
        AggregatorV2V3Interface priceFeedCurrency = AggregatorV2V3Interface(
            getOracle(assetID)
        );
        uint256 _roundId = priceFeedCurrency.latestRound();
        uint256 currTimeStamp = getCurrencyTimeStampForRoundId(
            uint80(_roundId),
            getOracle(assetID)
        );
        if (_timestamp >= currTimeStamp) {
            int256 price = getHistoricalPriceCurrency(
                uint80(_roundId),
                getOracle(assetID)
            );
            return price;
        }
        uint256 nRound = (currTimeStamp - _timestamp) / diff;
        //Add the logic here to find the round id with minimum difference with the given timestamp
        uint256 round = _roundId - nRound;
        uint256 min_diff = 10000000000000000000;
        uint256 lower = round;
        uint256 upper = round;
        if (nRound == 0) {
            lower = round - 1;
            upper = round;
        } else {
            lower = round - 1;
            upper = round + 1;
        }
        for (uint256 i = lower; i <= upper; i++) {
            address orcAd = getOracle(assetID);
            uint256 t = getCurrencyTimeStampForRoundId(uint80(i), orcAd);
            if (t == _timestamp) {
                round = i;
                break;
            } else if (t < _timestamp && min_diff > (_timestamp - t)) {
                min_diff = (_timestamp - t);
                round = i;
            } else if (t > _timestamp && min_diff > (t - _timestamp)) {
                min_diff = (t - _timestamp);
                round = i;
            }
        }
        int256 price = getHistoricalPriceCurrency(
            uint80(round),
            getOracle(assetID)
        );
        return price;
    }

    function getAsset(string memory _AssetID) public view returns (address) {
        return asset_id[_AssetID].Asset;
    }

    function getOracle(string memory _AssetID) public view returns (address) {
        return asset_id[_AssetID].Oracle;
    }

    function addAsset(
        string memory _AssetID,
        address _AssetAddress,
        address _AssetOracle
    ) public _ownerOnly {
        ASSET_DETAILS memory assetd;
        assetd = asset_id[_AssetID];
        assetd.Asset = _AssetAddress;
        assetd.Oracle = _AssetOracle;
        asset_id[_AssetID] = assetd;
    }

    function currtousd(uint256 curr, string memory assetID)
        public
        view
        returns (uint256)
    {
        AggregatorV2V3Interface priceFeedCurrency = AggregatorV2V3Interface(
            getOracle(assetID)
        );
        uint256 currval = (curr.mul(uint256(priceFeedCurrency.latestAnswer())))
        .div((10**18));
        return currval;
    }

    function usdtocurr(uint256 usd, string memory assetID)
        public
        view
        returns (uint256)
    {
        AggregatorV2V3Interface priceFeedCurrency = AggregatorV2V3Interface(
            getOracle(assetID)
        );
        uint256 ucurrval = (usd.mul((10**18))).div(
            uint256(priceFeedCurrency.latestAnswer())
        );
        return ucurrval;
    }
}

