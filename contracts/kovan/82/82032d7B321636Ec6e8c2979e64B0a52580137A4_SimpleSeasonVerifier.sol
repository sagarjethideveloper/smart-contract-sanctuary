pragma solidity ^0.8.0;

import { ERC721 } from '@openzeppelin/contracts/token/ERC721/ERC721.sol';
import { IERC20 } from '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import { Ownable } from '@openzeppelin/contracts/access/Ownable.sol';
import { Math } from '@openzeppelin/contracts/utils/math/Math.sol';
import { CastingMath } from './libraries/CastingMath.sol';

// TODO bring ERC721.sol in this project to allow initializing with name and symbol
contract Season is Ownable, ERC721 {
    using CastingMath for uint256;

    uint256 private constant PRECISION_FACTOR = 10**12; // type(uint40).max;

    /**
        Season Vars
     */
    uint32 public startBlock;
    uint32 public endBlock;
    uint48 public rewardPerBlock;
    uint32 public totalPoolWeight;

    address public rewardToken;
    address public scoreCardVerifier;

    /**
        Compressed Deposits

        ERC20 amounts need 256 bits to store.
        For increasing gas efficiency, we use Compressed amounts.
        That helps to reasonably fit those 256 bit balances in 48 bits.
        
        As an example: consider DAI token (that has 18 decimals)
        Almost nobody cares about any DAI they have below 0.01, which is still 10^16
        Also, for the time being if total DAI that practically could be is 1 Trillion DAI.
        So this gives a practical range for DAI: 0.01 to 1T. Total possible values: 10^14.
        Number of bits to represent these values = lb(10^14) = 46.5.
        So we can safely assume DAI balance under a uint48 for most practical purposes.

        In this contract, we assume individual user deposits to be between $0.1 to $10B.
        Any other token could be scaled to approximately fit this range using approriate
        compressionFactor value. This allows value to stay in 36 bits. Allowing more 10 bits for
        precision
        
        This does not limit TVL, as 96 bits (higher) are used

        1 Compressed unit = 10^compressionFactor POOLTOKEN

        There should be checks everywhere

        Any random token: 48 bits (using minification)
        ANON tokens: 48 bits
        accumulation: 48 + 48 = 96 bits (because of precision factor)

        Why precision factor is 40 bits?
        Formula for accumulated reward = blocks * rewardPerBlock / totalDepositsCompressed
        totalDepositsCompressed is assumed to be upto 40 bits (<= $100B). If it's more, then there will
        be a slight loss of precision, rewards will be very very slightly less.
     */

    struct Pool {
        uint96 totalDepositsCompressed;
        uint96 accRewardPerPoolToken;
        uint32 lastUpdateBlock;
        uint16 weight;
        uint8 compressionFactor; // compresssion is done by 10^compressionFactor
        bool isInitialized;
    }

    mapping(address => Pool) public pools;

    struct NFT {
        uint8 category;
        uint16 individualScore; // bps 0 to 10_000
        uint16 existingCategoryScore; // bps 0 to 10_000
    }

    struct NFTAsset {
        uint48 depositsCompressed; //  should pe properly choosen so that: max bits 30
        uint96 accRewardPerPoolTokenLast;
        uint48 farmed;
        uint48 claimed;
    }

    string private _name;
    string private _symbol;
    uint256 private lastTokenId;
    mapping(uint256 => NFT) public nfts;
    mapping(uint256 => mapping(address => NFTAsset)) public nftAssets;

    mapping(uint8 => uint16) public categoryScores;

    event PoolUpdated(address poolToken, uint16 weight);
    event Deposit(uint256 indexed tokenId, address poolToken, uint256 amount);
    event Claim(uint256 indexed tokenId, uint256 amount);
    event Withdraw(uint256 indexed tokenId, address poolToken, uint256 amount);

    modifier onlyApprovedOrOwner(uint256 tokenId) {
        require(_isApprovedOrOwner(msg.sender, tokenId), 'Season:NO');
        _;
    }

    modifier onlyScoreCardVerifier() {
        require(msg.sender == scoreCardVerifier, 'Season:NO');
        _;
    }

    constructor() ERC721('SEAON_IMPLEMENTATION', '') {
        // rewardToken = address(1); // makes implementation non-initializable
    }

    struct AddPoolArgs {
        address poolToken;
        uint16 weight;
        uint8 compressionFactor;
    }

    function initialize(
        uint32 _startBlock,
        uint32 _endBlock,
        uint48 _rewardPerBlock,
        address _rewardToken,
        address _governance,
        AddPoolArgs[] memory _initialPools,
        string memory name_,
        string memory symbol_
    ) external {
        require(rewardToken == address(0), 'Season:NO');
        require(_rewardToken != address(0), 'Season:IN');
        require(IERC20(_rewardToken).balanceOf(address(this)) >= (_endBlock - _startBlock + 1) * _rewardPerBlock);
        transferOwnership(_governance);
        startBlock = _startBlock;
        endBlock = _endBlock;
        rewardPerBlock = _rewardPerBlock;
        rewardToken = _rewardToken;
        _name = name_;
        _symbol = symbol_;
        for (uint256 i; i < _initialPools.length; i++) {
            _updatePool(_initialPools[i].poolToken, _initialPools[i].weight, _initialPools[i].compressionFactor);
        }
    }

    function setScoreVerifier(address _scoreCardVerifier) external {
        require(scoreCardVerifier == address(0), 'Season:NO');
        require(_scoreCardVerifier != address(0), 'Season:IN');
        scoreCardVerifier = _scoreCardVerifier;
    }

    function updatePool(
        address poolToken,
        uint16 weight,
        uint8 compressionFactor
    ) external onlyOwner {
        _updatePool(poolToken, weight, compressionFactor);
    }

    function _updatePool(
        address poolToken,
        uint16 weight,
        uint8 compressionFactor
    ) private {
        Pool memory pool = pools[poolToken];
        totalPoolWeight = totalPoolWeight + weight - pool.weight;
        pool.weight = weight;
        if (pool.isInitialized) {
            require(pool.compressionFactor == compressionFactor, 'Season:CF');
        } else {
            pool.isInitialized = true;
        }
        pool.compressionFactor = compressionFactor;
        pools[poolToken] = pool;
        emit PoolUpdated(poolToken, weight);
    }

    function reportCategoryScore(uint8 categoryId, uint16 score) external onlyScoreCardVerifier {
        categoryScores[categoryId] += score;
    }

    function reportIndividualScore(uint256 tokenId, uint16 score) external onlyScoreCardVerifier {
        nfts[tokenId].individualScore += score;
    }

    function mintAndDeposit(
        address poolToken,
        uint8 playerCategory,
        uint256 amount
    ) external {
        Pool memory pool = _updatePool(poolToken);

        amount -= amount % (10**pool.compressionFactor); // do not collect dust
        _collect(poolToken, amount);

        uint256 tokenId = ++lastTokenId;
        _mint(msg.sender, tokenId);

        nfts[tokenId] = NFT({
            category: playerCategory,
            individualScore: 0,
            existingCategoryScore: categoryScores[playerCategory]
        });
        nftAssets[tokenId][poolToken] = NFTAsset({
            depositsCompressed: (amount / (10**pool.compressionFactor)).toU48(),
            accRewardPerPoolTokenLast: pool.accRewardPerPoolToken,
            farmed: 0,
            claimed: 0
        });

        pool.totalDepositsCompressed += (amount / (10**pool.compressionFactor)).toU96();
        pools[poolToken] = pool;

        emit Deposit(tokenId, poolToken, amount);
    }

    function depositFor(
        uint256 tokenId,
        address poolToken,
        uint256 amount
    ) public {
        require(_exists(tokenId), 'Season:D');

        (Pool memory pool, NFTAsset memory nftAsset) = _updateNFTAsset(tokenId, poolToken);
        _collect(poolToken, amount);

        pool.totalDepositsCompressed += (amount / (10**pool.compressionFactor)).toU96();
        pools[poolToken] = pool;

        nftAsset.depositsCompressed += (amount / (10**pool.compressionFactor)).toU48();
        nftAssets[tokenId][poolToken] = nftAsset;

        emit Deposit(tokenId, poolToken, amount);
    }

    function claim(
        uint256 tokenId,
        address poolToken,
        address beneficiary
    ) public onlyApprovedOrOwner(tokenId) returns (Pool memory pool, NFTAsset memory nftAsset) {
        require(beneficiary != address(0), 'Season:BN');
        NFT memory nft = nfts[tokenId];
        (pool, nftAsset) = _updateNFTAsset(tokenId, poolToken);
        uint16 score = categoryScores[nft.category] + nft.individualScore - nft.existingCategoryScore;
        // require(score > 0, 'Season:C1');
        uint48 amount = ((nftAsset.farmed * Math.min(score, 10000)) / 10000).toU48() - nftAsset.claimed;
        if (amount > 0) {
            nftAsset.claimed += amount;
            emit Claim(tokenId, amount);
            IERC20(rewardToken).transfer(beneficiary, amount);
            nftAssets[tokenId][poolToken] = nftAsset;
        }
    }

    function withdraw(
        uint256 tokenId,
        address poolToken,
        address beneficiary
    ) public {
        (Pool memory pool, NFTAsset memory nftAsset) = claim(tokenId, poolToken, beneficiary);
        uint256 amount = nftAsset.depositsCompressed * (10**pool.compressionFactor);
        pool.totalDepositsCompressed -= nftAsset.depositsCompressed;
        nftAsset.depositsCompressed = 0;
        pools[poolToken] = pool;
        nftAssets[tokenId][poolToken] = nftAsset;
        IERC20(poolToken).transfer(beneficiary, amount);
        emit Withdraw(tokenId, poolToken, amount);
    }

    function close(
        uint256 tokenId,
        address[] calldata poolToken,
        address beneficiary
    ) external {
        for (uint256 i; i < poolToken.length; i++) {
            withdraw(tokenId, poolToken[i], beneficiary);
        }
    }

    function _collect(address token, uint256 amount) private {
        require(amount > 0, 'Season:T1');
        IERC20(token).transferFrom(msg.sender, address(this), amount);
    }

    function _updatePool(address poolAddr) private returns (Pool memory pool) {
        pool = pools[poolAddr]; // SLOAD

        uint256 fromBlock = Math.max(startBlock - 1, pool.lastUpdateBlock);
        uint256 toBlock = Math.min(endBlock, uint32(block.number));
        pool.lastUpdateBlock = uint32(toBlock);
        uint256 totalPoolTokenDepositsCompressed =
            IERC20(poolAddr).balanceOf(address(this)) / (10**pool.compressionFactor);
        if (totalPoolTokenDepositsCompressed > 0) {
            pool.accRewardPerPoolToken += ((((toBlock - fromBlock) * rewardPerBlock * pool.weight * PRECISION_FACTOR) /
                totalPoolWeight) / totalPoolTokenDepositsCompressed)
                .toU96();
        }

        pools[poolAddr] = pool; // SSTORE
    }

    function _updateNFTAsset(uint256 tokenId, address poolAddr)
        private
        returns (Pool memory pool, NFTAsset memory nftAsset)
    {
        nftAsset = nftAssets[tokenId][poolAddr]; // SLOAD
        pool = _updatePool(poolAddr);
        {
            nftAsset.farmed += (((pool.accRewardPerPoolToken - nftAsset.accRewardPerPoolTokenLast) *
                nftAsset.depositsCompressed) / PRECISION_FACTOR)
                .toU48();
            nftAsset.accRewardPerPoolTokenLast = pool.accRewardPerPoolToken;
        }
        nftAssets[tokenId][poolAddr] = nftAsset; // SSTORE
    }

    // ERC721
    function name() public view virtual override returns (string memory) {
        return _name;
    }

    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }
}

