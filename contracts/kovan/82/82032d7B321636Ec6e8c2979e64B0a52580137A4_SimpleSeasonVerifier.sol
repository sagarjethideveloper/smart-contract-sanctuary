pragma solidity ^0.8.0;

import { Ownable } from '@openzeppelin/contracts/access/Ownable.sol';
import { Season } from '../Season.sol';

contract SimpleSeasonVerifier is Ownable {
    address public season;

    bytes32 public merkleRoot;

    event Description(bytes32 ipfsHash);
    event Goals(bytes32 ipfsHash);
    event Commitment(bytes32 merkleRoot, bytes32 ipfsHash);

    function setSeason(address _season) external onlyOwner {
        require(season == address(0), 'V:NO');
        require(_season != address(0), 'V:IN');
        season = _season;
    }

    function updateDescription(bytes32 ipfsHash) external onlyOwner {
        emit Description(ipfsHash);
    }

    function submitGoal(bytes32 ipfsHash) external {
        emit Goals(ipfsHash);
    }

    function updateRoot(bytes32 _merkleRoot, bytes32 _ipfsHash) public {
        merkleRoot = _merkleRoot;
        emit Commitment(_merkleRoot, _ipfsHash);
    }

    function _prove(
        uint256 tokenId,
        uint16 goalIndex,
        uint16 score,
        bytes32[] memory proof
    ) private {}

    function reportIndividualScore(uint256 tokenId, uint16 score) external onlyOwner {
        Season(season).reportIndividualScore(tokenId, score);
    }

    function reportCategoryScore(uint8 categoryId, uint16 score) external onlyOwner {
        Season(season).reportCategoryScore(categoryId, score);
    }
}

