pragma solidity ^0.5.0;

import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./dependencies/uniswap/IUniswapV2Router02.sol";

contract CheckPrices {
    using SafeMath for uint256;

    function checkPrice(
        uint256 _amount,
        address _baseToken,
        address _swapToken,
        address _exchangeA,
        address _exchangeB
    ) public view returns (uint) {
        IUniswapV2Router02 exchangeA = IUniswapV2Router02(_exchangeA);
        IUniswapV2Router02 exchangeB = IUniswapV2Router02(_exchangeB);

        IERC20 baseToken = IERC20(_baseToken);
        IERC20 swapToken = IERC20(_swapToken);

        // swap baseToken -> swapToken on exchangeA, swapToken -> baseToken on exchangeB
        address[] memory tokens = new address[](2);
        tokens[0] = address(baseToken);
        tokens[1] = address(swapToken);

        uint[] memory tokensOut = exchangeA.getAmountsOut(_amount, tokens);

        tokens[0] = address(swapToken);
        tokens[1] = address(baseToken);

        tokensOut = exchangeB.getAmountsOut(tokensOut[1], tokens);

        return tokensOut[1];
    }
}

