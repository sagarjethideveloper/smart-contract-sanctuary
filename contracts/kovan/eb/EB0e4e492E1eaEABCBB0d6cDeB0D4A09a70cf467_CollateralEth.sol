contract ApeStreetModule {
    IApeStreetMaster internal master;

    /* modifiers */
    modifier onlyModuleOwner() {
        // require(msg.sender == Ownable(address(master)).owner(), "ApeStreetModule: ONLY_OWNER");
        _;
    }

    modifier onlyInternalModule() {
        require(master.isModule(msg.sender),  "ApeStreetModule: NON_MODULE_CALLER");
        _;
    }

    function initializeModule(address _master) internal {
        require(address(master) == address(0), "ApeStreetModule: ALREADY_INITIALIZED");
        require(_master != address(0), "ApeStreetModule: ZERO_ADDRESS_MASTER");
        master = IApeStreetMaster(_master);
    }

    function changeMaster(address _newMaster) external onlyModuleOwner {
        master = IApeStreetMaster(_newMaster);
    }

    function getModule(bytes32 key) internal view returns(address) {
        return master.getModule(key);
    }
}

