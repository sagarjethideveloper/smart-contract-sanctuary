import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";

import "./uniswap/IUniswapV2Router02.sol";

import "./interfaces/IVaultConfig.sol";
import "./interfaces/IStrategy.sol";
import "./interfaces/IVault.sol";
import "./utils/SafeToken.sol";
import "./WNativeRelayer.sol";
import "./interfaces/IMasterChef.sol";

contract Vault is IVault, ReentrancyGuard, Ownable {
  /// @notice Libraries
  using SafeToken for address;
  using SafeMath for uint256;

  /// @dev Flags for manage execution scope
  uint256 private constant _NOT_ENTERED = 1;
  uint256 private constant _ENTERED = 2;
  uint256 private constant _NO_ID = uint256(-1);
  address private constant _NO_ADDRESS = address(1);

  /// @dev Temporay variables to manage execution scope
  uint256 public _IN_EXEC_LOCK;
  address public STRATEGY;
  
  IUniswapV2Factory public factory;
  IUniswapV2Router02 public router;
  IMasterChef public masterChef;

  /// @dev Attributes for Vault
  IVaultConfig public config;

  /// @dev Require that the caller must be an EOA account if not whitelisted.
  modifier onlyEOAorWhitelisted() {
    if (!config.whitelistedCallers(msg.sender)) {
      require(msg.sender == tx.origin, "Vault::onlyEOAorWhitelisted:: not eoa");
    }
    _;
  }

  /// @dev Get baseToken from msg.sender
  modifier transferTokenToVault(address baseToken, uint256 value) {
    if (msg.value != 0) {
      require(baseToken == config.wrappedNative(), "Vault::transferTokenToVault:: baseToken is not wNative");
      require(value == msg.value, "Vault::transferTokenToVault:: value != msg.value");
      IWETH(config.wrappedNative()).deposit{ value: msg.value }();
    } else {
      SafeToken.safeTransferFrom(baseToken, msg.sender, address(this), value);
    }
    _;
  }

  /// @dev Ensure that the function is called with the execution scope
  modifier inExec() {
    require(STRATEGY == msg.sender, "Vault::inExec:: not from the strategy");
    require(_IN_EXEC_LOCK == _NOT_ENTERED, "Vault::inExec:: in exec lock");
    _IN_EXEC_LOCK = _ENTERED;
    _;
    _IN_EXEC_LOCK = _NOT_ENTERED;
  }

  constructor(IUniswapV2Router02 _router, IMasterChef _masterChef, IVaultConfig _config) public {
    factory = IUniswapV2Factory(_router.factory());
    router = _router;
    config = _config;
    masterChef = _masterChef;
    // free-up execution scope
    _IN_EXEC_LOCK = _NOT_ENTERED;
    STRATEGY = _NO_ADDRESS;
  }

  /// @dev Request Funds from user through Vault
  function requestFunds(address user, address targetedToken, uint256 amount) external override inExec {
    SafeToken.safeTransferFrom(targetedToken, user, msg.sender, amount);
  }

  /// @dev Create a new farming position to unlock your yield farming potential.
  /// @param data The calldata to pass along to the worker for more working context.
  function deposit(
    uint256 _pid,   
    address baseToken,
    uint256 principalAmount,
    bytes calldata data
  ) external payable onlyEOAorWhitelisted transferTokenToVault(baseToken, principalAmount) nonReentrant {
    (address strat, bytes memory ext) = abi.decode(data, (address, bytes));
    (, address farmingToken, ,) = abi.decode(ext, (address, address, uint256, uint256));
    STRATEGY = strat;
    require(config.strategies(strat), "Vault::strategy:: not a strategy");
    // Perform the actual work, using a new scope to avoid stack-too-deep errors.
    {
      require(principalAmount <= SafeToken.myBalance(baseToken), "Vault::work:: insufficient funds in the vault");
      SafeToken.safeTransfer(baseToken, strat, principalAmount);
      IStrategy(strat).execute(msg.sender, ext);
    }
    // deposit lp to masterchef
    IUniswapV2Pair lpToken = IUniswapV2Pair(factory.getPair(farmingToken, baseToken));
    address(lpToken).safeApprove(address(masterChef), uint256(-1));
    masterChef.deposit(msg.sender, _pid, lpToken.balanceOf(address(this)));  
    address(lpToken).safeApprove(address(masterChef), 0);
    // Release execution scope
    STRATEGY = _NO_ADDRESS;
  }

  /// @dev Update bank configuration to a new address. Must only be called by owner.
  /// @param _config The new configurator address.
  function updateConfig(IVaultConfig _config) external onlyOwner {
    config = _config;
  }

  /// @dev Fallback function to accept ETH. Workers will send ETH back the pool.
  receive() external payable {}
}

