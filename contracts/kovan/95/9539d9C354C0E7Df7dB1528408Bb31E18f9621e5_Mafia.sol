pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";

import "./DividendPayingToken.sol";
import "./PancakeSwapInterfaces.sol";

import "hardhat/console.sol";


contract Mafia is DividendPayingToken, Ownable {

    struct Lock {
        uint256 until;
        uint256 amount;
    }

    mapping(address => Lock[]) public locks;
    mapping(address => uint256) public lockedBalanceOf;
    mapping(address => bool) public canLock;

    mapping(address => bool) public excludedFromFee;

    uint256 public liquidityFee;
    uint256 public dividendFee;

    uint256 public amountToSwap;
    uint256 public collectedForLiquidity;
    uint256 public collectedForDividend;

    mapping (address => bool) public hasPurchased;
    mapping (address => uint256) public lastPurchase;
    mapping (address => uint256) public lastSale;

    uint256 public firstPurchaseLimit;
    uint256 public purchaseInterval;
    uint256 public saleInterval;

    address public immutable pancakePair;

    bool public pancakeSwapAllowed;

    bool private _inSwapAndLiquify;

    // CONSTRUCTOR

    constructor(
        IPancakeRouter02 pancakeRouter_,
        IERC20 rewardToken_,
        uint256 liquidityFee_,
        uint256 dividendFee_,
        uint256 amountToSwap_,
        uint256 firstPurchaseLimit_,
        uint256 purchaseInterval_,
        uint256 saleInterval_
    ) DividendPayingToken("Mafia", "MFA", pancakeRouter_, rewardToken_) Ownable() {
        _mint(msg.sender, 10**12 * 10**18);

        // Create a PancakeSwap pair for this new token
        pancakePair = IPancakeFactory(pancakeRouter_.factory()).createPair(
            address(this),
            pancakeRouter_.WETH()
        );

        excludedFromFee[address(this)] = true;

        liquidityFee = liquidityFee_;
        dividendFee = dividendFee_;
        amountToSwap = amountToSwap_;
        firstPurchaseLimit = firstPurchaseLimit_;
        purchaseInterval = purchaseInterval_;
        saleInterval = saleInterval_;
    }

    receive() payable external {}

    // RESTRICTED FUNCTIONS

    function transferAndLock(address to, uint256 amount, uint256 lockShare, uint256 lockDuration) external {
        require(canLock[msg.sender], "Sender can't lock");
        require(lockShare <= 100, "Can't lock more than 100%");

        uint256 balanceBefore = balanceOf(to);
        _transfer(msg.sender, to, amount);
        if (lockShare > 0) {
            uint256 amountToLock = (balanceOf(to) - balanceBefore) * lockShare / 100;
            locks[to].push(Lock({
                until: block.timestamp + lockDuration,
                amount: amountToLock
            }));
            lockedBalanceOf[to] += amountToLock;
        }     
    }

    function setCanLock(address account, bool can) external onlyOwner {
        canLock[account] = can;
    }

    function setExcludedFromFee(address account, bool excluded) external onlyOwner {
        excludedFromFee[account] = excluded;
    }

    function setLiquidityFee(uint256 fee) external onlyOwner {
        require(fee + dividendFee <= 100, "Total fees can't be greater than 100%");
        liquidityFee = fee;
    }

    function setDividendFee(uint256 fee) external onlyOwner {
        require(fee + liquidityFee <= 100, "Total fees can't be greater than 100%");
        dividendFee = fee;
    }

    function setAmountToSwap(uint256 amount) external onlyOwner {
        amountToSwap = amount;
    }

    function setFirstPurchaseLimit(uint256 limit) external onlyOwner {
        require(firstPurchaseLimit > 0, "Limit should be greater than zero");
        firstPurchaseLimit = limit;
    }

    function setPurchaseInterval(uint256 interval) external onlyOwner {
        purchaseInterval = interval;
    }

    function setSaleInterval(uint256 interval) external onlyOwner {
        saleInterval = interval;
    }

    function launchPancakeSwap() external onlyOwner {
        pancakeSwapAllowed = true;
    }

    // INTERNAL FUNCTIONS

    function _transfer(address from, address to, uint256 value) internal override {
        if (!pancakeSwapAllowed) {
            require(from != pancakePair && to != pancakePair, "Can't swap on Pancake before launch");
        }

        if (value + lockedBalanceOf[from] > balanceOf(from)) {
            _checkLocks(from);
        }
        require(value + lockedBalanceOf[from] <= balanceOf(from), "Not enough unlocked balance");

        if (to == pancakePair && from != address(this)) {
            require(block.timestamp > lastSale[from] + saleInterval, "Can't sale yet");
            lastSale[from] = block.timestamp;
        } else if (from == pancakePair && to != address(this)) {
            require(block.timestamp > lastPurchase[to] + purchaseInterval, "Can't purchase yet");
            lastPurchase[to] = block.timestamp;
            if (!hasPurchased[to]) {
                require(value <= firstPurchaseLimit, "Can't buy this amount at first purchase");
                hasPurchased[to] = true;
            }
        }

        if (excludedFromFee[from] || excludedFromFee[to]) {
            super._transfer(from, to, value);
        } else {
            uint256 liquidityFeeAmount = value * liquidityFee / 100;
            uint256 dividendFeeAmount = value * dividendFee / 100;
            uint256 valueToTransfer = value - liquidityFeeAmount - dividendFeeAmount;

            super._transfer(from, to, valueToTransfer);
            super._transfer(from, address(this), liquidityFeeAmount + dividendFeeAmount);
            collectedForLiquidity += liquidityFeeAmount;
            collectedForDividend += dividendFeeAmount;

            if (collectedForLiquidity + collectedForDividend >= amountToSwap && pancakeSwapAllowed) {
                _swapAndUtilize();
            }
        } 
    }

    function _checkLocks(address account) private {
        for (uint i = 0; i < locks[account].length; i++) {
            if (locks[account][i].until < block.timestamp) {
                lockedBalanceOf[account] -= locks[account][i].amount;
                locks[account][i] = locks[account][locks[account].length - 1];
                locks[account].pop();
            }
        }
    }

    function _swapAndUtilize() private lockTheSwap {
        uint256 liquidityHalfToSwap = collectedForLiquidity / 2;
        uint256 otherLiquidityHalf = collectedForLiquidity - liquidityHalfToSwap;

        _swapTokensForBnb(liquidityHalfToSwap + collectedForDividend);

        uint256 bnbForLiquidity = address(this).balance * liquidityHalfToSwap / (liquidityHalfToSwap + collectedForDividend);
        uint256 bnbForDividend = address(this).balance - bnbForLiquidity;

        _addLiquidity(otherLiquidityHalf, bnbForLiquidity);
        _distributeDividends(bnbForDividend);
    }

    function _swapTokensForBnb(uint256 tokenAmount) private {
        // generate the pancake pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = pancakeRouter.WETH();

        _approve(address(this), address(pancakeRouter), tokenAmount);

        // make the swap
        pancakeRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of BNB
            path,
            address(this),
            block.timestamp
        );
    }

    function _addLiquidity(uint256 tokenAmount, uint256 bnbAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(pancakeRouter), tokenAmount);

        // add the liquidity
        pancakeRouter.addLiquidityETH{value: bnbAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp
        );
    }

    // MODIFIERS

    modifier lockTheSwap {
        _inSwapAndLiquify = true;
        _;
        _inSwapAndLiquify = false;
    }
}

