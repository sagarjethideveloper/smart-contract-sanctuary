pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Pausable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
//import "./interfaces/IInvitationalNFT.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

contract InvitationalNFT is
    ERC721Burnable,
    ERC721Enumerable,
    ERC721URIStorage,
    AccessControl
{
    bytes32 public MINTER_BURNER_ROLE = keccak256("MINTER_BURNER_ROLE");
    bytes32 public VAULT_ROLE = keccak256("VAULT_ROLE");

    string public baseURI;
    //address public vaultContract;
    uint256 private _lastMintedId;
    mapping(string => bool) private hasTokenWithURI;
    mapping(uint256 => bool) private usedIds; //save used token-ids
    //mapping(address => bool) private vaults; //save vaults to give access to funcs
    mapping(address => bool) private haveAGift; //save which user has minted invitational nft cause of it's once
    mapping(uint256 => address) private vaultById; //each nft give access only in current vault instance

    constructor(
        string memory _name,
        string memory _symbol,
        string memory baseURI_,
        address _admin
    ) ERC721(_name, _symbol) {
        baseURI = baseURI_;
        _setupRole(DEFAULT_ADMIN_ROLE, _admin);
        _setupRole(MINTER_BURNER_ROLE, _admin);
    }

    modifier onlyMinterBurner(address sender) {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, sender) ||
                hasRole(MINTER_BURNER_ROLE, sender) ||
                hasRole(VAULT_ROLE, sender),
            "Caller is not in owner or manager or vault role"
        );
        _;
    }

    /**
     * @dev When new vault instance has deployed, neccessary to give a vault role. ROLES: ADMIN
     * @param _contract vault address
     */
    function setVaultAddress(address _contract) external {
        require(
            address(0) != _contract &&
                hasRole(DEFAULT_ADMIN_ROLE, _msgSender()) &&
                !hasRole(VAULT_ROLE, _contract),
            "Wrong params"
        );
        /* if (vaultContract != address(0))
            revokeRole(MINTER_BURNER_ROLE, vaultContract); */
        //vaultContract = _contract;
        //vaults[_contract] = true;
        _setupRole(VAULT_ROLE, _contract);
        //_setupRole(MINTER_BURNER_ROLE, _contract);
    }

    /**
     * @dev Mint nft. ROLES: ADMIN/MINTER-BURNER/VAULT.
     * @param _tokenURI token URI
     * @param mintTo current address to transfer nft
     * @param vault vault-address in which the owner of the token will have an access
     */
    function mint(string calldata _tokenURI, address mintTo, address vault)
        external
        onlyMinterBurner(_msgSender())
    {
        require(!hasTokenWithURI[_tokenURI], "ERC721Main: URI already exists");
        require(mintTo != address(0) && mintTo != address(this) /* && hasRole(VAULT_ROLE, _msgSender()) */, "Wrong addresses");

        uint256 tokenId = _lastMintedId++;
        _safeMint(mintTo, tokenId);

        hasTokenWithURI[_tokenURI] = true;
        _setTokenURI(tokenId, _tokenURI);

        vaultById[tokenId] = vault;
    }

    /**
     * @dev Set token statis: USED. ROLES: VAULT
     * @param _id token id.
     */
    function setUsedId(uint256 _id) external {
        require(hasRole(VAULT_ROLE, _msgSender()) && _exists(_id) && !usedIds[_id], "Wrong params");
        usedIds[_id] = true;
    }

    /**
     * @dev Set user status: HAVE A GIFT. ROLES: VAULT
     * @param user user address.
     */
    function setHaveAGift(address user) external {
        require(hasRole(VAULT_ROLE, _msgSender()) && !haveAGift[user], "Wrong params");
        haveAGift[user] = true;
    }

    /**
     * @dev View function to see token status: (NOT)USED.
     * @param _id token id.
     * @return true/fale
     */
    function getUsedId(uint256 _id) external view returns(bool){
        require(_exists(_id), "Not exist");
        return usedIds[_id];
    }

    /**
     * @dev View function to see user status: (NOT) HAVE A GIFT. (has minted nft or not)
     * @param user user address.
     * @return true/fale
     */
    function getHaveAGift(address user) external view returns(bool){
        return haveAGift[user];
    }

    /**
     * @dev View function to see token status: (NOT)USED.
     * @param _id token id.
     * @return address of vault in which token give an access
     */
    function getVaultById(uint256 _id) external view returns(address){
        require(_exists(_id), "Not exist");
        return vaultById[_id];
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, ERC721Enumerable, AccessControl)
        returns (bool)
    {
        return
            ERC721.supportsInterface(interfaceId) ||
            ERC721Enumerable.supportsInterface(interfaceId) ||
            AccessControl.supportsInterface(interfaceId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return ERC721URIStorage.tokenURI(tokenId);
    }

    /**
     * @dev View function to see contarct status: (NOT)VAULT.
     * @param _contract vault address.
     * @return true/false
     */
    function isVault(address _contract) public view returns(bool){
        return hasRole(VAULT_ROLE, _contract);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721, ERC721Enumerable) {
        ERC721._beforeTokenTransfer(from, to, tokenId);
        ERC721Enumerable._beforeTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId)
        internal
        virtual
        override(ERC721, ERC721URIStorage)
    {
        hasTokenWithURI[tokenURI(tokenId)] = false;
        ERC721URIStorage._burn(tokenId);
    }

    function _transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        revert("Token isn't transferable");
    }
}

