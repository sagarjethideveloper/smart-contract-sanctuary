pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./interface/ICoreBase.sol";
import "./interface/ICOREplug.sol";
import "./interface/IRouter.sol";

import "hardhat/console.sol";

abstract contract CoreBase is ICoreBase {
    struct Bet {
        uint256 conditionID;
        uint256 amount;
        uint64 rate;
        uint128 outcome;
        bool payed;
    }

    struct Condition {
        uint64 coreType; // type of service core type
        uint64 timestamp; // after this time user cant put bet on condition
        uint128 outcomeWin;
        uint256 reinforcement;
        uint256 marginality;
        uint256[] fundBank;
        uint256[] prizing;
        string ipfsHash;
    }

    IRouter public router;

    uint256 public constant DECIMALS = 10**9;

    address public oracle;
    uint256 public conditionalReinforcementFix; // should be 20k
    uint256 public conditionalMarginality;

    mapping(uint256 => Condition) public conditions;
    mapping(uint256 => Bet) public bets; // tokenID -> BET

    // CoreTypeID => COREplugin
    mapping(uint64 => address) coreTypetoCORE;

    event ConditionCreated(uint256 conditionID, uint256 timestamp);
    event ConditionResolved(
        uint256 conditionID,
        uint128 outcomeWin,
        uint256 amountForLP
    );

    function _putBet(
        uint256 conditionID,
        uint256 amount,
        uint256 outcomeWin,
        uint256 minRate,
        uint256 tokenId
    )
        internal
        returns (
            uint256,
            uint256,
            uint256[] memory
        )
    {
        //console.log("bet 05 02 01", gasleft());
        Condition storage condition = conditions[conditionID];
        //console.log("bet 05 02 02", gasleft());
        require(block.timestamp < condition.timestamp, "time exceeded");
        //console.log("bet 05 02 03", gasleft());
        require(
            ICOREplug(coreTypetoCORE[condition.coreType]).checkFunds(
                condition.fundBank
            ),
            "Big difference"
        );
        //console.log("bet 05 02 04", gasleft());

        require(
            ICOREplug(coreTypetoCORE[condition.coreType]).checkOutcome(
                outcomeWin
            ),
            "Wrong outcome"
        );
        //console.log("bet 05 02 05", gasleft());

        uint256 rate = ICOREplug(coreTypetoCORE[condition.coreType])
            .calculateOdds(
                amount,
                outcomeWin,
                condition.fundBank,
                condition.marginality,
                DECIMALS
            ); // 23019 gas
        //console.log("bet 05 02 06", gasleft());

        require(rate >= minRate, "rate too small");
        //console.log("bet 05 02 07", gasleft());

        //console.log("bet 05 02 08", gasleft());

        bets[tokenId] = Bet({
            conditionID: conditionID,
            amount: amount,
            rate: uint64(rate),
            outcome: uint128(outcomeWin),
            payed: false
        });
        //console.log("bet 05 02 12", gasleft());

        condition.fundBank[outcomeWin - 1] =
            condition.fundBank[outcomeWin - 1] +
            amount; // 4334 gas
        //console.log("bet 05 02 13", gasleft());
        condition.prizing[outcomeWin - 1] += (rate * amount) / DECIMALS; // 25348 gas
        //console.log("bet 05 02 14", gasleft());
        return (tokenId, rate, condition.fundBank);
    }

    function _resolvePrize(uint256 tokenId)
        internal
        returns (bool success, uint256 amount)
    {
        Bet storage currentBet = bets[tokenId];

        Condition memory condition = conditions[currentBet.conditionID];

        require(condition.outcomeWin != 0, "Event not happened yet");

        (success, amount) = _viewPrize(tokenId);

        currentBet.payed = (success && amount > 0);

        return (success, amount);
    }

    function viewPrize(uint256 tokenId)
        external
        view
        override
        returns (bool success, uint256 amount)
    {
        return (_viewPrize(tokenId));
    }

    function getconditions(uint256 id)
        external
        view
        returns (Condition memory)
    {
        return (conditions[id]);
    }

    /**
     * internal view, used resolve prize and ezternal views
     * @param tokenId - NFT token id
     */

    function _viewPrize(uint256 tokenId)
        internal
        view
        returns (bool success, uint256 amount)
    {
        (success, amount) = ICOREplug(
            coreTypetoCORE[conditions[bets[tokenId].conditionID].coreType]
        ).viewPrize(
                bets[tokenId].payed,
                bets[tokenId].outcome,
                bets[tokenId].rate,
                bets[tokenId].amount,
                conditions[bets[tokenId].conditionID].outcomeWin,
                DECIMALS
            );
    }

    function calculateOdds(
        uint256 conditionID,
        uint256 amount,
        uint256 outcomeWin
    ) external view returns (uint256) {
        return
            ICOREplug(coreTypetoCORE[conditions[conditionID].coreType])
                .calculateOdds(
                    amount,
                    outcomeWin,
                    conditions[conditionID].fundBank,
                    conditions[conditionID].marginality,
                    DECIMALS
                );
    }
}

