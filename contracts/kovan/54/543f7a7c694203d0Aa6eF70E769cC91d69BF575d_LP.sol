pragma solidity ^0.8.0;

import "@uniswap/lib/contracts/libraries/TransferHelper.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./interface/IAzuroBet.sol";
import "./interface/ILP.sol";

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "./CoreBase.sol";

//import "hardhat/console.sol";

contract LP is ERC20Upgradeable, OwnableUpgradeable, CoreBase, ILP {
    using SafeMath for uint256;

    uint256 public totalReserve;
    uint256 public lockedReserve; // pure reserve
    uint256 public BettingPending; // reserve amounts + bets
    uint256 public reinforcementAbility; // should be 50%
    uint256 public rateDecimals;

    /**
     * @dev init timestamp to work by 7 day intervals
     * LP modes: 1 - in/out | 2 - core
     * LP starts from mode 1
     */
    uint256 public initStartDate;

    /*periods before last change*/
    uint256 public savedPeriods;

    /**
     * @dev request is a structure for calc total LP withdraw requests value
     * @dev walletRequest is a structure for calc LP withdraw request values of some wallet
     */
    struct request {
        mapping(address => uint256) request;
        uint256 totalValue;
    }

    /**
     * @dev requests by period numbers, all previouse numbers can be deleted
     */
    mapping(uint256 => request) public requests;

    /**
     * @dev event NewBet created on new bet apeared
     * owner - message sender
     * betid - bet ID
     * conditionId - condition id
     * outcomeId - 1 or 2
     * amount - bet amount in payment tokens
     * rate - kef in decimals 10^9
     * fund1 - funds on 1st outcome
     * fund2 - funds on 2nd outcome
     */
    event NewBet(
        address indexed owner,
        uint256 indexed betId,
        uint256 indexed conditionId,
        uint256 outcomeId,
        uint256 amount,
        uint256 rate,
        uint256[] funds
    );

    event BetterWin(address better, uint256 amount);
    event LiquidityAdded(address account, uint256 amount);
    event LiquidityRemoved(address account, uint256 amount);

    modifier ensure(uint256 deadline) {
        require(deadline >= block.timestamp, "EXPIRED");
        _;
    }

    modifier onlyOwnerByRouter(address owner) {
        require(
            address(router) == msg.sender &&
                owner == router.getLPowner(address(this)),
            "only owner by router"
        );
        _;
    }

    modifier onlyRouter() {
        require(address(router) == msg.sender, "only owner");
        _;
    }

    /**
     * @dev mode check exact mode
     * @param isFirstMode - true - first mode, false - second mode
     */
    modifier mode(bool isFirstMode) {
        //require(getMode() == isFirstMode, "mode!");
        _;
    }

    /**
     * @dev init function
     */

    function initialize(
        address _router,
        uint256 _reinforcement,
        uint256 _marginality,
        address _owner
    ) external override initializer {
        __ERC20_init("BTTS LP tolen", "lp-BTTS");
        __Ownable_init();
        transferOwnership(_owner);
        router = IRouter(_router);
        rateDecimals = 1000000000;
        reinforcementAbility = rateDecimals / 2; // 50%
        conditionalReinforcementFix = _reinforcement; // in token decimals
        conditionalMarginality = _marginality; // in decimals ^9
    }

    /**
     * @dev add some liquidity and get LP tokens in return
     * @param _amount - token's amount
     */
    function addLiquidity(address sender, uint256 _amount)
        external
        override
        onlyRouter
        mode(true)
    {
        require(_amount > 0);
        _mint(
            sender,
            totalReserve == 0
                ? _amount
                : (_amount * totalSupply()) / totalReserve
        );
        totalReserve += _amount;
        emit LiquidityAdded(sender, _amount);
    }

    /**
     * @dev makes withdraw liquidity request, inc value for some wallet and inc total requests value
     * request links to next allowed period number, if now 1 period, withdraw at 3 period, if now 2, withdraw at 3
     * can be withdrawn next allowed period number
     * @param valueLP LP tokens amount for withdraw liquidity
     */

    function liquidityRequest(address sender, uint256 valueLP)
        external
        override
        onlyRouter
    {
        require(
            requests[_getPeriod()].request[sender] + valueLP <=
                balanceOf(sender)
        );
        //make withdrawal request in next+1 period
        requests[_getPeriod() + 2].request[sender] += valueLP;
        requests[_getPeriod() + 2].totalValue += valueLP;
    }

    /**
     * @dev withdraw back liquidity burning LP tokens
     * only according by previouse requests
     * @param valueLP - LP tokens amount to burn
     */
    function withdrawLiquidity(address sender, uint256 valueLP)
        external
        override
        mode(true)
    {
        require(
            valueLP <= requests[_getPeriod()].request[sender] &&
                valueLP <= requests[_getPeriod()].totalValue
        );

        uint256 withdrawValue = (valueLP * totalReserve) / totalSupply();

        _burn(sender, valueLP);

        TransferHelper.safeTransfer(getPaymentToken(), sender, withdrawValue);

        require(withdrawValue <= totalReserve);
        totalReserve -= withdrawValue;

        requests[_getPeriod()].request[sender] -= valueLP;
        requests[_getPeriod()].totalValue -= valueLP;

        emit LiquidityRemoved(sender, withdrawValue);
    }

    function withdrawPrize(address sender, uint256 tokenId)
        external
        override
        mode(false)
    {
        require(
            IAzuroBet(router.getAzuroToken()).ownerOftoken(tokenId) == sender,
            "not owner"
        );
        (bool success, uint256 amount) = _resolvePrize(tokenId);
        require(success, "No win no prize");
        BettingPending = BettingPending.sub(amount);
        TransferHelper.safeTransfer(getPaymentToken(), sender, amount);
        emit BetterWin(sender, amount);
    }

    function bet(
        address sender,
        uint256 conditionID,
        uint256 amount,
        uint256 outcomeID,
        uint256 deadline,
        uint256 minRate,
        uint256 tokenId
    ) external override ensure(deadline) onlyRouter returns (uint256) {
        //console.log("bet 05 01", gasleft());
        BettingPending = BettingPending.add(amount); //5872 gas
        //console.log("bet 05 02", gasleft());
        (, uint256 rate, uint256[] memory funds) = _putBet(
            conditionID,
            amount,
            outcomeID,
            minRate,
            tokenId
        ); //220585 gas
        //console.log("bet 05 03", gasleft());
        emit NewBet(
            sender,
            tokenId,
            conditionID,
            outcomeID,
            amount,
            rate,
            funds
        ); // 4890 gas
        //console.log("bet 05 04", gasleft());
        return tokenId;
    }

    function resolveConditionTest(uint256 conditionID_, uint128 outcomeWin_)
        external
        override
        mode(false)
    {
        Condition storage condition = conditions[conditionID_];
        require(block.timestamp > condition.timestamp, "timelimit");
        require(condition.outcomeWin == 0, "already set");
        condition.outcomeWin = outcomeWin_;
        uint256 bettersPrizing = _calcPrizing(
            condition.coreType,
            condition.prizing,
            condition.outcomeWin
        );
        uint256 profitReserve = (condition.fundBank[0] +
            condition.fundBank[1]) - bettersPrizing;
        _addReserve(condition.reinforcement, profitReserve);
        emit ConditionResolved(conditionID_, outcomeWin_, profitReserve);
    }

    function _calcPrizing(
        uint64 _coreType,
        uint256[] memory prizing,
        uint256 outcomeWin
    ) internal view returns (uint256) {
        return
            ICOREplug(coreTypetoCORE[_coreType]).calcPrizing(
                prizing,
                outcomeWin
            );
    }

    function _addReserve(uint256 initReserve, uint256 profitReserve)
        internal
        mode(false)
    {
        if (profitReserve >= initReserve) {
            totalReserve = totalReserve.add(profitReserve - initReserve); // pool win
        } else {
            totalReserve = totalReserve.sub(initReserve - profitReserve); // pool lose
        }
        BettingPending = BettingPending.sub(profitReserve);
        lockedReserve = lockedReserve.sub(initReserve);
    }

    function createConditionTest(
        uint256 oracleConditionID,
        uint256 _coreType,
        uint256[] calldata rates,
        uint256 timestamp,
        string memory ipfsHash
    ) external override onlyRouter mode(true) {
        require(timestamp < phase2end(), "phase issue");
        require(
            getPossibilityOfReinforcement(conditionalReinforcementFix),
            "Not enough liquidity"
        );

        Condition storage newCondition = conditions[oracleConditionID];
        require(newCondition.timestamp == 0, "condition exists");
        newCondition.coreType = uint64(_coreType);

        newCondition.fundBank = _calcFundBank(
            newCondition.coreType,
            rates,
            conditionalReinforcementFix
        );
        newCondition.reinforcement = conditionalReinforcementFix;
        newCondition.timestamp = uint64(timestamp);
        newCondition.ipfsHash = ipfsHash;
        // set prizing array dimensions according rates
        newCondition.prizing = new uint256[](rates.length);
        _lockReserve(conditionalReinforcementFix);

        // save new condition link
        newCondition.marginality = conditionalMarginality; //not used yet
        emit ConditionCreated(oracleConditionID, timestamp);
    }

    function _calcFundBank(
        uint64 _coreType,
        uint256[] memory _rates,
        uint256 _conditionalReinforcementFix
    ) internal view returns (uint256[] memory) {
        return
            ICOREplug(coreTypetoCORE[_coreType]).calcFundBank(
                _rates,
                _conditionalReinforcementFix
            );
    }

    // reserve some reinforcement
    function _lockReserve(uint256 amount) internal mode(true) {
        lockedReserve = lockedReserve.add(amount);
        BettingPending = BettingPending.add(amount);
        require(lockedReserve < totalReserve);
    }

    function setCoreType(uint64 coreType, address core)
        external
        override
        onlyRouter
    {
        coreTypetoCORE[coreType] = core;
    }

    /**
     * @dev setStartDate set/reset LP pool start date, 1st mode start from exact time
     *      2nd mode start from exact time by setting 1st mode for previouse PERIOD
     * @param newStartDate starting period
     * @param isModeOne true to set first mode
     */
    function setStartDate(uint256 newStartDate, bool isModeOne)
        public
        onlyOwner
    {
        require(initStartDate == 0);
        initStartDate = isModeOne ? newStartDate : newStartDate - getPERIOD();
    }

    /**
     * @dev change period length starting from next period, must called from Router, period length set in router
     * @param owner new PERIOD length
     */

    function changePeriod(address owner)
        external
        override
        onlyOwnerByRouter(owner)
    {
        require(
            initStartDate > 0 && block.timestamp >= initStartDate + getPERIOD()
        );
        // temporary vars needed to clearly set new values
        uint256 newinitStartDate = initStartDate +
            (_getPeriod() - savedPeriods + 1) *
            getPERIOD();
        uint256 newsavedPeriods = _getPeriod();

        initStartDate = newinitStartDate;
        savedPeriods = newsavedPeriods;
    }

    /**
     * @dev get period number from very start
     */

    function _getPeriod() internal view returns (uint256) {
        return (
            block.timestamp <= initStartDate
                ? savedPeriods
                : (block.timestamp - initStartDate) /
                    getPERIOD() +
                    savedPeriods +
                    (savedPeriods > 0 ? 1 : 0)
        );
    }

    /**
     * @dev get phase 2 end date for (condition create condition)
     */

    function phase2end() public view returns (uint256) {
        return
            initStartDate +
            (_getPeriod() - savedPeriods) *
            getPERIOD() +
            getPERIOD() *
            2;
    }

    // reserve some reinforcement
    function getReserve() external view override returns (uint256 reserve) {
        return totalReserve;
    }

    function getSupply() external view override returns (uint256) {
        return totalSupply();
    }

    function getPossibilityOfReinforcement(uint256 reinforcementAmount)
        internal
        view
        returns (bool status)
    {
        return (lockedReserve + reinforcementAmount <=
            (reinforcementAbility * totalReserve) / rateDecimals);
    }

    /**
     * @dev geting wallet liquidity according lp token requests
     * @param wallet personal wallet
     * @return total liquidity in requests
     * @return personal liquidity to withdraw in personal requests
     */

    function getLiquidityRequests(address wallet)
        external
        view
        override
        returns (uint256 total, uint256 personal)
    {
        personal = requests[_getPeriod() + 2].request[wallet];
        total = requests[_getPeriod() + 2].totalValue;
    }

    function getPaymentToken() internal view returns (address paymentToken) {
        return router.getLPpaymentToken(address(this));
    }

    function getPERIOD() internal view returns (uint256) {
        return router.getLPperiod(address(this));
    }

    function PERIOD() external view override returns (uint256) {
        return getPERIOD();
    }
}

