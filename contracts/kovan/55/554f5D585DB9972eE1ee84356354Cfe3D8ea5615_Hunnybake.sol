pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./DividendTracker.sol";
import "./PancakeSwapInterfaces.sol";


contract Hunnybake is ERC20, DividendTracker, Ownable {

    IPancakeRouter02 public immutable pancakeRouter;
    address public immutable pancakePair;

    uint256 public amountToSwap;
    uint256 public dividendFee;
    uint256 public liquidityFee;

    bool private _inSwapAndLiquify;
    uint256 private _collectedForLiquidity;

    constructor(
        IPancakeRouter02 pancakeRouter_,
        uint256 amountToSwap_,
        uint256 dividendFee_,
        uint256 liquidityFee_
    ) ERC20("Hunnybake", "HBAKE") DividendTracker(1000 * 10**18) Ownable() {

        pancakeRouter = pancakeRouter_;
        amountToSwap = amountToSwap_;
        dividendFee = dividendFee_;
        liquidityFee = liquidityFee_;

        excludedFromDividend[address(this)] = true;

        uint256 supply = 10**8 * 10**18;
        _mint(msg.sender, supply);
        _mintShares(msg.sender, supply);

        pancakePair = IPancakeFactory(pancakeRouter_.factory()).createPair(
            address(this),
            pancakeRouter_.WETH()
        );
    }

    // PUBLIC FUNCTIONS

    receive() external payable {}

    // RESTRICTED FUNCTIONS

    function setAmountToSwap(uint256 amountToSwap_) external onlyOwner {
        amountToSwap = amountToSwap_;
    }

    function setDividendFee(uint256 dividendFee_) external onlyOwner {
        require(liquidityFee + dividendFee_ <= 100, "Total fees shouldn't exceed 100%");
        dividendFee = dividendFee_;
    }

    function setLiquidityFee(uint256 liquidityFee_) external onlyOwner {
        require(dividendFee + liquidityFee_ <= 100, "Total fees shouldn't exceed 100%");
        liquidityFee = liquidityFee_;
    }

    function setExcludedFromDividend(address account, bool excluded) external onlyOwner {
        require(excluded != excludedFromDividend[account], "Should change exclusion");
        excludedFromDividend[account] = excluded;
        _setBalance(account, balanceOf(account));
    }

    // INTERNAL FUNCTIONS

    function _sendDividend(address to, uint256 amount) internal override {
        _transfer(address(this), to, amount);
    }

    function _transfer(address from, address to, uint256 value) internal override {
        if (from != address(this) && from != owner()) {
            uint256 liquidityFeeAmount = value * liquidityFee / 100;
            uint256 dividendFeeAmount = value * dividendFee / 100;

            super._transfer(from, address(this), liquidityFeeAmount + dividendFeeAmount);
            super._transfer(from, to, value - liquidityFeeAmount - dividendFeeAmount);
            _setBalance(from, balanceOf(from));
            _setBalance(to, balanceOf(to));

            _distributeDividends(dividendFeeAmount);
            _collectedForLiquidity += liquidityFeeAmount;
            if (!_inSwapAndLiquify && _collectedForLiquidity >= amountToSwap) {
                _swapAndLiquify();
            }
        } else {
            super._transfer(from, to, value);
        }
    }

    function _swapAndLiquify() private lockTheSwap {
        uint256 half = _collectedForLiquidity / 2;
        uint256 otherHalf = _collectedForLiquidity - half;
        _collectedForLiquidity = 0;
        _swapTokensForBnb(half);
        _addLiquidity(otherHalf, address(this).balance);
    }

    function _swapTokensForBnb(uint256 tokenAmount) private {
        // generate the pancake pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = pancakeRouter.WETH();

        _approve(address(this), address(pancakeRouter), tokenAmount);

        // make the swap
        pancakeRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of BNB
            path,
            address(this),
            block.timestamp
        );
    }

    function _addLiquidity(uint256 tokenAmount, uint256 bnbAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(pancakeRouter), tokenAmount);

        // add the liquidity
        pancakeRouter.addLiquidityETH{value: bnbAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp
        );
    }

    // MODIFIERS

    modifier lockTheSwap {
        _inSwapAndLiquify = true;
        _;
        _inSwapAndLiquify = false;
    }
}

