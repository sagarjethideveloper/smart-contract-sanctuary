pragma solidity ^0.8.4;

import "@openzeppelin/contracts/utils/math/SafeCast.sol";


abstract contract DividendTracker {
    using SafeCast for uint256;
    using SafeCast for int256;

    uint256 public immutable minBalanceForDividend;

    uint256 constant internal magnitude = 2**128;

    uint256 internal magnifiedDividendPerShare;
    uint256 internal totalShares;

    mapping(address => uint256) internal sharesOf;
    mapping(address => int256) internal magnifiedDividendCorrections;
    mapping(address => uint256) internal withdrawnDividends;

    mapping(address => bool) public excludedFromDividend;

    // EVENTS

    event DividendsDistributed(
        address indexed from,
        uint256 weiAmount
    );

    event DividendWithdrawn(
        address indexed to,
        uint256 weiAmount
    );

    // CONSTRUCTOR

    constructor(uint256 minBalanceForDividend_) {
        minBalanceForDividend = minBalanceForDividend_;
    }

    // PUBLIC FUNCTIONS

    function withdrawDividend() public {
        uint256 _withdrawableDividend = withdrawableDividendOf(msg.sender);
        if (_withdrawableDividend > 0) {
            withdrawnDividends[msg.sender] += _withdrawableDividend;
            emit DividendWithdrawn(msg.sender, _withdrawableDividend);
            _sendDividend(msg.sender, _withdrawableDividend);
        }
    }

    function dividendOf(address _owner) public view returns(uint256) {
        return withdrawableDividendOf(_owner);
    }

    function withdrawableDividendOf(address _owner) public view returns(uint256) {
        return accumulativeDividendOf(_owner) - withdrawnDividends[_owner];
    }

    function withdrawnDividendOf(address _owner) public view returns(uint256) {
        return withdrawnDividends[_owner];
    }

    function accumulativeDividendOf(address _owner) public view returns(uint256) {
        return ((magnifiedDividendPerShare * sharesOf[_owner]).toInt256() + 
            magnifiedDividendCorrections[_owner]).toUint256() / magnitude;
    }

    // INTERNAL FUNCTIONS

    function _distributeDividends(uint256 amount) internal {
        require(totalShares > 0);

        if (amount > 0) {
            magnifiedDividendPerShare += (amount * magnitude) / totalShares;
            emit DividendsDistributed(msg.sender, amount);
        }
    }

    function _setBalance(address account, uint256 balance) internal {
        if (balance < minBalanceForDividend || excludedFromDividend[account]) {
            if (sharesOf[account] != 0) {
                _burnShares(account, sharesOf[account]);
            }       
        } else if (balance < sharesOf[account]) {
            _burnShares(account, sharesOf[account] - balance);
        } else if (balance > sharesOf[account]) {
            _mintShares(account, balance - sharesOf[account]);
        }
    }

    function _mintShares(address account, uint256 amount) internal {
        sharesOf[account] += amount;
        totalShares += amount;
        magnifiedDividendCorrections[account] -= (magnifiedDividendPerShare * amount).toInt256();
    }

    function _burnShares(address account, uint256 amount) internal {
        sharesOf[account] -= amount;
        totalShares -= amount;
        magnifiedDividendCorrections[account] += (magnifiedDividendPerShare * amount).toInt256();
    }

    function _sendDividend(address to, uint256 amount) internal virtual;
}

