    IERC20(allowance_.token).approve(allowance_.spender, allowance_.amount);
  }

  function _setAllowances(AllowanceToSet[] memory allowances_) internal {
    for (uint256 i = 0; i < allowances_.length; i++) {
      _setAllowance(allowances_[i]);
    }
  }
}

