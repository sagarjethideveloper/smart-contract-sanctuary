    function lockGemAndDraw(
        address dsProxy,
        address dsProxyActions, address manager,
        address jug, address gemJoin, address daiJoin, 
        uint cdpId, uint collateralToLock, uint daiToBorrow, bool transferFrom) public {

        IDSProxy(dsProxy).execute(
            dsProxyActions,
            abi.encodeWithSignature("lockGemAndDraw(address,address,address,address,uint256,uint256,uint256,bool)",
                manager, jug, gemJoin, daiJoin, cdpId, collateralToLock, daiToBorrow, transferFrom)
        );

    }


    struct DecodedData{
        uint cdpId;
        address pair;
        uint gemToUse;
        address tokenAddress;
        uint tokenAmountToUseFromUser;
        uint tokensToBuyWithDai;
        address daiTokenAddress;
        uint daiAmountToUseFromUser;
        address router02;
        address dsProxy;
        address dsProxyActions;
        address manager;
        address jug;
        address gemJoin;
        address daiJoin;
        bool transferFrom;
        uint error;
    }

    struct LocalData{
        uint tokenUsedForGem;
        uint daiUsedForGem;
        uint gemMinted; 
        uint remainingToken;
        uint remainingDai;
        address token0;
        address token1;
        uint reserve0;
        uint reserve1;
        uint daiToReturn;
        uint daiToDraw;
        uint gemToLock;
        uint256 daiTotalAmount;
        uint256 daiBeforeBuyTokens;
    }

    // mapping (
    //     address => mapping (
    //         address => bool
    //     )
    // ) public dsProxyCanBeUsedBy; // dsProxy => dsProxyUser => True/False

    function transferFrom(address token, address from, address to, uint256 amount) private {
        if (amount > 0)
            IERC20(token).safeTransferFrom(from, to, amount);
    }

    function swapDaiForExactTokens(
        address daiTokenAddress, address tokenAddress, address router02,
        uint tokensToBuyWithDai, uint daiMaxAmount
        ) internal returns(uint daiUsedToBuyTokens){

        address[] memory path = new address[](2);
        path[0] = daiTokenAddress;
        path[1] = tokenAddress;

        // TODO Define value for amountOutMin
        uint[] memory swappedAmounts = IUniswapV2Router02(router02).swapTokensForExactTokens(
            tokensToBuyWithDai,
            daiMaxAmount,
            path,
            address(this),
            block.timestamp + 3600
        );

        return swappedAmounts[0];

    }

    function approveSpenderForToken(address tokenAddress, address spender, uint256 amount) public{
        // TODO OnlyOwner
        IERC20(tokenAddress).safeApprove(spender, amount);
    }

    function poolDaiAndTokens(
            address tokenAddress, address daiTokenAddress, address router02, 
            uint tokensToPool, uint daiToPool, uint error
        ) internal returns(uint gemMinted, uint remainingToken, uint remainingDai){

        uint256 minTokensToPool = tokensToPool.substractError(error);
        uint256 minDaiToPool = daiToPool.substractError(error);

        (uint tokenUsedForGem, uint daiUsedForGem, uint gemMinted) = 
            IUniswapV2Router02(router02).addLiquidity(
                tokenAddress,
                daiTokenAddress,
                tokensToPool,
                daiToPool,
                minTokensToPool,
                minDaiToPool,
                address(this),
                block.timestamp + 3600
            );

        uint remainingToken = tokensToPool - tokenUsedForGem;
        uint remainingDai = daiToPool - daiUsedForGem;

        return (gemMinted, remainingToken, remainingDai);

    }

    function buyGems(
        address daiTokenAddress, address tokenAddress, address router02,
        uint tokensToBuyWithDai, uint daiTotalAmount, uint tokenAmountToUseFromUser,
        uint error) internal returns(uint gemMinted, uint remainingToken, uint remainingDai){

        uint daiUsedToBuyTokens = swapDaiForExactTokens(
            daiTokenAddress, tokenAddress, router02, tokensToBuyWithDai, daiTotalAmount
        );

        uint256 tokensToPool = tokenAmountToUseFromUser.add(tokensToBuyWithDai);
        uint256 daiToPool = daiTotalAmount.sub(daiUsedToBuyTokens);

        return poolDaiAndTokens(
            tokenAddress, daiTokenAddress, router02, 
            tokensToPool, daiToPool, error
        );

    }

    /**
        This function is called after your contract has received the flash loaned amount
     */
    function executeOperation(
        address[] calldata assets,
        uint256[] calldata amounts,
        uint256[] calldata premiums,
        address initiator,
        bytes calldata params
    )
        external
        override
        returns (bool)
    {

        // TODO Check authorizations.

        DecodedData memory decodedData;
        LocalData memory localData;

        ( decodedData ) = abi.decode(params, (DecodedData));

        // transfer from
        transferFrom(decodedData.tokenAddress, initiator, address(this), decodedData.tokenAmountToUseFromUser); // Needs permission
        transferFrom(decodedData.daiTokenAddress, initiator, address(this), decodedData.daiAmountToUseFromUser); // Needs permission
        transferFrom(decodedData.pair, initiator, address(this), decodedData.gemToUse); // Needs permission

        uint daiLoan = amounts[0];

        localData.daiTotalAmount = decodedData.daiAmountToUseFromUser.add(daiLoan);

        (
            localData.gemMinted, localData.remainingToken, localData.remainingDai
        ) = buyGems(
            decodedData.daiTokenAddress, decodedData.tokenAddress, decodedData.router02,
            decodedData.tokensToBuyWithDai, localData.daiTotalAmount, decodedData.tokenAmountToUseFromUser,
            decodedData.error);

        // Check premiums to be lte 0.09%
        localData.daiToDraw = amounts[0].add(premiums[0]).sub(localData.remainingDai);
        localData.gemToLock = decodedData.gemToUse.add(localData.gemMinted);

        lockGemAndDraw(
            decodedData.dsProxy, decodedData.dsProxyActions, decodedData.manager, 
            decodedData.jug, decodedData.gemJoin, decodedData.daiJoin, 
            decodedData.cdpId, localData.gemToLock, localData.daiToDraw, decodedData.transferFrom
        );

        if (localData.remainingToken>0)
            IERC20(decodedData.tokenAddress).safeTransfer(initiator, localData.remainingToken);
        
        // if (localData.daiToDraw>0)
        //     IERC20(decodedData.daiTokenAddress).safeApprove(address(LENDING_POOL), amounts[0].add(premiums[0]));

        return true;
    }

