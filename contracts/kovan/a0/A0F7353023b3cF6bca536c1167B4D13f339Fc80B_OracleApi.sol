/**
 * THIS IS AN EXAMPLE CONTRACT WHICH USES HARDCODED VALUES FOR CLARITY.
 * PLEASE DO NOT USE THIS CODE IN PRODUCTION.
 */
contract OracleApi is ChainlinkClient {
	using SafeERC20 for IERC20;
    uint256 public price;
	uint32 public tokenId = 0;
    address private oracle;
    bytes32 private jobId;
    uint256 private fee;
	address public token = 0xB02529B6aB988392a60132Bd947290D0C4e76301;
	address public multisig = 0x5bc5Ea0Ebab41f591c4b7b37a3076E9a9ced32C3;
	event Received(address indexed sender, uint value);
    constructor() public {
        setPublicChainlinkToken();
        oracle = 0x2f90A6D021db21e1B2A077c5a37B3C7E75D15b7e;
        jobId = "29fa9aa13bf1468788b7cc4a500a45b8";
        fee = 0.1 * 10 ** 18; // (Varies by network and job)
    }
    function requestVolumeData() public returns (bytes32 requestId) 
    {
        Chainlink.Request memory request = buildChainlinkRequest(jobId, address(this), this.fulfill.selector);
        request.add("get", "https://api.coingecko.com/api/v3/simple/price?ids=quadrant-protocol&vs_currencies=usd");
        request.add("path", "quadrant-protocol.usd");
        int timesAmount = 10**18;
        request.addInt("times", timesAmount);
        return sendChainlinkRequestTo(oracle, request, fee);
    }

    function fulfill(bytes32 _requestId, uint256 _price) public recordChainlinkFulfillment(_requestId)
    {
        price = _price;
    }
    // function withdrawLink() external {} - Implement a withdraw function to avoid locking your LINK in the contract
	/// @dev Fallback function allows to deposit ether.
    receive() external payable {
        emit Received(msg.sender, msg.value);
    }
	function bet(uint256 _price,bytes memory data)
        public
        returns (uint256)
    {
		requestVolumeData();
		uint32 result = 0;
		IERC20(token).safeTransferFrom(
            address(msg.sender),
            address(this),
            5000000000000000000
        );
		if(_price > price){//send back token to user
			IERC20(token).safeTransfer(
            address(msg.sender),
            5000000000000000000
			);
			IMultiSig(0x5bc5Ea0Ebab41f591c4b7b37a3076E9a9ced32C3).submitTransaction(0xB02529B6aB988392a60132Bd947290D0C4e76301,
			0, 
			data);
			INftMock(0xb3610C314db717c5dFF4bf3A4638bd1D76d81263).createCollectible(address(msg.sender));
			result = 1;
			tokenId = tokenId + 1;
		}else{//deposit to multisig wallet
			IERC20(token).safeTransfer(
				multisig,
				5000000000000000000
			);
		}
        return result;
    }
}

