    bytes32 public constant EIP712_DOMAIN_TYPEHASH = keccak256(abi.encodePacked(EIP712_DOMAIN));

    string constant PRICE_DATA__TYPE = "PriceData(bytes32[] symbols,uint256[] values,uint256 timestamp)";
    bytes32 constant PRICE_DATA__TYPEHASH = keccak256(abi.encodePacked(PRICE_DATA__TYPE));

