import "@openzeppelin/contracts/utils/EnumerableSet.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "../utils/Timed.sol";
import "./ControllerRef.sol";
import "./IPoolWrapper.sol";
import "./IPoolBalancer.sol";

contract PoolBalancer is IPoolBalancer, Timed, ControllerRef {

    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeERC20 for IERC20;

    uint256 public WAD = 1e18;

    constructor(address _controller, uint256 _duration) public
        ControllerRef(_controller) 
        Timed(_duration) 
    {
        _initTimed(); // reset window
    }


    /// @notice rebalance dex pool by adjusting artha balance in it
    function rebalance() public override nonContract whenNotPaused {

        require(isTimeEnded(), "Pool Balancer: Can only be called after time has ended");

        for (uint i=0; i<controller().poolWrappersLength(); i++){

            IPoolWrapper pool = IPoolWrapper(controller().poolWrapperAt(i));
            pool.swapForRebalance();

        }
        _incentivize();
        _burnAUSDHeld();
    }


    /// @notice if window has passed, reward caller and reset window
    function _incentivize() internal virtual {
        if (isTimeEnded()) {
            _initTimed(); // reset window
            ausd().mint(msg.sender, controller().balancerRewardAmount());
        }
    }

    /// @notice sets the rebalance incentive frequency
    function setIncentiveFrequency(uint256 _frequency) public override onlyGovernor {
        _setDuration(_frequency);
    }

    /// @notice transfer eth to address, only callable by poolWrappers
    /// @param dst address to transfer to
    /// @param amount amount of eth to transfer
    function transferEth(address dst, uint256 amount) public override onlyPoolWrapperOrGovernor {
        Address.sendValue(payable(dst), amount);
    }

    /// @notice transfer token to address, only callable by poolWrappers
    /// @param token address of token
    /// @param dst address to transfer to
    /// @param amount amount of token to transfer
    function transferToken(address token, address dst, uint256 amount) public override onlyPoolWrapperOrGovernor {
        IERC20(token).safeTransfer(dst, amount);
    }

}

pragma solidity ^0.6.0;

import "@chainlink/contracts/src/v0.6/interfaces/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "./IPriceFeed.sol";

contract ChainlinkOraclePriceFeed is IPriceFeed{
    using SafeMath for uint256;
    uint256 public frequency;

    AggregatorV3Interface internal priceFeed;

    uint256 internal WAD = 10**18;

    uint256 public lastUpdatedTimestamp;
    uint256 public lastUpdatedPrice;
    bytes32 public pool = keccak256("ETH");

    constructor(uint256 _frequency, address _aggregator) public {
        frequency = _frequency;
        priceFeed = AggregatorV3Interface(_aggregator);
        update();
    }


    function priceWithValidity() public view override  returns (uint256, bool){
        return (lastUpdatedPrice, block.timestamp - lastUpdatedTimestamp <= frequency);
    }

    function update() public override {
        (
            , 
            int price,
            ,
            ,
            
        ) = priceFeed.latestRoundData();
        lastUpdatedPrice = uint256(price).mul(WAD).div(10 ** uint256(priceFeed.decimals()));
        lastUpdatedTimestamp = block.timestamp;
    }

    function price() public view  override returns (uint256) {
        require(block.timestamp - lastUpdatedTimestamp <= frequency, "Chainlink Price Feed: Stale Data");
        return lastUpdatedPrice;
    }


}

