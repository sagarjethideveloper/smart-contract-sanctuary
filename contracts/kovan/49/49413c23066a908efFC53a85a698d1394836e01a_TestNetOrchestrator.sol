pragma solidity ^0.6.0;

import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "../controller/AUSDIssuer.sol";
import "../controller/IController.sol";
import "../controller/UniPoolWrapper.sol";
import "../controller/PoolBalancer.sol";
import "../oracle/ChainlinkPriceFeed.sol";

contract TestNetOrchestrator is Ownable {

    address public constant ROUTER = address(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);

    address public constant WETH = address(0xd0A1E359811322d97991E03f863a0C30C2cF029C);

    address public constant CHAINLINK_AGGREGATOR = address(0x9326BFA02ADD2366b30bacB125260Af641031331);

    IUniswapV2Factory public constant UNISWAP_FACTORY =
        IUniswapV2Factory(0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f);
    
    address public ethAUSDPair;

    address public controller;

    function initPairs() public onlyOwner {
        address ausd = address(IController(controller).ausd());
        if(UNISWAP_FACTORY.getPair(ausd, WETH) != address(0)){
            ethAUSDPair = UNISWAP_FACTORY.getPair(ausd, WETH);
            return;
        }
        ethAUSDPair = UNISWAP_FACTORY.createPair(ausd, WETH);
    }

    function initBalancer() public onlyOwner {
        address poolBalancer = address(new PoolBalancer(controller, 2 hours));
        IController(controller).setBalancer(poolBalancer);
    }

    function initPoolWrapper() public onlyOwner {
        address poolWrapper = address(new UniPoolWrapper(controller, ethAUSDPair));
        IController(controller).addPoolWrapper(poolWrapper);
        address priceFeed = address(new ChainlinkOraclePriceFeed(15 minutes, CHAINLINK_AGGREGATOR));
        IController(controller).setPoolPriceFeed(UniPoolWrapper(poolWrapper).pool(), priceFeed);
    }

    function initAUSDIssuer() public onlyOwner {
        address ausdIssuer = address(new AUSDIssuer(WETH, controller));
        IController(controller).setPoolIssuer(AUSDIssuer(ausdIssuer).pool(), ausdIssuer);
    }

    function initController(address _controller) public onlyOwner {
        controller = _controller;
        IController(controller).init();
        IController(controller).grantGovernor(msg.sender);
    }

    function renounceGovernor() public onlyOwner {
        IController(controller).revokeGovernor(address(this));
        renounceOwnership();
    }

}

