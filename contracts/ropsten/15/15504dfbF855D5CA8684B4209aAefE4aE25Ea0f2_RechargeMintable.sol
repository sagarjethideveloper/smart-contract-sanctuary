pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";


contract RechargeBase is IERC20, Ownable {
    struct Utility {
        bool feesFrom;
        bool feesTo;
    }

    string private _name;
    string private _symbol;


    /// @dev DEXs care about slippage and the fee messes with it so
    /// we account for them here to allow trades to go through without
    /// slippage errors.
    mapping (address => bool) private _slippageAware;
    uint256 private _lastSlippageFee;

    /// @dev allows us to enable yield before receiving any tokens
    mapping (address => bool) _yieldEnabled;

    mapping (address => Utility) _utilities;

    mapping (address => uint256) _rOwned;
    mapping (address => uint256) _tOwned;
    uint256 _tOwnedTotal;
    uint256 _tROwnedTotal;

    uint256 _tSupply;
    uint256 _rTotal;
    uint256 _tFeeTotal;

    mapping (address => mapping (address => uint256)) _allowances;

    mapping (address => bool) _isExcluded;
    address[] _excluded;

    constructor (string memory name_, string memory symbol_, uint256 totalSupply_, uint256 rTotal_) {
        _name = name_;
        _symbol = symbol_;
        if (_tSupply != totalSupply_) {
            _tSupply = totalSupply_;
        }

        if (_rTotal != rTotal_) {
            _rTotal = rTotal_;
        }
    }

    function name() public view virtual returns (string memory) {
        return _name;
    }

    function symbol() public view virtual returns (string memory) {
        return _symbol;
    }

    function decimals() public view virtual returns (uint8) {
        return 8;
    }

    function fee() public pure virtual returns (uint256) {
        return 500;
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _tSupply;
    }

    function totalFees() public view returns (uint256) {
        return _tFeeTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        if (_tSupply == 0) {
            return 0;
        }

        uint256 feeSlippage;
        if (_slippageAware[msg.sender]) {
            feeSlippage = _lastSlippageFee;
        }

        if (_tOwned[account] > 0) return _tOwned[account] + feeSlippage;

        return tokenFromReflection(_rOwned[account]) + feeSlippage;
    }

//    function balanceOf(address account) public view virtual override returns (uint256) {
//        return tokenFromReflection(_rOwned[account]);
//    }

    function reflectionBalanceOf(address account) public view virtual returns (uint256) {
        return _rOwned[account];
    }

    function reflectionFromToken(uint256 tAmount, uint256 feePerc) public view virtual returns (uint256) {
        require(tAmount <= _tSupply, "Amount must be less than supply");
        (,uint256 rTransferAmount,,,) = _getValues(tAmount, feePerc);
        return rTransferAmount;
    }

    function tokenFromReflection(uint256 rAmount) public view virtual returns(uint256) {
        require(rAmount <= _rTotal, "Amount must be less than total reflections");
        uint256 currentRate =  _getRate();
        return rAmount / currentRate;
    }

    function transfer(address recipient, uint256 amount) public virtual override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view virtual override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public virtual override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public virtual override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()] - amount);
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender] + addedValue);
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender] - subtractedValue);
        return true;
    }

    function collectsYield(address account) external view virtual returns (bool) {
        return _tOwned[account] == 0 || _yieldEnabled[account];
    }

    function collectYield(bool collect) external virtual {
        if (collect) {
            _enableYield(msg.sender);
        } else {
            _disableYield(msg.sender);
        }
    }

    function isExcludedFromYield(address account) external view returns (bool) {
        return _tOwned[account] > 0;
    }

    function excludeAccountFromYield(address account) external onlyOwner() {
        require(_tOwned[account] == 0, "Account is already excluded");

        _disableYield(account);
    }

    function includeAccountForYield(address account) external onlyOwner() {
        require(_tOwned[account] > 0 || !_yieldEnabled[account], "Account is already included");

        _enableYield(account);
    }

    function addSlippageAwareAddress(address account) external onlyOwner {
        _slippageAware[account] = true;
    }

    function removeSlippageAwareAddress(address account) external onlyOwner {
        _slippageAware[account] = false;
    }

    function updateUtilityContract(address addr, bool feesFrom, bool feesTo) external onlyOwner {
        _utilities[addr] = Utility(feesFrom, feesTo);
    }

    function utilityContract(address addr) external view returns (bool, bool) {
        return (_utilities[addr].feesFrom, _utilities[addr].feesTo);
    }

    function reflect(uint256 tAmount) public {
        address sender = _msgSender();
        require(!_isExcluded[sender], "Excluded addresses cannot call this function");
        (uint256 rAmount,,,,) = _getValues(tAmount, 0);
        _rOwned[sender] = _rOwned[sender] - rAmount;
        _rTotal = _rTotal - rAmount;
        _tFeeTotal = _tFeeTotal - tAmount;
    }

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(address sender, address recipient, uint256 tAmount) private {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        require(tAmount > 0, "Transfer amount must be greater than zero");

        (uint256 rAmount, uint256 rTransferAmount, uint256 rFee, uint256 tTransferAmount, uint256 tFee) = _getValues(tAmount, fee());

        if (_tOwned[sender] > 0) {
            _tOwned[sender] -= tAmount;
            _tOwnedTotal -= tAmount;
            _tROwnedTotal -= rAmount;
        }

        _rOwned[sender] -= rAmount;

        bool deductFee = !_utilities[recipient].feesFrom && !_utilities[sender].feesTo;

        // Disable yield on new accounts or keep static on existing non-yield accounts
        if (!_yieldEnabled[recipient] && _rOwned[recipient] == 0 || _tOwned[recipient] > 0) {
            if (deductFee) {
                _addTokensToStaticBalance(recipient, tTransferAmount, rTransferAmount);
            } else {
                _addTokensToStaticBalance(recipient, tAmount, rAmount);
            }
        }
        _rOwned[recipient] += deductFee ? rTransferAmount : rAmount;

        if (deductFee) {
            _reflectFee(rFee, tFee);
        }

        if (_slippageAware[msg.sender] && deductFee) {
            _lastSlippageFee = tFee;
        }

        emit Transfer(sender, recipient, deductFee ? tTransferAmount : tAmount);
    }

    function _enableYield(address account) internal {
        uint256 tAmount = _tOwned[account];
        uint256 rAmount;

        if (_tSupply > 0) {
            rAmount = reflectionFromToken(tAmount, 0);
        }

        if (_tOwned[account] > 0) {
            _subTokensFromStaticBalance(account, tAmount, rAmount);
            _rOwned[account] = rAmount;
        } else {
            _yieldEnabled[account] = true;
        }
    }

    function _disableYield(address account) internal {
        if(_rOwned[account] > 0) {
            uint256 tAmount = tokenFromReflection(_rOwned[account]);

            _addTokensToStaticBalance(account, tAmount, _rOwned[account]);
        }

        if (_yieldEnabled[account]) {
            _yieldEnabled[account] = false;
        }
    }


    function _addTokensToStaticBalance(address account, uint256 tAmount, uint256 rAmount) internal {
        _tOwned[account] += tAmount;
        _tOwnedTotal += tAmount;
        _tROwnedTotal += rAmount;
    }

    function _subTokensFromStaticBalance(address account, uint256 tAmount, uint256 rAmount) internal {
        _tOwned[account] -= tAmount;
        _tOwnedTotal -= tAmount;
        _tROwnedTotal -= rAmount;
    }

    function _mint(uint256 tokens, uint256 r) internal {
        _tSupply += tokens;
        _rTotal += r;
    }

    function _burn(uint256 tokens, uint256 r) internal {
        _tSupply -= tokens;
        _rTotal -= r;
    }

    function _reflectFee(uint256 rFee, uint256 tFee) private {
        _rTotal -= rFee;
        _tFeeTotal += tFee;
    }

    function _getValues(uint256 tAmount, uint256 feePerc) internal view returns (uint256, uint256, uint256, uint256, uint256) {
        (uint256 tTransferAmount, uint256 tFee) = _getTValues(tAmount, feePerc);
        uint256 currentRate =  _getRate();
        (uint256 rAmount, uint256 rTransferAmount, uint256 rFee) = _getRValues(tAmount, tFee, currentRate);
        return (rAmount, rTransferAmount, rFee, tTransferAmount, tFee);
    }

    function _getTValues(uint256 tAmount, uint256 feePerc) private pure returns (uint256, uint256) {
        uint256 tFee;

        if (feePerc > 0) {
            tFee = tAmount * feePerc / 10000;
        } else {
            tFee = 0;
        }

        uint256 tTransferAmount = tAmount - tFee;
        return (tTransferAmount, tFee);
    }

    function _getRValues(uint256 tAmount, uint256 tFee, uint256 currentRate) private pure returns (uint256, uint256, uint256) {
        uint256 rAmount = tAmount * currentRate;
        uint256 rFee = tFee * currentRate;
        uint256 rTransferAmount = rAmount - rFee;
        return (rAmount, rTransferAmount, rFee);
    }

    function _getRate() private view returns (uint256) {
        uint256 rSupply = _rTotal;
        uint256 tSupply = _tSupply;

        if (_tROwnedTotal > _rTotal || _tOwnedTotal > _tSupply) {
            return _rTotal / _tSupply;
        }

        rSupply -= _tROwnedTotal;
        tSupply -= _tOwnedTotal;

        if (rSupply < _rTotal / _tSupply) {
            return _rTotal / _tSupply;
        }

        return rSupply / tSupply;
    }
}

