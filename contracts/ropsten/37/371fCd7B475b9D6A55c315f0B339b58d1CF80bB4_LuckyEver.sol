library QueueUintsFuns {
    struct QueueUints {
        uint256[] items;
        uint256 head;
        uint256 tail;
        uint256 used;
    }

    function create(QueueUints storage self, uint256 size) internal {
        self.head = 0;
        self.tail = 0;
        self.used = 0;
        self.items = new uint256[](size);
    }

    function append(QueueUints storage self, uint256 item)
        internal
        returns (bool result)
    {
        if (self.used < self.items.length) {
            self.items[self.tail] = item;
            self.tail = uint256((self.tail + 1) % self.items.length);
            self.used++;
            return true;
        }
    }

    function remove(QueueUints storage self)
        internal
        returns (uint256 item, bool result)
    {
        if (self.used > 0) {
            item = self.items[self.head];
            self.head = uint256((self.head + 1) % self.items.length);
            self.used--;
            result = true;
        }
    }
}

library QueueAddressFuns {
    struct QueueAddress {
        address[] items;
        uint256 head;
        uint256 tail;
        uint256 used;
    }

    function create(QueueAddress storage self, uint256 size) internal {
        self.head = 0;
        self.tail = 0;
        self.used = 0;
        self.items = new address[](size);
    }

    function append(QueueAddress storage self, address item)
        internal
        returns (bool result)
    {
        if (self.used < self.items.length) {
            self.items[self.tail] = item;
            self.tail = uint16((self.tail + 1) % self.items.length);
            self.used++;
            return true;
        }
    }

    function remove(QueueAddress storage self)
        internal
        returns (address item, bool result)
    {
        if (self.used > 0) {
            item = self.items[self.head];
            self.head = uint16((self.head + 1) % self.items.length);
            self.used--;
            result = true;
        }
    }
}

contract LuckyEver is ERC20("LUCKYEVER", "LUCKYEVER"), Ownable {
