    uint256 private _previousTaxFee = _taxFee;

    uint256 public _liquidityFee = 3;
    uint256 private _previousLiquidityFee = _liquidityFee;

    uint256 public rewardThreshold = 5*1e16;
    uint256 public threshHoldTopUpRate = 1; // anti-whale
    uint256 public minTokenNumberToSell = (totalSupply() / 10000) / 10;

    bool public swapAndLiquifyEnabled = false;
    bool inSwapAndLiquify;

    QueueAddressFuns.QueueAddress luckyDogs;
    QueueAddressFuns.QueueAddress winners;
    QueueUintsFuns.QueueUints dogsTime;
    uint256 queueLength = 500;

    uint256 public seed = block.timestamp;

    IPancakeRouter02 public immutable pancakeRouter;
    IPancakePair public immutable pancakePair;
