pragma solidity ^0.7.6;

abstract contract GatewayValidators {

    struct Validator {
        address owner;
        uint16 feeShare;
    }

    mapping(uint16 => Validator) validators;
    mapping(address => uint16) public validatorAddresses;

    uint16 public validatorIndex;
    uint8 public totalValidators;

    event ValidatorRegistered(address validator);
    event ValidatorDeregistered(address validator);

    modifier onlyValidator() {
        require(validatorAddresses[msg.sender] > 0, "Validators only");
        _;
    }

    modifier requiresActiveValidators() {
        require(totalValidators > 0, "No active validators");
        _;
    }

    constructor() {
    }

    function _addValidator(address v) internal {
        uint16 validatorID = ++validatorIndex;

        validators[validatorID] = Validator(v, 0);
        validatorAddresses[v] = validatorID;

        totalValidators += 1;

        emit ValidatorRegistered(v);
    }

    /**
    * @dev Remove a validator and lower consensus if higher than total validators
    */
    function _removeValidator(address v) internal {
        delete validatorAddresses[v];
        delete validators[validatorAddresses[v]];

        totalValidators -= 1;

        emit ValidatorDeregistered(v);
    }

    function _getValidator(address v) view internal returns (Validator storage) {
        return validators[validatorAddresses[v]];
    }

    function isValidator(address v) view public returns (bool) {
        return validatorAddresses[v] > 0;
    }
}

