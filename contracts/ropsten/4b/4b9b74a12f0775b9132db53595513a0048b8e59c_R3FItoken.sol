// SPDX-License-Identifier: MIT
pragma solidity ^0.7.6;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";

import "./IReflectERC20.sol";
import "./GatewayValidators.sol";
import "./ConsensusBased.sol";

interface GatewayInterface {
    function transferNativeIn(address targetGw, address to, uint256 amount) external;

    function transferNativeOut(address sendingGw, address to, uint blockNum, uint amount) external;
}

abstract contract Gateway is Ownable, ConsensusBased, GatewayValidators, GatewayInterface {
    using SafeMath for uint256;

    uint256 public lockedTokens;

    uint256 public approvedClaimable;

    mapping(address => uint256) public minimumTransferAmounts;

    bool public paused;

    uint32 public claimableTotalShares;
    uint32 public ownerClaimableShares;
    uint32 public reflectClaimableShares;
    uint32 public validatorsClaimableShares;

    /// @dev total to go towards DAO funds, listings, dev, etc
    uint8 ownerShareWeight;

    /// @dev total to be returned to holders
    uint8 reflectShareWeight;

    /// @dev total to be paid out to validators
    uint8 perValidatorShareWeight;

    IReflectERC20 public token;
    bool tokenSet;

    /// @dev gateways this one is sending too that should deduct fees this side
    mapping(address => bool) deductFeesOnTransferIn;

    /// @dev gateways that sent to this one that should deduct fees this side
    mapping(address => bool) deductFeesOnWithdraw;

    mapping(address => bool) gatewayAddresses;

    /// @dev hash of address + blockNum => amount awaiting votes
    mapping(bytes32 => uint) pendingOut;

    event PausedStateChange(bool p);

    event TransferNativeIn(address gw, address recipient, uint256 bNo , uint amount, bool feesDeducted);
    event TransferNativeOut(address recipient, uint amount, address finalValidator);

    event ReflectToGateway(address gw, uint256 bNo, uint256 amount);
    event ReflectFromGateway(address gw, uint256 amount);


    modifier onlyOwnerOrValidator() {
        require(msg.sender == owner() || validatorAddresses[msg.sender] > 0, "access denied");
        _;
    }

    modifier meetsMinimumTransfer(address targetGw, uint256 amount) {
        require(amount >= minimumTransferAmounts[targetGw], "Minimum requirement not met");
        _;
    }

    modifier ifNotPaused() {
        require(!paused, "Gateway temporarily paused");
        _;
    }

    constructor() {
        _addValidator(msg.sender);
        consensusThreshold = 1;

        ownerShareWeight = 1;
        reflectShareWeight = 1;
        perValidatorShareWeight = 1;
    }


    /**
     @notice Check the number of approvals for an incoming transfer
     @return Number of votes received
    */
    function pendingWithdrawVotes(bytes32 hash) view public returns (uint) {
        Consensus storage _c = ballots[hash];
        return _c.supporting;
    }

    /**
    * @notice verify if validator has voted on transaction already
    */
    function validatorVoted(address to, uint256 blockNum) view public returns (bool) {
        bytes32 hash = keccak256(abi.encodePacked(to, blockNum));
        Consensus storage cons = ballots[hash];
        return cons.votes[msg.sender] == Vote.For;
    }

    /**
    * @notice verify if a transaction has been completed already.
    */
    function transactionCompleted(address to, uint256 blockNum) view public returns (bool) {
        bytes32 hash = keccak256(abi.encodePacked(to, blockNum));
        Consensus storage cons = ballots[hash];
        return _consensusPassed(cons);
    }

    function viewShareWeight() view public returns (uint8, uint8, uint8) {
        return (ownerShareWeight, perValidatorShareWeight, reflectShareWeight);
    }

    /**
    * @notice return the shares and their distribution
    */
    function viewShareDistribution() view public returns (uint256, uint256, uint256, uint256) {
        return (claimableTotalShares, ownerClaimableShares, reflectClaimableShares, validatorsClaimableShares);
    }

    function viewValidatorShare(address v) view public returns (uint256) {
        return validators[validatorAddresses[v]].feeShare;
    }

    function shareValue() view public returns (uint256) {
        uint fees = unclaimedTotalFees();
        return fees > 0 && claimableTotalShares > 0 ? unclaimedTotalFees().div(claimableTotalShares) : 0;
    }

    function valueOfShares(uint256 _shares) view public returns (uint256) {
        uint256 v = shareValue();
        return v.mul(_shares);
    }

    function unclaimedTotalFees() view public returns (uint256) {
        uint256 balance = token.balanceOf(address(this));

        /// @dev on new reflect tokens balanceOf can be incorrect
        if (balance < lockedTokens) {
            return 0;
        }

        uint256 leftover = balance.sub(lockedTokens);
        leftover = leftover.sub(approvedClaimable);

        return leftover;
    }

    /**
    * @notice view the fees available to the owner
    */
    function unclaimedOwnerFees() view public returns (uint256) {
        return valueOfShares(ownerClaimableShares);
    }

    /**
    * @notice amount available to distribute
    */
    function unclaimedReflectFees() view public returns (uint256) {
        return valueOfShares(reflectClaimableShares);
    }

    /**
     * @notice view fees owed to validator `_v`
     */
    function unclaimedValidatorFees(address _v) view public returns (uint256) {
        Validator storage val = validators[validatorAddresses[_v]];
        return valueOfShares(val.feeShare);
    }

    function setToken(address tokenAddr) public onlyOwner {
        require(!tokenSet, "Token already registered.");

        token = IReflectERC20(tokenAddr);
        tokenSet = true;
    }

    /**
    * @notice Owner can adjust how shares are distributed
    */
    function configureShareWeight(uint8 owner, uint8 validator, uint8 reflect) public onlyOwner {
        ownerShareWeight = owner;
        perValidatorShareWeight = validator;
        reflectShareWeight = reflect;
    }

    /**
       @dev Owner sets minimum validations for a transfer
    */
    function changeConsensus(uint8 newC) public onlyOwner {
        require(newC <= totalValidators, "consensus exceeds validators");
        _consensusChange(newC);
    }

    /**
    * @notice allow the owner or validators to adjust minimum transfer amounts depending on gas prices
    */
    function setMinimumTransfer(address targetGw, uint256 min) public onlyOwnerOrValidator {
        minimumTransferAmounts[targetGw] = min;
    }

    function linkGateway(address gw) public onlyOwner {
        gatewayAddresses[gw] = true;
    }

    function unlinkGateway(address gw) public onlyOwner {
        gatewayAddresses[gw] = false;
    }

    /**
    * @notice add new validators to the gateway
    */
    function registerValidator(address v) public onlyOwner {
        require(validatorAddresses[v] == 0, "Validator already exists");

        _addValidator(v);
    }

    /**
    * @notice remove a validator from the gateway, validators can remove themselves
    * owner can remove others
    */
    function deregisterValidator(address v) public onlyOwnerOrValidator {
        require(validatorAddresses[v] > 0, "Address must be a validator");

        if (msg.sender == owner() || msg.sender == v) {
            // reset validator shares before removing
            Validator storage val = _getValidator(v);
            validatorsClaimableShares -= val.feeShare;
            claimableTotalShares -= val.feeShare;

            _removeValidator(v);

            if (totalValidators < consensusThreshold) {
                consensusThreshold -= 1;
            }

            if (totalValidators == 0) {
                setPaused(true);
            }
        }
    }

    /**
    * @dev gateways added here have already applied the fees their side
    */
    function deductFeesOnTransferInToGateway(address gw, bool v) public onlyValidator {
        deductFeesOnTransferIn[gw] = v;
    }

    /**
    * @dev gateways added here need the fees to be distributed this side
    */
    function deductFeesOnWithdrawFromGateway(address gw, bool v) public onlyValidator {
        deductFeesOnWithdraw[gw] = v;
    }

    function setPaused(bool pause) public onlyOwnerOrValidator {
        paused = pause;

        emit PausedStateChange(pause);
    }

    /**
    * @notice Administrative function to reset share counters to zero, allows us to recalculate shares if unfairly balanced.
    */
    function reinitializeShares() public onlyOwner {
        require(totalValidators == 0, "Reset validators first");

        claimableTotalShares = 0;
        ownerClaimableShares = 0;
        reflectClaimableShares = 0;
        validatorsClaimableShares = 0;
    }

    /**
    * @notice Administrative function to give all passive fees on the gateway to the locked token balance.
    */
    function lockupFees() public onlyOwner {
        require(totalValidators == 0, "Reset validators first");
        require(claimableTotalShares == 0, "Reinitialize shares first");

        uint256 fees = unclaimedTotalFees();
        lockedTokens += fees;
    }

    /**
     @dev called by validators who received the TransferNativeIn event from the other gateway.
    */
    function transferNativeOut(address sendingGw, address to, uint blockNum, uint amount) external override onlyValidator ifNotPaused {
        require(lockedTokens >= amount, "Insufficient liquidity");

        bytes32 hash = keccak256(abi.encodePacked(to, blockNum));
        Consensus storage cons = ballots[hash];

        require(!_consensusPassed(cons), "Already approved");
        require(pendingOut[hash] == amount || pendingOut[hash] == 0, "Outgoing transaction amount incorrect.");

        require(cons.votes[msg.sender] == Vote.Absent, "Duplicate vote");

        cons.supporting += 1;
        cons.votes[msg.sender] = Vote.For;

        pendingOut[hash] = amount;

        Validator storage val = _getValidator(msg.sender);

        uint16 feeShare = perValidatorShareWeight;

        if (_consensusPassed(cons)) {
            cons.state = ConsensusState.Passed;

            /// @dev pay validator who handles the transfer extra to cover additional gas
            feeShare += 1;

            lockedTokens = lockedTokens.sub(amount);

            if (deductFeesOnWithdraw[sendingGw]) {
                uint256 fee = amount.mul(5).div(100);
                amount = amount.sub(fee);
                token.reflect(fee);
            }

            delete pendingOut[hash];

            token.transfer(to, amount);

            emit TransferNativeOut(to, amount, msg.sender);
        }

        val.feeShare += feeShare;
        updateShares(feeShare);
    }

    /**
     @notice allow validators to withdraw their share of fees to an address
    */
    function withdrawValidatorFees(address _v) public onlyValidator {
        Validator storage val = validators[validatorAddresses[msg.sender]];
        uint256 fees = unclaimedValidatorFees(msg.sender);
        require(fees > 0 && val.feeShare > 0 && validatorsClaimableShares >= val.feeShare, "No fees to claim");

        validatorsClaimableShares = validatorsClaimableShares - val.feeShare;
        claimableTotalShares = claimableTotalShares - val.feeShare;
        val.feeShare = 0;
        token.transfer(_v, fees);
    }

    /**
     * @notice withdraw `_shares` worth of fees to the contract owner
     */
    function withdrawFunding() public onlyOwner {
        require(ownerClaimableShares > 0, "No fees to claim");
        uint256 fees = unclaimedOwnerFees();
        claimableTotalShares = claimableTotalShares - ownerClaimableShares;
        ownerClaimableShares = 0;

        token.transfer(owner(), fees);
    }

    function reflectToGateway(address gw, uint32 shares) public onlyOwner {
        require(gatewayAddresses[gw], "Address is not linked as gateway");
        require(shares <= reflectClaimableShares, "Insufficient shares");

        uint amount = valueOfShares(shares);

        reflectClaimableShares = reflectClaimableShares - shares;
        claimableTotalShares = claimableTotalShares - shares;

        lockedTokens = lockedTokens.add(amount);

        emit ReflectToGateway(gw, block.number, amount);
    }

    function reflectFromGateway(address gw, uint256 bNo, uint256 amount) public onlyValidator {
        require(amount <= lockedTokens, "Exceeds available liquidity");

        bytes32 hash = keccak256(abi.encodePacked(gw, bNo));
        Consensus storage con = ballots[hash];
        require(!_consensusPassed(con), "Already approved");
        require(pendingOut[hash] == amount || pendingOut[hash] == 0, "Outgoing transaction amount incorrect.");
        require(con.votes[msg.sender] == Vote.Absent, "Duplicate vote");

        con.supporting += 1;
        con.votes[msg.sender] = Vote.For;

        pendingOut[hash] = amount;

        Validator storage val = _getValidator(msg.sender);

        uint16 feeShare = perValidatorShareWeight;

        if (_consensusPassed(con)) {
            con.state = ConsensusState.Passed;

            /// @dev pay validator who handles the transfer extra to cover additional gas
            feeShare += 1;

            lockedTokens = lockedTokens.sub(amount);
            delete pendingOut[hash];

            token.reflect(amount);
            emit ReflectFromGateway(gw, amount);
        }

        val.feeShare = feeShare;
        updateShares(feeShare);
    }

    /**
     * @notice The contract owner can call this to redistribute X _shares (`reflectClaimableShares`) within the same chain.
     */
    function reflectNatively(uint32 shares) public onlyOwner {
        require(shares <= reflectClaimableShares, "Insufficient shares");

        uint256 amount = valueOfShares(shares);

        reflectClaimableShares = reflectClaimableShares - shares;
        claimableTotalShares = claimableTotalShares - shares;

        token.reflect(amount);

        emit ReflectFromGateway(address(this), amount);
    }

    /**
    * @dev update the global shares during a transaction
    */
    function updateShares(uint16 validatorShare) internal {
        validatorsClaimableShares += validatorShare;
        claimableTotalShares += validatorShare + ownerShareWeight + reflectShareWeight;
        ownerClaimableShares += ownerShareWeight;
        reflectClaimableShares += reflectShareWeight;
    }

}

