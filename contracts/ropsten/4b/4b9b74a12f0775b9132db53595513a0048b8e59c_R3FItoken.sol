pragma solidity ^0.7.6;

contract ConsensusBased {

    enum Vote { Absent, For, Against }
    enum ConsensusState { Incomplete, Passed, Rejected }

    /// @dev current consensus threshold
    uint8 consensusThreshold;

    struct Consensus {
        uint8 supporting;
        uint8 against;
        ConsensusState state;

        mapping(address => Vote) votes;
    }

    mapping(bytes32 => Consensus) ballots;

    event ConsensusChanged(uint consensus, uint prev_consensus);

    function viewConsensusThreshold() view public returns (uint) {
        return consensusThreshold;
    }

    function _consensusPassed(Consensus storage c) view internal returns (bool) {
        return c.state == ConsensusState.Passed || c.supporting >= consensusThreshold;
    }

    function _consensusRejected(Consensus storage c) view internal returns (bool) {
        return c.state == ConsensusState.Rejected || c.against >= consensusThreshold;
    }

