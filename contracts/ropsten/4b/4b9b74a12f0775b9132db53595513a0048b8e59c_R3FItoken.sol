// SPDX-License-Identifier: MIT
pragma solidity ^0.7.6;

import "@openzeppelin/contracts/math/SafeMath.sol";
import "./Gateway.sol";

contract PeggedGateway is Gateway {
    using SafeMath for uint256;
    /**
        @dev call after approving the gateway allowance on the token contract.
        @notice transferNativeIn doesn't cost fees, they'll be deducted when withdrawn by the other gateway.
    */
    function transferNativeIn(address targetGw, address to, uint256 amount)
    meetsMinimumTransfer(targetGw, amount)
    requiresActiveValidators
    ifNotPaused
    override public {
        require(token.transferFrom(msg.sender, address(this), amount), "Failed to transfer in");

        lockedTokens = lockedTokens.add(amount);

        emit TransferNativeIn(targetGw, to, block.number, amount, false);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.7.6;

import "@openzeppelin/contracts/math/SafeMath.sol";
import "./Gateway.sol";

/**
* Gateway linked to the original renounced token.
*/
contract EthGateway is Gateway {
    using SafeMath for uint256;
    /**
     *   @dev fees are always applied on the ETH R3FI contract
     */
    function transferNativeIn(address targetGw, address to, uint256 amount)
    meetsMinimumTransfer(targetGw, amount)
    requiresActiveValidators
    ifNotPaused
    override public {
        require(token.transferFrom(msg.sender, address(this), amount), "Failed to transfer in");

        uint256 fees = amount.mul(5).div(100);
        amount = amount.sub(fees);
        lockedTokens = lockedTokens.add(amount);

        emit TransferNativeIn(targetGw, to, block.number, amount, true);
    }
}

