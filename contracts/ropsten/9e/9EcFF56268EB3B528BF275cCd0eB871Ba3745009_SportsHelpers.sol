    function cityExists(string memory city) public view returns (bool) {
        uint length = cities.length;

        for (uint k=0; k<length; ++k) {
          if(keccak256(bytes(cities[k])) == keccak256(bytes(city))) {
            return true;
          }
        }

        return false;
    }

    function sportExists(string memory sport) public view returns (bool) {
        uint length = sports.length;

        for (uint k=0; k<length; ++k) {
          if(keccak256(bytes(sports[k])) == keccak256(bytes(sport))) {
            return true;
          }
        }

        return false;
    }
