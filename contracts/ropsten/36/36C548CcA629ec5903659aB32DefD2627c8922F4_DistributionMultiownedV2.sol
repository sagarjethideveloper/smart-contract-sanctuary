pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableMapUpgradeable.sol";
import "hardhat/console.sol";
/// note: during any ownership changes all pending operations (waiting for more signatures) are cancelled
contract Multiowned is Initializable {
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.UintToAddressMap;

    // FIELDS
    uint256 constant c_maxOwners = 16;
    uint256 constant maxOperationsPerOwner = 5;
    // the number of owners that must confirm the same operation before it is run.
    uint256 public m_multiOwnedRequired;
    // pointer used to find a free slot in m_owners
    uint256 public m_numOwners;
    EnumerableMapUpgradeable.UintToAddressMap private m_owners;
    // index on the list of owners to allow reverse lookup: owner address => index in m_owners
    mapping(address => uint256) internal m_ownerIndex;
    mapping(address => uint256) public signedOperations; // how much operations the signer already signed
    // the ongoing operations.
    mapping(bytes32 => MultiOwnedOperationPendingState) internal m_multiOwnedPending;
    bytes32[] internal m_multiOwnedPendingIndex;
    mapping(address => bytes32[]) operations;
    address[] activeSigners;
    // TYPES
    // struct for the status of a pending operation.
    struct MultiOwnedOperationPendingState {
        // count of confirmations needed
        uint256 yetNeeded;
        // bitmap of confirmations where owner #ownerIndex's decision corresponds to 2**ownerIndex bit
        uint256 ownersDone;
        // position of this operation key in m_multiOwnedPendingIndex
        uint256 index;
        address[] signers;
    }

    // EVENTS
    event Confirmation(address owner, bytes32 operation);
    event Revoke(address owner, bytes32 operation);
    event FinalConfirmation(address owner, bytes32 operation);
    // some others are in the case of an owner changing.
    event OwnerChanged(address oldOwner, address newOwner);
    event OwnerAdded(address newOwner);
    event OwnerRemoved(address oldOwner);
    // the last one is emitted if the required signatures change
    event RequirementChanged(uint256 newRequirement);

    // MODIFIERS
    // simple single-sig function modifier.
    modifier onlyowner {
        require(isOwner(msg.sender), "Multiowned: A sender should be an owner");
        _;
    }

    // multi-sig function modifier: the operation must have an intrinsic hash in order
    // that later attempts can be realised as the same underlying operation and
    // thus count as confirmations.
    modifier onlymanyowners(bytes32 _operation) {
        if (confirmAndCheck(_operation)) {
          _;
        }
        // Even if required number of confirmations has't been collected yet,
        // we can't throw here - because changes to the state have to be preserved.
        // But, confirmAndCheck itself will throw in case sender is not an owner.
    }

    modifier validNumOwners(uint256 _numOwners) {
        require(_numOwners > 1 && _numOwners <= c_maxOwners, "Multiowned: Invalid number of owners");
        _;
    }

    modifier multiOwnedValidRequirement(uint256 _required, uint256 _numOwners) {
        require(_required > 0 && _required <= _numOwners, "Multiowned: Invalid required number of owners");
        _;
    }

    modifier ownerExists(address _address) {
        require(isOwner(_address), "Multiowned: The owner does not exist");
        _;
    }

    modifier ownerDoesNotExist(address _address) {
        require(!isOwner(_address), "Multiowned: The owner exists");
        _;
    }

    modifier multiOwnedOperationIsActive(bytes32 _operation) {
        require(isOperationActive(_operation), "Multiowned: The operation is not active");
        _;
    }

  // METHODS

    function __Multiowned_init(address[] memory _owners, uint256 _required)
    public
    validNumOwners(_owners.length)
    multiOwnedValidRequirement(_required, _owners.length)
    initializer 
    {
        require(c_maxOwners <= 255, "Multiowned: max owners can not be more then 255");

        m_numOwners = _owners.length;
        m_multiOwnedRequired = _required;

        for (uint256 i = 0; i < _owners.length; ++i) {
            address owner = _owners[i];
            // invalid and duplicate addresses are not allowed
            require(address(0) != owner && !isOwner(owner), "Gath3r: not isOwner yet!");

            uint256 currentOwnerIndex = checkOwnerIndex(i + 1 /* first slot is unused */);
            m_owners.set(currentOwnerIndex, owner);
            m_ownerIndex[owner] = currentOwnerIndex;
        }
        assertOwnersAreConsistent();
    }
  

    /* 
    @notice replaces an owner `_from` with another `_to`.
    @param _from address of owner to replace
    @param _to address of new owner
    All pending operations will be canceled!
    */
    function changeOwner(address _from, address _to)
    external
    ownerExists(_from)
    ownerDoesNotExist(_to)
    onlymanyowners(keccak256(msg.data))
    {
        assertOwnersAreConsistent();
        clearPending();

        uint256 ownerIndex = checkOwnerIndex(m_ownerIndex[_from]);
        m_owners.set(ownerIndex, _to);
        m_ownerIndex[_from] = 0;
        m_ownerIndex[_to] = ownerIndex;

        assertOwnersAreConsistent();
        emit OwnerChanged(_from, _to);
    }

    /*
    @notice adds an owner
    @param _owner address of new owner
    All pending operations will be canceled!
    */
    function addOwner(address _owner)
    external
    ownerDoesNotExist(_owner)
    validNumOwners(m_numOwners + 1)
    onlymanyowners(keccak256(msg.data))
    {
        assertOwnersAreConsistent();
        clearPending();
        m_numOwners++;
        m_owners.set(m_numOwners, _owner);
        m_ownerIndex[_owner] = checkOwnerIndex(m_numOwners);

        assertOwnersAreConsistent();
        emit OwnerAdded(_owner);
    }

    /*
    @notice removes an owner
    @param _owner address of owner to remove
    All pending operations will be canceled!
    */
    function removeOwner(address _owner)
    external
    ownerExists(_owner)
    validNumOwners(m_numOwners - 1)
    multiOwnedValidRequirement(m_multiOwnedRequired, m_numOwners - 1)
    onlymanyowners(keccak256(msg.data))
    {
        assertOwnersAreConsistent();
        clearPending();

        uint256 ownerIndex = checkOwnerIndex(m_ownerIndex[_owner]);
        m_owners.remove(ownerIndex);
        m_ownerIndex[_owner] = 0;
        
        if(ownerIndex != m_numOwners){
            m_owners.set(ownerIndex, m_owners.get(m_numOwners));
            m_owners.set(m_numOwners, address(0));
        } else {
            m_owners.set(ownerIndex, address(0));
        }
        m_numOwners--;
        m_ownerIndex[m_owners.get(ownerIndex)] = ownerIndex;
        assertOwnersAreConsistent();
        emit OwnerRemoved(_owner);
    }

    /* 
    @notice changes the required number of owner signatures
    @param _newRequired new number of signatures required
    All pending operations will be canceled!
    */
    function changeRequirement(uint256 _newRequired)
    external
    multiOwnedValidRequirement(_newRequired, m_numOwners)
    onlymanyowners(keccak256(msg.data))
    {
        m_multiOwnedRequired = _newRequired;
        clearPending();

        emit RequirementChanged(_newRequired);
    }

    /*
    @notice Gets an owner by 0-indexed position
    @param ownerIndex 0-indexed owner position
    */
    function getOwner(uint256 ownerIndex) public view returns (address) {
        (, address addr) = m_owners.tryGet(ownerIndex + 1);
        return addr;
    }

    /*
    @notice Gets owners
    @return memory array of owners
    */
    function getOwners() public view returns (address[] memory) {
        address[] memory result = new address[](m_numOwners);
        for (uint256 i = 0; i < m_numOwners; i++)
            result[i] = getOwner(i);
        return result;
    }

    /*
    @notice checks if provided address is an owner address
    @param _addr address to check
    @return true if it's an owner
    */
    function isOwner(address _addr) public view returns (bool) {
        return m_ownerIndex[_addr] > 0;
    }

    /*
    @notice Tests ownership of the current caller.
    @return true if it's an owner
    
    It's advisable to call it by new owner to make sure that
    the same erroneous address is not copy-pasted to
    addOwner/changeOwner and to isOwner.
    */
    function amIOwner() external view onlyowner returns (bool) {
        return true;
    }

    // INTERNAL METHODS
    function _checkAndAddActiveSinger() private {
        address sender = msg.sender;
        bool isExist = false;
        for(uint256 i = 0; i < activeSigners.length; i++) {
            if(activeSigners[i] == sender) {
                isExist = true;
                break;
            }
        }

        if(!isExist) activeSigners.push(sender);
    }

    function confirmAndCheck(bytes32 _operation) private onlyowner returns (bool) {   
        _checkAndAddActiveSinger();
        _checkAddress(_operation);
        MultiOwnedOperationPendingState memory pending = m_multiOwnedPending[_operation];
        if (pending.signers.length > 0) {
            operations[msg.sender].push(_operation);
        }
        _checkOrClearPending();
        _checkPendingTxPerOwner();
        operations[msg.sender].push(_operation);
        bool status = _checkOperation(_operation, m_multiOwnedRequired);
        if(status) assertOperationIsConsistent(_operation);

        bool isFinalConfirmation = _checkConfirmationStep(msg.sender, _operation);
        if(!isFinalConfirmation) assertOperationIsConsistent(_operation);
        clearPendingPerOwnerDirectly(pending.signers);
        return isFinalConfirmation;    
    }

    function _checkOperation(bytes32 _operation, uint256 _value) internal returns (bool) {
        MultiOwnedOperationPendingState storage pending = m_multiOwnedPending[_operation];
        // if we're not yet working on this operation, switch over and reset the confirmation status.
        if (!isOperationActive(_operation)) {
            // reset count of confirmations needed.
            pending.yetNeeded = _value;
            // reset which owners have confirmed (none) - set our bitmap to 0.
            pending.ownersDone = 0;
            pending.index = m_multiOwnedPendingIndex.length;
            m_multiOwnedPendingIndex.push(_operation);
            return true;
        }
        return false;
    }

    function getOperationLenghtByOwner(address _owner) public view returns (uint256) {
        return operations[_owner].length;
    }

    function getMultiOwnedPendingIndexLength() public view returns (uint256) {
        return m_multiOwnedPendingIndex.length;
    }

    function _checkConfirmationStep(address _sender, bytes32 _operation) internal returns (bool){
        MultiOwnedOperationPendingState storage pending = m_multiOwnedPending[_operation];
        // determine the bit to set for this owner.
        pending.signers.push(msg.sender);
        uint256 ownerIndexBit = makeOwnerBitmapBit(_sender);
        // make sure we (the message sender) haven't confirmed this operation previously.
        if (pending.ownersDone & ownerIndexBit == 0) {
            // ok - check if count is enough to go ahead.
            // signedOperations[msg.sender]++;
            require(pending.yetNeeded > 0, "Multiowned: pending needed confirmations should be more then 0");
            if (pending.yetNeeded == 1) {
                // enough confirmations: reset and run interior.
                delete m_multiOwnedPendingIndex[m_multiOwnedPending[_operation].index];
                delete m_multiOwnedPending[_operation];
                // delete pending.signers;
                emit FinalConfirmation(msg.sender, _operation);
                return true;
            } else {
                // not enough: record that this owner in particular confirmed.
                pending.yetNeeded--;
                pending.ownersDone |= ownerIndexBit;
            }
        }
        return false;
    }

    function _checkAmountOfOperations() internal view {
        require(
            signedOperations[msg.sender] + 1 <= maxOperationsPerOwner,
            "Multiowned: you can sign only limited amount of operations. Check maxOperationsPerOwner parameter"
        );
    }

    function _checkAddress(bytes32 _operation) internal view {
        MultiOwnedOperationPendingState memory pending = m_multiOwnedPending[_operation];
        for(uint256 i; i < pending.signers.length; i++){
            require(
                pending.signers[i] != msg.sender,
                "Multiowned: you have already singed this transaction"
            );
        }
    }

    function _checkPendingTxPerOwner() view internal {
        uint256 _length = getOperationLenghtByOwner(msg.sender);
        if (maxOperationsPerOwner == _length) {
            revert("You have already confirm maximum operations that is allowed. Wait till other owners approve some of them.");
        }
    }

    function _checkOrClearPending() internal {

        if (512 == m_multiOwnedPendingIndex.length) {
            // In case m_multiOwnedPendingIndex grows too much we have to shrink it: otherwise at some point
            // we won't be able to do it because of block gas limit.
            // Yes, pending confirmations will be lost. Dont see any security or stability implications.
            // TODO use more graceful approach like compact or removal of clearPending completely
            clearPending();
        }
    }

    function clearPendingPerOwnerDirectly(address[] memory _senders) private onlyowner {
        if(_senders.length > 0) {
            address currentSender;
            for(uint256 j = 0; j < _senders.length; j ++) {
                currentSender = _senders[j];
                delete operations[currentSender];
            }
            delete operations[msg.sender];
        }
    }

    function clearPending() private onlyowner {
        for(uint256 i = 0; i < activeSigners.length; i++) {
            delete operations[activeSigners[i]];
        }

        uint256 length = m_multiOwnedPendingIndex.length;
        // TODO block gas limit
        bytes32 currentOp;
        for (uint256 i = 0; i < length; ++i) {
            currentOp = m_multiOwnedPendingIndex[i];
            if (m_multiOwnedPendingIndex[i] != 0) {
                delete m_multiOwnedPending[currentOp];
            }
        }
        delete m_multiOwnedPendingIndex;
    }

    function checkOwnerIndex(uint256 ownerIndex) private pure returns (uint256) {
        require(
            0 != ownerIndex && ownerIndex <= c_maxOwners,
            "Multiowned: owner index should be more then 0 and less or equal then c_maxOwners"
        );
        return ownerIndex;
    }

    function makeOwnerBitmapBit(address owner) private view returns (uint256) {
        uint256 ownerIndex = checkOwnerIndex(m_ownerIndex[owner]);
        return 2 ** ownerIndex;
    }

    function isOperationActive(bytes32 _operation) private view returns (bool) {
        return 0 != m_multiOwnedPending[_operation].yetNeeded;
    }

    function assertOwnersAreConsistent() private view {
        require(m_numOwners > 0, "Multiowned: the pointer for free slot should be more than 0");
        require(
            m_numOwners <= c_maxOwners,
            "Multiowned: the pointer for free slot should be less or equal then c_maxOwners"
        );
        (bool isExist, address adr) = m_owners.tryGet(0);
        require(
            !isExist && address(0) == adr,
            "Multiowned: the owner with zero index should not exist"
        );
        require(
            0 != m_multiOwnedRequired && m_multiOwnedRequired <= m_numOwners,
            "Multiowned: required owner value should be more then 0 and less or equal then amount of owners"
        );
    }

    function assertOperationIsConsistent(bytes32 _operation) private view {
        MultiOwnedOperationPendingState storage pending = m_multiOwnedPending[_operation];
        require(0 != pending.yetNeeded, "Multiowned: needed owners should not be a zero value");
        require(
            m_multiOwnedPendingIndex[pending.index] == _operation,
            "Multiowned: pending operation should be the same with an executed operation"
        );
        require(
            pending.yetNeeded <= m_multiOwnedRequired,
            "Multiowned: needed owners should be less or equal then owners required"
        );
    }

    function assertOperationIsConsistentForAll(bytes32 _operation) private view {
        MultiOwnedOperationPendingState storage pending = m_multiOwnedPending[_operation];
        require(0 != pending.yetNeeded, "Multiowned: needed owners should not be a zero value");
        require(
            m_multiOwnedPendingIndex[pending.index] == _operation,
            "Multiowned: pending operation should be the same with an executed operation"
        );
        require(
            pending.yetNeeded <= m_numOwners,
            "Multiowned: needed owners should be less or equal then number of owners"
        );
    }

    function assertOperationIsConsistentForAlmostAll(bytes32 _operation) private view {
        MultiOwnedOperationPendingState storage pending = m_multiOwnedPending[_operation];
        require(0 != pending.yetNeeded, "Multiowned: needed owners should not be a zero value");
        require(
            m_multiOwnedPendingIndex[pending.index] == _operation,
            "Multiowned: pending operation should be the same with an executed operation"
        );
        require(
            pending.yetNeeded < m_numOwners,
            "Multiowned: needed owners should be less then number of owners"
        );
    }
}

