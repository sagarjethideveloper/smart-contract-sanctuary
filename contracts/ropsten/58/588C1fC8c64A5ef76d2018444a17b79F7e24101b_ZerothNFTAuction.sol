pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./SendValueWithFallbackWithdraw.sol";
import "../library/LibNFTAuction.sol";

abstract contract NFTAuction is Context, AccessControlEnumerable, SendValueWithFallbackWithdraw {

    using SafeMath for uint256;
    using Counters for Counters.Counter;
    using LibNFTAuction for LibNFTAuction.Auction;

    bytes32 public constant COLLECTOR_ROLE = keccak256("COLLECTOR_ROLE");

    uint256 internal constant BASIS_POINTS = 10000;

    Counters.Counter private _auctionIdTracker;
    mapping(uint256 => LibNFTAuction.Auction) public auctions;

    uint256 private _minimumExtraHolding;
    uint256 private _minimumBidIncrement;
    uint256 private _fee;

    uint256 internal holdingBidsAmount;

    event AuctionCreated(
        address indexed seller,
        IERC721 indexed nftContract,
        uint256 indexed tokenId,
        uint256 duration,
        uint256 extensionDuration,
        uint256 reservePrice,
        uint256 auctionId
    );
    event AuctionCanceled(uint256 indexed auctionId);
    event AuctionBidPlaced(uint256 indexed auctionId, address indexed bidder, uint256 amount, uint256 endsAt);
    event AuctionFinalized(uint256 indexed auctionId, address indexed seller, address indexed bidder);

    constructor(uint256 minimumExtraHolding, uint256 minimumBidIncrement, uint256 fee, address owner) {
        _minimumExtraHolding = minimumExtraHolding;
        _minimumBidIncrement = minimumBidIncrement;
        _fee = fee;

        _setupRole(DEFAULT_ADMIN_ROLE, owner);
        _setupRole(COLLECTOR_ROLE, owner);
    }

    function createAuction(
        address nftContract,
        uint256 tokenId,
        uint256 duration,
        uint256 extensionDuration,
        uint256 reservePrice,
        uint256 fixedPrice
    ) public nonReentrant {
        require(reservePrice > 0, "NFTAuction: Reserve price must be at least 1 wei");
        if (fixedPrice > 0) {
            require(fixedPrice >= reservePrice, "NFTAuction: Fixed price must be >= Reserve price");
        }

        uint256 auctionId = _auctionIdTracker.current();
        _auctionIdTracker.increment();

        LibNFTAuction.Auction memory auction = LibNFTAuction.Auction(
            auctionId,
            LibNFTAuction.Product(IERC721(nftContract), tokenId),
            payable(_msgSender()),
            duration,
            extensionDuration,
            0,
            fixedPrice,
            LibNFTAuction.Bid(payable(address(0)), reservePrice)
        );
        auctions[auctionId] = auction;

        auction.product.nftContract.transferFrom(_msgSender(), address(this), tokenId);

        emit AuctionCreated(
            auction.seller,
            auction.product.nftContract,
            auction.product.tokenId,
            auction.duration,
            auction.extensionDuration,
            reservePrice,
            auction.id
        );
    }

    function cancelAuction(uint256 auctionId) public nonReentrant {
        LibNFTAuction.Auction memory auction = auctions[auctionId];
        require(auction.seller == _msgSender(), "NFTAuction: Not your auction");
        require(auction.endsAt == 0, "NFTAuction: Auction in progress");

        delete auctions[auctionId];
        auction.product.nftContract.transferFrom(address(this), auction.seller, auction.product.tokenId);

        emit AuctionCanceled(auctionId);
    }

    function placeBid(uint256 auctionId) public payable nonReentrant {
        LibNFTAuction.Auction storage auction = auctions[auctionId];
        require(auction.currentBid.amount > 0, "NFTAuction: Auction not found");

        address bidder = _msgSender();
        uint256 amount = msg.value;

        require(amount >= _getMinimumBidAmount(auction), "NFTAuction: Bid must be higher than current bid amount");
        require(auction.currentBid.bidder != bidder, "NFTAuction: You already have an bid");
        if (auction.endsAt > 0) {
            require(auction.endsAt >= block.timestamp, "NFTAuction: Auction is closed");
        }

        address payable lastBidder = auction.currentBid.bidder;
        uint256 lastAmount = auction.currentBid.amount;
        auction.currentBid.bidder = payable(bidder);
        auction.currentBid.amount = amount;

        if (auction.fixedPrice > 0 && amount >= auction.fixedPrice) {
            auction.endsAt = block.timestamp - 1;
        } else if (lastBidder == address(0)) {
            auction.endsAt = block.timestamp + auction.duration;
        } else {
            if (auction.endsAt - block.timestamp < auction.extensionDuration) {
                auction.endsAt = block.timestamp + auction.extensionDuration;
            }
        }

        holdingBidsAmount = holdingBidsAmount.add(amount);
        if (lastBidder != address(0)) {
            holdingBidsAmount = holdingBidsAmount.sub(lastAmount);
            _sendValueWithFallbackWithdraw(lastBidder, lastAmount);
        }

        emit AuctionBidPlaced(auction.id, bidder, amount, auction.endsAt);
    }

    function finalizeAuction(uint256 auctionId) public nonReentrant {
        LibNFTAuction.Auction memory auction = auctions[auctionId];
        require(auction.endsAt > 0, "NFTAuction: Auction has not started");
        require(auction.endsAt < block.timestamp, "NFTAuction: Auction is not closed");

        auction.product.nftContract.transferFrom(address(this), auction.currentBid.bidder, auction.product.tokenId);
        _distributeFunds(auction);

        delete auctions[auctionId];

        emit AuctionFinalized(auctionId, auction.seller, auction.currentBid.bidder);
    }

    function _distributeFunds(LibNFTAuction.Auction memory auction) internal {
        uint256 feeForAuction = auction.currentBid.amount.mul(_fee.mul(100)).div(BASIS_POINTS);
        uint256 revenue = auction.currentBid.amount.sub(feeForAuction);
        holdingBidsAmount = holdingBidsAmount.sub(auction.currentBid.amount);
        _sendValueWithFallbackWithdraw(auction.seller, revenue);
    }

    function withdrawFunds() public {
        require(hasRole(COLLECTOR_ROLE, _msgSender()), "NFTAuction: must have collector role to mint");
        uint256 mustHoldAmount = pendingWithdrawalAmount.add(holdingBidsAmount).add(_minimumExtraHolding);
        uint256 balance = address(this).balance;
        require(balance > mustHoldAmount, "NFTAuction: withdrawal amount must be > 0");
        uint256 withdrawalAmount = balance.sub(mustHoldAmount);
        _sendValueWithFallbackWithdraw(payable(_msgSender()), withdrawalAmount);
    }

    function getMinimumBidAmount(uint256 auctionId) public view returns (uint256) {
        LibNFTAuction.Auction memory auction = auctions[auctionId];
        if (auction.endsAt == 0) {
            return auction.currentBid.amount;
        }
        return _getMinimumBidAmount(auction);
    }

    function _getMinimumBidAmount(LibNFTAuction.Auction memory auction) internal view returns (uint256) {
        if (auction.currentBid.bidder == address(0)) {
            return auction.currentBid.amount;
        }
        uint256 minimumIncrement = auction.currentBid.amount.mul(_minimumBidIncrement.mul(100)).div(BASIS_POINTS);
        if (minimumIncrement == 0) {
            return auction.currentBid.amount.add(1);
        }
        return minimumIncrement.add(auction.currentBid.amount);
    }
}

