pragma solidity 0.8.4;

import "./Auction.sol";

contract MarketplaceSignature {
    struct EIP712Domain {
        string name;
        string version;
        uint256 chainId;
        address verifyingContract;
    }

    struct TradeData {
        address lotCreator;
        uint256 tokenId;
        uint256 price;
        uint128 amount;
        uint128 endTime;
    }

    struct AuctionData {
        address lotCreator;
        uint256 tokenId;
        uint256 amount;
        uint256 startPrice;
        uint256 buyNow;
        uint128 endTime;
        uint128 minDelta;
    }

    bytes32 constant EIP712DOMAIN_TYPEHASH =
        keccak256(
            "EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)"
        );

    bytes32 constant AUCTIONDATA_TYPEHASH =
        keccak256(
            "AuctionData(address lotCreator,uint256 tokenId,uint256 amount,uint256 startPrice,uint256 buyNow,uint128 endTime,uint128 minDelta)"
        );

    bytes32 constant TRADEDATA_TYPEHASH =
        keccak256(
            "TradeData(address lotCreator,uint256 tokenId,uint256 price,uint128 amount,uint128 endTime)"
        );

    bytes32 EIP712DOMAIN_SEPARATOR;

    function __Signature_init(string memory _name, string memory _version)
        internal
    {
        EIP712DOMAIN_SEPARATOR = _hash(
            EIP712Domain({
                name: _name,
                version: _version,
                chainId: block.chainid,
                verifyingContract: address(this)
            })
        );
    }

    function _hash(EIP712Domain memory domain) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    EIP712DOMAIN_TYPEHASH,
                    keccak256(bytes(domain.name)),
                    keccak256(bytes(domain.version)),
                    domain.chainId,
                    domain.verifyingContract
                )
            );
    }

    function _hash(AuctionData memory auctionData)
        internal
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encode(
                    AUCTIONDATA_TYPEHASH,
                    auctionData.lotCreator,
                    auctionData.tokenId,
                    auctionData.amount,
                    auctionData.startPrice,
                    auctionData.buyNow,
                    auctionData.endTime,
                    auctionData.minDelta
                )
            );
    }

    function _hash(TradeData memory tradeData) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    TRADEDATA_TYPEHASH,
                    tradeData.lotCreator,
                    tradeData.tokenId,
                    tradeData.price,
                    tradeData.amount,
                    tradeData.endTime
                )
            );
    }

    function _getAuctionSigner(
        address lotCreator,
        uint256 tokenId,
        uint256 amount,
        uint256 startPrice,
        uint256 buyNow,
        uint128 endTime,
        uint128 minDelta,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal view returns (address) {
        bytes32 digest = keccak256(
            abi.encodePacked(
                "\x19\x01",
                EIP712DOMAIN_SEPARATOR,
                _hash(
                    AuctionData({
                        lotCreator: lotCreator,
                        tokenId: tokenId,
                        amount: amount,
                        startPrice: startPrice,
                        buyNow: buyNow,
                        endTime: endTime,
                        minDelta: minDelta
                    })
                )
            )
        );
        return ecrecover(digest, v, r, s);
    }

    function _getTradeSigner(
        address lotCreator,
        uint256 tokenId,
        uint256 price,
        uint128 amount,
        uint128 endTime,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal view returns (address) {
        bytes32 digest = keccak256(
            abi.encodePacked(
                "\x19\x01",
                EIP712DOMAIN_SEPARATOR,
                _hash(
                    TradeData({
                        lotCreator: lotCreator,
                        tokenId: tokenId,
                        price: price,
                        amount: amount,
                        endTime: endTime
                    })
                )
            )
        );
        return ecrecover(digest, v, r, s);
    }
}

