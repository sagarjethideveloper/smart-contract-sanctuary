contract Voting is Ownable {
    event VoteCommitted(
        uint256 indexed pollID,
        uint256 numTokens,
        address indexed voter
    );
    event VoteRevealed(
        uint256 indexed pollID,
        uint256 numTokens,
        uint256 votesFor,
        uint256 votesAgainst,
        uint256 indexed choice,
        address indexed voter
    );
    event PollCreated(
        uint256 punishRateOnNotReveal,
        uint256 voteQuorum,
        uint256 commitEndDate,
        uint256 revealEndDate,
        uint256 indexed pollID,
        address indexed creator
    );
    event TokensRescued(
        uint256 indexed pollID,
        uint256 numTokens, // 投票数量
        uint256 rescueAmount, // 实际取回数量，剩下惩罚
        address indexed voter
    );
    // event TokenTranferred(address indexed receiveAddr, uint256 amount);

    struct Poll {
        uint256 commitEndDate; /// expiration date of commit period for poll
        uint256 revealEndDate; /// expiration date of reveal period for poll
        uint256 voteQuorum; /// number of votes required for a proposal to pass
        uint256 punishRateOnNotReveal; /// 如果不揭露，惩罚比例，为社区贡献
        uint256 votesFor; /// tally of votes supporting proposal
        uint256 votesAgainst; /// tally of votes countering proposal
        // bool votesTransferred; /// votes transfer to challenge for reward
        mapping(address => bool) didCommit; /// indicates whether an address committed a vote for this poll
        mapping(address => uint256) votesRevealFor; /// amount of votes reveal by address
        mapping(address => uint256) votesRevealAginst; /// amount of votes reveal by address
    }

    using AttributeStore for AttributeStore.Data;
    AttributeStore.Data private store;

    uint256 public constant INITIAL_POLL_NONCE = 0;
    uint256 public pollNonce;

    mapping(uint256 => Poll) public pollMap; // maps pollID to Poll struct

    address public challengeAddr;

    constructor() {
        pollNonce = INITIAL_POLL_NONCE;
    }

    function setChallengeAddr(address pChallengeAddr) external onlyOwner {
        require(pChallengeAddr != address(0), "address zero");
        challengeAddr = pChallengeAddr;
    }

    modifier onlyPoiChallenge {
        require(msg.sender == challengeAddr, "not allowed");
        _;
    }

    /**
    开始新的的投票
     */
    function startPoll(
        uint256 voteQuorum,
        uint256 punishRateOnNotReveal,
        uint256 commitDuration,
        uint256 revealDuration
    ) public onlyPoiChallenge returns (uint256 pollID) {
        uint256 commitEndDate = block.timestamp + commitDuration;
        uint256 revealEndDate = commitEndDate + revealDuration;

        Poll storage poll = pollMap[++pollNonce];
        poll.voteQuorum = voteQuorum;
        poll.commitEndDate = commitEndDate;
        poll.revealEndDate = revealEndDate;
        poll.punishRateOnNotReveal = punishRateOnNotReveal;

        emit PollCreated(
            punishRateOnNotReveal,
            voteQuorum,
            commitEndDate,
            revealEndDate,
            pollNonce,
            msg.sender
        );
        return pollNonce;
    }

    /**
    提交新的投票, 每个poll只能投票一次
     */
    function commitVote(
        uint256 pollID,
        bytes32 secretHash,
        uint256 numTokens,
        address sender
    ) public onlyPoiChallenge {
        require(numTokens > 0, "numTokens is zero");
        require(commitPeriodActive(pollID), "poll not active");
        require(!pollMap[pollID].didCommit[sender], "already committed");

        // token.safeTransferFrom(sender, address(this), numTokens);

        // prevent user from committing a secretHash of 0
        require(secretHash != 0, "secretHash is zero");

        bytes32 uuid = attrUUID(sender, pollID);

        store.setAttribute(uuid, "numTokens", numTokens);
        store.setAttribute(uuid, "commitHash", uint256(secretHash));

        pollMap[pollID].didCommit[sender] = true;
        emit VoteCommitted(pollID, numTokens, sender);
    }

    // function commitVotes(
    //     uint256[] memory _pollIDs,
    //     bytes32[] memory _secretHashes,
    //     uint256[] memory _numsTokens,
    //     address[] memory _senders
    // ) external {
    //     // make sure the array lengths are all the same
    //     require(
    //         _pollIDs.length == _secretHashes.length,
    //         "secretHashes length diff"
    //     );
    //     require(_pollIDs.length == _numsTokens.length, "numTokens length diff");
    //     require(_pollIDs.length == _senders.length, "senders length diff");

    //     // loop through arrays, committing each individual vote values
    //     for (uint256 i = 0; i < _pollIDs.length; i++) {
    //         commitVote(
    //             _pollIDs[i],
    //             _secretHashes[i],
    //             _numsTokens[i],
    //             _senders[i]
    //         );
    //     }
    // }

    function didCommit(uint256 pollId, address account)
        public
        view
        returns (bool)
    {
        return pollMap[pollId].didCommit[account];
    }

    /**
    揭露答案
     */
    function revealVote(
        uint256 pollID,
        uint256 voteOption,
        uint256 salt,
        address sender
    ) public onlyPoiChallenge {
        // Make sure the reveal period is active
        require(revealPeriodActive(pollID), "not reveal active");
        require(pollMap[pollID].didCommit[sender], "not voted"); // make sure user has committed a vote for this poll
        require(
            keccak256(abi.encodePacked(voteOption, salt)) ==
                getCommitHash(sender, pollID),
            "wrong option or salt"
        ); // compare resultant hash from inputs to original commitHash

        uint256 numTokens = getNumTokens(sender, pollID);

        if (voteOption == 1) {
            require(
                pollMap[pollID].votesRevealFor[sender] == 0,
                "revealed for"
            ); // prevent user from revealing multiple times
            pollMap[pollID].votesRevealFor[sender] = numTokens;
            // apply numTokens to appropriate poll choice
            pollMap[pollID].votesFor += numTokens;
        } else {
            require(
                pollMap[pollID].votesRevealAginst[sender] == 0,
                "revealed aginst"
            ); // prevent user from revealing multiple times
            pollMap[pollID].votesRevealAginst[sender] = numTokens;
            pollMap[pollID].votesAgainst += numTokens;
        }

        emit VoteRevealed(
            pollID,
            numTokens,
            pollMap[pollID].votesFor,
            pollMap[pollID].votesAgainst,
            voteOption,
            sender
        );
    }

    function getVotesFor(uint256 pollId, address voter)
        external
        view
        returns (uint256, uint256)
    {
        require(pollEnded(pollId), "not end");

        Poll storage poll = pollMap[pollId];
        if (!poll.didCommit[voter]) {
            return (0, poll.votesFor);
        }
        return (poll.votesRevealFor[voter], poll.votesFor);
    }

    function getVotesAginst(uint256 pollId, address voter)
        external
        view
        returns (uint256, uint256)
    {
        require(pollEnded(pollId), "not end");

        Poll storage poll = pollMap[pollId];
        if (!poll.didCommit[voter]) {
            return (0, poll.votesAgainst);
        }
        return (poll.votesRevealAginst[voter], poll.votesAgainst);
    }

    // function revealVotes(
    //     uint256[] memory _pollIDs,
    //     uint256[] memory _voteOptions,
    //     uint256[] memory _salts,
    //     address[] memory _senders
    // ) external {
    //     // make sure the array lengths are all the same
    //     require(_pollIDs.length == _voteOptions.length, "voteOptions diff");
    //     require(_pollIDs.length == _salts.length, "salts diff");
    //     require(_pollIDs.length == _senders.length, "sender diff");

    //     // loop through arrays, revealing each individual vote values
    //     for (uint256 i = 0; i < _pollIDs.length; i++) {
    //         revealVote(_pollIDs[i], _voteOptions[i], _salts[i], _senders[i]);
    //     }
    // }

    /**
    @dev Unlocks tokens locked in unrevealed vote where poll has ended
    @param pollID Integer identifier associated with the target poll
    */
    function settleRescueTokens(uint256 pollID, address sender)
        external
        onlyPoiChallenge
        returns (uint256, uint256)
    {
        Poll storage poll = pollMap[pollID];
        require(isExpired(poll.revealEndDate), "in progress");
        require(poll.didCommit[sender], "no commit");
        require(
            poll.votesRevealAginst[sender] == 0 &&
                poll.votesRevealFor[sender] == 0,
            "revealed"
        );

        uint256 numTokens = getNumTokens(sender, pollID);
        require(numTokens > 0, "vote zero");

        poll.didCommit[sender] = false;

        uint256 punishAmount = (numTokens * poll.punishRateOnNotReveal) / 10000;
        uint256 rescueAmount = numTokens - punishAmount;

        emit TokensRescued(pollID, numTokens, rescueAmount, sender);

        return (rescueAmount, punishAmount);
    }

    // function transferVotesToChallenge(uint256 pollId) external {
    //     require(msg.sender == challengeAddr, "not allow");

    //     Poll storage poll = pollMap[pollId];
    //     require(isExpired(poll.revealEndDate), "in progress");
    //     require(!poll.votesTransferred, "tranferred");

    //     poll.votesTransferred = true;

    //     uint256 amount = poll.votesFor + poll.votesAgainst;

    //     token.safeTransfer(msg.sender, poll.votesFor + poll.votesAgainst);

    //     emit TokenTranferred(msg.sender, amount);
    // }

    /**
    @notice Determines if proposal has passed
    @dev Check if votesFor out of totalVotes exceeds votesQuorum (requires pollEnded)
    @param _pollID Integer identifier associated with target poll
    */
    function isPassed(uint256 _pollID)
        public
        view
        returns (bool passed, bool isValid)
    {
        require(pollEnded(_pollID), "not end");

        Poll storage poll = pollMap[_pollID];
        return (
            10000 * poll.votesFor >
                (poll.voteQuorum * (poll.votesFor + poll.votesAgainst)),
            poll.votesFor > 0 || poll.votesAgainst > 0
        );
    }

    function pollEnded(uint256 _pollID) public view returns (bool) {
        require(pollExists(_pollID), "not exist");

        return isExpired(pollMap[_pollID].revealEndDate);
    }

    function pollExists(uint256 _pollID) public view returns (bool) {
        return (_pollID != 0 && _pollID <= pollNonce);
    }

    function isExpired(uint256 _terminationDate)
        public
        view
        returns (bool expired)
    {
        return (block.timestamp > _terminationDate);
    }

    function attrUUID(address _user, uint256 _pollID)
        public
        pure
        returns (bytes32)
    {
        return keccak256(abi.encodePacked(_user, _pollID));
    }

    function commitPeriodActive(uint256 _pollID) public view returns (bool) {
        require(pollExists(_pollID), "poll not exist");

        return !isExpired(pollMap[_pollID].commitEndDate);
    }

    function revealPeriodActive(uint256 _pollID)
        public
        view
        returns (bool active)
    {
        require(pollExists(_pollID), "poll not exist");

        return
            !isExpired(pollMap[_pollID].revealEndDate) &&
            !commitPeriodActive(_pollID);
    }

    function getCommitHash(address _voter, uint256 _pollID)
        public
        view
        returns (bytes32)
    {
        return
            bytes32(
                store.getAttribute(attrUUID(_voter, _pollID), "commitHash")
            );
    }

    function getNumTokens(address _voter, uint256 _pollID)
        public
        view
        returns (uint256)
    {
        return store.getAttribute(attrUUID(_voter, _pollID), "numTokens");
    }

    function getTotalNumberOfTokensForOption(uint256 _pollID)
        public
        view
        returns (uint256, uint256)
    {
        require(pollEnded(_pollID), "not end");

        return (pollMap[_pollID].votesFor, pollMap[_pollID].votesAgainst);
    }
}

