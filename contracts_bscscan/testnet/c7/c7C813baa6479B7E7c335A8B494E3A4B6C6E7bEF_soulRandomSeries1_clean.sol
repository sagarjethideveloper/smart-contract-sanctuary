contract soulRandomSeries1_clean is
    ERC721,
    VRFConsumerBase,
    ERC721Burnable,
    ERC721Enumerable
{
    using SafeMath for uint256;
    using SafeMath for uint8;
    using Address for address;
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableMap for EnumerableMap.UintToAddressMap;
    using Strings for uint256;

    // init token Id
    uint16 tokenId = 0;

    // base URI internal
    string public baseURI;

    // chainlink constants
    bytes32 internal keyHash;
    uint256 internal fee;
    // randomness ^ chainlink mappings
    mapping(bytes32 => address) public requestIdToAddress;
    mapping(uint16 => uint256) public tokenIdToRandomNumber;
    mapping(bytes32 => uint16) public requestIdToTokenId;

    address public curve;
    address public lottery;
    address public creator;
    mapping(address => bool) public burners;
    bool setCurveAddress = false;
    bool setLotteryAddress = false;

    uint256 public totalEverMinted = 0;

    /**
     * @dev Initializes the contract by setting a `name` and a `symbol` to the token collection.
     */
    constructor()
        public
        VRFConsumerBase(
            0xa555fC018435bef5A13C6c6870a9d4C11DEC329C, // VRF Coordinator
            0x84b9B910527Ad5C03A9Ca831909E21e236EA7b06 // LINK Token
        )
        ERC721("penalty", "PNL")
    {
        creator = msg.sender;
        _setBaseURI("https://metadata.apoyield.com/.netlify/functions/server/");
        keyHash = 0xcaf3c3727e033261d383b315559476f48034c13b18f8cafed4d871abe5049186;
        fee = 0.1 * 10**18; // 0.1 LINK (Varies by network)
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721, ERC721Enumerable) {
        ERC721Enumerable._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /**
     * Requests randomness
     */
    function getRandomNumber(address to) internal {
        require(
            LINK.balanceOf(address(this)) >= fee,
            "Not enough LINK - fill contract with faucet"
        );
        bytes32 requestId = requestRandomness(keyHash, fee);
        tokenId++;
        requestIdToTokenId[requestId] = tokenId;
        requestIdToAddress[requestId] = to;
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness)
        internal
        override
    {
        tokenIdToRandomNumber[requestIdToTokenId[requestId]] = randomness;
        _mint(requestIdToAddress[requestId], requestIdToTokenId[requestId]);
    }

    function setCurve(address _curve) public {
        require(msg.sender == creator, "only owner");
        curve = _curve;
        setCurveAddress = true;
    }

    function setLottery(address _lottery) public {
        require(msg.sender == creator, "only owner");
        lottery = _lottery;
        setLotteryAddress = true;
    }

    function addPool(address _burner) public {
        require(msg.sender == creator, "!governance");
        burners[_burner] = true;
    }

    function removePool(address _burner) public {
        require(msg.sender == creator, "!governance");
        burners[_burner] = false;
    }

    function setCreator(address _creator) public {
        require(msg.sender == creator, "!governance");
        creator = _creator;
    }

    function toUint8(bytes memory _bytes, uint256 _start)
        internal
        pure
        returns (uint8)
    {
        require(_start + 1 >= _start, "toUint8_overflow");
        require(_bytes.length >= _start + 1, "toUint8_outOfBounds");
        uint8 tempUint;

        assembly {
            tempUint := mload(add(add(_bytes, 0x1), _start))
        }

        return tempUint;
    }

    function mint(address to) public virtual returns (uint256 newTokenId) {
        require(msg.sender == curve, "soulRANDOMS: Minter is not the curve");

        getRandomNumber(to);
        totalEverMinted += 1;

        return tokenId;
    }

    function _baseURI()
        internal
        view
        virtual
        override(ERC721)
        returns (string memory)
    {
        return baseURI;
    }

    function _setBaseURI(string memory baseURI_) internal virtual {
        baseURI = baseURI_;
    }

    function generateRARITYofTokenById(uint256 _tokenId)
        public
        view
        virtual
        returns (uint256)
    {
        _tokenId = uint256(tokenIdToRandomNumber[uint16(_tokenId)]);
        return generateRARITYFromHash(bytes32(_tokenId));
    }

    function generateRARITYFromHash(bytes32 _hash)
        public
        view
        virtual
        returns (uint256)
    {
        bytes memory bhash = abi.encodePacked(_hash);

        /*
 			explain the rarity in here, or not
        */
        uint256 RARITY = (toUint8(bhash, 4).div(64));
        return RARITY;
    }
}

contract Curve {
    using SafeMath for uint256;
    using SafeMath for uint8;
    using SafeMath for uint112;

    uint256 initMintPriceBNB = 80000000000000000; // 0.05 BNB -> re-set to 50000000000000000
    uint256 public mintPriceSOUL;
    uint256 public mintPriceDSL;
    uint256 feeBurn = 10;
    uint256 feeLottery = 10;
    uint256 feeBurnDSL = 80;
    uint256 feeTreasury = 75;
    uint256 public factorSOUL = 10025;
    uint256 public factorDSL = 10025;
    uint256 public controlFactor = 10001;

    //uint256 public reserve;

    address payable public creator;
    IERC20 public DSL = IERC20(0x72FEAC4C0887c12db21CEB161533Fd8467469e6b);
    IERC20 public SOUL = IERC20(0x67d012F731c23F0313CEA1186d0121779c77fcFE);
    address public burnAddress = 0x000000000000000000000000000000000000dEaD;
    address public treasuryAddres = 0xB00b57cc4b922A2C009319c1B714CF2149Af1393;
    address public artisAddress = 0x5B88b9636bEC1c1C768A66606aD9A39Ec901832d;
    IPancakePair public DSLBNB =
        IPancakePair(0x3a6aF09e9a1C51C827f58886dE8ecC1A66fB3031);
    IPancakePair public SOULBNB =
        IPancakePair(0x5CB8C8DE0465D0c22cB949556cD79236fD6fbBe5);
    soulRandomSeries1_clean public soulRANDOMS;

    mapping(uint256 => uint8) private _whatMinted;

    event Minted(uint256 indexed tokenId, uint256 indexed pricePaid);

    constructor(address _soulRANDOMS) public {
        //reserve = 0;
        creator = payable(msg.sender);
        soulRANDOMS = soulRandomSeries1_clean(_soulRANDOMS); // todo: check naming availability
    }

    function mintSOUL() public virtual returns (uint256 _tokenId) {
        // you can only mint one at a time.

        //require(msg.value > 0, "C: No ETH sent");
        uint256 priceInSoul = initMintPriceBNB.mul(10**18).div(getSOULPrice());
        mintPriceSOUL = priceInSoul.mul(factorSOUL).div(10000);
        uint256 burnAmount = mintPriceSOUL.mul(feeBurn).div(100);
        uint256 lotteryAmount = mintPriceSOUL.mul(feeLottery).div(100);
        uint256 treasuryAmount = mintPriceSOUL.mul(feeTreasury).div(100);
        uint256 artistAmount = mintPriceSOUL
        .sub(burnAmount)
        .sub(treasuryAmount)
        .sub(lotteryAmount);

        //require(msg.value >= mintPrice, "C: Not enough ETH sent");
        SOUL.transferFrom(address(msg.sender), address(this), lotteryAmount);
        SOUL.transferFrom(
            address(msg.sender),
            address(burnAddress),
            burnAmount
        );
        SOUL.transferFrom(
            address(msg.sender),
            address(treasuryAddres),
            treasuryAmount
        );
        SOUL.transferFrom(
            address(msg.sender),
            address(artisAddress),
            artistAmount
        );

        // mint first to increase supply
        uint256 tokenId = soulRANDOMS.mint(msg.sender);
        _whatMinted[tokenId] = 1;
        factorSOUL = (factorSOUL.mul(controlFactor)).div(10000);
        emit Minted(tokenId, priceInSoul);

        return tokenId; // returns tokenId in case its useful to check it
    }

    function mintDSL() public virtual returns (uint256 _tokenId) {
        // you can only mint one at a time.

        //require(msg.value > 0, "C: No ETH sent");

        uint256 priceInDSL = initMintPriceBNB.mul(10**18).div(getDSLPrice());
        mintPriceDSL = priceInDSL.mul(factorDSL).div(10000);
        uint256 burnAmount = mintPriceDSL.mul(feeBurnDSL).div(100);
        uint256 lotteryAmount = mintPriceDSL.mul(feeLottery).div(100);
        uint256 treasuryAmount = mintPriceDSL.sub(burnAmount).sub(
            lotteryAmount
        );

        //require(msg.value >= mintPrice, "C: Not enough ETH sent");
        DSL.transferFrom(address(msg.sender), address(this), lotteryAmount);
        DSL.transferFrom(address(msg.sender), address(burnAddress), burnAmount);
        DSL.transferFrom(
            address(msg.sender),
            address(treasuryAddres),
            treasuryAmount
        );

        // mint first to increase supply
        uint256 tokenId = soulRANDOMS.mint(msg.sender);
        _whatMinted[tokenId] = 2;
        factorDSL = (factorDSL.mul(controlFactor)).div(10000);

        emit Minted(tokenId, priceInDSL);

        return tokenId; // returns tokenId in case its useful to check it
    }

    function mintBNB() public payable virtual returns (uint256 _tokenId) {
        // you can only mint one at a time.

        require(msg.value > 0, "C: No BNB sent");

        uint256 mintPrice = 10 * 10**18;
        require(msg.value >= mintPrice, "C: Not enough ETH sent");
        // mint first to increase supply
        uint256 tokenId = soulRANDOMS.mint(msg.sender);

        if (msg.value.sub(mintPrice) > 0) {
            payable(msg.sender).transfer(msg.value.sub(mintPrice)); // excess/padding/buffer
        }

        emit Minted(tokenId, mintPrice);

        return tokenId; // returns tokenId in case its useful to check it
    }

    function changeFeeBurn(uint256 _feeBurn) public virtual {
        require(msg.sender == creator, "only owner can run");
        require(_feeBurn > 0, "must be positive");
        require(
            _feeBurn + feeTreasury + feeLottery < 99,
            "sum must be less than 99"
        );
        feeBurn = _feeBurn;
    }

    function changeFeeBurnDSL(uint256 _feeBurnDSL) public virtual {
        require(msg.sender == creator, "only owner can run");
        require(
            _feeBurnDSL + feeLottery > 0 && _feeBurnDSL + feeLottery < 99,
            "must be positive"
        );
        feeBurnDSL = _feeBurnDSL;
    }

    function changeFeeTreasury(uint256 _feeTreasury) public virtual {
        require(msg.sender == creator, "only owner can run");
        require(_feeTreasury > 0, "must be positive");
        require(
            feeBurn + _feeTreasury + feeLottery < 99,
            "sum must be less than 99"
        );
        feeTreasury = _feeTreasury;
    }

    function changeFeeLottery(uint256 _feeLottery) public virtual {
        require(msg.sender == creator, "only owner can run");
        require(_feeLottery > 0, "must be positive");
        require(
            feeBurn + feeTreasury + _feeLottery < 99,
            "sum must be less than 99"
        );
        feeLottery = _feeLottery;
    }

    function changeFactor(uint256 _controlFactor) public virtual {
        require(msg.sender == creator, "only owner can run");
        require(_controlFactor > 0, "must be positive");
        controlFactor = _controlFactor;
    }

    function changeMinted(uint256 _id, uint8 _what) public virtual {
        require(msg.sender == creator, "only owner can run");
        _whatMinted[_id] = _what;
    }

    function changeBasePrice(uint256 _initMintPriceBNB) public virtual {
        require(msg.sender == creator, "only owner can run");
        require(_initMintPriceBNB > 0, "must be positive");
        initMintPriceBNB = _initMintPriceBNB;
    }

    function didIwin(uint256 tokenId) public virtual {
        require(
            msg.sender == soulRANDOMS.ownerOf(tokenId),
            "you're not the owner"
        );
        uint256 rarity = soulRANDOMS.generateRARITYofTokenById(tokenId);
        require((rarity == 3), "sorry dude");
        if (rarity == 3) {
            if (_whatMinted[tokenId] == 1) {
                SOUL.transfer(
                    address(msg.sender),
                    SOUL.balanceOf(address(this))
                );
            }
            if (_whatMinted[tokenId] == 2) {
                DSL.transfer(address(msg.sender), DSL.balanceOf(address(this)));
            }
        }
        soulRANDOMS.burn(tokenId);
    }

    function getCurrentSupply() public view virtual returns (uint256) {
        return soulRANDOMS.totalSupply();
    }

    function getDSLPrice() public view virtual returns (uint256) {
        (uint112 _reserve0, uint112 _reserve1, ) = DSLBNB.getReserves();
        uint256 price = _reserve1.mul(10**18).div(_reserve0);
        return price;
    }

    function getSOULPrice() public view virtual returns (uint256) {
        (uint112 _reserve0, uint112 _reserve1, ) = SOULBNB.getReserves();
        uint256 price = _reserve1.mul(10**18).div(_reserve0);
        return price;
    }
}

