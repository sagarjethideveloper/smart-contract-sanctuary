    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }
    
    
    
    constructor() ERC20("FireZard","ZARD")  {
	
        rewardAddress = payable(0x0FA5c03b4802bF7e98f1BD2A13CE59FB04E2698c); // Reward Address
