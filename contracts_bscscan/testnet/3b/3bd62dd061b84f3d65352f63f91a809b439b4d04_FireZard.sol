        _symbol = "Zard";
        _decimals = 9;
        
		_setupRole(DEFAULT_ADMIN_ROLE, 0xfB729409f30cfEa7DD24Ae1E2822eE1604074990);
        _setupRole(
            TIMELOCK_ADMIN_ROLE,
            0xfB729409f30cfEa7DD24Ae1E2822eE1604074990 );
		
		
