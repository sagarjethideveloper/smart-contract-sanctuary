		OWNER = payable (0xfB729409f30cfEa7DD24Ae1E2822eE1604074990);
		
        uint256 MAX = ~uint256(0);
        _tTotal = 1000000000 * 10**6 * 10**9;
        _rTotal = (MAX - (MAX % _tTotal));
		
