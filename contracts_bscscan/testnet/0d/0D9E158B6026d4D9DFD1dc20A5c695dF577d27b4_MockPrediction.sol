pragma solidity 0.6.12;

import "./Prediction.sol";

contract MockPrediction is Prediction {
    constructor(        
        address _devFeeAddress,
        address _betTokenAddress,
        uint256 _betFeePercent,
        uint256 _loserLockupPeriodInDays,
        address _rewardTokenAddress,
        uint256 _rewardPercent,
        uint256 _skipLockupFeePercent
    ) Prediction(
        _devFeeAddress,
        _betTokenAddress,
        _betFeePercent,
        _loserLockupPeriodInDays,
        _rewardTokenAddress,
        _rewardPercent,
        _skipLockupFeePercent
    ) public { }

    uint256 private shiftTimeInHours;
    function currentTime() public override view returns (uint256) {
        return now.add(shiftTimeInHours.mul(1 hours));
    }

    function setShiftTimeInHours(uint256 _shiftTimeInHours) external onlyOwner {
        shiftTimeInHours = _shiftTimeInHours;
    }

}

