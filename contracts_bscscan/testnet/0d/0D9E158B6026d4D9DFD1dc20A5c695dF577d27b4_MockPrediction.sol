pragma solidity 0.6.12;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "hardhat/console.sol";

interface IRewardToken {
    function mint(address receiver, uint256 amount) external;
    function burnFrom(address account, uint256 amount) external;
}

contract Prediction is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    enum Status {
        NoResult,
        Win,
        Lose,
        Draw,
        Cancel
    }

    struct Pool {
        uint256 id;
        string team1;
        string team2;
        uint256 handicap1;
        uint256 handicap2;
        Status status;
        uint256 startTime;
        uint256 endTime;
        uint256 betFeePercent;
        uint256 bonusReward;
        bool active;
    }

    uint256 public constant FEE_PERCENT_PRECISON = 1000;
    uint256 public constant SCORE_PRECISION = 10;
    
    uint256 public constant MAX_BET_FEE_PERCENT = 3000; // 3%
    uint256 public constant MAX_LOCKUP_PERIOD_IN_DAYS = 60; // 60 days

    uint256 public poolLength;

    uint256 public betFeePercent; // 1%
    uint256 public loserLockupPeriodInDays; // 30 days
    address public devFeeAddress;

    address public betTokenAddress;

    address public rewardTokenAddress;
    uint256 public rewardPercent; // 10%

    uint256 public skipLockupFeePercent; // 30%

    mapping(uint256 => Pool) public pools;
    mapping(address => mapping(uint256 => mapping(Status => uint256)))
        public vault;
    mapping(uint256 => mapping(Status => uint256)) public totalBetAmount;
    mapping(uint256 => mapping(Status => uint256)) public remainTotalBetAmount;
    mapping(uint256 => mapping(Status => uint256)) public totalBetCount;

    event Bet(
        address indexed sender,
        uint256 poolId,
        Status status,
        uint256 amount
    );

    event Claim(
        address indexed sender,
        uint256 poolId,
        Status status,
        uint256 amount
    );

    event ClaimReward(
        address indexed sender,
        uint256 poolId,
        Status status,
        uint256 amount
    );

    event SetDevFeeAddress(
        address indexed sender,
        address indexed oldAddress,
        address indexed newAddress
    );

    event SetDevFeePercent(
        address indexed sender,
        uint256 oldDevFeePercent,
        uint256 newDevFeePercent
    );

    event SetBetFeePercent(
        address indexed sender,
        uint256 oldBetFeePercent,
        uint256 newBetFeePercent
    );

    event SetLoserLockupPeriodInDays(
        address indexed sender,
        uint256 oldLockupPeriodInDays,
        uint256 newLockupPeriodInDays
    );

    event SetRewardPercent(
        address indexed sender,
        uint256 oldRewardPercent,
        uint256 newRewardPercent
    );

    event SetSkipLockupRewardTokenAmount(
        address indexed sender,
        uint256 oldSkipLockupRewardTokenAmount,
        uint256 newSkipLockupRewardTokenAmount
    );

    event SetSkipLockupFeePercent(
        address indexed sender,
        uint256 oldSetSkipLockupFeePercent,
        uint256 newSetSkipLockupFeePercent
    );

    event AddPool(
        address indexed sender,
        string team1,
        string team2,
        uint256 handicap1,
        uint256 handicap2,
        uint256 start,
        uint256 end,
        uint256 bonusReward
    );

    event SetPoolStatus(address indexed sender, uint256 poolId, Status status);

    event SetPoolScore(
        address indexed sender,
        uint256 poolId,
        uint256 score1,
        uint256 score2
    );

    event TransferDevFee(
        address indexed devFeeAddress,
        uint256 poolId,
        Status status,
        uint256 amount
    );

    modifier isClaimable(uint256 _poolId, Status _status) {
        require(_poolId < poolLength);
        require(_status != Status.NoResult);
        require(pools[_poolId].status != Status.NoResult);
        require(vault[msg.sender][_poolId][_status] > 0);
        _;
    }

    constructor(
        address _devFeeAddress,
        address _betTokenAddress,
        uint256 _betFeePercent,
        uint256 _loserLockupPeriodInDays,
        address _rewardTokenAddress,
        uint256 _rewardPercent,
        uint256 _skipLockupFeePercent
    ) public {
        devFeeAddress = _devFeeAddress;
        betTokenAddress = _betTokenAddress;
        betFeePercent = _betFeePercent;
        loserLockupPeriodInDays = _loserLockupPeriodInDays;
        rewardTokenAddress = _rewardTokenAddress;
        rewardPercent = _rewardPercent;
        skipLockupFeePercent = _skipLockupFeePercent;

        emit SetDevFeeAddress(msg.sender, address(0), devFeeAddress);
        emit SetBetFeePercent(msg.sender, 0, betFeePercent);
        emit SetLoserLockupPeriodInDays(msg.sender, 0, loserLockupPeriodInDays);
        emit SetRewardPercent(msg.sender, 0, rewardPercent);
        emit SetSkipLockupFeePercent(msg.sender, 0, skipLockupFeePercent);
    }

    function currentTime() public view virtual returns (uint256) {
        return now;
    }

    function _totalBetAmount(uint256 _poolId)
        internal
        view
        returns (uint256 amount)
    {
        amount = totalBetAmount[_poolId][Status.Win]
            .add(totalBetAmount[_poolId][Status.Lose])
            .add(totalBetAmount[_poolId][Status.Draw]);
    }

    function _remainTotalBetAmount(uint256 _poolId)
        internal
        view
        returns (uint256 amount)
    {
        amount = remainTotalBetAmount[_poolId][Status.Win]
            .add(remainTotalBetAmount[_poolId][Status.Lose])
            .add(remainTotalBetAmount[_poolId][Status.Draw]);
    }

    function _computeWinnerReward(uint256 _poolId, Status _status)
        internal
        view
        returns (uint256 reward)
    {
        Status loserStatus1;
        Status loserStatus2;
        if (_status == Status.Win) {
            loserStatus1 = Status.Lose;
            loserStatus2 = Status.Draw;
        } else if (_status == Status.Lose) {
            loserStatus1 = Status.Win;
            loserStatus2 = Status.Draw;
        } else if (_status == Status.Draw) {
            loserStatus1 = Status.Lose;
            loserStatus2 = Status.Win;
        } else {
            revert("Invalid status");
        }
        Pool memory pool = pools[_poolId];
        uint256 loserAmount = totalBetAmount[_poolId][loserStatus1].add(
            totalBetAmount[_poolId][loserStatus2]
        );
        reward = loserAmount.mul(rewardPercent).div(FEE_PERCENT_PRECISON * 100);
        reward = reward.add(pool.bonusReward);
    }

    function _winnerClaim(uint256 _poolId, Status _status) internal {
        Pool memory pool = pools[_poolId];
        uint256 totalRewardAmount = _computeWinnerReward(_poolId, _status);
        uint256 winnerAmount = totalBetAmount[_poolId][_status];
        uint256 betAmount = vault[msg.sender][_poolId][_status];
        uint256 rewardAmount = totalRewardAmount.mul(betAmount).div(
            winnerAmount
        );

        remainTotalBetAmount[_poolId][_status] = remainTotalBetAmount[_poolId][_status].sub(betAmount);
        vault[msg.sender][_poolId][_status] = 0;

        uint256 betFeeAmount = betAmount.mul(pool.betFeePercent).div(
            FEE_PERCENT_PRECISON * 100
        );
        IERC20(betTokenAddress).safeTransfer(devFeeAddress, betFeeAmount);
        emit TransferDevFee(devFeeAddress, _poolId, _status, betFeeAmount);

        IRewardToken(rewardTokenAddress).mint(msg.sender, rewardAmount);
        IERC20(betTokenAddress).safeTransfer(msg.sender, betAmount.sub(betFeeAmount));

        emit Claim(msg.sender, _poolId, _status, betAmount.sub(betFeeAmount));
        emit ClaimReward(msg.sender, _poolId, _status, rewardAmount);
    }

    function _loserClaim(uint256 _poolId, Status _status) internal {
        Pool memory pool = pools[_poolId];
        uint256 unlockTime = pool.endTime.add(
            loserLockupPeriodInDays.mul(1 days)
        );
        require(currentTime() >= unlockTime);
        uint256 betAmount = vault[msg.sender][_poolId][_status];

        remainTotalBetAmount[_poolId][_status] = remainTotalBetAmount[_poolId][_status].sub(betAmount);
        vault[msg.sender][_poolId][_status] = 0;
        
        uint256 betFeeAmount = betAmount.mul(pool.betFeePercent).div(
            FEE_PERCENT_PRECISON * 100
        );
        IERC20(betTokenAddress).safeTransfer(devFeeAddress, betFeeAmount);
        emit TransferDevFee(devFeeAddress, _poolId, _status, betFeeAmount);

        IERC20(betTokenAddress).safeTransfer(msg.sender, betAmount.sub(betFeeAmount));
        emit Claim(msg.sender, _poolId, _status, betAmount.sub(betFeeAmount));
    }

    function computeSkipLockupFeePercent(uint256 _poolEndTime)
        public
        view
        returns (uint256)
    {
        uint256 timeDelta = currentTime() > _poolEndTime
            ? currentTime().sub(_poolEndTime)
            : 0;

        uint256 discountPercent = timeDelta.mul(skipLockupFeePercent).div(
            loserLockupPeriodInDays.mul(1 days)
        );

        return
            skipLockupFeePercent > discountPercent
                ? skipLockupFeePercent.sub(discountPercent)
                : 0;
    }

    function forceClaimForLoser(uint256 _poolId, Status _status)
        external
        isClaimable(_poolId, _status)
    {
        Pool memory pool = pools[_poolId];
        require(pool.status != _status);

        uint256 betAmount = vault[msg.sender][_poolId][_status];
        uint256 skipLockupFeeAmount = betAmount
            .mul(computeSkipLockupFeePercent(pool.endTime))
            .div(FEE_PERCENT_PRECISON * 100);

        remainTotalBetAmount[_poolId][_status] = remainTotalBetAmount[_poolId][_status].sub(betAmount);
        vault[msg.sender][_poolId][_status] = 0;

        if (skipLockupFeeAmount > 0) {
            IERC20(betTokenAddress).safeTransfer(
                devFeeAddress,
                skipLockupFeeAmount
            );
            emit TransferDevFee(devFeeAddress, _poolId, _status, skipLockupFeeAmount);
        }

        uint256 betFeeAmount = betAmount.mul(pool.betFeePercent).div(
            FEE_PERCENT_PRECISON * 100
        );
        IERC20(betTokenAddress).safeTransfer(devFeeAddress, betFeeAmount);
        emit TransferDevFee(devFeeAddress, _poolId, _status, betFeeAmount);

        uint256 claimableAmount = betAmount.sub(skipLockupFeeAmount).sub(betFeeAmount);
        if (claimableAmount > 0) {
            IERC20(betTokenAddress).safeTransfer(msg.sender, claimableAmount);
            emit Claim(msg.sender, _poolId, _status, claimableAmount);
        }
    }

    function forceClaimForLoserWithRewardToken(uint256 _poolId, Status _status)
        external
        isClaimable(_poolId, _status)
    {
        Pool memory pool = pools[_poolId];
        require(pool.status != _status);
        uint256 unlockTime = pool.endTime.add(
            loserLockupPeriodInDays.mul(1 days)
        );
        require(currentTime() < unlockTime);

        uint256 betAmount = vault[msg.sender][_poolId][_status];

        uint256 rewardTokenAmount = IERC20(rewardTokenAddress).balanceOf(msg.sender);
        require(rewardTokenAmount >= betAmount);

        remainTotalBetAmount[_poolId][_status] = remainTotalBetAmount[_poolId][_status].sub(betAmount);
        vault[msg.sender][_poolId][_status] = 0;

        uint256 betFeeAmount = betAmount.mul(pool.betFeePercent).div(
            FEE_PERCENT_PRECISON * 100
        );
        IERC20(betTokenAddress).safeTransfer(devFeeAddress, betFeeAmount);
        emit TransferDevFee(devFeeAddress, _poolId, _status, betFeeAmount);

        IRewardToken(rewardTokenAddress).burnFrom(msg.sender, betAmount);
        IERC20(betTokenAddress).safeTransfer(msg.sender, betAmount.sub(betFeeAmount));
        emit Claim(msg.sender, _poolId, _status, betAmount.sub(betFeeAmount));
    }

    function _withdraw(uint256 _poolId, Status _status) internal {
        require(_status == Status.Win || _status == Status.Lose);
        uint256 betAmount = vault[msg.sender][_poolId][_status];
        
        remainTotalBetAmount[_poolId][_status] = remainTotalBetAmount[_poolId][_status].sub(betAmount);
        vault[msg.sender][_poolId][_status] = 0;

        IERC20(betTokenAddress).safeTransfer(msg.sender, betAmount);
        emit Claim(msg.sender, _poolId, _status, betAmount);
    }

    function claim(uint256 _poolId, Status _status)
        external
        isClaimable(_poolId, _status)
    {
        if (pools[_poolId].status == Status.Cancel || pools[_poolId].status == Status.Draw) {
            _withdraw(_poolId, _status);
        } else if (pools[_poolId].status == _status) {
            _winnerClaim(_poolId, _status);
        } else {
            _loserClaim(_poolId, _status);
        }

        if (_remainTotalBetAmount(_poolId) == 0) {
            pools[_poolId].active = false;
        }
    }

    function bet(
        uint256 _poolId,
        Status _status,
        uint256 _amount
    ) external {
        require(_poolId < poolLength);
        require(_amount > 0);
        require(_status == Status.Win || _status == Status.Lose);
        Pool memory pool = pools[_poolId];
        require(currentTime() <= pool.endTime);

        uint256 previousAmount = vault[msg.sender][_poolId][_status];
        if (previousAmount == 0) {
            totalBetCount[_poolId][_status] += 1;
        }
        vault[msg.sender][_poolId][_status] = previousAmount.add(_amount);

        uint256 previousTotalBetAmount = totalBetAmount[_poolId][_status];
        totalBetAmount[_poolId][_status] = previousTotalBetAmount.add(_amount);
        remainTotalBetAmount[_poolId][_status] = totalBetAmount[_poolId][_status];

        IERC20(betTokenAddress).safeTransferFrom(
            msg.sender,
            address(this),
            _amount
        );        

        emit Bet(msg.sender, _poolId, _status, _amount);
    }

    function addPool(
        string calldata _team1,
        string calldata _team2,
        uint256 _handicap1,
        uint256 _handicap2,
        uint256 _closedInHours,
        uint256 _bonusReward
    ) external onlyOwner returns (uint256 poolId) {
        require(_closedInHours > 0);
        uint256 startTime = currentTime();
        poolId = poolLength;
        uint256 endTime = startTime.add(_closedInHours.mul(1 hours));
        pools[poolId] = Pool(
            poolLength,
            _team1,
            _team2,
            _handicap1,
            _handicap2,
            Status.NoResult,
            startTime,
            endTime,
            betFeePercent,
            _bonusReward,
            true
        );

        poolLength = poolLength.add(1);

        emit AddPool(
            msg.sender,
            _team1,
            _team2,
            _handicap1,
            _handicap2,
            startTime,
            endTime,
            _bonusReward
        );
    }

    function setPoolScore(
        uint256 _poolId,
        uint256 score1,
        uint256 score2
    ) external onlyOwner {
        require(_poolId < poolLength);
        Pool memory pool = pools[_poolId];
        require(currentTime() > pool.endTime);
        require(pool.status == Status.NoResult);

        uint256 totalScore1 = pool.handicap1 + score1;
        uint256 totalScore2 = pool.handicap2 + score2;

        Status status = Status.Draw;
        if (totalScore1 > totalScore2) {
            status = Status.Win;
        } else if (totalScore1 < totalScore2) {
            status = Status.Lose;
        }
        emit SetPoolScore(msg.sender, _poolId, score1, score2);
        setPoolStatus(_poolId, status);
    }

    function setPoolStatus(uint256 _poolId, Status _status) public onlyOwner {
        require(_poolId < poolLength);
        require(_status != Status.NoResult);
        Pool memory pool = pools[_poolId];
        require(_status == Status.Cancel || currentTime() > pool.endTime);        
        require(pool.status == Status.NoResult);

        uint256 totalAmount = totalBetAmount[_poolId][Status.Win]
            .add(totalBetAmount[_poolId][Status.Lose])
            .add(totalBetAmount[_poolId][Status.Draw]);

        if (totalAmount == 0) {
            pools[_poolId].active = false;
        }

        pools[_poolId].status = _status;
        emit SetPoolStatus(msg.sender, _poolId, _status);
    }

    function cancel(uint256 _poolId) external onlyOwner {
        setPoolStatus(_poolId, Status.Cancel);
    }

    function setDevFeeAddress(address _devFeeAddress) external onlyOwner {
        require(_devFeeAddress != address(0));
        require(_devFeeAddress != devFeeAddress);
        emit SetDevFeeAddress(msg.sender, devFeeAddress, _devFeeAddress);
        devFeeAddress = _devFeeAddress;
    }

    function setBetFeePercent(uint256 _betFeePercent) external onlyOwner {
        require(_betFeePercent <= MAX_BET_FEE_PERCENT);
        require(betFeePercent != _betFeePercent);
        emit SetBetFeePercent(msg.sender, betFeePercent, _betFeePercent);
        betFeePercent = _betFeePercent;
    }

    function setLoserLockupPeriodInDays(uint256 _loserLockupPeriodInDays)
        external
        onlyOwner
    {
        require(_loserLockupPeriodInDays <= MAX_LOCKUP_PERIOD_IN_DAYS);
        require(loserLockupPeriodInDays != _loserLockupPeriodInDays);
        emit SetLoserLockupPeriodInDays(
            msg.sender,
            loserLockupPeriodInDays,
            _loserLockupPeriodInDays
        );
        loserLockupPeriodInDays = _loserLockupPeriodInDays;
    }

    function setRewardPercent(uint256 _rewardPercent) external onlyOwner {
        require(rewardPercent != _rewardPercent);
        emit SetRewardPercent(msg.sender, rewardPercent, _rewardPercent);
        rewardPercent = _rewardPercent;
    }

    function setSkipLockupFeePercent(uint256 _skipLockupFeePercent)
        external
        onlyOwner
    {
        require(_skipLockupFeePercent <= FEE_PERCENT_PRECISON.mul(100));
        require(skipLockupFeePercent != _skipLockupFeePercent);

        emit SetSkipLockupFeePercent(
            msg.sender,
            skipLockupFeePercent,
            _skipLockupFeePercent
        );
        skipLockupFeePercent = _skipLockupFeePercent;
    }
}

