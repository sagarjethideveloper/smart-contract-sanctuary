pragma solidity >=0.6.12 <0.9.0;

import "../../../base/nft/token-ERC721-ERC721.sol";
import "../../../base/nft/token-ERC721-extensions-ERC721Burnable.sol";
import "../../../base/nft/token-ERC721-extensions-ERC721Enumerable.sol";
import "../../../model/CharacterInfo.sol";
import "../../../model/CharacterFightingMaterial.sol";

contract NftCharacters is ERC721, ERC721Burnable, ERC721Enumerable, Ownable {

  mapping(uint256 => uint256) private _expList;
  mapping(uint256 => uint256) private _rankRewards;
  mapping(uint256 => uint256) private _infoList;

  mapping(uint256 => mapping(address => mapping(uint256 => uint256))) _userIndexedTokenByType;
  mapping(uint256 => mapping(address => mapping(uint256 => uint256))) _userTokenIndexesByType;
  mapping(uint256 => mapping(address => uint256)) _userTotalOwnedByType;

  uint256 private _maxTokenId = 0;
  mapping(uint256 => uint256) private _maxOwnedByType;

  uint256 map;
  bool private _isInitialized;

  constructor(string memory name_, string memory symbol_) ERC721(name_, symbol_) {
  }

  function setInitialized() public onlyAuthorized {
    _isInitialized = true;
  }

  function initialize(
    uint256 _map, string memory name, string memory symbol, address owner, address actor
  ) public {
    require(!_isInitialized, "Already initialized");
    _isInitialized = true;
    map = _map;
    _name = name;
    _symbol = symbol;
    _owner = owner;
    _authorizedActor[actor] = true;
    initData();
  }

  function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable) {
    super._beforeTokenTransfer(from, to, tokenId);

    uint256 characterType = uint256(uint8(_infoList[tokenId]));
    if(to != address(this)) {
      require(_userTotalOwnedByType[characterType][to] < _maxOwnedByType[characterType], "Exceed limit for this type");
    }

    if(from == address(0)) { // mint
      addCharacterForUser(to, characterType, tokenId);
      return;
    }

    if(to == address(0)) { // burn
      removeCharacterOfUser(from, characterType, tokenId);
      return;
    }

    removeCharacterOfUser(from, characterType, tokenId);
    addCharacterForUser(to, characterType, tokenId);
  }

  function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC721Enumerable)
    returns (bool) {
      return super.supportsInterface(interfaceId);
  }

  function mintCharacter(
    address owner, uint256 characterType, uint256 maxMana, uint256 rank, uint256 power
  ) public onlyOwner returns(uint256) {
    _maxTokenId += 1;

    _infoList[_maxTokenId] = makeCharacterInfo(characterType, rank, 1, maxMana, 2, 0);
    _rankRewards[_maxTokenId] = power;
    _safeMint(owner, _maxTokenId);

    return _maxTokenId;
  }

  function mintCharacterWithFullInfo(
    address owner, CharacterInfo memory character
  ) external onlyOwner returns(uint256) {
    _maxTokenId += 1;

    _infoList[_maxTokenId] = makeCharacterInfo(
      character.characterType, character.rank, character.level, character.mana, 2, 0
    );
    _expList[_maxTokenId] = character.exp;
    _rankRewards[_maxTokenId] = character.power;
    _safeMint(owner, _maxTokenId);

    return _maxTokenId;
  }

  function getCharacter(uint256 tokenId) public view returns(CharacterInfo memory) {
    CharacterInfo memory character = parseCharacterInfo(tokenId);

    character.tokenId = tokenId;
    character.exp = _expList[tokenId];
    character.power = _rankRewards[tokenId];

    return character;
  }

  function ownerOf(uint256 tokenId) public view override returns(address) {
    require(tokenId > 0 && tokenId <= _maxTokenId, "TokenId Invalid");

    return super.ownerOf(tokenId);
  }

  function isOwnerOfAll(address owner, uint256[] memory tokenIds) public view returns(bool) {
    for(uint256 i=0;i<tokenIds.length;i++) {
      if(_owners[tokenIds[i]] != owner) {
        return false;
      }
    }

    return true;
  }

  function isTypeExceedLimit(address owner, uint256 characterType) external view returns(bool) {
    return _maxOwnedByType[characterType] == _userTotalOwnedByType[characterType][owner];
  }

  function updateInfoByFightResult(CharacterInfo memory character) public onlyAuthorized {
    uint256 tokenId = character.tokenId;
    require(tokenId > 0 && tokenId <= _maxTokenId, "TokenId Invalid");

    _expList[tokenId] = character.exp;
    uint256 info = _infoList[tokenId];
    uint256 characterType = uint256(uint8(info));
    uint256 rank = uint256(uint8(info >> 8));
    uint256 status = uint256(uint8(info >> 16));

    _infoList[tokenId] = makeCharacterInfo(
      characterType, rank, character.level, character.mana, status, character.lastUsedManaTime
    );
  }

  function getFightingMaterial(address owner, uint256 tokenId) public onlyAuthorized view returns(
    CharacterFightingMaterial memory
  ) {
    require(_owners[tokenId] == owner, "Not character owner");
    uint256 info = _infoList[tokenId];

    CharacterFightingMaterial memory material;
    material.rank = uint256(uint8(info >> 8));
    material.status = uint256(uint8(info >> 16));
    material.level = uint256(uint8(info >> 24));
    material.mana = uint256(uint16(info >> 32));
    material.lastUsedManaTime = uint256(uint48(info >> 48));
    material.reward = _rankRewards[tokenId];

    return material;
  }

  function getFinishFightingMaterial(uint256 tokenId) public onlyAuthorized view returns(uint256, uint256) {
    uint256 info = _infoList[tokenId];
    uint256 level = uint256(uint8(info >> 24));

    return (level, _expList[tokenId]);
  }

  function updateCharacterInfo(
    uint256 tokenId, CharacterInfo memory character
  ) public onlyAuthorized {
    require(tokenId > 0 && tokenId <= _maxTokenId, "TokenId Invalid");

    _expList[tokenId] = character.exp;
    _rankRewards[tokenId] = character.power;
    _infoList[tokenId] = makeCharacterInfo(
      character.characterType, character.rank, character.level, character.mana,
      character.status, character.lastUsedManaTime
    );
  }

  function updateMaxOwned(uint256 characterType, uint256 maxOwned) public onlyAuthorized {
    require(maxOwned > 0, "Cannot be zero");
    _maxOwnedByType[characterType] = maxOwned;
  }

  function getUserTokenIdByTypeAndIndex(
    address owner, uint256 characterType, uint256 index
  ) public view returns(uint256) {
    return _userIndexedTokenByType[characterType][owner][index];
  }

  function getMaxTokenId() public view returns(uint256) {
    return _maxTokenId;
  }

  function getBalanceByType(address owner, uint256 characterType) public view returns(uint256) {
    return _userTotalOwnedByType[characterType][owner];
  }

  function addCharacterForUserWithPermission(
    address user, uint256 characterType, uint256 tokenId
  ) public onlyAuthorized {
    addCharacterForUser(user, characterType, tokenId);
  }

  function removeItemOfUserWithPermission(
    address user, uint256 characterType, uint256 tokenId
  ) public onlyAuthorized {
    removeCharacterOfUser(user, characterType, tokenId);
  }

  function makeCharacterInfo(
    uint256 characterType, uint256 rank, uint256 level, uint256 mana,
    uint256 status, uint256 lastUsedManaTime
  ) public pure returns(uint256) {
    uint256 info = characterType; // uint8
    info |= rank << 8; // uint8
    info |= status << 16; // uint8
    info |= level << 24; // uint8
    info |= mana << 32; // uint16
    info |= lastUsedManaTime << 48; // uint48

    return info;
  }

  function parseCharacterInfo(uint256 tokenId) private view returns(CharacterInfo memory) {
    CharacterInfo memory characterInfo;
    uint256 info = _infoList[tokenId];

    characterInfo.characterType = uint256(uint8(info));
    characterInfo.rank = uint256(uint8(info >> 8));
    characterInfo.status = uint256(uint8(info >> 16));
    characterInfo.level = uint256(uint8(info >> 24));
    characterInfo.mana = uint256(uint16(info >> 32));
    characterInfo.lastUsedManaTime = uint256(uint48(info >> 48));
    characterInfo.elementType = map;
    characterInfo.breed = map;

    return characterInfo;
  }

  function addCharacterForUser(address owner, uint256 characterType, uint256 tokenId) private {
    uint256 totalOwned = _userTotalOwnedByType[characterType][owner] + 1;

    _userTotalOwnedByType[characterType][owner] = totalOwned;
    _userTokenIndexesByType[characterType][owner][tokenId] = totalOwned;
    _userIndexedTokenByType[characterType][owner][totalOwned] = tokenId;
  }

  function removeCharacterOfUser(address owner, uint256 characterType, uint256 tokenId) private {
    uint256 totalOwned = _userTotalOwnedByType[characterType][owner];
    uint256 currentTokenIndex = _userTokenIndexesByType[characterType][owner][tokenId];

    uint256 lastCharacterTokenId = _userIndexedTokenByType[characterType][owner][totalOwned];
    _userIndexedTokenByType[characterType][owner][currentTokenIndex] = lastCharacterTokenId;
    _userTokenIndexesByType[characterType][owner][lastCharacterTokenId] = currentTokenIndex;

    delete _userIndexedTokenByType[characterType][owner][totalOwned];
    totalOwned--;
    _userTotalOwnedByType[characterType][owner] = totalOwned;
    delete _userTokenIndexesByType[characterType][owner][tokenId];
  }

  function initData() private {
    _maxOwnedByType[1] = 4;
    _maxOwnedByType[2] = 4;
  }
}

