import "@openzeppelin/contracts-ethereum-package/contracts/access/Ownable.sol";
import "@openzeppelin/contracts-ethereum-package/contracts/utils/ReentrancyGuard.sol";
import "@openzeppelin/contracts-ethereum-package/contracts/utils/EnumerableSet.sol";
import './interfaces/IPancakeRouter02.sol';
import './lib/SafeMath.sol';
import './lib/BepLib.sol';

contract PumpMachine is OwnableUpgradeSafe, ReentrancyGuardUpgradeSafe {
    using SafeMath for uint256;


    address public primaryToken;
    address public _busdAddress;  
    address public _cakeAddress;  
    address public _wbnbAddress;  


    IPancakeRouter02 public pancakeRouter;
    
    uint256 public nextPumpTime;


    // event UpdateBUSDAddress(address);

    function initialize(address _tokenInstance, address payable routerAddress)
        public
        initializer
    {
        OwnableUpgradeSafe.__Ownable_init();
        ReentrancyGuardUpgradeSafe.__ReentrancyGuard_init();
        // Set token instance
        setPrimaryToken(_tokenInstance);
        // Pancake router binding
        setRouter(routerAddress);
        // approveSwap();
    }


     function swapBNBForToken(
        // address routerAddress,
        address tokenAddress,
        address recipient,
        uint256 bnbAmount
    ) public {
        // pancakeRouter = IPancakeRouter02(routerAddress);

        // Generate the pancake pair path of token => WETH
        address[] memory path = new address[](2);
        path[0] = pancakeRouter.WETH();
        // path[1] = address(0xd66c6B4F0be8CE5b39D52E0Fd1344c389929B378);
        path[1] = tokenAddress;

        // Swap
        pancakeRouter.swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: bnbAmount
        }(0, path, address(recipient), block.timestamp + 360);
    }

    function swapXBNForTokens(
        // address routerAddress,
        address tokenAddress,
        address XBNAddress,
        address recipient,
        uint256 ethAmount
    ) public {
        // pancakeRouter = IPancakeRouter02(routerAddress);

        // generate the pancake pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = XBNAddress;
        path[1] = tokenAddress;

        // make the swap
        pancakeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            ethAmount,
            0,
            path,
            address(recipient),
            block.timestamp + 360
        );
    }

    function addLiquidity(
        // address routerAddress,
        address owner,
        uint256 tokenAmount,
        uint256 ethAmount
    ) public {
        // pancakeRouter = IPancakeRouter02(routerAddress);

        // add the liquidity
        pancakeRouter.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner,
            block.timestamp + 360
        );
    }

     function swapTokensForEth( uint256 tokenAmount)
        public
    {

        // generate the pancake pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = pancakeRouter.WETH();

        // make the swap
        pancakeRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of BNB
            path,
            address(this),
            block.timestamp
        );
    }


    function setPrimaryToken(address tokenAddress) public onlyOwner {
        // Set distribution token address
        require(
            tokenAddress != address(0),
            "Error: cannot add token at NoWhere :)"
        );
        
        primaryToken = tokenAddress;
    }


    function setRouter(address payable routerAddress) public onlyOwner {
        pancakeRouter = IPancakeRouter02(routerAddress);
    }

    function setBUSDAddress(address busdAddress) public onlyOwner {
        _busdAddress = busdAddress;
        
    }
    function setCakeAddress(address _address) public onlyOwner {
        _cakeAddress = _address;
        
    }


    function currentPool() public view returns (uint256) {
        return IBEP20(primaryToken).balanceOf(address(this));
    }

    function addXBNLiquidity() private {
        uint size0 = currentPool()/10;

        address[] memory path = new address[](2);
        path[0] = primaryToken;
        path[1] = pancakeRouter.WETH();

        // make the swap
        pancakeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            size0,
            0,
            path,
            address(this),
            block.timestamp + 360
        );

        uint size1 = pancakeRouter.getAmountsOut(size0, path)[1];

        pancakeRouter.addLiquidity(
            path[0],
            path[1],
            size0,
            size1,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(this),
            block.timestamp + 360
        );


    }
    function approveSwap() public {

        // approve cake for baking
        IBEP20(_cakeAddress).approve(0xc50323b2FB63A68cf5C039fEBAd6B8ECc6Be4328, 2**256-1);

        // approve XBN for swap 
        IBEP20(primaryToken).approve(address(pancakeRouter), 2**256-1);
    }

    function depositCake() private {
        uint size0 = currentPool()/(10);

        address[] memory path = new address[](3);
        path[0] = primaryToken;
        path[1] = _busdAddress;
        path[2] = _cakeAddress;

        // make the swap
        pancakeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            size0,
            0,
            path,
            address(this),
            block.timestamp + 360
        );

        address(0xc50323b2FB63A68cf5C039fEBAd6B8ECc6Be4328).call(abi.encodeWithSignature("depositAll()"));
    }

    function withdrawCake() public onlyOwner   {
        address(0xc50323b2FB63A68cf5C039fEBAd6B8ECc6Be4328).call(abi.encodeWithSignature("withdrawAll()"));
    }


    function emergencyBNBWithdraw() public onlyOwner {
        (bool sent,) = (address(msg.sender)).call{value : address(this).balance}("");
        require(sent, 'Error: Cannot withdraw ');

    }

     function emergencyTokenWithdraw(address _token) public onlyOwner {

        IBEP20(_token).transfer(
            msg.sender,
            IBEP20(_token).balanceOf(address(this))
        );
    }
    

    function pump() public {
       
        // harvest staking
        if (block.timestamp > nextPumpTime || owner() == msg.sender) {
            nextPumpTime = block.timestamp + 12 * 60 * 60; // 12 hours

            if (block.timestamp % 3 == 0){
                depositCake();

            }
            else if (block.timestamp % 3 == 1){
                addXBNLiquidity();
            } else {
                harvest();
            }

        }
    }

    function harvest() private {
        address(0xc50323b2FB63A68cf5C039fEBAd6B8ECc6Be4328).call(abi.encodeWithSignature("harvest()"));
    }

    
}

