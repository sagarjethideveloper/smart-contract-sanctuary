pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

import "./XType.sol";
import "./XLib.sol";
import "./interfaces/IPancakeCallee.sol";

contract X is
    Initializable,
    IPancakeCallee,
    AccessControlUpgradeable,
    PausableUpgradeable,
    UUPSUpgradeable
{
    using XLib for XType.Universe;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    XType.Universe state;

    function initialize() public initializer {
        state.initProtoX();

        _setupRole(DEFAULT_ADMIN_ROLE, state.daoCouncil);
        _setupRole(PAUSER_ROLE, state.daoCouncil);
    }

    function claimBounty(uint256 tokenId) external virtual {
        state.claimBounty(tokenId);
    }

    function getToken(uint256 tokenId)
        external
        view
        virtual
        returns (XType.Token memory token)
    {
        return state.getToken(tokenId);
    }

    function pancakeCall(
        address sender,
        uint256 amount0,
        uint256 amount1,
        bytes calldata data
    ) external override {
        state.pancakeCall(sender, amount0, amount1, data);
    }

    function setToken(
        uint256 tokenId,
        uint256 minTimeSinceLastEvent,
        uint256 volumeForToken,
        uint256 maxBountyPerClaim
    ) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        state.setToken(
            tokenId,
            minTimeSinceLastEvent,
            volumeForToken,
            maxBountyPerClaim
        );
    }

    function initToken(
        address tokenAddress,
        uint256 minTimeSinceLastEvent,
        uint256 volumeForToken,
        uint256 maxBountyPerClaim
    ) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        state.initToken(
            tokenAddress,
            minTimeSinceLastEvent,
            volumeForToken,
            maxBountyPerClaim
        );
    }

    function pause() external virtual {
        require(hasRole(PAUSER_ROLE, msg.sender));
        _pause();
    }

    function unpause() external virtual {
        require(hasRole(PAUSER_ROLE, msg.sender));
        _unpause();
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        virtual
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {}

    receive() external payable virtual {}

    function name() external view virtual returns (string memory) {
        return state.name;
    }
}

