import "./XType.sol";

import "./interfaces/IBEP20.sol";
import "./interfaces/IPancakeFactory.sol";
import "./interfaces/IPancakePair.sol";
import "./interfaces/IPancakeRouterV02.sol";

library XLib {
    event TokenInitialized(
        uint256 nextTokenId,
        address tokenAddress,
        uint256 minTimeSinceLastEvent,
        uint256 volumeForToken,
        uint256 maxBountyPerClaim
    );

    event TokenParamsChanged(
        uint256 tokenId,
        uint256 minTimeSinceLastEvent,
        uint256 volumeForToken,
        uint256 maxBountyPerClaim
    );

    event FlashInitiated(address token, uint256 volume);

    event PancakeCallbackReceived(
        address tokenBorrow,
        uint256 amountBorrowed,
        uint256 currentBalance
    );

    event SwappedTokensForWBNB(
        address tokenBorrow,
        uint256 amountBorrowed,
        uint256 receivedWBNB
    );

    event SwappedWBNBForTokens(
        address tokenBorrow,
        uint256 receivedWBNB,
        uint256 accumulatedTokens
    );

    function initProtoX(XType.Universe storage state) external {
        state.daoCouncil = address(0x9BD722adD17e8A8ae75ECBC55d9Fa89Da13459b4);
        state.pancakeRouterAddress = 0xD99D1c33F9fC3444f8101754aBC46c52416550D1;
        state.factory = 0x6725F303b657a9451d8BA641348b6761A6CC7a17;

        state.name = "Proto X";

        initToken(
            state,
            0xf5B6cCCc3290b45Ef452B8A9a9e33bAf7057e00f,
            0,
            100000e18,
            123e18
        );
    }

    function claimBounty(XType.Universe storage state, uint256 tokenId)
        external
    {
        XType.Token storage token = state.tokens[tokenId];
        require(token.volumeForToken > 0, "No volume allowed");
        require(
            block.timestamp - token.lastEvent >= token.minTimeSinceLastEvent,
            "minTimeSinceLastEvent is not yet met"
        );

        flashSwap(state, token.tokenAddress, token.id, token.volumeForToken);
        token.lastEvent = block.timestamp;
    }

    function flashSwap(
        XType.Universe storage state,
        address _tokenBorrow,
        uint256 tokenId,
        uint256 _amount
    ) internal {
        emit FlashInitiated(_tokenBorrow, _amount);

        address pair = IPancakeFactory(state.factory).getPair(
            _tokenBorrow,
            IPancakeSwapV2Router02(state.pancakeRouterAddress).WETH()
        );
        require(pair != address(0), "!pair");

        address token0 = IPancakePair(pair).token0();
        address token1 = IPancakePair(pair).token1();
        uint256 amount0Out = _tokenBorrow == token0 ? _amount : 0;
        uint256 amount1Out = _tokenBorrow == token1 ? _amount : 0;

        bytes memory data = abi.encode(tokenId, _amount, msg.sender);

        IPancakePair(pair).swap(amount0Out, amount1Out, address(this), data);
    }

    function getToken(XType.Universe storage state, uint256 tokenId)
        external
        view
        returns (XType.Token memory token)
    {
        return state.tokens[tokenId];
    }

    function setToken(
        XType.Universe storage state,
        uint256 tokenId,
        uint256 minTimeSinceLastEvent,
        uint256 volumeForToken,
        uint256 maxBountyPerClaim
    ) public {
        XType.Token storage token = state.tokens[tokenId];
        require(token.id == tokenId, "Token not initialized");
        token.minTimeSinceLastEvent = minTimeSinceLastEvent;
        token.volumeForToken = volumeForToken;
        token.maxBountyPerClaim = maxBountyPerClaim;
        emit TokenParamsChanged(
            tokenId,
            minTimeSinceLastEvent,
            volumeForToken,
            maxBountyPerClaim
        );
    }

    function initToken(
        XType.Universe storage state,
        address tokenAddress,
        uint256 minTimeSinceLastEvent,
        uint256 volumeForToken,
        uint256 maxBountyPerClaim
    ) public {
        uint256 nextTokenId = ++state.tokenCounter;
        state.tokens[nextTokenId] = XType.Token({
            id: nextTokenId,
            tokenAddress: tokenAddress,
            lastEvent: 0,
            minTimeSinceLastEvent: minTimeSinceLastEvent,
            maxBountyPerClaim: maxBountyPerClaim,
            volumeForToken: volumeForToken,
            bountyClaimed: 0,
            bountyClaimedCount: 0,
            volumeBought: 0,
            volumeSold: 0
        });
        emit TokenInitialized(
            nextTokenId,
            tokenAddress,
            minTimeSinceLastEvent,
            volumeForToken,
            maxBountyPerClaim
        );
    }

    function pancakeCall(
        XType.Universe storage state,
        address _sender,
        uint256 _amount0,
        uint256 _amount1,
        bytes calldata _data
    ) external {
        address token0 = IPancakePair(msg.sender).token0();
        address token1 = IPancakePair(msg.sender).token1();
        address pair = IPancakeFactory(state.factory).getPair(token0, token1);

        require(msg.sender == pair, "!pair");
        require(_sender == address(this), "!sender");

        (uint256 tokenId, uint256 amount, address flashSwapInitiator) = abi
            .decode(_data, (uint256, uint256, address));

        XType.Token storage token = state.tokens[tokenId];

        address tokenBorrow = token.tokenAddress;
        // about 0.3%
        uint256 amountToRepay = amount + ((amount * 3) / 997) + 1;

        IPancakeSwapV2Router02 pancakeSwapV2Router = IPancakeSwapV2Router02(
            state.pancakeRouterAddress
        );

        IBEP20 WBNB = IBEP20(pancakeSwapV2Router.WETH());
        IBEP20 tokenBorrowBEP20 = IBEP20(tokenBorrow);

        emit PancakeCallbackReceived(
            tokenBorrow,
            amount,
            tokenBorrowBEP20.balanceOf(address(this))
        );

        // address[] memory path = new address[](2);
        // path[0] = address(tokenBorrow);
        // path[1] = pancakeSwapV2Router.WETH();

        // tokenBorrowBEP20.approve(address(pancakeSwapV2Router), amount);

        // pancakeSwapV2Router
        //     .swapExactTokensForTokensSupportingFeeOnTransferTokens(
        //         amount,
