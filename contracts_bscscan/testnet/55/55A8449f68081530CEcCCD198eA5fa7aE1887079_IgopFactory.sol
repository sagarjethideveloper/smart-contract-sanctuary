pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Counters.sol";
import "./CloneFactory.sol";
import "./TokenPair.sol";
import "./ProjectToken.sol";
import "./interfaces/IProjectToken.sol";

contract IgopFactory is CloneFactory {
    using Counters for Counters.Counter;
    Counters.Counter private _projectIds;
    address private _nekoin;
    address private _wallet;
    uint256 private _mintFee;
    address private _priceFeedAggregator;

    ProjectToken[] projectTokens;
    address _projectTokenImplement;
    address _tokenPairImplement;
    mapping(address => mapping(address => address)) public getPair;
    address[] public allPairs;

    // Status definition
    // Pending   - 0
    // Approved  - 1
    // Rejected  - 2
    // Canceled  - 3
    enum Status {PENDING, APPROVED, REJECTED, CANCELLED}

    struct Igop {
        string projectCID;
        string tokenName;
        string tokenSymbol;
        uint256 tokenTotalSupply;
        address creator;
        address contractAddress;
        Status status;
    }

    Igop[] private igopList;

    mapping(uint256 => Igop) public igops;

    constructor(address projectTokenImplement, address tokenPairImplement, address nekoin, address wallet, address priceFeedAggregator) {
        _projectTokenImplement = projectTokenImplement;
        _tokenPairImplement = tokenPairImplement;
        _nekoin = nekoin;
        _wallet = wallet;
        _mintFee = 3173000000000000000;
        _priceFeedAggregator = priceFeedAggregator;
    }

    function proposeIgop(
        string memory projectCID,
        string memory tokenName, 
        string memory tokenSymbol, 
        uint256 tokenTotalSupply
    ) external {
        _projectIds.increment();
        
        uint256 igopId = _projectIds.current();
        Igop storage project = igops[igopId];
        project.projectCID = projectCID;
        project.tokenName = tokenName;
        project.tokenSymbol = tokenSymbol;
        project.tokenTotalSupply = tokenTotalSupply;
        project.creator = msg.sender;
        project.status = Status.PENDING;
        igopList.push(project);

        IBEP20(_nekoin).transferFrom(msg.sender, address(this), _mintFee);
    }

    function approveIgop(uint256 igopId) external onlyOwner {
        Igop storage project = igops[igopId];
        require(project.creator != address(0), 'IGOP does not exist');
        require(project.status == Status.PENDING, 'IGOP status should be pending to call this function.');
        ProjectToken token = ProjectToken(createClone(_projectTokenImplement));
        token.init(project.tokenName, project.tokenSymbol, 18, 0);
        projectTokens.push(token);
        address igopAddress = createPair(address(token));

        IBEP20(_nekoin).transfer(_wallet, _mintFee);
        project.status = Status.APPROVED;
        project.contractAddress = igopAddress;
        igopList.push(project);
    }

    function rejectIgop(uint256 igopId) external onlyOwner {
        Igop storage project = igops[igopId];
        require(project.creator != address(0), 'IGOP does not exist');
        require(project.status == Status.PENDING, 'IGOP status should be pending to call this function.');

        uint256 proccessFee = _mintFee / 10000;
        uint256 amount = _mintFee - proccessFee;
        project.status = Status.REJECTED;
        igopList.push(project);
        IBEP20(_nekoin).transfer(project.creator, amount);
        IBEP20(_nekoin).transfer(_wallet, proccessFee);
    }

    function createPair(address projectToken) internal returns (address) {
        TokenPair pair = TokenPair(createClone(_tokenPairImplement));
        pair.init(_nekoin, projectToken, _priceFeedAggregator);

        getPair[_nekoin][projectToken] = address(pair); // populate mapping
        allPairs.push(address(pair));
        IProjectToken(projectToken).addMinter(address(pair));

        return address(pair);
    }

    function _createIgop() internal {}

    function getProjectToken() external view returns(ProjectToken[] memory) {
        return projectTokens;
    }

    function getallPairs() external view returns(address[] memory) {
        return allPairs;
    }

    modifier onlyOwner {
        require(msg.sender == _wallet, 'Caller is not the owner');
        _;
    }

}

