pragma solidity ^0.8.0;

import './token/BEP20/IBEP20.sol';
import "./interfaces/IProjectToken.sol";
import "./chainlink/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "./ReferenceToken1155.sol";

contract TokenPair is ReferenceToken1155 {
    address public _nekoin;
    address public _projectToken;
    AggregatorV3Interface private _priceFeedAggregator;

    function init(address nekoin, address projectToken, address priceFeedAggregator) external {
        _nekoin = nekoin;
        _projectToken = projectToken;
        _priceFeedAggregator = AggregatorV3Interface(priceFeedAggregator);
    }

    event TokensPurchased(address indexed purchaser, uint256 amount);

    function getLatestPrice() public view returns (int) {
        (,int price,,,) = _priceFeedAggregator.latestRoundData();
        return price * 10 ** 10;
    }

    function buyProjectToken() external payable {
        require(msg.sender != address(0), "wallet is the zero address");
        require(msg.value != 0, "Amount is 0");
        int256 lp = getLatestPrice();
        uint256 bnbInUsd = msg.value * SafeCast.toUint256(lp);
        uint256 token = bnbInUsd / 2000000000000000000;
        IProjectToken(_projectToken).mint(msg.sender, token);
        emit TokensPurchased(msg.sender, token);
    }

    function addFund(uint256 amount) external {
        require(msg.sender != address(0), "wallet is the zero address");
        require(amount != 0, "Amount is 0");
        require(IBEP20(_nekoin).transferFrom(msg.sender, address(this), amount), "Nekoin is require");
        _mint(msg.sender, NekoinReferenceToken, amount, "");
    }
}

