pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "./interfaces/IUniswapV2Router02.sol";
import "./interfaces/IUniswapV2Pair.sol";
import "./interfaces/IERC20.sol";
import "./interfaces/ISAFU.sol";

contract TankLottery is Initializable, OwnableUpgradeable {
  using SafeMathUpgradeable for uint256;

  address public SAFU;
  address public TANK;
  address public BUSD;
  address public manager;

  IUniswapV2Router02 private uniswapV2Router;

  uint256 private COST_BUSD;
  uint256 private COST_TANK;
  uint256 private COST_SAFU;

  mapping(bytes32 => mapping(address => Play)) private plays;
  uint256[] public prizes;

  struct Play {
    address player;
    bool isPlayed;
    uint8 prizeId;
  }

  modifier onlyManager {
    require(msg.sender == manager || msg.sender == owner(), "Not manager or owner");
    _;
  }

  event PlayRequest(address indexed player, bytes32 indexed seed);
  event PlayResult(address indexed player, bytes32 indexed seed, uint256 indexed salt, uint256 point);
  event PlayFeeUpdate(address indexed token, uint256 fee);

  function initialize(
    address safu_,
    address tank_,
    address busd_
  ) external virtual initializer {
    __Ownable_init();

    SAFU = safu_;
    TANK = tank_;
    BUSD = busd_;
    // uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
    uniswapV2Router = IUniswapV2Router02(0xD99D1c33F9fC3444f8101754aBC46c52416550D1);
  }

  receive() external payable {}

  function getBUSDFee() external view returns(uint256) {
    return COST_BUSD;
  }

  function setBUSDFee(uint256 fee) external onlyManager() {
    COST_BUSD = fee;
    emit PlayFeeUpdate(BUSD, fee);
  }

  function getTANKFee() external view returns(uint256) {
    return COST_TANK;
  }

  function setTANKFee(uint256 fee) external onlyManager() {
    COST_TANK = fee;
    emit PlayFeeUpdate(TANK, fee);
  }

  function getSAFUFee() external view returns(uint256) {
    return COST_SAFU;
  }

  function setSAFUFee(uint256 fee) external onlyManager() {
    COST_SAFU = fee;
    emit PlayFeeUpdate(SAFU, fee);
  }

  function getPlay(bytes32 seed, address player) external view returns(Play memory) {
    return plays[seed][player];
  }

  function setPrizes(uint256[] memory newPrizes) external onlyOwner() {
    require(newPrizes.length == 6, "Unmatched prize info");
    for (uint8 i = 0; i < newPrizes.length; i ++) {
      if (newPrizes[i] > 1000) {
        revert();
      }
    }
    prizes = newPrizes;
  }

  function playToBurn(uint8 mode, bytes32 seed) external {
    require(mode < 3, "Unsupported mode");

    if (mode == 0) {
      _takeBUSD();
    } else if (mode == 1) {
      _takeTANK();
    } else {
      _takeSAFU();
    }

    plays[seed][_msgSender()] = Play(_msgSender(), false, 0);
    emit PlayRequest(_msgSender(), seed);
  }

  function receiveRandomness(bytes32 seed, address player, uint256 salt) external onlyManager() {
    require(plays[seed][player].player == player);
    require(plays[seed][player].isPlayed == false);
    uint256 random = uint256(keccak256(abi.encodePacked(
      ((uint256(keccak256(abi.encodePacked(block.coinbase)))) / block.timestamp), 
      ((uint256(keccak256(abi.encodePacked(player)))) / block.timestamp), 
      blockhash(block.number - 1), 
      seed,
      salt
    )));
    uint256 point = random % 1000;

    plays[seed][player].isPlayed = true;
    if (point == 999) {
      plays[seed][player].prizeId = 5;
    } else if (point > 989) {
      plays[seed][player].prizeId = 4;
    } else if (point > 939) {
      plays[seed][player].prizeId = 3;
    } else if (point > 739) {
      plays[seed][player].prizeId = 2;
    } else if (point > 39) {
      plays[seed][player].prizeId = 1;
    }

    _buyback(seed, player, point, salt);
  }

  function _takeBUSD() internal {
    require(IERC20(BUSD).transferFrom(_msgSender(), address(this), COST_BUSD));
  }

  function _takeTANK() internal {
    require(IERC20(TANK).transferFrom(_msgSender(), address(this), COST_TANK));
    
    uint256 amount = IERC20(TANK).balanceOf(address(this));
    address[] memory path = new address[](2);
    path[0] = TANK;
    path[1] = BUSD;

    IERC20(TANK).approve(address(uniswapV2Router), amount);
    uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(amount, 0, path, address(this), block.timestamp);
  }

  function _takeSAFU() internal {
    require(IERC20(SAFU).transferFrom(_msgSender(), address(this), COST_SAFU));

    uint256 amount = IERC20(TANK).balanceOf(address(this));
    address[] memory path = new address[](3);
    path[0] = SAFU;
    path[1] = uniswapV2Router.WETH();
    path[1] = BUSD;

    IERC20(SAFU).approve(address(uniswapV2Router), amount);
    uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(amount, 0, path, address(this), block.timestamp);
  }

  function _buyback(bytes32 seed, address player, uint256 point, uint256 salt) internal {
    uint256 contractBal = IERC20(BUSD).balanceOf(address(this));
    uint256 prizeIndex = plays[seed][player].prizeId;
    uint256 playAmount = contractBal.mul(prizes[prizeIndex]).div(1000);
    if (playAmount > 0) {
      uint256 transAmount = playAmount.div(10);
      IERC20(BUSD).transfer(player, transAmount);
      playAmount = playAmount.sub(transAmount);
      _buybackSAFU(playAmount.div(2));
      _buybackTANK(playAmount.div(2));
    }
    emit PlayResult(player, seed, salt, point);
  }

  function _buybackSAFU(uint256 amount) internal {
    address[] memory path = new address[](3);
    path[0] = BUSD;
    path[1] = uniswapV2Router.WETH();
    path[2] = SAFU;

    IERC20(BUSD).approve(address(uniswapV2Router), amount);
    uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(amount, 0, path, address(this), block.timestamp);

    uint256 safuAmt = IERC20(SAFU).balanceOf(address(this));
    ISAFU(SAFU).burn(safuAmt);
  }

  function _buybackTANK(uint256 amount) internal {
    address[] memory path = new address[](2);
    path[0] = BUSD;
    path[1] = TANK;

    IERC20(BUSD).approve(address(uniswapV2Router), amount);
    uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(amount, 0, path, address(this), block.timestamp);

    uint256 tankAmt = IERC20(TANK).balanceOf(address(this));
    IERC20(TANK).transfer(0x000000000000000000000000000000000000dEaD, tankAmt);
  }
}

