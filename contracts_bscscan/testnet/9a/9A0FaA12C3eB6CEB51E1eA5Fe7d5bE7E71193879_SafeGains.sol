contract SafeGains is  Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    mapping (address => uint256) public balances;


    ISafeGainsReferral public safeGainsReferral;



    SafeGainsDividendTracker public dividendTracker;

    address public deadWallet = 0x000000000000000000000000000000000000dEaD;

    IERC20 public  baseToken;

    

    uint256 public distributionFee = 8; //0.8%
    uint256 public referralComission = 2; //0.2%



    // use by default 300,000 gas to process auto-claiming dividends
    uint256 public gasForProcessing = 300000;


    event onWithdraw(uint256 amount);
    event UpdateDividendTracker(address indexed newAddress, address indexed oldAddress);


    event ReferralComissionPaid(address indexed user, address indexed referrer,uint256 amount);


    event LiquidityWalletUpdated(address indexed newLiquidityWallet, address indexed oldLiquidityWallet);

    event GasForProcessingUpdated(uint256 indexed newValue, uint256 indexed oldValue);


    event SendDividends(
    	uint256 amount
    );

    event ProcessedDividendTracker(
    	uint256 iterations,
    	uint256 claims,
        uint256 lastProcessedIndex,
    	bool indexed automatic,
    	uint256 gas,
    	address indexed processor
    );

    constructor(address _baseToken) public {


    	dividendTracker = new SafeGainsDividendTracker(_baseToken);


        baseToken = IERC20(_baseToken);


        // exclude from receiving dividends
        dividendTracker.excludeFromDividends(address(dividendTracker));
        dividendTracker.excludeFromDividends(address(this));
        dividendTracker.excludeFromDividends(deadWallet);


    }


    function updateDividendTracker(address newAddress) public onlyOwner {
        require(newAddress != address(dividendTracker), "SafeGains: The dividend tracker already has that address");

        SafeGainsDividendTracker newDividendTracker = SafeGainsDividendTracker(payable(newAddress));

        require(newDividendTracker.owner() == address(this), "SafeGains: The new dividend tracker must be owned by the SafeGains token contract");

        newDividendTracker.excludeFromDividends(address(newDividendTracker));
        newDividendTracker.excludeFromDividends(address(this));
        newDividendTracker.excludeFromDividends(owner());

        emit UpdateDividendTracker(newAddress, address(dividendTracker));

        dividendTracker = newDividendTracker;
    }

   

  
  


    function setReferralComission(uint256 value) external onlyOwner{
        referralComission = value;

    }

    function setDistributionFee(uint256 value) external onlyOwner{
        distributionFee = value;
    }




