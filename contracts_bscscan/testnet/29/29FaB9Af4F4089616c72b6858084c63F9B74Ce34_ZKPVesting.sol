pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

import "./interfaces/IZKPToken.sol";

contract ZKPVesting is Ownable {
    struct VestingInfo {
        bool isPreMinted; // mint options
        bool isAdjustable; // if the owner allowed to change the duration and/or start
        uint32 start; // starting timestamp
        uint16 duration; // vesting duration in day
        uint8 unlocked; // percent
        uint96 amount; // vesting total amount
        uint96 released; // released amount from starting point
    }
    uint256 private constant ZKP_MAX_SUPPLY = 1e27;

    IZKPToken private zkpToken;
    address[] public beneficiaries;
    mapping(address => VestingInfo) public pools;

    event Released(address indexed beneficiary, uint256 amount);
    event VestingPoolAdded(address indexed beneficiary, uint256 amount);

    constructor(address _token) {
        zkpToken = IZKPToken(_token);
    }

    // Modifier to check if it's valid beneficiary address.
    modifier onlyValidPool(address _beneficiary) {
        require(pools[_beneficiary].start != 0, "ZkpVest: pool not started");
        _;
    }

    /**
     * @notice Return zkpToken address
     */
    function token() public view returns (address) {
        return _tokenAddr();
    }

    /**
     * @notice Creates a vesting contract that vests its balance of any ERC20 token to the
     * _beneficiary, gradually in a linear fashion until _start + _duration. By then all
     * of the balance will have vested.
     * @param _beneficiary address of the beneficiary to whom vested tokens are transferred.
     * @param _start duration in seconds of the start in which tokens will begin to vest.
     * @param _duration duration in seconds of the period in which the tokens will vest.
     * @param _amount the amount of vesting
     */
    function addVestingPool(
        address _beneficiary,
        uint32 _start,
        uint16 _duration,
        uint8 _unlocked,
        uint96 _amount,
        bool _isPreMinted,
        bool _isAdjustable
    ) public onlyOwner {
        require(pools[_beneficiary].start == 0, "ZkpVest: pool already started");
        require(zkpToken.totalSupply() + _amount <= ZKP_MAX_SUPPLY, "ZkpVest: total amount exceeded");
        require(_start >= block.timestamp, "ZkpVest: invalid start timestamp");
        require(_unlocked <= uint8(100), "ZkpVest: bigger than 100");

        beneficiaries.push(_beneficiary);
        pools[_beneficiary] = VestingInfo(_isPreMinted, _isAdjustable, _start, _duration, _unlocked, _amount, 0);

        if (_isPreMinted) {
            zkpToken.mint(address(this), _amount);
        }

        if (_unlocked > 0) {
            pools[_beneficiary].released = _safe96((_amount * _unlocked) / 100);
            zkpToken.mint(_beneficiary, (_amount * _unlocked) / 100);
        }

        emit VestingPoolAdded(_beneficiary, _amount);
    }

    /**
     * @notice Stop Vesting Pool
     * @param _beneficiary address of the beneficiary to whom vested tokens are transferred.
     */
    function stopVestingPool(address _beneficiary, bool _vestPending) public onlyOwner onlyValidPool(_beneficiary) {
        if (_vestPending) {
            _release(_beneficiary);
        }

        uint16 i;
        for (i = 0; i < beneficiaries.length; i++) {
            if (beneficiaries[i] == _beneficiary) break;
        }
        beneficiaries[i] = beneficiaries[beneficiaries.length - 1];
        beneficiaries.pop();
        delete pools[_beneficiary];
    }

    /**
     * @notice Creates a vesting contract that vests its balance of any ERC20 token to the
     * _beneficiary, gradually in a linear fashion until _start + _duration. By then all
     * of the balance will have vested.
     * @param _beneficiary address of the beneficiary to whom vested tokens are transferred.
     * @param _start duration in seconds of the start in which tokens will begin to vest.
     * @param _duration duration in day of the period in which the tokens will vest.
     * @param _amount the amount of vesting
     */
    function addVestingPools(
        address[] memory _beneficiary,
        uint32[] memory _start,
        uint16[] memory _duration,
        uint8[] memory _unlocked,
        uint96[] memory _amount,
        bool[] memory _isPreMinted,
        bool[] memory _isAdjustable
    ) external onlyOwner {
        require(_beneficiary.length != 0, "ZkpVest: empty array");
        require(_beneficiary.length == _start.length, "ZkpVest: array length not matched");
        require(_beneficiary.length == _amount.length, "ZkpVest: array length not matched");
        require(_beneficiary.length == _duration.length, "ZkpVest: array length not matched");
        require(_beneficiary.length == _isPreMinted.length, "ZkpVest: array length not matched");
        require(_beneficiary.length == _isAdjustable.length, "ZkpVest: array length not matched");

        uint256 sumAmount = zkpToken.totalSupply();
        for (uint8 i = 0; i < _amount.length; i++) {
            sumAmount = sumAmount + _amount[i];
            require(sumAmount <= ZKP_MAX_SUPPLY, "ZkpVest: total amount exceeded");
        }

        for (uint8 i = 0; i < _amount.length; i++) {
            addVestingPool(
                _beneficiary[i],
                _start[i],
                _duration[i],
                _unlocked[i],
                _amount[i],
                _isPreMinted[i],
                _isAdjustable[i]
            );
        }
    }

    /**
     * @notice Transfers vested tokens to beneficiary.
     */
    function release() external onlyValidPool(msg.sender) {
        _release(msg.sender);
    }

    /**
     * @notice Transfers vested tokens to all pools' beneficiary address.
     */
    function massRelease() external onlyOwner {
        for (uint256 i = 0; i < beneficiaries.length; i++) {
            _release(beneficiaries[i]);
        }
    }

    /**
     * @notice Update the start timestamp of pool.
     * @param _beneficiary address that will receive ERC20 token which is being vested.
     * @param _start duration in day of the start in which tokens will begin to vest.
     */
    function updatePoolStart(address _beneficiary, uint32 _start) external onlyOwner onlyValidPool(_beneficiary) {
        VestingInfo storage pool = pools[_beneficiary];
        require(pool.isAdjustable, "ZkpVest: not able to adjust");
        pool.start = _start;
    }

    /**
     * @notice Update the start timestamp of pool.
     * @param _beneficiary address that will receive ERC20 token which is being vested.
     * @param _duration duration in day of the start in which tokens will begin to vest.
     */
    function updatePoolDuration(address _beneficiary, uint16 _duration) external onlyOwner onlyValidPool(_beneficiary) {
        VestingInfo storage pool = pools[_beneficiary];
        require(pool.isAdjustable, "ZkpVest: not able to adjust");
        pool.duration = _duration;
    }

    /**
     * @dev Calculates the amount that has already vested but hasn't been released yet.
     * @param _beneficiary address that will receive ERC20 token which is being vested.
     */
    function releasableAmount(address _beneficiary) public view returns (uint96) {
        VestingInfo memory pool = pools[_beneficiary];

        if (block.timestamp < pool.start) return 0;

        uint96 end = _safe96(pool.start + pool.duration * 86400);
        if (block.timestamp >= end || pool.duration == 0) return pool.amount - pool.released;
        return _safe96(((block.timestamp - pool.start) * pool.amount) / pool.duration / 86400 - pool.released);
    }

    /**
     * @dev Calculates the amount that has already vested.
     * @param _beneficiary address that will receive ERC20 token which is being vested.
     */
    function vestedAmount(address _beneficiary) public view returns (uint256) {
        VestingInfo memory pool = pools[_beneficiary];

        return block.timestamp < pool.start ? 0 : pool.released;
    }

    //////////////////
    //// Internal ////
    //////////////////

    /**
     * @notice Return zkpToken address
     */
    function _tokenAddr() internal view virtual returns (address) {
        return address(zkpToken);
    }

    /**
     * @dev Internal method that transfers vested tokens to beneficiary.
     * @param _beneficiary address that will receive ERC20 token which is being vested.
     */
    function _release(address _beneficiary) internal {
        VestingInfo storage pool = pools[_beneficiary];

        uint96 unreleased = releasableAmount(_beneficiary);
        require(unreleased > 0, "ZkpVest: not enough to release");
        pool.released = _safe96(pool.released + unreleased);
        if (pool.isPreMinted) {
            zkpToken.transfer(_beneficiary, unreleased);
        } else {
            zkpToken.mint(_beneficiary, unreleased);
        }

        emit Released(_beneficiary, unreleased);
    }

    /**
     * @dev Internal method that check if it's not overflowed uint96.
     * @param n unsigned 256 integer value.
     */
    function _safe96(uint256 n) internal pure returns (uint96) {
        require(n < 2**96, "Unsafe96");
        return uint96(n);
    }
}

