import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./settings.sol";
import "./seclib.sol";
import "./uniswap.sol";
import "hardhat/console.sol";

abstract contract Liquid is ERC20, Ownable, Settings {
    using SafeMath for uint256;
    using Address for address;
    using SafeERC20 for IERC20;

    mapping(address => mapping(address => uint256)) internal _allowances;

    IUniswapV2Router02 internal _uniswapV2Router;
    address internal _uniswapV2Pair = address(0);
    address internal _uniswapV2Factory;
    bool internal inSwapAndLiquify;
    bool internal cooldownEnabled = false;

    event MinTokensBeforeSwapUpdated(uint256 minTokensBeforeSwap);
    event AutoLiquidityDisabled();
    event AutoLiquidityEnabled();
    event LiquidityPoolInitialized();
    event LiquidityAdded(uint256 tokenAmount, uint256 ethAmount);
    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    function getPairLive() external view returns (address lpPair) {
        return _uniswapV2Pair;
    }

    function getPairHash() external view returns (address) {
        address factory = 0xcA143Ce32Fe78f1f7019d7d551a6402fC5350c73;

        address pair = address(
            uint256(
                keccak256(
                    abi.encodePacked(
                        hex"ff",
                        factory,
                        keccak256(
                            abi.encodePacked(
                                address(this),
                                _uniswapV2Router.WETH()
                            )
                        ),
                        hex"00fb7f630766e6a796048ea87d01acd3068e8ff67d078148a3fa3f4a84f69bd5"
                    )
                )
            )
        );

        return pair;
    }

    function updateRouter(address newRouterAddress)
        external
        onlyOwner
        lockTheSwap
        returns (address pairAddr)
    {
        IUniswapV2Router02 newUniswapV2Router = IUniswapV2Router02(
            newRouterAddress
        );

        uint256 wrappedBnbBalance = IERC20(newUniswapV2Router.WETH()).balanceOf(
            address(this)
        );
        require(
            //wrappedBnbBalance >= 25,
            wrappedBnbBalance >= 5,
            "updateRouter failed: bnb/eth tokens required for LP"
        );

        pairAddr = IUniswapV2Factory(newUniswapV2Router.factory()).getPair(
            address(this),
            newUniswapV2Router.WETH()
        );

        if (pairAddr == address(0)) {
            pairAddr = IUniswapV2Factory(newUniswapV2Router.factory())
            .createPair(address(this), newUniswapV2Router.WETH());
        }

        require(
            pairAddr != address(0),
            "updateRouter failed: provided router failed to acquire liquidity pair"
        );

        _approve(address(this), address(newUniswapV2Router), _tTotal);
        IERC20(pairAddr).approve(address(newUniswapV2Router), MAX);

        _uniswapV2Pair = pairAddr;
        _uniswapV2Router = newUniswapV2Router;
    }

    // main router: 0x10ED43C718714eb63d5aA57B78B54704E256024E
    // test router: 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3
    // test WBNB: 0xae13d989dac2f0debff460ac112a837c89baa7cd
    // main WBNB: 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c
    // test BNB: 0x1290d3BD9EDf9506a309d77Fb5310441D3487038
    // BUSD 0x8301f2213c0eed49a7e28ae4c3e91722919b8b47

    // function openTrading() external onlyOwner() {
    //     require(!tradingOpen,"trading is already open");
    //     IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
    //     uniswapV2Router = _uniswapV2Router;
    //     _approve(address(this), address(uniswapV2Router), _tTotal);
    //     uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), _uniswapV2Router.WETH());
    //     uniswapV2Router.addLiquidityETH{value: address(this).balance}(address(this),balanceOf(address(this)),0,0,owner(),block.timestamp);
    //     swapEnabled = true;
    //     cooldownEnabled = true;
    //     _maxTxAmount = 4500000000 * 10**9;
    //     tradingOpen = true;
    //     IERC20(uniswapV2Pair).approve(address(uniswapV2Router), type(uint).max);
    // }

    function swapAndLiquifyNow() external onlyOwner {
        uint256 contractBalance = this.balanceOf(address(this));
        swapTokensForEth(contractBalance);
    }

    function startRouting() public onlyOwner returns (bool) {
        require(
            _uniswapV2Factory != address(0),
            "startRouting failed: factory address invalid"
        );

        IUniswapV2Router02 uniswapV2Router = IUniswapV2Router02(_uniswapV2Factory);

        uint256 wrappedBnbBalance = IERC20(uniswapV2Router.WETH()).balanceOf(
            address(this)
        );
        console.log("wrappedBnbBalance: %s", wrappedBnbBalance);

        console.log("tokenBalance: %s", this.balanceOf(address(this)));

        require(
            //wrappedBnbBalance >= 25,
            wrappedBnbBalance >= 1,
            "startRouting failed: bnb/eth tokens required for LP"
        );

        console.log("startRouter: checking for existing liquidity pool");
        // Initialize liquidity pool with half of total tokens

        address uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory())
        .getPair(address(this), _uniswapV2Router.WETH());

        if (uniswapV2Pair == address(0)) {
            console.log("startRouter: creating new liquidity pool");
            uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory())
            .createPair(address(this), uniswapV2Router.WETH());
            console.log("startRouter: setting initial token value");
            setExchangeRateAddLiquidity();
        }

        console.log("startRouter: liquidity pool ready");

        require(
            uniswapV2Pair != address(0),
            "startRouting failed: router failed to acquire liquidity pair"
        );

        _uniswapV2Pair = uniswapV2Pair;
        _uniswapV2Router = uniswapV2Router;

        _approve(address(this), address(uniswapV2Router), _tTotal);
        IERC20(_uniswapV2Pair).approve(address(uniswapV2Router), MAX);


        cooldownEnabled = true;
        console.log("startRouter: complete");
        return true;
    }

    function setExchangeRateAddLiquidity() private lockTheSwap {
        //IERC20(getPair()).approve(address(_uniswapV2Router), type(uint).max);

        //Add liquidity
        _uniswapV2Router.addLiquidityETH{value: address(this).balance}(
            address(this),
            this.balanceOf(address(this)),
            0,
            0,
            address(this),
            block.timestamp
        );

        emit LiquidityPoolInitialized();
    }

    function enableAutoLP(uint256 liquidityFee) public onlyOwner {
        _liquidityFee = liquidityFee;
        swapAndLiquifyEnabled = true;
        emit AutoLiquidityEnabled();
    }

    function disableAutoLP() public onlyOwner {
        swapAndLiquifyEnabled = false;
        _liquidityFee = 0;
        emit AutoLiquidityDisabled();
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        _approve(address(this), address(_uniswapV2Router), tokenAmount);

        _uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0,
            0,
            address(this),
            block.timestamp
        );

        emit LiquidityAdded(tokenAmount, ethAmount);
    }

    function swapAndLiquify(uint256 contractTokenBalance) internal lockTheSwap {
        uint256 half = contractTokenBalance.div(2);
        uint256 otherHalf = contractTokenBalance.sub(half);

        uint256 initialBalance = address(this).balance;

        swapTokensForEth(half);

        uint256 newBalance = address(this).balance.sub(initialBalance);

        addLiquidity(otherHalf, newBalance);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = _uniswapV2Router.WETH();

        _approve(address(this), address(_uniswapV2Router), tokenAmount);

        _uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }
}

