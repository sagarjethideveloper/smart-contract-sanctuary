import "./RewardVault.sol";


interface IMigratorChef {
    // Perform LP token migration
    function migrate(IBEP20 token) external returns (IBEP20);
}

contract Farm is Ownable {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    

    // Info of each user.
    struct UserInfo {
        uint256 amount;     // How many LP tokens the user has provided.
        uint256 storedRewardToken; // RewardToken that's here for safe keeping.
        uint256 lastStoredRewardTokenBlockNo; // we keep track of the most recent storage of rewardTokens. 
        // ^^^^ This only updates on withdrawls, deposits and harvests
    }


    // Info of each pool.
    struct PoolInfo {
        IBEP20 lpToken;           // Address of LP token contract.
        uint256 allocPoint;       // How many allocation points assigned to this pool. RewardToken to distribute per block.
        uint256 lastRewardBlock; // Last block number that RewardToken distribution occurs.
        uint256 accRewardTokenPerShare; // Accumulated RewardToken per share, times 1e12. See below.
        uint256 depositFee;       // deposit fee for staking.
    }

    struct PoolSnapshots {
        mapping (uint256 => uint256) lpSupplyMapping; // Everytime someone makes a deposit, keep track of before and after LP supply
        uint256[] depositBlocks; // deposit block numbers, to be used as keys for lpSupply mapping;
    }

    mapping (uint256 => uint256) rewardTokenPerBlockSnapshot; //rewardTokens per block by block snapshot

    // The RewardToken TOKEN!
    IBEP20 public rewardToken;

    // RewardToken tokens released per block.
    uint256 private _rewardTokenPerBlock = 0;

    // RewardToken tokens released per block.
    uint256 private _estNextRewardUpdate = 0;
    
    // Max blocks per month - used to calculate how many rewards are being released per block.
    uint256 public MAX_BLOCKS_PER_MONTH = 893000;

    // Early harvest cutoff time (12 hours by default)
    uint256 public EARLY_HARVEST_BLOCK_COUNT = 14400;

    // Early harvest fee.
    uint256 public EARLY_HARVEST_FEE = 10;

    // The vault where rewards are stored.
    RewardVault public rewardVault;

    // The migrator contract. It has a lot of power. Can only be set through owner.
    IMigratorChef public migrator;

    // Info of each pool.
    PoolInfo[] public poolInfo;
    mapping (uint256 => PoolSnapshots) poolSnapshots;

    // Info of each user that stakes LP tokens.
    mapping (uint256 => mapping (address => UserInfo)) public userInfo;

    // Total allocation points. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint = 0;

    // The block number when RewardToken emission starts.
    uint256 public startBlock;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);
    
    constructor(
        IBEP20 _rewardToken,
        RewardVault _rewardVault,
        uint256 _startBlock,
        uint256 _maxBlocksPerMonth,
        uint256 _earlyHarvestBlockCount
    ) public {
        rewardToken = _rewardToken;
        _rewardTokenPerBlock = 0;

        rewardVault = _rewardVault;
        
        // 28k blocks per day. We want each top up to last a month.
        // 893k blocks per month. We want to release 0.000001119820829
        // or 0.0001119820829% per block.
        // this is calculated upon depositing rewards.
        // Max multiplier in this case is 893000.

        startBlock = _startBlock;
        totalAllocPoint = 0;

        MAX_BLOCKS_PER_MONTH = _maxBlocksPerMonth;
        EARLY_HARVEST_BLOCK_COUNT = _earlyHarvestBlockCount;
        

    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    // Top up rewards by adding rewardTokens to this contract.
    // This happens once a month (manual on launch)
    // Will switch to an automatic emission system in the future
    // RewardTokens will eventually be emitted via smart contract.
    function addRewards(uint256 amount) public {
        rewardToken.transferFrom(msg.sender, address(rewardVault), amount);
        _rewardTokenPerBlock = rewardToken.balanceOf((address(rewardVault))).div(MAX_BLOCKS_PER_MONTH);
        rewardTokenPerBlockSnapshot[block.number] = _rewardTokenPerBlock;
        _estNextRewardUpdate = (block.number).add(MAX_BLOCKS_PER_MONTH);
        // update the current rewardTokenPerblock for each pool. 
        // throw in a new deposit mapping per pool
        massUpdatePools();
    }

    // In the event this contract is buggy, this is a failsafe.
    function _removeRewards(uint256 amount, address account) internal {
        rewardVault.sendReward(account, amount);
        _rewardTokenPerBlock = rewardToken.balanceOf((address(rewardVault))).div(MAX_BLOCKS_PER_MONTH);
        rewardTokenPerBlockSnapshot[block.number] = _rewardTokenPerBlock;
        _estNextRewardUpdate = (block.number).add(MAX_BLOCKS_PER_MONTH);
        massUpdatePools();
    }

    function removeRewards(uint256 amount) public onlyOwner {
        _removeRewards(amount, msg.sender);
    }

    // Add a new lp to the pool. Can only be called by the owner.
    // XXX DO NOT add the same LP token more than once. Rewards will be messed up if you do.
    function add(uint256 _allocPoint, IBEP20 _lpToken, bool _withUpdate) public onlyOwner {
        if (_withUpdate) {
            // massUpdatePools();
        }
        uint256 lastRewardBlock =
            block.number > startBlock ? block.number : startBlock;

        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        uint256[] memory depositBlocks;
        poolInfo.push(PoolInfo({
            lpToken: _lpToken,
            allocPoint: _allocPoint,
            lastRewardBlock: lastRewardBlock,
            accRewardTokenPerShare: 0,
            depositFee: 4
        }));
        poolSnapshots[poolInfo.length - 1] = PoolSnapshots({
            depositBlocks: depositBlocks
        });
    }


    function setEarlyHarvestFee(uint256 fee, uint256 blockcount) public onlyOwner{
        EARLY_HARVEST_FEE = fee;
        EARLY_HARVEST_BLOCK_COUNT = blockcount;
    }

    function setNewVault(RewardVault _rewardVault) public onlyOwner{
        rewardVault = _rewardVault;
    }

    // Update the pool settings.
    function setPoolVariables(uint256 _pid, uint256 _allocPoint, uint256 depositFee, bool _withUpdate) public onlyOwner {
        uint256 prevAllocPoint = poolInfo[_pid].allocPoint;
        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].depositFee = depositFee;
        if (prevAllocPoint != _allocPoint) {
            totalAllocPoint = totalAllocPoint.sub(prevAllocPoint).add(_allocPoint);
        }
        if (_withUpdate) {
            massUpdatePools();
        }
    }

    // Set the migrator contract. Can only be called by the owner.
    function setMigrator(IMigratorChef _migrator) public onlyOwner {
        migrator = _migrator;
    }

    // Migrate lp token to another lp contract. 
    // Can be called by anyone. We trust that migrator contract is good.
    function migrate(uint256 _pid) public {
        require(address(migrator) != address(0), "migrate: no migrator");
        PoolInfo storage pool = poolInfo[_pid];
        IBEP20 lpToken = pool.lpToken;
        uint256 bal = lpToken.balanceOf(address(this));
        lpToken.safeApprove(address(migrator), bal);
        IBEP20 newLpToken = migrator.migrate(lpToken);
        require(bal == newLpToken.balanceOf(address(this)), "migrate: bad");
        pool.lpToken = newLpToken;
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to) public view returns (uint256) {
        uint256 multiplier = _to.sub(_from) < MAX_BLOCKS_PER_MONTH ? _to.sub(_from) : MAX_BLOCKS_PER_MONTH;
        return multiplier;
    }

    function _getPoolSnapshots(uint256 _pid) view internal returns (
        mapping(uint256 => uint256) storage, uint256[] memory
    ){
        return (
            poolSnapshots[_pid].lpSupplyMapping,
            poolSnapshots[_pid].depositBlocks
        );
    }

    function _updatePoolaccRewardTokenPerShare(uint256 _pid, uint256 amount) internal 
    {  
        PoolInfo storage pool = poolInfo[_pid];
        pool.accRewardTokenPerShare = amount;
    }
    
    function _getPoolAllocPoint(uint256 _pid) view internal returns (uint256){
        PoolInfo storage pool = poolInfo[_pid];
        return pool.allocPoint;
    }

    function _getRewardTokenTotalRewardDebt(
        uint256 multiplier, uint256 _perBlockRewardToken, uint256 _pid
    ) view internal returns (uint256){
        uint256 allocPoint = _getPoolAllocPoint(_pid);
        return multiplier.mul(_perBlockRewardToken).mul(allocPoint).div(totalAllocPoint);
    }

    function _calcPendingAmounts(
        uint256 accRewardTokenPerShare, address _user, uint256 _pid
    ) view internal returns (uint256){
        UserInfo storage user = userInfo[_pid][_user];
        return user.amount
            .mul(accRewardTokenPerShare)
            .div(1e12);
    }

    function getAndUpdatePendingRewardToken(uint256 _pid, address _user) public returns (uint256) {
        UserInfo storage user = userInfo[_pid][_user];
        PoolInfo storage pool = poolInfo[_pid];
        (
            uint256 pendingAmount, 
            uint256 lastStoredRewardTokenBlockNo, 
            uint256 accRewardTokenPerShare
        ) = _pendingRewardToken(_pid, _user);

        user.storedRewardToken = pendingAmount;
        user.lastStoredRewardTokenBlockNo = lastStoredRewardTokenBlockNo;
        pool.accRewardTokenPerShare = accRewardTokenPerShare;
        return pendingAmount;
    }

    function pendingRewardToken(uint256 _pid, address _user) public view returns (uint256) {
       (uint256 pendingAmount,,) = _pendingRewardToken(_pid, _user);
        return pendingAmount;
    }

    function rewardTokenPerBlock() public view returns (uint256) {
        if (block.number > _estNextRewardUpdate || 
            rewardToken.balanceOf(address(rewardVault)) < _rewardTokenPerBlock) {
            return 0;
        }
        return _rewardTokenPerBlock;
    }

    function estNextRewardUpdate() public view returns (uint256) {
        return _estNextRewardUpdate;
    }

    // View function to see pending RewardTokens on frontend.
    function _pendingRewardToken(uint256 _pid, address _user) public view returns (uint256, uint256, uint256) {
        UserInfo storage user = userInfo[_pid][_user];
        PoolInfo storage pool = poolInfo[_pid];
        (
            mapping(uint256 => uint256) storage lpSupplyMapping, 
            uint256[] memory depositBlocks
        ) = _getPoolSnapshots(_pid);
        
        uint256[13] memory values;
        // we're doing this funky array to represent variables because
        // of solidity's BS "stack too deep" crap
        values = [
            uint256(0),     // 0 lpSupply
                    0,      // 1 debtAccRewardTokenPerShare
                    0,      // 2 accRewardTokenPerShare
                    0,      // 3 depositIndex,
                    0,      // 4 arrayElement
                    0,      // 5 from
                    0,      // 6 to
                    0,      // 7 user.storedRewardToken
                    0,      // 8 user.lastStoredRewardTokenBlockNo
                    0,      // 9 _rewardTokenPerBlock
                    0,      // 10 rewardTokenReward
                    0,      // 11 amountWithoutDebt
                    0       // 12 userDebt
        ];
        values[7] = user.storedRewardToken;
        values[8] = user.lastStoredRewardTokenBlockNo;
        values[2] = pool.accRewardTokenPerShare;
        // for every block above lastStoredRewardTokenBlockNo, we need to calculate and sum
        if ( depositBlocks.length == 0 ){
            // return user.storedRewardToken, user.lastStoredRewardTokenBlockNo, accRewardTokenPerShare
            return (values[7], values[8], values[2]);
        }
        
        // We store all blocks where a deposit was made.
        // We're getting the index in our array of the block where 
        // the user last made a deposit

        // Deposit Index
        values[3] = binarySearch(depositBlocks, values[8]);
        
        // arrayElement -- This should be equivalent to user.lastStoredRewardTokenBlockNo
        // This is not always the case though because binarySearch is not perfect.

        // arrayElement
        values[4] = depositBlocks[values[3]];

        // Look at element in array. If exactly matches, then index is correct.
        // If element is smaller than userBlock, add one. if element is larger, minus 1.
        if (values[4] > values[8]){
            values[3]--;
        }

        // i = depositIndex; -- this is the starting block number that we start calculating
        // rewards from.
        uint256 i = values[3];
        
        // Iterate from deposit block before lastStoredRewardToken to Now.
        // For each interval in depositBlocks where reward per share ratio changes
        // (ie anytime anyone makes a deposit)
        // we calculate how much we owe the user for that interval. 
        // Then sum it with the next intervals until we get to the current block no.
        while(i < depositBlocks.length) {
                // check each interval within depositBlocks.

                // from = user.lastStoredRewardTokenBlockNo
                values[5] = values[8];
                
                // to = block.number
                values[6] = block.number;

                // If the "to" pointer / deposit index is not the most recent deposit block,
                // set the "to" pointer to the end of the interval we're on.
                if (i + 1 < depositBlocks.length){
                    values[6] = depositBlocks[i + 1];
                }

                // get rewardToken per block for the user's last stored rewardToken block no.
                // rewardTokenPerBlock = ...
                values[9] = rewardTokenPerBlockSnapshot[values[5]];

                // calculate the rewardToken reward for this interval
                // rewardTokenReward = ...
                values[10] = _getRewardTokenTotalRewardDebt(
                    getMultiplier(values[5], values[6]), values[9], _pid);

                // get the LP supply at the time of the "to" pointer.
                // lpSupply = mapping[to]
                values[0] = lpSupplyMapping[values[5]];
                // if rewardTokenReward != 0 && lpSupply != 0
                if (values[10] != 0 && values[0] != 0) {
                    // calculate accRewardTokenPerShare
                    // accRewardTokenPerShare = rewardTokenReward divided by lpSupply
                    values[2] = values[10].mul(1e12).div(values[0]);
                    
                    // 
                    uint256 rewardTokenDebt = _getRewardTokenTotalRewardDebt(
                        // multiplier is the number of blocks in the interval
                        // we're getting the blocks from user.lastStoredBlock -- to.
                        getMultiplier(values[5], values[8]), // blocks in interval
                        values[9], // rewardToken per block
                        _pid); // pool number.
                    // debtAccRewardTokenPerShare = rewardTokenDebt div lpSupply.
                    values[1] = rewardTokenDebt.mul(1e12).div(values[0]);
                } else {
                    // rewardToken per share for this interval is zero
                    // accRewardTokenPerShare = 0
                    values[2] = 0;
                    // debtAccRewardTokenPerShare = 0
                    values[1] = 0;
                }

                // amount without Debt = fn(accRewardTokenPerShare, user, pool).add(storedRewardToken)
                values[11] = _calcPendingAmounts(values[2], _user, _pid)
                                            .add(values[7]);
                // user Debt = 
                values[12] = _calcPendingAmounts(values[1], _user, _pid);
                
                // update storedRewardToken and prepare to move onto next interval
                values[7] = values[11].sub(values[12]);
                
                {   
                    // update user.lastStoredRewardToken (for the purposes of this function)
                    // to be equal to the beginning of the next intreval
                    // user.lastStoredRewardTokenBlockNo = to;
                    values[8] = values[6];
                    // increase pointer to look at next interval.
                    i++;
                }
        }

        if (values[7] > rewardToken.balanceOf((address(rewardVault)))){
            // return this balance, user.laststoredRewardToken, accRewardTokenPershare
            return (rewardToken.balanceOf((address(rewardVault))), values[8], values[2]);
        }

        // return user.storedRewardToken, user.lastStoredRewardTokenBlockNo, accRewardTokenPerShare.
        return (values[7], values[8], values[2]);
    }

    // Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            PoolInfo storage pool = poolInfo[pid];
            PoolSnapshots storage poolSnaps = poolSnapshots[pid];
            pool.lastRewardBlock = block.number;
            poolSnaps.lpSupplyMapping[block.number] = 
                poolSnaps.lpSupplyMapping[poolSnaps.depositBlocks.length - 1];
            poolSnaps.depositBlocks.push(block.number);
            updatePool(pid);
        }
    }


    // Update reward variables of the given pool to be up-to-date.
    // this happens before depositing though. 
    function updatePool(uint256 _pid) internal {
        PoolInfo storage pool = poolInfo[_pid];
        PoolSnapshots storage poolSnaps = poolSnapshots[_pid];
        if (block.number < pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0) {
            delete poolSnaps.depositBlocks;
            pool.lastRewardBlock = block.number;
            return;
        }
        // these are not needed as they are calculated in pending rewardToken
    }

    // Deposit LP tokens to farm for RewardToken allocation.
    // We also call this function if we are harvesting lol. This should probably be changed.
    function deposit(uint256 _pid, uint256 _amount) public {
        PoolInfo storage pool = poolInfo[_pid];
        PoolSnapshots storage poolSnaps = poolSnapshots[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        
        // if harvesting, see if harvestTax needs to be applied
        uint256 harvestTax = 0;
        if (_amount == 0 && 
            block.number < user.lastStoredRewardTokenBlockNo.add(EARLY_HARVEST_BLOCK_COUNT) 
        ){
            harvestTax = 1;
        }

        // Ensure all variables are up to date
        updatePool(_pid);
        uint256 pending = getAndUpdatePendingRewardToken(_pid, msg.sender);
        
        // Perform harvest function.
        if(pending > 0 && _amount == 0) {
            // if before 12h from last action, apply early harvest harvestTax
            if (harvestTax > 0) {
                harvestTax = pending.mul(EARLY_HARVEST_FEE).div(100);
                pending = pending.sub(harvestTax);
            }
            //if they're depositing nothing, then it's a harvest.
            if (pending > rewardToken.balanceOf(address(rewardVault))){
                pending = rewardToken.balanceOf(address(rewardVault));
                //if rewards running out, reset all.
                _removeRewards(pending, msg.sender);
            } else {
                rewardVault.sendReward(msg.sender, pending);
            }
            user.storedRewardToken = 0;
        }
        else  {
            // else, user is adding to their deposit,
            // store their existing pending rewardToken to prepare for new reward calculations.
            user.storedRewardToken = pending;
        }
        if (_amount > 0) {
            
            // deposit lps
            uint256 depositTax = pool.depositFee.mul(_amount).div(100);
            pool.lpToken.transferFrom(
                address(msg.sender), address(this), _amount.sub(depositTax));
            pool.lpToken.transferFrom(address(msg.sender), owner(), depositTax);
            user.amount = user.amount.add(_amount.sub(depositTax));

            uint256 lpSupply = pool.lpToken.balanceOf((address(this)));
            poolSnaps.lpSupplyMapping[block.number] = lpSupply;
            rewardTokenPerBlockSnapshot[block.number] = _rewardTokenPerBlock;
            poolSnaps.depositBlocks.push(block.number);
        }
        // use this block number for future pending calculations.
        // add future pending to existing stored rewardToken.
        user.lastStoredRewardTokenBlockNo = block.number; 
        emit Deposit(msg.sender, _pid, _amount);
    }

    // Withdraw LP tokens from Pool.
    // NOTE: to "harvest" rewards, perform a deposit with amount = 0;
    function withdraw(uint256 _pid, uint256 _amount) public {
        PoolInfo storage pool = poolInfo[_pid];
        PoolSnapshots storage poolSnaps = poolSnapshots[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");
        
        uint256 pending = getAndUpdatePendingRewardToken(_pid, msg.sender);
        if(pending > 0) {
            // store their rewardToken for safekeeping.
            user.storedRewardToken = pending;
        }
        if(_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.lpToken.transfer(address(msg.sender), _amount);
            uint256 lpSupply = pool.lpToken.balanceOf((address(this)));

            poolSnaps.lpSupplyMapping[block.number] = lpSupply;
            rewardTokenPerBlockSnapshot[block.number] = _rewardTokenPerBlock;
            poolSnaps.depositBlocks.push(block.number);
        }
        user.lastStoredRewardTokenBlockNo = block.number;
        updatePool(_pid);
        emit Withdraw(msg.sender, _pid, _amount);
    }


    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        pool.lpToken.safeTransfer(address(msg.sender), user.amount);
        emit EmergencyWithdraw(msg.sender, _pid, user.amount);
        user.amount = 0;
        user.lastStoredRewardTokenBlockNo = block.number;
    }

    // Small binary search function to assist with performance.
    function find_internal(uint[] memory array, uint begin, uint end, uint value) internal view returns (uint ret) {
        uint len = end - begin;
        if (len == 0 || (len == 1 && array[begin] != value)) {
            return begin; // return closest index
        }
        uint mid = begin + len / 2;
        uint v = array[mid];
        if (value < v)
            return find_internal(array, begin, mid, value);
        else if (value > v)
            return find_internal(array, mid + 1, end, value);
        else
            return mid;
    }
  
    function binarySearch(uint[] memory array, uint value) public view returns (uint ret) {
        uint returnIndex = find_internal(array, 0, array.length, value);
        return returnIndex >= array.length ? array.length - 1 : returnIndex;
    }
}

