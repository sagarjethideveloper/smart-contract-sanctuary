pragma solidity 0.8.7;
pragma experimental ABIEncoderV2;

// SPDX-License-Identifier: MIT



interface IFairLaunchV1 {
    // Data structure
    struct UserInfo {
        uint256 amount;
        uint256 rewardDebt;
        uint256 bonusDebt;
        address fundedBy;
    }
    struct PoolInfo {
        address stakeToken;
        uint256 allocPoint;
        uint256 lastRewardBlock;
        uint256 accAlpacaPerShare;
        uint256 accAlpacaPerShareTilBonusEnd;
