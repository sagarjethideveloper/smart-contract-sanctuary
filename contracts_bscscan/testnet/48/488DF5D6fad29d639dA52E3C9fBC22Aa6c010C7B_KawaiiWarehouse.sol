    }
}

contract KawaiiWarehouse is Ownable {
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeMath for uint256;


    EnumerableSet.UintSet internal _notTransferableNFT;
    mapping(address => mapping(string => uint256)) public maxCategoryOfUser;

    mapping(string => uint256) public limitDefaults;

    mapping(address => bool) public isSigner;


    constructor() public {
        isSigner[msg.sender] = true;
        limitDefaults['trees'] = 50;
        limitDefaults['animals'] = 50;
        limitDefaults['fields'] = 20;
    }



    function setSigner(address account, bool _is) public onlyOwner {
        isSigner[account] = _is;
    }


    function setLimitDefault(string calldata _category, uint256 _limited) external onlyOwner {
        limitDefaults[_category] = _limited;
    }

    function setNotTransferableNFT(uint256 _id) public onlyOwner {
        _notTransferableNFT.add(_id);
    }

    function removeNotTransferableNFT(uint256 _id) public onlyOwner {
        _notTransferableNFT.remove(_id);
    }

    function notTransferableNFT(uint256 _id) public view returns (bool){
        return _notTransferableNFT.contains(_id);
    }

    function getNotTransferableNFT(uint256 _at) public view returns (uint256){
        return _notTransferableNFT.at(_at);
    }

    function getNotTransferableNFTLength() public view returns (uint256){
        return _notTransferableNFT.length();
    }


    function getLimited(address _user, string calldata _category) external view returns (uint256){
        uint256 _maxCategoryOfUser = maxCategoryOfUser[_user][_category];
        if (_maxCategoryOfUser > 0) return _maxCategoryOfUser;
        uint256 _limitDefault = limitDefaults[_category];
        if (_limitDefault > 0) return _limitDefault;
        return uint256(- 1);
    }


    function upMaxCategoryOfUser(string calldata category, uint256 amount, address sender) external {
        require(isSigner[msg.sender], "WD: Signer should sign");
        maxCategoryOfUser[sender][category] = amount;

    }

}

