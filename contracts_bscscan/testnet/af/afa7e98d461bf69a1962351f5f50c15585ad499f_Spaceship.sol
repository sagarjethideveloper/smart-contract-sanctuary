contract SpaceshipAttr is AccessControlUpgradeable {
    bytes32 public constant GAME_ADMIN = keccak256("GAME_ADMIN");

    uint256 currentVersion;

    mapping(uint256 => mapping(uint256 => uint256[2])) public baseAttr; // rarity => attr => min, max
    // 0: Fual
    // 1: HP
    // 2: Attack
    // 3: Level

    mapping(uint256 => ShipAttr) public attrs;

    function initialize() public initializer {
        __AccessControl_init();

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(GAME_ADMIN, msg.sender);

        // https://docs.google.com/spreadsheets/d/1HxMgT5B1VL6yRrKh_oItbhFnl47yg-pDYRCZ-nJu10A/edit?usp=sharing
        baseAttr[0][0] = [15, 25];
        baseAttr[0][1] = [15, 25];
        baseAttr[0][2] = [15, 25];
        baseAttr[0][3] = [0, 20];

        baseAttr[1][0] = [30, 40];
        baseAttr[1][1] = [30, 40];
        baseAttr[1][2] = [30, 40];
        baseAttr[1][3] = [0, 40];

        baseAttr[2][0] = [45, 55];
        baseAttr[2][1] = [45, 55];
        baseAttr[2][2] = [45, 55];
        baseAttr[2][3] = [0, 60];

        baseAttr[3][0] = [60, 70];
        baseAttr[3][1] = [60, 70];
        baseAttr[3][2] = [60, 70];
        baseAttr[3][3] = [0, 100];

        baseAttr[4][0] = [75, 85];
        baseAttr[4][1] = [75, 85];
        baseAttr[4][2] = [75, 85];
        baseAttr[4][3] = [0, 140];

        currentVersion = 0;
    }

    struct ShipAttr {
        bool getSuccess;
        uint256 tokenId;
        uint256 seed;
        uint256 version;
        uint256 currentEXP;
        uint256 currentLevel;
        uint256 maxLevel;
        uint256 currentFual;
        uint256 baseFual;
        uint256 baseHP;
        uint256 baseAttack;
    }

    function setMinMaxAttr(
        uint256 rarity,
        uint256 attr,
        uint256 min,
        uint256 max
    ) public restricted {
        baseAttr[rarity][attr] = [min, max];
    }

    function setSpaceshipAttr(
        uint256 tokenId,
        uint256 attrSeed,
        uint256 rarity
    ) public restricted {
        uint256 baseFual = RandomUtil.getRandomAttr(attrSeed, 0, baseAttr[rarity][0][0], baseAttr[rarity][0][1]);
        uint256 baseHP = RandomUtil.getRandomAttr(attrSeed, 1, baseAttr[rarity][1][0], baseAttr[rarity][1][1]);
        uint256 baseAttack = RandomUtil.getRandomAttr(attrSeed, 2, baseAttr[rarity][2][0], baseAttr[rarity][2][1]);

        ShipAttr memory sAttr = ShipAttr(
            true,
            tokenId,
            attrSeed, // seed
            currentVersion, // version
            0,
            baseAttr[rarity][3][0], // curentLevel
            baseAttr[rarity][3][1], // maxLevel
            baseFual, // currentFual
            baseFual, // baseFual
            baseHP,
            baseAttack
        );

        attrs[tokenId] = sAttr;
    }

    function getSpaceshipAttr(uint256 tokenId)
        public
        view
        shipRequired(tokenId)
        returns (
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        ShipAttr memory ship = attrs[tokenId];
        // TODO: calc hp and attack
        return (ship.currentFual, ship.currentLevel, ship.baseHP, ship.baseAttack);
    }

    function getCurrentLevel(uint256 tokenId) public view shipRequired(tokenId) returns (uint256) {
        ShipAttr memory ship = attrs[tokenId];
        return (ship.currentLevel);
    }

    function getCurrentFual(uint256 tokenId) public view shipRequired(tokenId) returns (uint256) {
        ShipAttr memory ship = attrs[tokenId];
        return (ship.currentFual);
    }

    function getCurrentEXP(uint256 tokenId) public view shipRequired(tokenId) returns (uint256) {
        ShipAttr memory ship = attrs[tokenId];
        return (ship.currentEXP);
    }

    function setCurrentEXP(uint256 tokenId, uint256 _exp) public restricted shipRequired(tokenId) {
        attrs[tokenId].currentEXP = _exp;
    }

    function increaseLevel(uint256 tokenId) public restricted shipRequired(tokenId) {
        ShipAttr memory ship = attrs[tokenId];
        if (ship.currentLevel >= ship.maxLevel) {
            return;
        }
        attrs[tokenId].currentLevel += 1;
    }

    function changeFual(uint256 tokenId, uint256 _fual) public restricted shipRequired(tokenId) {
        ShipAttr memory ship = attrs[tokenId];
        if (_fual >= ship.baseFual) {
            attrs[tokenId].currentFual = ship.baseFual;
            return;
        }
        attrs[tokenId].currentFual = _fual;
    }

    modifier shipRequired(uint256 tokenId) {
        ShipAttr memory ship = attrs[tokenId];
        require(ship.getSuccess, "SNF");
        _;
    }

    modifier restricted() {
        _restricted();
        _;
    }

    function _restricted() internal view {
        require(hasRole(GAME_ADMIN, msg.sender), "NGA"); // NGA: Not Game Admin
    }

    function addAdmin(address _admin) public restricted {
        _setupRole(GAME_ADMIN, _admin);
    }
}

