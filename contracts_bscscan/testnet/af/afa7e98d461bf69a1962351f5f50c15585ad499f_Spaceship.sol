pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "abdk-libraries-solidity/ABDKMath64x64.sol";
import "hardhat/console.sol";
import "./interfaces/IPriceOracle.sol";
import "./interfaces/IRandoms.sol";
import "./Utils.sol";
import "./SpaceshipAttr.sol";

contract Spaceship is ERC721EnumerableUpgradeable, ERC721URIStorageUpgradeable, AccessControlUpgradeable {
    using SafeERC20 for IERC20;
    using ABDKMath64x64 for int128;

    using SafeMath for uint8;
    using SafeMath for uint16;
    using SafeMath for uint64;
    using SafeMath for uint256;

    SpaceshipAttr public spaceshipAttr;

    uint256 private lastMintedBlock;
    uint256 private firstMintedOfLastBlock;

    mapping(uint256 => uint256) public lastTransferTimestamp;

    bytes32 public constant GAME_ADMIN = keccak256("GAME_ADMIN");
    bytes32 public constant RECEIVE_DOES_NOT_SET_TRANSFER_TIMESTAMP =
        keccak256("RECEIVE_DOES_NOT_SET_TRANSFER_TIMESTAMP");

    uint256 public transferCooldown;
    bool public isTransferCooldown;

    Ship[] public tokens;

    function initialize() public initializer {
        __ERC721_init("SIP.SPACE (space SIP) Ship", "SIPS");

        __AccessControl_init_unchained();

        __ERC721Enumerable_init_unchained();
        __ERC721URIStorage_init_unchained();

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(GAME_ADMIN, msg.sender);

        transferCooldown = 1 days;
        isTransferCooldown = false;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable, AccessControlUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _burn(uint256 tokenId) internal virtual override(ERC721Upgradeable, ERC721URIStorageUpgradeable) {
        return super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    struct Ship {
        bool getSuccess;
        uint256 tokenId;
        uint256 rarity;
        uint256 class;
        uint256 element;
    }

    function setSpaceshipAttr(address spaceshipAttrAddr) public restricted {
        spaceshipAttr = SpaceshipAttr(spaceshipAttrAddr);
    }

    function mint(
        address to,
        uint256 tokenId,
        uint256 rarity,
        uint256 class,
        uint256 element,
        uint256 seed
    ) external restricted {
        uint256 attrSeed = RandomUtil.combineSeeds(seed, 1);

        tokens.push(Ship(true, tokenId, rarity, class, element));
        spaceshipAttr.setSpaceshipAttr(tokenId, attrSeed, rarity);

        if (block.number != lastMintedBlock) firstMintedOfLastBlock = tokenId;
        lastMintedBlock = block.number;

        _mint(to, tokenId);
    }

    function transferCooldownEnd(uint256 tokenId) public view returns (uint256) {
        return lastTransferTimestamp[tokenId].add(transferCooldown);
    }

    function transferCooldownLeft(uint256 tokenId) public view returns (uint256) {
        (bool success, uint256 secondsLeft) = lastTransferTimestamp[tokenId].trySub(
            block.timestamp.sub(transferCooldown)
        );
        return success ? secondsLeft : 0;
    }

    function setTransferCooldown(uint256 _transferCooldown) public restricted {
        transferCooldown = _transferCooldown;
    }

    function setIsTransferCooldown(bool _isTransferCooldown) public restricted {
        isTransferCooldown = _isTransferCooldown;
    }

    function addAdmin(address _admin) public restricted {
        _setupRole(GAME_ADMIN, _admin);
    }

    function setTokenURI(uint256 tokenId, string memory _tokenURI) public restricted {
        _setTokenURI(tokenId, _tokenURI);
    }

    function mySpaceshipIds() public view returns (uint256[] memory ownerTokens) {
        return spaceshipsOfOwner(msg.sender);
    }

    // function mySpaceships() public view returns (ShipDetail[] memory) {
    //     uint256[] memory ids = spaceshipsOfOwner(msg.sender);
    //     uint256 length = ids.length;

    //     ShipDetail[] memory _ships = new ShipDetail[](length);

    //     for (uint256 i = 0; i < length; i++) {
    //         uint256 id = ids[i];
    //         if (_isNotFreshLookup(id)) {
    //             _ships[i] = get(id);
    //         }
    //     }

    //     return _ships;
    // }

    function spaceshipsOfOwner(address _owner) public view returns (uint256[] memory ownerTokens) {
        uint256 tokenCount = balanceOf(_owner);

        if (tokenCount == 0) {
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            for (uint256 shipId = 0; shipId <= tokenCount - 1; shipId++) {
                result[shipId] = tokenOfOwnerByIndex(_owner, shipId);
            }
            return result;
        }
    }

    function get(uint256 tokenId)
        public
        view
        shipRequired(tokenId)
        noFreshLookup(tokenId)
        returns (
            uint256,
            uint256,
            uint256
        )
    {
        Ship memory ship = tokens[tokenId];
        return (ship.rarity, ship.class, ship.element);
    }

    modifier shipRequired(uint256 tokenId) {
        Ship memory ship = tokens[tokenId];
        require(ship.getSuccess, "SNF");
        _;
    }

    modifier onlySelf() {
        require(address(this) == _msgSender(), "OS"); // OS: Only Self
        _;
    }

    modifier restricted() {
        _restricted();
        _;
    }

    function _restricted() internal view {
        require(hasRole(GAME_ADMIN, msg.sender), "NGA"); // NGA: Not Game Admin
    }

    modifier noFreshLookup(uint256 id) {
        _noFreshLookup(id);
        _;
    }

    function _noFreshLookup(uint256 id) internal view {
        require(_isNotFreshLookup(id), "LBL"); // Last block locked
    }

    function _isNotFreshLookup(uint256 id) internal view returns (bool) {
        return id < firstMintedOfLastBlock || lastMintedBlock < block.number;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721EnumerableUpgradeable, ERC721Upgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
        // when not minting or burning...
        if (from != address(0) && to != address(0)) {
            // only allow transferring a particular token every transferCooldown seconds
            if (isTransferCooldown) {
                require(lastTransferTimestamp[tokenId] < block.timestamp.sub(transferCooldown), "CD");
            }

            if (!hasRole(RECEIVE_DOES_NOT_SET_TRANSFER_TIMESTAMP, to)) {
                lastTransferTimestamp[tokenId] = block.timestamp;
            }
        }
    }
}

