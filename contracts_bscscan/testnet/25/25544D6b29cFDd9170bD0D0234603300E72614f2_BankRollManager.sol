contract BankRollManager is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    address public BANKER_MANAGER;
    address public BANKER_STAFF;

    address public bankRollWalletAddress;
    address public dividendPoolAddress;
    address public devPoolAddress;
    address public marketingPoolAddress;

    uint256 public bankRollMaxSupply;
    uint256 public dividendPercent;
    uint256 public devPercent;
    uint256 public marketingPercent;
    uint256 public swapDiff = 5;

    address public gembleTokenAddress;
    address public stableTokenAddress;
    address public exchangeRouterAddress;
    address public exchangeFactoryAddress;

    bool public isInitialized;
    bool public isInitializedShare;
    event swapDividendBankRoll(
        uint256 amount0,
        uint256 amount1,
        address indexed token0,
        address indexed token1
    );
    event DividendBankRoll(
        uint256 amount,
        uint256 dividendPoolShare,
        uint256 devPoolShare,
        uint256 marketingPoolShare
    );
    event RebalanceBankRoll(
        uint256 amount,
        address indexed token0,
        address indexed token1
    );

    constructor() public {
        BANKER_MANAGER = msg.sender;
    }

    function initialize(
        address _gembleAddress,
        address _stableTokenAddress,
        address _staffAddress,
        address _bankrollWallet,
        address _dividendPool,
        address _devAddress,
        address _marketingAddress,
        uint256 _bankrollSupply
    ) external {
        require(!isInitialized, "Already initialized");
        require(msg.sender == BANKER_MANAGER, "Not manager");

        isInitialized = true;
        gembleTokenAddress = _gembleAddress;
        stableTokenAddress = _stableTokenAddress;
        bankRollWalletAddress = _bankrollWallet;
        dividendPoolAddress = _dividendPool;
        devPoolAddress = _devAddress;
        marketingPoolAddress = _marketingAddress;
        bankRollMaxSupply = _bankrollSupply;
        BANKER_STAFF = _staffAddress;
    }

    modifier isHighLevelRole() {
        require(
            (msg.sender == BANKER_MANAGER || msg.sender == BANKER_STAFF),
            "Access Deny"
        );
        _;
    }

    function setPercentPerShare(
        uint256 _dividendPool,
        uint256 _dev,
        uint256 _marketing
    ) external onlyOwner {
        require(isInitialized, "please initialize");

        uint256 sumPercent = _marketing.add(_dividendPool.add(_dev));
        require(sumPercent == 100, "invalid setting share");

        dividendPercent = _dividendPool;
        devPercent = _dev;
        marketingPercent = _marketing;
        isInitializedShare = true;
    }

    function setExchangeAddress(address _routerAddress, address _factoryAddress)
        external
        onlyOwner
    {
        require(isInitialized, "please initialize");
        exchangeRouterAddress = _routerAddress;
        exchangeFactoryAddress = _factoryAddress;
    }

    function setTokenAddress(address _govToken, address _stableToken)
        external
        onlyOwner
    {
        require(isInitialized, "please initialize");
        gembleTokenAddress = _govToken;
        stableTokenAddress = _stableToken;
    }

    function commitDividendBankRoll() external isHighLevelRole {
        require(isInitialized, "please initialize");
        require(isInitializedShare, "please initialize share percent");
        require(bankRollMaxSupply > 0, "invalid bankRoll supply");
        require(exchangeRouterAddress != address(0), "invalid router address");
        IBEP20 gembleToken = IBEP20(gembleTokenAddress);
        uint256 gembleBalance = gembleToken.balanceOf(bankRollWalletAddress);
        uint256 stableBalance = IBEP20(stableTokenAddress).balanceOf(
            bankRollWalletAddress
        );
        uint256 bankRollBalance = gembleBalance;
        address[] memory addressPath = new address[](2);
        uint256[] memory amountOut = new uint256[](2);
        if (stableBalance > 0) {
            addressPath[0] = stableTokenAddress;
            addressPath[1] = gembleTokenAddress;
            amountOut = IPancakeRouter02(exchangeRouterAddress).getAmountsOut(
                stableBalance,
                addressPath
            );

            bankRollBalance = gembleBalance.add(amountOut[1]);
        }
        require(
            bankRollBalance > 0 && bankRollBalance > bankRollMaxSupply,
            "insufficient bankroll profit"
        );

        uint256 amount = bankRollBalance.sub(bankRollMaxSupply);
        require(amount > 0, "invalid amount");

        uint256 allowance = gembleToken.allowance(
            bankRollWalletAddress,
            address(this)
        );

        require(allowance >= amount, "check bankroll token allowance");
        if (gembleBalance < amount) {
            uint256 splitAmount = gembleBalance.div(2);
            uint256 swapAmount = amount.sub(gembleBalance).add(splitAmount);

            addressPath[0] = gembleTokenAddress;
            addressPath[1] = stableTokenAddress;
            amountOut = IPancakeRouter02(exchangeRouterAddress).getAmountsOut(
                swapAmount,
                addressPath
            );

            if (amountOut[0] == swapAmount && amountOut[1] > 0) {
                allowance = IBEP20(stableTokenAddress).allowance(
                    bankRollWalletAddress,
                    address(this)
                );
                require(
                    allowance >= amountOut[1],
                    "check bankroll stable token allowance"
                );
                addressPath[0] = stableTokenAddress;
                addressPath[1] = gembleTokenAddress;
                swapToken(
                    amountOut[1],
                    0,
                    addressPath,
                    bankRollWalletAddress,
                    bankRollWalletAddress
                );
                emit swapDividendBankRoll(amountOut[0], amountOut[1], addressPath[0],addressPath[1]);
            }
            return;
        }
        require(gembleBalance > amount, "please swap dividend");

        uint256 dividendPoolShare = amount.mul(dividendPercent).div(100);
        uint256 devPoolShare = amount.mul(devPercent).div(100);
        uint256 marketingPoolShare = amount.mul(marketingPercent).div(100);

        uint256 calcShare = marketingPoolShare.add(dividendPoolShare).add(
            devPoolShare
        );

        require(amount >= calcShare, "invalid calculated share");

        if (dividendPoolShare > 0) {
            gembleToken.safeTransferFrom(
                bankRollWalletAddress,
                dividendPoolAddress,
                dividendPoolShare
            );
        }
        if (devPoolShare > 0) {
            gembleToken.safeTransferFrom(
                bankRollWalletAddress,
                devPoolAddress,
                devPoolShare
            );
        }

        if (marketingPoolShare > 0) {
            gembleToken.safeTransferFrom(
                bankRollWalletAddress,
                marketingPoolAddress,
                marketingPoolShare
            );
        }

        emit DividendBankRoll(
            calcShare,
            dividendPoolShare,
            devPoolShare,
            marketingPoolShare
        );
    }

    function commitRebalanceBankRoll() external isHighLevelRole {
        require(isInitialized, "please initialize");
        require(exchangeRouterAddress != address(0), "invalid router address");

        uint256 gembleBalance = IBEP20(gembleTokenAddress).balanceOf(
            bankRollWalletAddress
        );
        require(gembleBalance > 0, "insufficient gemble");
        uint256 stableBalance = IBEP20(stableTokenAddress).balanceOf(
            bankRollWalletAddress
        );

        address[] memory addressPath = new address[](2);
        uint256[] memory amountOut = new uint256[](2);

        addressPath[0] = gembleTokenAddress;
        addressPath[1] = stableTokenAddress;
        amountOut = IPancakeRouter02(exchangeRouterAddress).getAmountsOut(
            gembleBalance,
            addressPath
        );
        uint256 assetValue = amountOut[1];
        amountOut = IPancakeRouter02(exchangeRouterAddress).getAmountsOut(
            (10**18),
            addressPath
        );
        uint256 percentDiff = 0;
        uint256 targetValue = 0;
        uint256 quanity = 0;

        if (assetValue > stableBalance) {
            percentDiff = percentChange(assetValue, stableBalance);
            if (percentDiff >= swapDiff) {
                targetValue = assetValue.add(stableBalance).div(2).sub(
                    stableBalance
                );
                //Swap gov to stable
                quanity = targetValue.div(amountOut[1]).mul(10**18);
                require(quanity > 0, "insufficient swap target");

                uint256 allowance = IBEP20(gembleTokenAddress).allowance(
                    bankRollWalletAddress,
                    address(this)
                );

                require(allowance >= quanity, "check bankroll token allowance");

                swapToken(
                    quanity,
                    0,
                    addressPath,
                    bankRollWalletAddress,
                    bankRollWalletAddress
                );
                emit RebalanceBankRoll(quanity, addressPath[0], addressPath[1]);
            }
        }

        if (stableBalance > assetValue) {
            addressPath[0] = stableTokenAddress;
            addressPath[1] = gembleTokenAddress;

            percentDiff = percentChange(stableBalance, assetValue);
            if (percentDiff >= swapDiff) {
                targetValue = stableBalance.add(assetValue).div(2).sub(
                    assetValue
                );
                require(targetValue > 0, "insufficient swap target");

                uint256 allowance = IBEP20(stableTokenAddress).allowance(
                    bankRollWalletAddress,
                    address(this)
                );

                require(
                    allowance >= targetValue,
                    "check bankroll token allowance"
                );
                //Swap stable to gov
                swapToken(
                    targetValue,
                    0,
                    addressPath,
                    bankRollWalletAddress,
                    bankRollWalletAddress
                );
                emit RebalanceBankRoll(
                    targetValue,
                    addressPath[0],
                    addressPath[1]
                );
            }
        }
    }

