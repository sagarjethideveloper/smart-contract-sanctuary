    function percentChange(uint256 a, uint256 b)
        internal
        pure
        returns (uint256)
    {
        uint256 c = a.mul(10000);
        uint256 d = b.mul(10000);
        uint256 e = c.sub(d).div(a);
        return e.mul(100).div(10000);
    }

    function _swapSupportingFeeOnTransferTokens(
        address[] memory path,
        address _to
    ) internal {
        for (uint256 i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0, ) = PancakeLibrary.sortTokens(input, output);
            IPancakePair pair = IPancakePair(
                PancakeLibrary.pairFor(exchangeFactoryAddress, input, output)
            );
            uint256 amountInput;
            uint256 amountOutput;
            {
                // scope to avoid stack too deep errors
                (uint256 reserve0, uint256 reserve1, ) = pair.getReserves();
                (uint256 reserveInput, uint256 reserveOutput) = input == token0
                    ? (reserve0, reserve1)
                    : (reserve1, reserve0);
                amountInput = IBEP20(input).balanceOf(address(pair)).sub(
                    reserveInput
                );
                amountOutput = PancakeLibrary.getAmountOut(
                    amountInput,
                    reserveInput,
                    reserveOutput
                );
            }
            (uint256 amount0Out, uint256 amount1Out) = input == token0
                ? (uint256(0), amountOutput)
                : (amountOutput, uint256(0));
            address to = i < path.length - 2
                ? PancakeLibrary.pairFor(
                    exchangeFactoryAddress,
                    output,
                    path[i + 2]
                )
                : _to;
            pair.swap(amount0Out, amount1Out, to, new bytes(0));
        }
    }

    function swapToken(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] memory path,
        address from,
        address to
    ) internal {
        require(
            exchangeFactoryAddress != address(0),
            "invalid factory address"
        );
        address lpAddress = PancakeLibrary.pairFor(
            exchangeFactoryAddress,
            path[0],
            path[1]
        );
        uint256 checkSupply = IPancakePair(lpAddress).totalSupply();
        require(checkSupply > 0, "invalid lp supply");
        SafeBEP20.safeTransferFrom(IBEP20(path[0]), from, lpAddress, amountIn);
        uint256 balanceBefore = IBEP20(path[path.length - 1]).balanceOf(to);
        _swapSupportingFeeOnTransferTokens(path, to);
        require(
            IBEP20(path[path.length - 1]).balanceOf(to).sub(balanceBefore) >=
                amountOutMin,
            "BankRollManager: INSUFFICIENT_OUTPUT_AMOUNT"
        );
    }
}

