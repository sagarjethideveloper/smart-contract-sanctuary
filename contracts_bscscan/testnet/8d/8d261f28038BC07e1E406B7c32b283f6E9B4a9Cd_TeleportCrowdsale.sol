pragma solidity =0.5.16;

import "./helpers/Context.sol";
import "./library/Crowdsale.sol";
import "./library/CappedCrowdsale.sol";
import "./library/TimedCrowdsale.sol";
import "./helpers/Ownable.sol";
import "./library/ITeleportReferral.sol";

import "hardhat/console.sol";

contract TeleportCrowdsale is
    Crowdsale,
    TimedCrowdsale,
    CappedCrowdsale,
    Ownable
{
    constructor(
        uint256 rate, // rate, in TKNbits
        address payable wallet, // wallet to send Ether
        IERC20 token, // the token
        uint256 cap, // total cap, in wei
        uint256 openingTime, // opening time in unix epoch seconds
        uint256 closingTime // closing time in unix epoch seconds
    )
        public
        CappedCrowdsale(cap)
        TimedCrowdsale(openingTime, closingTime)
        Crowdsale(rate, wallet, token)
        Ownable()
    {
        _whitelist[_msgSender()] = true;
    }

    mapping(address => address) private _referral; //  A is refered by B
    mapping(address => uint256) private _referralCredit;
    mapping(address => bool) private _whitelist;
    // Referral fee
    uint256 private _referralPercent = 2;

    event ReferrerAdded(address indexed referee, address indexed referrer);
    event ReferrerWhitelisted(address indexed whitelist);

    function getReferralPercent() public view returns (uint256) {
        return _referralPercent;
    }

    function getReferral(address referee) public view returns (address) {
        return _referral[referee];
    }

    function addReferral(address referrer) public {
        require(
            _referral[_msgSender()] == address(0),
            "already exist referrer"
        );
        require(_whitelist[referrer], "referrer must be whilelisted.");
        require(referrer != _msgSender(), "you cannot refer yourself");
        _referral[_msgSender()] = referrer;
        emit ReferrerAdded(_msgSender(), referrer);
    }

    function isWhitelisted() public view returns (bool) {
        return _whitelist[_msgSender()];
    }

    function isWhitelistedBy(address referrer) public view returns (bool) {
        return _whitelist[referrer];
    }

    function addWhitelist(address account) public onlyOwner {
        require(
            _whitelist[account] == false,
            "account is already whitelisted."
        );
        _whitelist[account] = true;
        emit ReferrerWhitelisted(account);
    }

    function getCredit() public view returns (uint256) {
        return _referralCredit[_msgSender()];
    }

    function _preValidatePurchase(address beneficiary, uint256 weiAmount)
        internal
        view
    {
        super._preValidatePurchase(beneficiary, weiAmount);
        require(weiAmount >= 10**12, "too low amount");
        require(_referral[beneficiary] != address(0), "no referrer");
    }

    function _getTokenAmount(uint256 weiAmount)
        internal
        view
        returns (uint256)
    {
        uint256 devFee = weiAmount.mul(_referralPercent).div(100);
        uint256 userReceive = weiAmount.sub(devFee);
        // console.log("devFee %s", devFee);
        // console.log("userReceive %s", userReceive);
        return userReceive.mul(_rate);
    }

    function _updatePurchasingState(address beneficiary, uint256 weiAmount)
        internal
    {
        uint256 creditAmount = weiAmount.mul(_rate).mul(_referralPercent).div(
            100
        );
        _referralCredit[_referral[beneficiary]] = _referralCredit[
            _referral[beneficiary]
        ]
        .add(creditAmount);
        _whitelist[beneficiary] = true;
        emit ReferrerWhitelisted(beneficiary);
        super._deliverTokens(_referral[beneficiary], creditAmount);
    }

    function refundUnRaised(uint256 tokenAmount) public onlyOwner {
        _deliverTokens(owner(), tokenAmount);
    }
}

