pragma solidity 0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";
import "./interfaces/IBEP20.sol";
import "./interfaces/IVenus.sol";
import "./interfaces/IEgoToken.sol";
import "./Vault.sol";
import "./BasePool.sol";

contract VenusPool is BasePool {

    IVenus private _venus;
    constructor( IBEP20 assetToken,
        IVenus venus,
        IEgoToken egoToken,
        uint256 initialVEGORate,
        address platformFeeAddress) 
            BasePool(assetToken,
                egoToken,
                initialVEGORate, 1000, 1e12, platformFeeAddress)
        {
        _venus = venus;
        assetToken.approve(address(_venus), MAX_INT);
    }

    function _stakeAsset(uint256 assetStakeAmount, StakerInfo memory stakerInfo, CelebrityInfo memory celebrityInfo) internal virtual override{
        if(assetStakeAmount  > 0)
            require(_assetToken.transferFrom(msg.sender, address(this), assetStakeAmount));
        //stake on venus  vtoken = actualMintAmount / exchangeRate
        uint256 vBalance = _venus.balanceOf(address(this));
        require(_venus.mint(assetStakeAmount) == 0, "stake: mint fail");
        vBalance = _venus.balanceOf(address(this)) - vBalance;

        stakerInfo.assetBalanceInfo.amount += assetStakeAmount;
        stakerInfo.assetBalanceInfo.vBalance += vBalance;
        celebrityInfo.assetBalanceInfo.amount += assetStakeAmount;
        celebrityInfo.assetBalanceInfo.vBalance += vBalance - _getExitRatio(vBalance);
    }
    
    function _celebrityClaimAsset(address celebrity, CelebrityInfo memory celebrityInfo)  internal virtual override {
        ClaimAmount memory venusClaim = calculateCelebrityAssetClaimAmount(celebrity);
        if (venusClaim.claimAmount > 0)
        {
            celebrityInfo.assetBalanceInfo.vBalance -= venusClaim.vClaimAmount;

            uint256 balance = _assetToken.balanceOf(address(this));
            require(_venus.redeem(venusClaim.vClaimAmount) == 0, "unstake: redeem error");
            balance = _assetToken.balanceOf(address(this)) - balance;

            uint256 systemReward = balance * PLATFORM_FEE / DEFAULT_BASE;
            if(balance > systemReward)
                require(_assetToken.transfer(celebrity, balance - systemReward));
            if(systemReward > 0)
                require(_assetToken.transfer(_platformFeeAddress, systemReward));
        }
    }

    function _unstakeAsset(StakerInfo memory stakerInfo, CelebrityInfo memory celebrityInfo, uint256 amount) internal virtual override {
        //-------------venus unstake--------------
        uint256 venusRate = _venus.exchangeRateStored();

        //stake on venus  vtoken = actualMintAmount / exchangeRate
        // actualMintAmount = v* exchangeRate;
        uint256 avalibleVenusWithdrawal = stakerInfo.assetBalanceInfo.vBalance * venusRate / 1e18 - _getExitRatio(amount);
        if(amount > stakerInfo.assetBalanceInfo.amount)
            amount = stakerInfo.assetBalanceInfo.amount;
        if(amount + _getExitRatio(stakerInfo.assetBalanceInfo.amount) < stakerInfo.assetBalanceInfo.amount){
            amount = amount +_getExitRatio(amount);
        }
        uint256 originalAmount = amount;
        if(amount > avalibleVenusWithdrawal)
            amount = avalibleVenusWithdrawal;

        uint256 celebrityDecreaseVBalance = 0;

        uint256 balance = _assetToken.balanceOf(address(this));
        if (avalibleVenusWithdrawal >= amount) {
            //witdrawal original
            require(_venus.redeemUnderlying(amount) == 0, "unstake: redeem underlying error");
            celebrityDecreaseVBalance = (amount * DEFAULT_OFFSET) * 1e18 / venusRate / DEFAULT_BASE;
        }
        else {
            //witdrawal original
            require(_venus.redeemUnderlying(avalibleVenusWithdrawal) == 0, "unstake: redeem2 underlying error");
            celebrityDecreaseVBalance = stakerInfo.assetBalanceInfo.vBalance;
        }
        balance = _assetToken.balanceOf(address(this)) - balance;
        
        //return funds in asset to the staker
        if(balance > 0)
            require(_assetToken.transfer(msg.sender, balance));
        //-------------venus unstake--------------


        //-----------venus calc---------------
        uint256 vAmount = originalAmount * 1e18 / venusRate;
        if( celebrityInfo.assetBalanceInfo.vBalance > vAmount)
            celebrityInfo.assetBalanceInfo.vBalance -= vAmount;
        else
            celebrityInfo.assetBalanceInfo.vBalance = 0;

        celebrityInfo.assetBalanceInfo.amount -= originalAmount;
        
        if(vAmount > stakerInfo.assetBalanceInfo.vBalance)
            stakerInfo.assetBalanceInfo.vBalance = 0;
        else
            stakerInfo.assetBalanceInfo.vBalance -= vAmount;
        stakerInfo.assetBalanceInfo.amount -= originalAmount;
        //-----------venus  calc---------------
    }

    function _getRate() view internal virtual override returns(uint256){
        return _venus.exchangeRateStored();
    }

    function _getExitRatio(uint256 amount)
    internal
    view
    returns (uint256)
    {
        uint256 redeemFee =
        (amount * DEFAULT_OFFSET * IComptroller(_venus.comptroller()).treasuryPercent()) / DEFAULT_BASE / 1e18;
        return redeemFee;
    }
}

