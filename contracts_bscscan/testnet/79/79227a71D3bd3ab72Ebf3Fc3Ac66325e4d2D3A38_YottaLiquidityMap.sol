pragma solidity ^0.8.0;
pragma experimental ABIEncoderV2;

import "./lib/IPancakeFactory.sol";
import "./lib/IPancakeRouter01.sol";
import "./lib/PancakeLibrary.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract YottaLiquidityMap is Ownable {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    struct LPMsg {
        uint256 id;
        uint256 amount;
        uint256 unlockTime;
        uint8 status;
    }

    struct accountLPMsg {
        mapping (address => LPMsg[]) _LPData;
        mapping (address => mapping (uint256 => uint256)) _LPDataIndex;
    }

    enum assetType {
        LP,
        YTA
    }

    address private _YTAContract;
    address private _USDTContract;
    address private _swapRouterContract;
    address private _swapFactoryContract;
    uint256 private _LPLockTime;
    mapping (address => uint256) private _YTABalance;
    mapping (address => uint256) private _LPBalance;
    mapping (string => bool) private _mapRecord;
    accountLPMsg accountData;

    event ChargeYTA(address indexed account, string yottaAccount, string yottaHash, uint256 amount);
    event WithdrawLP(address indexed account, uint256 amount);
    event AddLiquidity(address indexed account, uint256 YTAAmount, uint256 USDTAmount, uint256 LPAmount);
    event SetLPLockTime(uint256 oldLockTime, uint256 newLockTime);

    constructor(
        address YTAContract_,
        address USDTContract_,
        address swapRouterContract_,
        address swapFactoryContract_,
        uint256 LPLockTime
    )
    {
        _YTAContract = YTAContract_;
        _USDTContract = USDTContract_;
        _swapRouterContract = swapRouterContract_;
        _swapFactoryContract = swapFactoryContract_;
        _LPLockTime = LPLockTime;
    }

    /*********************************************************************************************************************************************/

    function chargeYTA(address account, string memory yottaAccount, string memory yottaHash, uint256 amount) external onlyOwner returns (bool)
    {
        _chargeYTA(account, yottaAccount, yottaHash, amount);
        return true;
    }

    function multiChargeYTA(
        address[] memory accounts,
        string[] memory yottaAccounts,
        string[] memory yottaHashs,
        uint256[] memory amounts
    ) external onlyOwner returns (bool)
    {
        for (uint i = 0; i < accounts.length; i ++) {
            _chargeYTA(accounts[i], yottaAccounts[i], yottaHashs[i], amounts[i]);
        }
        return true;
    }

    function setLPLockTime(uint256 newLockTime) external onlyOwner returns (bool) {
        uint256 oldLockTime = _LPLockTime;
        _LPLockTime = newLockTime;

        emit SetLPLockTime(oldLockTime, newLockTime);
        return true;
    }

    /*********************************************************************************************************************************************/

    function addLiquidity(uint256 uamount) external returns (bool)
    {
        uint256 amountYDesired = _YTABalance[_msgSender()];
        (uint reserveU, uint reserveY) = PancakeLibrary.getReserves(_swapFactoryContract, _USDTContract, _YTAContract);
        uint amountUOptimal = PancakeLibrary.quote(amountYDesired, reserveY, reserveU);
        require(amountUOptimal <= uamount, "INSUFFICIENT_USDT_AMOUNT");

        IERC20 usdt = IERC20(_USDTContract);
        usdt.safeTransferFrom(_msgSender(), address(this), amountUOptimal);
        usdt.approve(_swapRouterContract, amountUOptimal);
        //TODO：授权YTA

        (uint256 amountU, uint256 amountY, uint256 amountLP) = IPancakeRouter01(_swapRouterContract).addLiquidity(
            _USDTContract, 
            _YTAContract, 
            amountUOptimal, 
            amountYDesired, 
            0, 
            0, 
            address(this), 
            block.timestamp
        );

        _addBalance(assetType.LP, _msgSender(), amountLP);
        _subBalance(assetType.YTA, _msgSender(), amountY);

        uint256 id = block.timestamp;
        while (accountData._LPDataIndex[_msgSender()][id] > 0) {
            id++;
        }

        accountData._LPData[_msgSender()].push(LPMsg({
            id: id,
            amount: amountLP,
            unlockTime: block.timestamp.add(_LPLockTime),
            status:0
        }));
        accountData._LPDataIndex[_msgSender()][id] = accountData._LPData[_msgSender()].length;

        if (amountU < amountUOptimal) {
            uint256 backAmount = amountUOptimal.sub(amountU);
            usdt.transfer(_msgSender(), backAmount);
        }

        emit AddLiquidity(_msgSender(), amountY, amountU, amountLP);

        return true;
    }

    function withdrawLP(uint256 id) external returns (bool)
    {
        uint256 index = accountData._LPDataIndex[_msgSender()][id] - 1;
        require(block.timestamp >= accountData._LPData[_msgSender()][index].unlockTime, "");
        require(accountData._LPData[_msgSender()][index].status == 0, "");

        accountData._LPData[_msgSender()][index].status = 1;
        uint256 amount = accountData._LPData[_msgSender()][index].amount;
        address LPContract = IPancakeFactory(_swapFactoryContract).getPair(_USDTContract, _YTAContract);
        IERC20 lp = IERC20(LPContract);

        lp.safeTransfer(_msgSender(), amount);

        emit WithdrawLP(_msgSender(), amount);
        return true;
    }

