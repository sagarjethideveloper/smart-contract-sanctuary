        return _LPLockTime;
    }

    /**********************************************************************************************************************************************/

    function _chargeYTA(address account, string memory yottaAccount, string memory yottaHash, uint256 amount) internal
    {
        require(account != address(0), "account is a zero value");
        IERC20 yta = IERC20(_YTAContract);
        yta.safeTransferFrom(_msgSender(), address(this), amount);

        _addBalance(assetType.YTA, account, amount);

        emit ChargeYTA(account, yottaAccount, yottaHash, amount);
    }

    function _addBalance(assetType at, address account, uint256 amount) internal {
        mapping (address => uint256) storage _balances = _YTABalance;
        if (at == assetType.LP) {
            _balances = _LPBalance;
        }
        _balances[account] = _balances[account].sub(amount, "transfer amount exceeds balance");
    }

    function _subBalance(assetType at, address account, uint256 amount) internal {
        mapping (address => uint256) storage _balances = _YTABalance;
        if (at == assetType.LP) {
            _balances = _LPBalance;
        }
        _balances[account] = _balances[account].add(amount);
