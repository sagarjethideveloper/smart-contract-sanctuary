pragma solidity 0.8.2;

import "./interfaces/IBEP20.sol";
import './utils/SafeBEP20.sol';
import "@openzeppelin/contracts/access/Ownable.sol";

// SyrupBar with Governance.
contract MasterJancokWallet is Ownable {
    using SafeBEP20 for IBEP20;
    // The JANCOK TOKEN!
    IBEP20 public jancok;
    address public MasterJancok;

    constructor(
        IBEP20 _jancok
    ) {
        jancok = _jancok;
    }

    function setMasterJancok(address _MasterJancok) onlyOwner external{
        MasterJancok = _MasterJancok;
    }

    // Safe jancok transfer function, just in case if rounding error causes pool to not have enough JANCOKs.
    function safeJancokTransfer(address _to, uint256 _amount) public returns(uint) {
        require(msg.sender == MasterJancok || msg.sender == owner(), "Wallet: Only MasterJancok and Owner can transfer");
        uint256 jancokBal = jancok.balanceOf(address(this));
        if (_amount > jancokBal) {
            jancok.safeTransfer(_to, jancokBal);
            return jancokBal;
        } else {
            jancok.safeTransfer(_to, _amount);
            return _amount;
        }
    }
}

