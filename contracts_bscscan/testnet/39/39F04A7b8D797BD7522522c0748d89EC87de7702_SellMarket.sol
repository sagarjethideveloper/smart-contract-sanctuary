pragma solidity ^0.8.0;

import '@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol';
import 'hardhat/console.sol';

contract SellMarket is AccessControlUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    bytes32 public constant OPERATOR_ROLE = keccak256('OPERATOR_ROLE');
    bytes32 public constant MEMBER_ROLE = keccak256('MEMBER_ROLE');

    struct ShopInfo {
        address seller; // seller address
        uint256 amount; // Deposit amount
        uint256 sellAmount; // amount to sell
        uint256 devAmount; // amount to dev
        uint256 rate; // rate
        uint256 minOrder; // min amount of deal
        uint256 maxOrder; // max amount of deal
    }

    struct Deal {
        address seller; // seller address
        address buyer; // buyer address
        uint256 amount; // amount to buy
        uint256 buyRate; // buying rate
        uint256 cost; // amount * sellRate in THB
        uint256 dealDate; // timestamp ask to buy
        uint256 depositDate; // timestamp deposited
        DealStatus status;
    }

    enum DealStatus {
        WaitForDeposit, // 0
        Deposited, // 1
        Released, // 2
        Complaint, // 3
        Revoked // 4
    }

    IERC20Upgradeable public _token;
    bool public initialized;
    uint256 public marketFee;
    uint256 public dealTime; // time require buyer to complete the deal
    uint256 public releaseTime; // time waiting seller to revoke the deal
    address public devAddress; // dev address to store 1% of TELE-B

    mapping(address => uint256) public sellerToShop; // seller address -> shopId
    mapping(uint256 => uint256) public shopToDeal; // shopId -> dealId
    mapping(address => uint256) public buyerToShop; // buyer -> shopId

    // // Crime record
    mapping(address => uint256) public notDepositing; // buyer not deposit in time
    mapping(address => uint256) public notReleasing; // seller not release in time
    // Good record
    mapping(address => uint256) public successBuy; // buyer completed deal
    mapping(address => uint256) public successSell; // seller completed deal

    ShopInfo[] private allShops; // access by [shopId]
    Deal[] private allDeals; // access by [dealId]

    event ShopCreated(
        uint256 indexed shopId,
        address indexed seller,
        uint256 amount,
        uint256 sellAmount,
        uint256 rate,
        uint256 minOrder,
        uint256 maxOrder
    );
    event ShopModified(uint256 indexed shopId, uint256 sellRate, uint256 minOrder, uint256 maxOrder);
    event ShopDeposited(
        uint256 indexed shopId,
        address indexed seller,
        uint256 amount,
        uint256 sellAmount,
        uint256 sellRate
    );
    event DealCreated(uint256 indexed shopId, address indexed buyer, uint256 amount, uint256 rate, uint256 cost);
    event DealConfirmed(uint256 indexed dealId);
    event DealReleased(uint256 indexed dealId);
    event DealComplaint(uint256 indexed dealId, address indexed seller, address indexed buyer);
    event DealRevoked(uint256 indexed dealId);

    function getShop(uint256 shopId) public view returns (ShopInfo memory) {
        require(shopId <= allShops.length - 1, 'length overflow');
        ShopInfo storage shop = allShops[shopId];
        uint256 dealId = shopToDeal[shopId];
        if (dealId != 0) {
            Deal storage deal = allDeals[dealId];
            uint256 devAmount = (shop.devAmount * shop.sellAmount) / deal.amount;
            ShopInfo memory calculatedShop = ShopInfo({
                seller: shop.seller,
                amount: shop.amount - (deal.amount + devAmount),
                sellAmount: shop.sellAmount - deal.amount,
                devAmount: shop.devAmount - devAmount,
                rate: shop.rate,
                minOrder: shop.minOrder,
                maxOrder: shop.maxOrder
            });
            return calculatedShop;
        } else {
            return shop;
        }
    }

    function getDeal(uint256 dealId) public view returns (Deal memory) {
        require(dealId <= allDeals.length - 1, 'length overflow');
        return allDeals[dealId];
    }

    function shopCount() public view returns (uint256) {
        return allShops.length;
    }

    function dealCount() public view returns (uint256) {
        return allDeals.length;
    }

    function isBuyerLocked(address buyer) public view returns (bool) {
        uint256 shopId = buyerToShop[buyer];
        uint256 dealId = shopToDeal[shopId];
        if (buyerToShop[buyer] == 0) return false;
        return dealId != 0;
    }

    function isShopLocked(uint256 shopId) public view returns (bool) {
        if (shopToDeal[shopId] == 0) return false;
        return shopToDeal[shopId] != 0;
    }

    // return selling to shop id
    function getMyReserveSell() public view returns (uint256) {
        return sellerToShop[_msgSender()];
    }

    // return buying from shop id
    function getMyReserveBuy() public view returns (uint256) {
        return buyerToShop[_msgSender()];
    }

    function getMySellShop() public view returns (ShopInfo memory) {
        uint256 shopId = sellerToShop[_msgSender()];
        require(shopId != 0, 'no shop is created');
        return getShop(shopId);
    }

    function getMySellShopDeal() public view returns (Deal memory) {
        uint256 shopId = sellerToShop[_msgSender()];
        require(shopId != 0, 'not selling now');
        uint256 dealId = shopToDeal[shopId];
        require(dealId != 0, 'currently no selling deal');
        return getDeal(dealId);
    }

    function getMyBuyShop() public view returns (ShopInfo memory) {
        uint256 shopId = buyerToShop[_msgSender()];
        require(shopId != 0, 'no buying shop');
        return getShop(shopId);
    }

    function getMyBuyShopDeal() public view returns (Deal memory) {
        uint256 shopId = buyerToShop[_msgSender()];
        require(shopId != 0, 'not buying now');
        uint256 dealId = shopToDeal[shopId];
        require(dealId != 0, 'currently no buying deal');
        return getDeal(dealId);
    }

    function isInitialized() public view returns (bool) {
        return initialized;
    }

    function updateShops() public {
        for (uint256 i = 0; i < allShops.length; i++) {
            updateShop(i);
        }
    }

    function updateShop(uint256 shopId) public {
        uint256 dealId = shopToDeal[shopId];
        if (dealId != 0) {
            _updateDeal(dealId);
        }
    }

    function createShop(
        uint256 amount,
        uint256 rate,
        uint256 minOrder,
        uint256 maxOrder
    ) public onlyRole(MEMBER_ROLE) {
        require(maxOrder >= minOrder, 'maxOrder must be greater than minOrder');
        require(_token.allowance(_msgSender(), address(this)) >= amount, 'no enough allowance for this contract');
        require(sellerToShop[_msgSender()] == 0, 'already have opening shop');
        _token.safeTransferFrom(_msgSender(), address(this), amount);
        require(rate >= 10000 && rate <= 1000000, 'rate must be in range: 100000 = 0.01B, and 1000000 = 1B');
        uint256 devAmount = (amount * marketFee) / 100;
        uint256 sellAmount = amount - devAmount;

        allShops.push(
            ShopInfo({
                seller: _msgSender(),
                amount: amount,
                sellAmount: sellAmount,
                devAmount: devAmount,
                rate: rate,
                minOrder: minOrder,
                maxOrder: maxOrder
            })
        );
        sellerToShop[_msgSender()] = allShops.length - 1;
        emit ShopCreated(allShops.length - 1, _msgSender(), amount, sellAmount, rate, minOrder, maxOrder);
    }

    function depositShop(uint256 amount) public onlyRole(MEMBER_ROLE) {
        require(_token.allowance(_msgSender(), address(this)) >= amount, 'no enough allowance for this contract');
        uint256 shopId = sellerToShop[_msgSender()];
        require(shopId != 0, 'no shop is created');
        _token.safeTransferFrom(_msgSender(), address(this), amount);
        ShopInfo storage shop = allShops[shopId];
        uint256 devAmount = (amount * marketFee) / 100;
        uint256 sellAmount = amount - devAmount;
        shop.amount += amount;
        shop.devAmount += devAmount;
        shop.sellAmount += sellAmount;
        emit ShopDeposited(shopId, _msgSender(), amount, sellAmount, shop.rate);
    }

    function withdrawShop() public onlyRole(MEMBER_ROLE) {
        uint256 shopId = sellerToShop[_msgSender()];
        require(shopId != 0, 'no shop is created');
        require(shopToDeal[shopId] == 0, 'deal is going on, cannot close');
        ShopInfo storage shop = allShops[shopId];
        if (shop.sellAmount > 0) {
            _token.safeTransfer(_msgSender(), shop.amount);
            shop.amount = 0;
            shop.devAmount = 0;
            shop.sellAmount = 0;
        }
    }

    function setShop(
        uint256 rate,
        uint256 minOrder,
        uint256 maxOrder
    ) public onlyRole(MEMBER_ROLE) {
        uint256 shopId = sellerToShop[_msgSender()];
        require(shopId != 0, 'no shop is created');
        require(shopToDeal[shopId] == 0, 'deal is going on, cannot modify');
        require(rate >= 10000 && rate <= 1000000, 'rate must be in range: 100000 = 0.01B, and 1000000 = 1B');
        ShopInfo storage shop = allShops[shopId];
        shop.rate = rate;
        shop.minOrder = minOrder;
        shop.maxOrder = maxOrder;
        emit ShopModified(shopId, rate, minOrder, maxOrder);
    }

    function reserveBuy(uint256 shopId, uint256 buyAmount) public onlyRole(MEMBER_ROLE) {
        require(shopId != 0 && shopId < allShops.length, 'invalid shop id');
        uint256 dealId = shopToDeal[shopId];
        _updateDeal(dealId);
        require(buyerToShop[_msgSender()] == 0, 'cannot buy from 2 shop simultaneously');
        require(shopToDeal[shopId] == 0, 'shop is engaging with another buyer');
        ShopInfo storage shop = allShops[shopId];
        require(shop.sellAmount >= buyAmount, 'no enough token sold to buy');
        require(shop.minOrder <= buyAmount, 'buy amount is too low');
        require(shop.maxOrder >= buyAmount, 'buy amount is too high');
        uint256 cost = (buyAmount * shop.rate) / 1000000;
        allDeals.push(
            Deal({
                seller: shop.seller,
                buyer: _msgSender(),
                amount: buyAmount,
                buyRate: shop.rate,
                cost: cost,
                dealDate: block.timestamp,
                depositDate: 0,
                status: DealStatus.WaitForDeposit
            })
        );
        buyerToShop[_msgSender()] = allDeals.length - 1;
        shopToDeal[shopId] = allDeals.length - 1;
        emit DealCreated(shopId, _msgSender(), buyAmount, allShops[shopId].rate, cost);
    }

    function buyerCancel() public onlyRole(MEMBER_ROLE) {
        uint256 shopId = buyerToShop[_msgSender()];
        require(shopId != 0, 'not buying now');
        uint256 dealId = shopToDeal[shopId];
        require(dealId != 0, 'not reserve buying now');
        Deal storage deal = allDeals[dealId];
        if (deal.status == DealStatus.WaitForDeposit) {
            uint256 record = notDepositing[deal.buyer];
            notDepositing[deal.buyer] = record + 1;
            _forceRevoke(dealId);
        }
    }

    function confirmBuy() public onlyRole(MEMBER_ROLE) {
        uint256 shopId = buyerToShop[_msgSender()];
        require(shopId != 0, 'not buying now');
        uint256 dealId = shopToDeal[shopId];
        require(dealId != 0, 'not reserve buying now');
        _updateDeal(dealId);
        Deal storage deal = allDeals[dealId];
        require(deal.status == DealStatus.WaitForDeposit, 'status is not wait for deposit');
        deal.status = DealStatus.Deposited;
        deal.depositDate = block.timestamp;
        emit DealConfirmed(dealId);
    }

    function buyerReleaseToken() public onlyRole(MEMBER_ROLE) {
        uint256 shopId = buyerToShop[_msgSender()];
        require(shopId != 0, 'not buying now');
        uint256 dealId = shopToDeal[shopId];
        require(dealId != 0, 'not reserve buying now');
        Deal storage deal = allDeals[dealId];
        require(
            deal.status == DealStatus.Deposited && deal.depositDate + releaseTime < block.timestamp,
            ' wait the shop please'
        );
        _updateDeal(dealId);
    }

    function shopReleaseToken() public onlyRole(MEMBER_ROLE) {
        uint256 shopId = sellerToShop[_msgSender()];
        require(shopId != 0, 'no shop is created');
        uint256 dealId = shopToDeal[shopId];
        require(dealId != 0, 'currently no buyer');
        Deal storage deal = allDeals[dealId];
        require(deal.status == DealStatus.Deposited, 'deal is not deposited');
        _forceReleaseToken(dealId);
    }

    function complainBuyer() public onlyRole(MEMBER_ROLE) {
        uint256 shopId = sellerToShop[_msgSender()];
        require(shopId != 0, 'no shop is created');
        uint256 dealId = shopToDeal[shopId];
        require(dealId != 0, 'currently no buyer');
        Deal storage deal = allDeals[dealId];
        if (deal.status == DealStatus.Deposited) {
            deal.status = DealStatus.Complaint;
            emit DealComplaint(dealId, deal.seller, deal.buyer);
        }
    }

    function complainSeller() public onlyRole(MEMBER_ROLE) {
        uint256 shopId = buyerToShop[_msgSender()];
        require(shopId != 0, 'not buying now');
        uint256 dealId = shopToDeal[shopId];
        require(dealId != 0, 'not reserve buying now');
        Deal storage deal = allDeals[dealId];
        if (deal.status == DealStatus.Deposited) {
            deal.status = DealStatus.Complaint;
            emit DealComplaint(dealId, deal.seller, deal.buyer);
        }
    }

    function _forceRevoke(uint256 dealId) private {
        Deal storage deal = allDeals[dealId];
        deal.status = DealStatus.Revoked;
        uint256 shopId = sellerToShop[deal.seller];
        shopToDeal[shopId] = 0;
        buyerToShop[deal.buyer] = 0;
        emit DealRevoked(dealId);
    }

    function _forceReleaseToken(uint256 dealId) private {
        Deal storage deal = allDeals[dealId];
        uint256 shopId = sellerToShop[deal.seller];
        ShopInfo storage shop = allShops[shopId];
        uint256 devAmount = (shop.devAmount * shop.sellAmount) / deal.amount;
        _token.safeTransfer(devAddress, devAmount);
        _token.safeTransfer(deal.buyer, deal.amount);
        // update deal
        deal.status = DealStatus.Released;
        uint256 recordBuy = successBuy[deal.buyer];
        successBuy[deal.buyer] = recordBuy + 1;
        uint256 recordSell = successSell[deal.seller];
        successSell[deal.seller] = recordSell + 1;
        // update shop remaining account
        shop.sellAmount -= deal.amount;
        shop.devAmount -= devAmount;
        shop.amount -= deal.amount + devAmount;
        buyerToShop[deal.buyer] = 0;
        shopToDeal[shopId] = 0;
        emit DealReleased(dealId);
    }

    function _updateDeal(uint256 dealId) private {
        Deal storage deal = allDeals[dealId];
        if (deal.status == DealStatus.Deposited && deal.depositDate + releaseTime < block.timestamp) {
            uint256 record = notReleasing[deal.seller];
            notReleasing[deal.seller] = record + 1;
            _forceReleaseToken(dealId);
            return;
        }
        if (deal.status == DealStatus.WaitForDeposit && deal.dealDate + dealTime < block.timestamp) {
            uint256 record = notDepositing[deal.buyer];
            notDepositing[deal.buyer] = record + 1;
            _forceRevoke(dealId);
            return;
        }
    }

    function forceRevoke(uint256 dealId, bool force) public onlyRole(OPERATOR_ROLE) {
        require(dealId != 0 && dealId < allDeals.length, 'invalid deal id');
        if (!force) {
            Deal storage currentDeal = allDeals[dealId];
            require(currentDeal.status == DealStatus.Complaint, 'deal is not in complaint');
        }
        _forceRevoke(dealId);
    }

    function forceRelease(uint256 dealId, bool force) public onlyRole(OPERATOR_ROLE) {
        require(dealId != 0 && dealId < allDeals.length, 'invalid deal id');
        if (!force) {
            Deal storage currentDeal = allDeals[dealId];
            require(currentDeal.status == DealStatus.Complaint, 'deal is not in complaint');
        }

        _forceReleaseToken(dealId);
    }

    function setDevAddress(address newDev) public onlyRole(DEFAULT_ADMIN_ROLE) {
        devAddress = newDev;
    }

    function setDealTime(uint256 newTime) public onlyRole(DEFAULT_ADMIN_ROLE) {
        dealTime = newTime;
    }

    function setReleaseTime(uint256 newTime) public onlyRole(DEFAULT_ADMIN_ROLE) {
        releaseTime = newTime;
    }

    function setMarketFee(uint256 newFee) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(newFee >= 1 && newFee < 100, 'fee is in percentage');
        marketFee = newFee;
    }

    function initialize(address token) public {
        require(!initialized, 'this contract is initialized');
        __AccessControl_init();
        initialized = true;
        marketFee = 1;
        dealTime = 20 minutes;
        releaseTime = 3 hours;
        _token = IERC20Upgradeable(token);
        devAddress = _msgSender();
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setRoleAdmin(OPERATOR_ROLE, DEFAULT_ADMIN_ROLE);
        _setRoleAdmin(MEMBER_ROLE, OPERATOR_ROLE);
        // prevent index 0
        allShops.push(
            ShopInfo({seller: _msgSender(), amount: 0, sellAmount: 0, devAmount: 0, rate: 0, minOrder: 0, maxOrder: 0})
        );
        allDeals.push(
            Deal({
                seller: _msgSender(),
                buyer: _msgSender(),
                amount: 0,
                buyRate: 0,
                cost: 0,
                dealDate: 0,
                depositDate: 0,
                status: DealStatus.Revoked
            })
        );
    }
}

