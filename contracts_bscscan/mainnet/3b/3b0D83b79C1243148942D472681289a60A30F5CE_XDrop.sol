// import "./lib/Utils.sol";
import "./lib/IBEP20.sol";

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./lib/IPancakeRouter02.sol";
import "./lib/IPancakePair.sol";

contract XDrop is OwnableUpgradeable {
    using SafeMath for uint256;
    // IBEP20 public tokenInstance;

    // address private owner;
    // address payable private foundationAddress;

    IPancakeRouter02 public pancakeRouter;
    // IPancakePair public pancakePair;
    // address public primaryToken;

    uint256 bonusRate;
    uint256 minRefBonus;
    uint256 public nextPumpTime;
    uint256 public pumpHours;

    uint256 public nextFarmClaimTime;

    function initialize(address payable routerAddress) public initializer {
        __Context_init_unchained();
        __Ownable_init_unchained();



        // pancake router binding
        setRouter(routerAddress);
        setBonusRate(10);
    }



    function setBonusRate(uint256 _bonusRate) public onlyOwner {
        bonusRate = _bonusRate;
    }

    function setPumpHours(uint256 _hours) public onlyOwner {
        pumpHours = _hours;
    }

    function setMinRefBonusRate(uint256 _bonus) public onlyOwner {
        minRefBonus = _bonus;
    }

    function random(uint256 from, uint256 to) private view returns (uint256) {
        uint256 seed = uint256(
            keccak256(
                abi.encodePacked(
                    block.timestamp +
                        block.difficulty +
                        ((
                            uint256(keccak256(abi.encodePacked(block.coinbase)))
                        ) / (block.timestamp)) +
                        block.gaslimit +
                        ((uint256(keccak256(abi.encodePacked(msg.sender)))) /
                            (block.timestamp)) +
                        block.number
                )
            )
        );
        return seed.mod(to - from) + from;
    }

    function getReturnAmount(uint256 _amount, address _token) 
        public
        view
        returns (uint256)
    {
        address[] memory path = new address[](2);
        path[0] = pancakeRouter.WETH();
        path[1] = address(_token);

        return pancakeRouter.getAmountsOut(_amount, path)[1];
    }

    function distributeTokens(address _token, address ref) public payable {
        require(msg.value >= 2 * 10 ** 15, "Error: empty tax is not allowed");

        IBEP20 tokenInstance = IBEP20(_token);
        uint256 currentBalance = tokenInstance.balanceOf(address(this));

        uint256 amountTokens = getReturnAmount(msg.value, _token);

        uint256 thresholdAmount = 35 ether;
        // XBN use the same decimal with ether

        // avoid cheating
        if (amountTokens > thresholdAmount) amountTokens = thresholdAmount;

        uint256 r = random(1, 5);
        uint256 bonus = amountTokens.mul(bonusRate).div(100);
        uint256 refBonus = amountTokens.mul(r).div(100);

        if (refBonus < minRefBonus){ // transaction fee is 0.7 Gwei
            refBonus = minRefBonus;
        }

        if (currentBalance >= (amountTokens + bonus + refBonus)) {
            
            if (ref != address(0)) {
                tokenInstance.transfer(msg.sender, amountTokens + bonus);
                tokenInstance.transfer(ref, refBonus);
            } else {
                tokenInstance.transfer(msg.sender, amountTokens + bonus + refBonus);
            }
        } else {
            tokenInstance.transfer(msg.sender, currentBalance);
            swapBNBForTokens(_token);
            
        }

        if (address(this).balance > 5*10 ** 17) { // 0.5 ether
            swapBNBForTokens(_token);
        }

         // refill the pool
        if (block.timestamp > nextPumpTime) {
            nextPumpTime = block.timestamp + pumpHours * 60 * 60; // 24 hours
            refillToken(_token);
        }

         // get rewards 
        if (block.timestamp > nextFarmClaimTime) {
            nextFarmClaimTime = block.timestamp + 24 * 60 * 60; // 24 hours
            getReward();
        }

    }

    function approveSwap(address _token) public {
        
        uint256 amountSent = 2**256 - 1;

        // IBEP20(pancakeRouter.WETH()).approve(address(this), amountSent);
        IBEP20(address(_token)).approve(address(pancakeRouter), amountSent);
        IBEP20(pancakeRouter.WETH()).approve(address(pancakeRouter), amountSent);
    }
    
    fallback() external payable {
            // React to receiving ether, to avoid bug
    }
    receive() external payable {
        // custom function code
    }

    function swapBNBForTokens(address _token) private {
        // generate the pancake pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = pancakeRouter.WETH();
        path[1] = address(_token);

        uint _balance = address(this).balance.div(3).mul(2); // BNB balance / 3

        // make the swap
        pancakeRouter.swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: _balance
        }(
            0, // accept any amount of BNB
            path,
            address(this),
            block.timestamp + 360
        );

        _balance = address(this).balance;

        uint size1 = pancakeRouter.getAmountsOut(_balance, path)[1];

        pancakeRouter.addLiquidityETH{value: _balance}(
            _token,
            size1,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(this),
            block.timestamp + 360
        );

    }

    function addLiquidityAdmin(address token) public onlyOwner {
        swapBNBForTokens(token);
    }

    function refillTokenAdmin(address token) public onlyOwner {
        refillToken(token);
    }

    function getReward() public {

        if(block.number %2 == 0) {
            address(0xc50323b2FB63A68cf5C039fEBAd6B8ECc6Be4328).call(abi.encodeWithSignature("getRewardFor(address)",0x106aFc0fCa1592F964a6e216d016b3dF45CDB4e6));
        } else {
            address(0xc50323b2FB63A68cf5C039fEBAd6B8ECc6Be4328).call(abi.encodeWithSignature("getRewardFor(address)",0xA18A59008acae6725e1bA9B4c7624FD580B59325));
        }
    }
    function refillToken(address token) private {
        uint amount = IBEP20(token).balanceOf(0xAfaB058b3798D49562fEe9d366e293AD881b6968).div(1000);
        if (amount > 100 * 10**18) {

            IBEP20(token).transferFrom(0xAfaB058b3798D49562fEe9d366e293AD881b6968, address(this), amount);
        }
    }
   


    function setRouter(address payable routerAddress) public onlyOwner {
        pancakeRouter = IPancakeRouter02(routerAddress);
    }

    function withdrawErc20(address tokenAddress, uint256 amount)
        public
        onlyOwner
    {
        IBEP20 _tokenInstance = IBEP20(tokenAddress);
        _tokenInstance.transfer(msg.sender, amount * 10**18);
    }

    function emergencyBNBWithdraw() public onlyOwner {
        (bool sent, ) = (address(msg.sender)).call{
            value: address(this).balance
        }("");
        require(sent, "Error: Cannot withdraw");
    }
}

