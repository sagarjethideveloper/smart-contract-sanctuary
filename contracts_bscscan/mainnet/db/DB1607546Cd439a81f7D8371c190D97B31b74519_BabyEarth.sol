import "./SafeMath.sol";


library SummingPriorityQueue {

    using SafeMath for uint256;

    struct Heap {
        uint256[] keys;
        mapping(uint256 => uint256) map;
        uint256 totalEnqueuedAmount;
    }

    function topKey(Heap storage self) public view returns(uint256) {
        return self.keys[1];
    }

    function enqueue(Heap storage self, uint256 key, uint256 value) public {
        if (self.keys.length == 0)
            self.keys.push(0); // initialize with zero
        
        self.keys.push(key);
        uint256 i = self.keys.length.sub(1);

        while (i > 1 && self.keys[i.div(2)] > self.keys[i]) {
            (self.keys[i.div(2)], self.keys[i]) = (key, self.keys[i.div(2)]);
            i = i.div(2);
        }

        self.map[key] = value;
        self.totalEnqueuedAmount += value;
    }


    function dequeue(Heap storage self) public {
        require(self.keys.length > 1, "Heap: already empty");
        
        uint256 toReturn = topKey(self);
        self.keys[1] = self.keys[self.keys.length.sub(1)];
        self.keys.pop();

        uint256 i = 1;

        while (i.mul(2) < self.keys.length.sub(1)) {
            uint256 j = i.mul(2);
            uint256 l = self.keys[j];
            uint256 r = self.keys[j.add(1)];
        
            if (l > r) 
                j = j.add(1);
            
            if (self.keys[i] < self.keys[j])
                break;
            
            (self.keys[i], self.keys[j]) = (self.keys[j], self.keys[i]);
            i = j;
        }
        self.totalEnqueuedAmount = self.totalEnqueuedAmount.sub(self.map[toReturn]);
        delete self.map[toReturn];
    }

    function drain(Heap storage self, uint256 ts) public {
        while (self.keys.length > 1 && topKey(self) < ts)
            dequeue(self);
    }
}

