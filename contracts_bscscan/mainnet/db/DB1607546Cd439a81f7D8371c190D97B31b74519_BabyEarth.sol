import "./SafeMath.sol";

library IterableSummingTimeBasedQueue {

    using SafeMath for uint256;

    struct Queue {
        mapping(uint256 => uint256) timestamps;
        mapping(uint256 => uint256) amounts;
        uint256 firstKey;
        uint256 lastKey;
        uint256 totalEnqueuedAmount;
    }

    function isEmpty(Queue storage queue) public view returns(bool) {
        return (queue.firstKey > queue.lastKey);
    }

    function dequeueDrainedAmount(Queue storage queue, uint256 amount) public {
        uint256 amountLeft = amount;
        while(!isEmpty(queue) && amountLeft > 0) {
            if (queue.amounts[queue.firstKey] > amountLeft) {
                queue.amounts[queue.firstKey] = queue.amounts[queue.firstKey].sub(amountLeft);
                amountLeft = 0;
            } else {
                amountLeft = amountLeft.sub(queue.amounts[queue.firstKey]);
                delete queue.timestamps[queue.firstKey];
                delete queue.amounts[queue.firstKey];
                queue.firstKey++;
            }
        }
        queue.totalEnqueuedAmount = queue.totalEnqueuedAmount.sub(amount);
        require(amountLeft == 0);
    }

    function dequeueOutdated(Queue storage queue, uint256 timeOffset) public {
        uint256 ts = block.timestamp;
        while (!isEmpty(queue) && (ts.sub(queue.timestamps[queue.firstKey]) > timeOffset)) {
            queue.totalEnqueuedAmount = queue.totalEnqueuedAmount.sub(queue.amounts[queue.firstKey]);
            delete queue.timestamps[queue.firstKey];
            delete queue.amounts[queue.firstKey];
            queue.firstKey++;
        }
    }

    function enqueue(Queue storage queue, uint256 amount) public {
        if (isEmpty(queue)) {
            queue.firstKey = 1;
            queue.lastKey = 0;
        }
        queue.lastKey++;
        queue.timestamps[queue.lastKey] = block.timestamp;
        queue.amounts[queue.lastKey] = amount;
        queue.totalEnqueuedAmount = queue.totalEnqueuedAmount.add(amount);
    }
}

