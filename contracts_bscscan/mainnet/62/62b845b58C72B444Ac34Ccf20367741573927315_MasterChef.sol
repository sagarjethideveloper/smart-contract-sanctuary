pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "../BEP20.sol";
import "../IPancakeRouter02.sol";
import "../IPancakeFactory.sol";

contract BaseToken is BEP20 {
    using SafeMath for uint256;

    // Transfer tax rate in basis points.
    uint16 public transferTaxRate;
    // Burn rate % of transfer tax.
    uint16 public burnRate;

    // Max transfer tax rate: 5%.
    uint16 public constant MAXIMUM_TRANSFER_TAX_RATE = 500;
    // Max burn rate: 1%
    uint16 public constant MAXIMUM_BURN_RATE = 100;
    // Burn address
    address public constant BURN_ADDRESS =
        0x000000000000000000000000000000000000dEaD;
    IBEP20 public BUSD = IBEP20(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56); 
    // Where to send the transfer tax fees
    address public transferFeeAddress;
    // Min amount to liquify. (50 tokens)
    uint256 public minAmountToLiquify = 5000000000000000000;
    IPancakeRouter02 public pancakeRouter;
    address public dipSwapPair;

    // In swap and liquify
    bool private _inSwapAndLiquify = false;

    event PancakeRouterUpdated(
        address indexed operator,
        address indexed router,
        address indexed pair
    );

    modifier lockTheSwap() {
        _inSwapAndLiquify = true;
        _;
        _inSwapAndLiquify = false;
    }

    modifier transferTaxFree() {
        uint16 _transferTaxRate = transferTaxRate;
        transferTaxRate = 0;
        _;
        transferTaxRate = _transferTaxRate;
    }

    /**
     * @notice Constructs the BaseToken contract.
     */
    constructor(
        string memory _name,
        string memory _symbol,
        address _mintAddress,
        uint256 _mintAmount,
        address _transferFeeAddress,
        uint16 _transferTaxRate,
        uint16 _burnRate
    ) public BEP20(_name, _symbol) {
        transferTaxRate = _transferTaxRate;
        burnRate = _burnRate;
        transferFeeAddress = _transferFeeAddress;

        _mint(_mintAddress, _mintAmount);
    }

    /// @notice Creates `_amount` token to `_to`. Must only be called by the owner (MasterChef).
    function mint(address _to, uint256 _amount) public onlyOwner {
        _mint(_to, _amount);
    }

    /// @dev overrides transfer function to meet tokenomics of the token
    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override {
        // swap and liquify
        if (
            _inSwapAndLiquify == false &&
            address(pancakeRouter) != address(0) &&
            dipSwapPair != address(0) &&
            sender != dipSwapPair &&
            sender != owner()
        ) {
            swapAndLiquify();
        }

        if (
            recipient == owner() ||
            sender == owner() ||
            recipient == BURN_ADDRESS ||
            transferTaxRate == 0
        ) {
            super._transfer(sender, recipient, amount);
        } else {
            uint256 taxAmount = (amount * transferTaxRate) / 10000;
            uint256 burnAmount = (taxAmount * burnRate) / 100;
            uint256 feeAmount = taxAmount - burnAmount;
            require(taxAmount == burnAmount + feeAmount, "Burn value invalid");

            uint256 sendAmount = amount - taxAmount;
            require(amount == sendAmount + taxAmount, "Tax value invalid");

            super._transfer(sender, BURN_ADDRESS, burnAmount);
            super._transfer(sender, address(this), feeAmount);
            super._transfer(sender, recipient, sendAmount);
        }
    }

    function setMasterchef(address _masterchef) public onlyOwner {
        transferFeeAddress = _masterchef;
        transferOwnership(_masterchef);
    }

    // @dev Swap and add to masterchef
    function swapAndLiquify() private lockTheSwap transferTaxFree {
        uint256 contractTokenBalance = balanceOf(address(this));
        if (contractTokenBalance >= minAmountToLiquify) {
            // capture the contract's current balance.
            // this is so that we can capture exactly the amount of BUSD that the
            // swap creates, and not make the liquidity event include any BUSD that
            // has been manually sent to the contract
            uint256 initialBalance = BUSD.balanceOf(
                address(transferFeeAddress)
            );

            swapTokensForBUSD(contractTokenBalance);

            uint256 newBalance = BUSD
                .balanceOf(address(transferFeeAddress))
                .sub(initialBalance);
        }
    }

    function swapTokensForBUSD(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = address(BUSD);

        _approve(address(this), address(pancakeRouter), tokenAmount);

        // make the swap
        pancakeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(transferFeeAddress),
            block.timestamp
        );
    }

    /**
     * @dev Update the swap router.
     * Can only be called by the current operator.
     */
    function updatePancakeRouter(address _router) public onlyOwner {
        pancakeRouter = IPancakeRouter02(_router);
        dipSwapPair = IPancakeFactory(pancakeRouter.factory()).getPair(
            address(this),
            address(BUSD)
        );
        require(dipSwapPair != address(0), "Invalid pair address.");
        emit PancakeRouterUpdated(
            msg.sender,
            address(pancakeRouter),
            dipSwapPair
        );
    }
}

