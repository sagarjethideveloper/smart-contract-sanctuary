pragma solidity ^0.8.0;
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./MooncakeAirdropTicket.sol";
import "./MooncakeToken.sol";

// This contract will start off owning the total supply of airdrop tickets
contract MooncakeAirdrop is Context, Ownable {
  using SafeMath for uint256;
  using Address for address;

  event TicketPurchase(
    address buyer
  );

  uint256 ticketPrice = 10**18;
  uint256 public ticketsSold = 0;
  mapping (uint256 => address) idToTicketHolder;
  mapping (address => bool) isTicketHolder;
  mapping (address => bool) whitelist;

  MooncakeAirdropTicket public ticket;
  MooncakeToken public token;

  bool eventActive = false;
  bool luckyWinnerSent = false;

  constructor () {
    ticket = new MooncakeAirdropTicket();
  }

  function purchaseTicket() public payable {
    require(eventActive, "Purchasing not enabled!");
    require(whitelist[_msgSender()], "Address not on whitelist!");
    require(!isTicketHolder[_msgSender()], "Address has already purchased a ticket!");
    isTicketHolder[_msgSender()] = true;
    require(msg.value == ticketPrice, "Payment not exactly 1 BNB!");
    require(ticket.balanceOf(address(this)) > 0, "Airdrop tickets are sold out!");
    ticket.transfer(_msgSender(), 1);
    ticketsSold = ticketsSold.add(1);
    idToTicketHolder[ticketsSold] = _msgSender();
    if (ticketsSold == 500) {
      eventActive = false;
    }
    emit TicketPurchase(_msgSender());
  }

  // this function will be batch called on the front end 500 times for each id
  function airdrop(uint256 ticketId, bool luckyWinner) public onlyOwner returns (bool) {
    uint256 transferAmount = 11000 * 10**18;

    // one lucky winner gets 5x more than everyone else
    if (luckyWinner && !luckyWinnerSent) {
      transferAmount = 66555 * 10**18;
      luckyWinnerSent = true;
    }
    token.transfer(idToTicketHolder[ticketId], transferAmount);
    return true;
  }

  // start purchase event
  function startEvent() public onlyOwner {
    eventActive = true;
  }

  // manually end purchase event in case of bug or emergency
  function endEvent() public onlyOwner {
    eventActive = false;
  }
  
  // give this contract the address of the mooncake token contract
  function setTokenAddress(address tokenAddress) public onlyOwner {
    token = MooncakeToken(tokenAddress);
  }

  // add address to whitelist
  function addToWhitelist(address buyer) public onlyOwner {
    whitelist[buyer] = true;
  }

  // get the ticket holder's address by id
  function getTicketHolderById(uint256 id) public view returns (address) {
    return idToTicketHolder[id];
  }

  // collect contract balance
  function collectBalance() public onlyOwner {
    address payable sender = payable(_msgSender());
    sender.transfer(getBalance());
  }

  // get balance of contract
  function getBalance() public view returns (uint) {
    return address(this).balance;
  }

  receive() external payable {
    purchaseTicket();
  }

}

