   * - `spender` cannot be the zero address.
   */
  function _approve(address owner, address spender, uint256 amount) internal {
    require(owner != address(0), "BEP20: approve from the zero address");
    require(spender != address(0), "BEP20: approve to the zero address");

    _allowances[owner][spender] = amount;
    emit Approval(owner, spender, amount);
  }
}

pragma solidity ^0.8.0;
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./IBEP20.sol";
import "./IBEP20.sol";
import "./IPancakeRouter.sol";
import "./IMasterChef.sol";
import "./RewardsToken.sol";

contract MooncakeOven is Context, Ownable {
  using SafeMath for uint256;
  using Address for address;

  // Percentage of rewards claimed to be re-staked, not to exceed 100
  // The difference to be sold for mooncake and burned
  uint8 public reStakingPercentage = 50;

  // Amount of rewards tokens already liquidated from the staking pool for the Mid Autumn Festival
  // To be exchanged for mooncake and burned over a predefined period of time
  uint256 public midAutumnFestivalTokensToBuybackAndBurnWith = 0;

  // Amount of tokens already exchanged for mooncake and burned
  uint256 public midAutumnFestivalAmountLiquidated;

  // Percentage of rewards tokens to unstake for the Mid Autumn Festival, not to exceed 100
  uint8 public midAutumnFestivalUnstakePercentage = 88;

  // Burn address - Euler's constant
  address burn = 0x0577215664901532860606512090082402431042;

  // Staking Pool contract - Starts off as the PCS MasterChef, can be interchanged with any other staking pool that generates more yield
  address public stakingPoolAddress;

  // MooncakeToken contract address
  address public mooncakeTokenAddress;

  // Rewards token contract - starts off as the $CAKE token contract address, can be modified based on preferred token
  address public rewardsTokenAddress;

  // Router contract address - for executing swaps + adding/removing liquidity
  address public routerAddress;

  constructor(address mooncakeToken) {
    // Set MooncakeToken contract address
    mooncakeTokenAddress = mooncakeToken;
  }

  /// @notice Gets the balance of Mooncake owned by this contract
  function getMooncakeBalance() public view returns (uint256) {
    return IBEP20(mooncakeTokenAddress).balanceOf(address(this));
  }

  /// @notice Gets the balance of Rewards tokens owned by this contract
  function getRewardsBalance() public view returns (uint256) {
    return IBEP20(rewardsTokenAddress).balanceOf(address(this));
  }

  /// @notice Set address of staking pool contract
  function setStakingPoolAddress(address pool) external onlyOwner {
    stakingPoolAddress = pool;
  }

  /// @notice Set address of mooncake token contract (in case of an upgraded Mooncake Token)
  function setMooncakeTokenAddress(address mooncakeToken) external onlyOwner {
    mooncakeTokenAddress = mooncakeToken;
  }

  /// @notice Set address of rewards token contract
  function setRewardsTokenAddress(address rewardsToken) external onlyOwner {
    rewardsTokenAddress = rewardsToken;
  }

  /// @notice Set address of router contract
  function setRouterAddress(address router) external onlyOwner {
    routerAddress = router;
  }

  /// @notice Changes the re-staking percentage
  /// @param percentage The new percentage
  function updateReStakingPercentage(uint8 percentage) external onlyOwner {
    reStakingPercentage = percentage;
  }

  /// @notice Changes the percentage of staked to kens to unstake for the Mid Autumn Festival
  /// @param percentage The new percentage
  function updateMidAutumnUnstakePercentage(uint8 percentage) external onlyOwner {
    midAutumnFestivalUnstakePercentage = percentage;
  }

  /// @notice Lower level swapping function, calls the router
  /// @param tokenA address of first token in path
  /// @param tokenB address of second token in path
  /// @param amountIn amount of token A to be swapped
  /// @param amountOut minimum amount of token B to receive
  function _swap(address tokenA, address tokenB, uint256 amountIn, uint256 amountOut) internal {
    address[] memory path = new address[](2);
    path[0] = tokenA;
    path[1] = tokenB;
    IPancakeRouter02(routerAddress).swapExactTokensForTokensSupportingFeeOnTransferTokens(
      amountIn,
      amountOut,
      path,
      address(this),
      block.timestamp + 1 minutes
    );
  }

  /// @notice Pump entire balance into staking pool
  function stakeRewardsTokenBalance() onlyOwner public {
    uint256 tokenBalance = IBEP20(rewardsTokenAddress).balanceOf(address(this));
    IBEP20(rewardsTokenAddress).approve(stakingPoolAddress, tokenBalance);
    IMasterChef(stakingPoolAddress).enterStaking(tokenBalance);
  }

  /// @notice Sells taxed Mooncake for Rewards tokens and stakes them in the pool
  /// @param amountOut Amount of rewards token expected to receive from the swap
  function sellTaxedMooncakeAndStakeRewards(uint256 amountOut) public {
    uint256 mooncakeBalance = getMooncakeBalance();
    
    // Approve Router for spending Mooncake
    IBEP20(mooncakeTokenAddress).approve(routerAddress, mooncakeBalance);

    uint256 initialRewardsTokenBalance = getRewardsBalance();
    _swap(mooncakeTokenAddress, rewardsTokenAddress, mooncakeBalance, amountOut);
    uint256 newRewardsTokenBalance = getRewardsBalance();

    uint256 amountToStake = newRewardsTokenBalance.sub(initialRewardsTokenBalance);

    // Approve staking contract for spending rewards token
    IBEP20(rewardsTokenAddress).approve(stakingPoolAddress, amountToStake);
    IMasterChef(stakingPoolAddress).enterStaking(amountToStake);
  }

  /// @notice Withdraws rewards, sells for mooncake and burns mooncake
  /// @param amountOut Amount of mooncake expected to receive from the swap - needs to be calculated externally due to potential arbitrage 
  function withdrawRewardsSellForMooncakeAndBurn(uint256 amountOut) onlyOwner public {
    uint256 pendingRewards = IMasterChef(stakingPoolAddress).pendingCake(0, address(this));
    uint256 restakeAmount = pendingRewards.mul(reStakingPercentage).div(100);
    uint256 sellAmount = pendingRewards.mul(uint(100).sub(reStakingPercentage)).div(100);
    IBEP20(rewardsTokenAddress).approve(stakingPoolAddress, restakeAmount);
    IMasterChef(stakingPoolAddress).enterStaking(restakeAmount);
    // approve Router for spending rewards token
    IBEP20(rewardsTokenAddress).approve(routerAddress, sellAmount);
    uint256 initialMooncakeBalance = IBEP20(mooncakeTokenAddress).balanceOf(address(this));

    _swap(rewardsTokenAddress, mooncakeTokenAddress, sellAmount, amountOut);
    uint256 newMooncakeBalance = IBEP20(mooncakeTokenAddress).balanceOf(address(this));
    IBEP20(mooncakeTokenAddress).transfer(burn, newMooncakeBalance.sub(initialMooncakeBalance));  
  }
  
  /// @notice Withdraws a percentage of the staked rewards token in preparation for the Mid Autumn Festival 
  function unStakeForMidAutumnFestival() onlyOwner public {
    // Put this in global scope so the contract knows how many tokens it's supposed to liquidate for the festival
    (uint256 amountStaked,) = IMasterChef(stakingPoolAddress).userInfo(0, address(this));
    midAutumnFestivalTokensToBuybackAndBurnWith = amountStaked.mul(midAutumnFestivalUnstakePercentage).div(100);
    IMasterChef(stakingPoolAddress).leaveStaking(midAutumnFestivalTokensToBuybackAndBurnWith);
  }

  /// @notice Buys back mooncake and burns it based on amount specified buy amount
  /// @param buyAmount amount of rewards token to buy with
  /// @param amountOut minimum amount of mooncake expected to receive from the swap
  function buyBackAndBurnForMidAutumnFestival(uint256 buyAmount, uint256 amountOut) onlyOwner public {
    require(buyAmount <= midAutumnFestivalTokensToBuybackAndBurnWith, "Amount must not exceed midAutumnFestivalTokensToBuybackAndBurnWith");
    midAutumnFestivalTokensToBuybackAndBurnWith = midAutumnFestivalTokensToBuybackAndBurnWith.sub(buyAmount);
    // Approve Router for spending rewards tokens
    IBEP20(rewardsTokenAddress).approve(routerAddress, buyAmount);
    uint256 initialMooncakeBalance = IBEP20(mooncakeTokenAddress).balanceOf(address(this));

    _swap(rewardsTokenAddress, mooncakeTokenAddress, buyAmount, amountOut);

    uint256 newMooncakeBalance = IBEP20(mooncakeTokenAddress).balanceOf(address(this));
    IBEP20(mooncakeTokenAddress).transfer(burn, newMooncakeBalance.sub(initialMooncakeBalance));    
  }

  /// @notice Withdraws all the cake from the syrup pool to the owner's address. Only use if there's a problem with this contract
  function emergencyWithdraw() external onlyOwner {
    (uint256 amountStaked,) = IMasterChef(stakingPoolAddress).userInfo(0, address(this));
    IMasterChef(stakingPoolAddress).leaveStaking(amountStaked);
    uint256 rewardsBalance = IBEP20(rewardsTokenAddress).balanceOf(address(this));
    IBEP20(rewardsTokenAddress).transfer(owner(), rewardsBalance);
  } 
}

