import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Pausable.sol";
import "@0x/contracts-utils/contracts/src/v06/LibBytesV06.sol";
import "./interfaces/IZerox.sol";

contract FloozRouterV2 is Ownable, Pausable {
    using LibBytesV06 for bytes;

    address public immutable zerox;

    event ReturnValues(bool success, bytes returnValue);

    constructor() public {
        zerox = address(0xDef1C0ded9bec7F1a1670819833240f027b25EfF);
    }

    function executeTrade(bytes memory _data) public payable {
        bytes4 selector = _data.readBytes4(0);
        address impl = IZerox(zerox).getFunctionImplementation(selector);
        (bool success, bytes memory returnedData) = impl.delegatecall(_data);
        emit ReturnValues(success, returnedData);
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }
}

