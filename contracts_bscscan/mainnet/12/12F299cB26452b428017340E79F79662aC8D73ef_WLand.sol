    event UpgradeLandFeesChanged(uint256 indexed newUpgradeFee);
    event MaxLevelChanged(uint256 indexed newMaxLevel);
    event RateForFloorPriceChanged(uint256 indexed newRateForFloorPrice);
    event BaseUriChanged(string tokenBaseURI);
