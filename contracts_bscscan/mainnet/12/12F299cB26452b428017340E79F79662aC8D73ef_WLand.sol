pragma solidity 0.8.0;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";

import "../commons/NftBaseContract.sol";
import "../commons/GameCurrency.sol";

import "../erc20/WaiToken.sol";
import "../interfaces/IWLand.sol";

import { Utils } from "../libs/utils.sol";

contract WLand is NftBaseContract, IWLand, GameCurrency, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using SafeCast for uint256;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenCounter;
    // mapping for land
    mapping(uint256 => Land) private _landByIds;
    uint256 private constant _MIN_DURATION = 5 minutes;

    // This stored all ReleaseVersion from game system
    ReleaseVersion[] public releases;
    // The rate (in percent) to get the salePrice from release price
    // rate calculation is based on 10000
    uint256 public rateForSalePriceInBps = 1000;
    // The map stored Land's name state
    mapping(string => bool) public landNameStorage;
    // The maxLevel of Wanaka Land
    uint256 public maxLevel = 100;
    uint256 public upgradeLandFee;

    /**
     * @dev Initialize this contract. Acts as a constructor
     * @param _exchangeCurrency - Address of the IERC20 accepted for this marketplace
     * @param _gameCurrency - Address of the IBEP20 accepted for this game
     * @param _owner - owner of this contract
     * @param _tokenName - Token name
     * @param _tokenSymbol - Token symbol
     */
    constructor(
        IERC20 _exchangeCurrency,
        WaiToken _gameCurrency,
        address _owner,
        string memory _tokenName,
        string memory _tokenSymbol
    ) NftBaseContract(_tokenName, _tokenSymbol) GameCurrency(_exchangeCurrency, _gameCurrency) {
        // set to admin group
        _addAdminUser(_owner);
        _addOperatorUser(_owner);
        // transfer owner
        transferOwnership(_owner);
    }

    modifier onlyWLandOwner(uint256 itemId) {
        require(ownerOf(itemId) == msg.sender, "Wanaka: not WLand owner");
        _;
    }

    modifier readyToTransfer(uint256 itemId) {
        Land storage land = _landByIds[itemId];
        require(land.id != 0, "Wanaka: Land does not available");
        require(land.inInventory, "Wanaka: Land must be in Inventory");
        _;
    }

    function getLandData(uint256 itemId)
        external
        view
        override
        returns (
            address ownerOfItem,
            uint256 salePrice,
            uint128 level,
            uint128 releaseVersionIndex,
            bool inInventory,
            string memory name
        )
    {
        Land memory land = _landByIds[itemId];
        if (land.id > 0) {
            ownerOfItem = ownerOf(itemId);
            salePrice = land.salePrice;
            name = land.name;
            level = land.level;
            inInventory = land.inInventory;
            releaseVersionIndex = land.releaseVersionIndex;
        }
    }

    function latestReleaseVersion() external view override returns (uint128, bool) {
        return _latestReleaseVersion();
    }

    function releaseVersionInfo(uint128 versionIndex)
        external
        view
        override
        returns (ReleaseVersion memory version, bool exists)
    {
        if (releases.length >= versionIndex + 1) {
            version = releases[versionIndex];
            exists = true;
        }
    }

    function totalSupply() external view returns (uint256) {
        return _tokenCounter.current();
    }

    function addNewRelease(
        uint128 maxSupply,
        uint256 price,
        uint256 startTime,
        uint256 endTime,
        Environment environment,
        string memory data
    ) external onlyAdmin {
        require(price > 0, "Wanaka: price must be greater than 0");
        require(startTime >= block.timestamp, "Wanaka: startTime should be in the future");
        require(
            endTime >= (startTime + _MIN_DURATION),
            "Wanaka: endTime should be more than 5 minutes in the startTime"
        );

        (uint128 latestReleaseIndex, bool isSet) = _latestReleaseVersion();
        if (isSet) {
            // ensure that all Lands is sold out before add a new version
            ReleaseVersion memory latestRelease = releases[latestReleaseIndex];
            if (latestRelease.currentSupply < latestRelease.maxSupply) {
                // raise event to updates version's info for the client
                emit NewReleaseExists(
                    latestReleaseIndex,
                    latestRelease.currentSupply,
                    latestRelease.maxSupply,
                    latestRelease.price,
                    latestRelease.startTime,
                    latestRelease.endTime,
                    latestRelease.environment,
                    data
                );
                return;
            }
        }

        ReleaseVersion memory releaseVersion = ReleaseVersion({
            currentSupply: 0,
            maxSupply: maxSupply,
            price: price,
            startTime: startTime,
            endTime: endTime,
            environment: environment,
            data: data
        });
        releases.push(releaseVersion);
        uint256 newIndex = releases.length - 1;

        emit NewReleaseAdded(
            newIndex.toUint128(),
            maxSupply,
            price,
            startTime,
            endTime,
            environment,
            data
        );
    }

    function updateLatestReleaseVersion(
        uint256 maxSupply,
        uint256 price,
        uint256 startTime,
        uint256 endTime,
        Environment environment,
        string memory data
    ) external onlyAdmin {
        (uint128 latestReleaseIndex, bool isSet) = _latestReleaseVersion();
        if (!isSet) return;
        // ensure that all Lands is sold out before add a new version
        ReleaseVersion storage latestRelease = releases[latestReleaseIndex];
        require(latestRelease.currentSupply == 0, "Wanaka: The version already started");
        latestRelease.price = price;
        latestRelease.startTime = startTime;
        latestRelease.endTime = endTime;
        latestRelease.maxSupply = maxSupply.toUint128();
        latestRelease.environment = environment;
        // raise event to updates version's info for the client
        emit NewReleaseExists(
            latestReleaseIndex,
            latestRelease.currentSupply,
            latestRelease.maxSupply,
            latestRelease.price,
            latestRelease.startTime,
            latestRelease.endTime,
            latestRelease.environment,
            data
        );
    }

    // sets the fee to upgrades Land fees
    function setUpgradeLandFees(uint256 _upgradeFee) external onlyAdmin {
        require(_upgradeFee > 0, "Wanaka: The upgrade fee must be greater than 0");
        upgradeLandFee = _upgradeFee;
        emit UpgradeLandFeesChanged(_upgradeFee);
    }

    // sets the maxlevel of Land
    function setMaxLevel(uint256 newMaxLevel) external onlyAdmin {
        require(newMaxLevel > maxLevel, "Wanaka: The newMaxLevel must be greater than current");
        maxLevel = newMaxLevel;
        emit MaxLevelChanged(newMaxLevel);
    }

    /**  
    sets the newRateForFloorPrice of Land
    rate calculation is based on 10000 so to set 15% the newPriceRate must be input 1500
    */
    function setRateForFloorPrice(uint256 newPriceRate) external onlyAdmin {
        require(newPriceRate <= 1500, "Wanaka: The newPriceRate must be <= 1500");
        rateForSalePriceInBps = newPriceRate;
        emit RateForFloorPriceChanged(newPriceRate);
    }

    // set baseUri, calls this function when the baseURI changed
    function setBaseUri(string memory tokenBaseURI) external override onlyAdmin {
        require(bytes(tokenBaseURI).length > 0, "Wanaka: Url is required");
        _baseUri = tokenBaseURI;
        emit BaseUriChanged(tokenBaseURI);
    }

    /**
     * @notice Upgrades for land
     * - @param account - the address of the player
     * - @param itemId - the id of the Land
     * - @param incrementValue - the value to increase the level of Land
     */
    function levelUp(
        address account,
        uint256 itemId,
        uint128 incrementValue
    ) external override onlyOperator {
        require(incrementValue > 0, "Wanaka: The increment value must be greater than 0");
        Land storage wland = _landByIds[itemId];
        require(wland.id != 0, "Wanaka: Land does not available");
        require(ownerOf(itemId) == account, "Wanaka: account does not own the Land");

        uint128 newLevel = wland.level + incrementValue;

        require(newLevel <= maxLevel, "Wanaka: The max level has reached");
        // burn the upgrade fee
        gameCurrency.burnFrom(account, upgradeLandFee);
        wland.level = newLevel;

        emit LandLeveledUp(itemId, newLevel - incrementValue, newLevel);
    }

    /**
     * @notice Updates a land's price.
     *
     * Requirements:
     * - caller must be own
     * @param itemId - the id of the Land
     * @param newPrice -  The new price of the land, must be greater than current price.
     */
    function updateBasePrice(uint256 itemId, uint256 newPrice)
        external
        override
        onlyWLandOwner(itemId)
    {
        address sender = _msgSender();
        Land storage wland = _landByIds[itemId];
        require(newPrice > wland.salePrice, "Wanaka: newPrice must be greater than current price");
        uint256 incPrice = newPrice - wland.salePrice;

        // plus the diff price to contract
        exchangeCurrency.safeTransferFrom(sender, address(this), incPrice);

        uint256 currentPrice = wland.salePrice;
        wland.salePrice = newPrice;

        emit LandSalePriceUpdated(itemId, currentPrice, newPrice);
    }

    /**
     * @notice Changes a land's price.
     *
     * Requirements:
     * - caller must be operator, this function will be call from some method
     * @param itemId - the id of the Land
     * - `newPrice` The new price will be change to the base price of the Land.
     */
    function increaseBasePrice(uint256 itemId, uint256 newPrice) external override onlyOperator {
        Land storage wland = _landByIds[itemId];
        require(
            newPrice >= wland.salePrice,
            "Wanaka: the new price must be greater than the current price"
        );
        uint256 currentPrice = wland.salePrice;
        wland.salePrice = newPrice;

        emit LandSalePriceUpdated(itemId, currentPrice, newPrice);
    }

    /**
     * @notice Moves land from / to the inventory.
     *
     * Requirements:
     * - caller must be owner of Land
     * - the "inInventory" field will be changed value to true or false
     * - @param itemId - the id of the Land
     */
    function changeInventoryState(uint256 itemId) external override onlyWLandOwner(itemId) {
        Land storage wland = _landByIds[itemId];
        wland.inInventory = !wland.inInventory;

        emit LandInventoryMoved(itemId, wland.inInventory);
    }

    /**
     * @notice Changes a land's name.
     *
     * Requirements:
     * - caller must be owner of Land
     * - `name` must be a valid string.
     * - `name` must be unique.
     * @param itemId - the id of the Land
     * @param name The name must be a valid [A-Z0-9]
     */
    function updateLandName(uint256 itemId, string memory name)
        external
        override
        onlyWLandOwner(itemId)
    {
        require(Utils._validateName(name), "Wanaka: The name must be a valid [A-Z0-9]");
        require(landNameStorage[name] == false, "Wanaka: The name is taken");

        address ownerOfItem = ownerOf(itemId);
        Land storage wland = _landByIds[itemId];
        // If already named, set name to inactive
        if (bytes(wland.name).length > 0) {
            landNameStorage[wland.name] = false;
        }
        wland.name = name;
        landNameStorage[name] = true;

        emit LandNameUpdated(ownerOfItem, itemId, name);
    }

    function deleteLand(uint256 itemId) external override onlyWLandOwner(itemId) nonReentrant {
        Land storage wland = _landByIds[itemId];
        require(wland.inInventory, "Wanaka: The Land must move to the inventory first");
        uint256 refund = wland.salePrice;
        if (refund > 0) {
            // Transfer share amount to NFT Own
            exchangeCurrency.safeTransfer(_msgSender(), refund);
        }
        _deleteItem(itemId);
    }

    /**
     * @notice Mints WLands when it's on presale phase.
     * - @param receiver - the receiver to mint
     * - @param quantity - the quantity to mint
     * - @param isReserved - the isReserved true: for marketing plan and no charge money
     */
    function mint(
        address receiver,
        uint128 quantity,
        bool isReserved
    ) external override onlyOperator nonReentrant {
        require(quantity > 0, "Wanaka: quantity should be greater than zero");
        (uint128 latestReleaseIndex, bool isExists) = _latestReleaseVersion();
        require(isExists, "Wanaka: There are not any release versions to claim");
        ReleaseVersion storage releaseVersion = releases[latestReleaseIndex];
        require(
            releaseVersion.startTime <= block.timestamp,
            "Wanaka: The version does not start yet"
        );
        require(
            releaseVersion.endTime >= block.timestamp || inOperatorGroup(receiver),
            "Wanaka: The Land sale has closed"
        );
        require(
            releaseVersion.currentSupply + quantity <= releaseVersion.maxSupply,
            "Wanaka: All of Lands has sold out"
        );
        // increase currentSupply
        releaseVersion.currentSupply += quantity;
        _createLands(receiver, quantity, isReserved, latestReleaseIndex, releaseVersion);
    }

    function _createLands(
        address receiver,
        uint256 quantity,
        bool isReserved,
        uint128 versionIndex,
        ReleaseVersion memory releaseVersion
    ) private {
        uint256 floorPrice = 0;
        if (!isReserved) {
            floorPrice = (releaseVersion.price * rateForSalePriceInBps) / BPS;
            uint256 totalPrice = quantity * releaseVersion.price;
            uint256 holdPrice = quantity * floorPrice;
            // Hold floor price of NFT to contract
            exchangeCurrency.safeTransferFrom(receiver, address(this), holdPrice);
            // Transfer fee to Owner
            exchangeCurrency.safeTransferFrom(receiver, owner(), totalPrice - holdPrice);
        }

        for (uint256 i = 0; i < quantity; i++) {
            uint256 itemId = _createLand(versionIndex, floorPrice);
            _safeMint(receiver, itemId);

            emit LandCreated(
                receiver,
                itemId,
                floorPrice,
                releaseVersion.environment,
                versionIndex
            );
        }
    }

    /**
     * @dev See {IERC721-transferFrom}.
     */
    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override readyToTransfer(tokenId) {
        super.transferFrom(from, to, tokenId);
    }

    /**
     * @dev See {IERC721-safeTransferFrom}.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override readyToTransfer(tokenId) {
        safeTransferFrom(from, to, tokenId, "");
    }

    /**
     * @dev See {IERC721-safeTransferFrom}.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) public virtual override readyToTransfer(tokenId) {
        super.safeTransferFrom(from, to, tokenId, _data);
    }

    function _createLand(uint128 releaseVersionIndex, uint256 salePrice) private returns (uint256) {
        _tokenCounter.increment();
        uint256 newTokenId = _tokenCounter.current();

        _landByIds[newTokenId] = Land({
            id: newTokenId,
            name: "Wanaka Land",
            salePrice: salePrice,
            level: 1,
            inInventory: true,
            releaseVersionIndex: releaseVersionIndex
        });

        return newTokenId;
    }

    function _deleteItem(uint256 itemId) private {
        address ownerAddr = ownerOf(itemId);

        _burn(itemId);
        delete _landByIds[itemId];

        emit LandDeleted(ownerAddr, itemId);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseUri;
    }

    function _latestReleaseVersion() private view returns (uint128, bool) {
        if (releases.length == 0) {
            return (0, false);
        }
        return ((releases.length - 1).toUint128(), true);
    }
}

