    ) public onlyOwner {
        isInitialized = true;
        totalContributedAmount = 0;

        PancakeRouter = IPancakeRouter02(_routerAddress);
        token = IERC20(_mnstrTokenAddress);
        nft = IBlockMonsters(_sailMinter);

        privateSaleRate = _privateSaleRate;
        preSaleRate = _preSaleRate;
        pancakeSwapRate = _pancakeSwapRate;
    }

    receive() external payable {}
    
    function activateClaim() public onlyOwner {
        require(isInitialized == true, "Sale: is not inizialized");
        require(canClaim == false, "claim is already activated");
        
        uint256 seventyPercent = totalContributedAmount.mul(700).div(1000);
        uint256 tokensLiquidity = seventyPercent.mul(pancakeSwapRate);

        token.approve(address(PancakeRouter), tokensLiquidity);
        PancakeRouter.addLiquidityETH{value: seventyPercent}(address(token), tokensLiquidity, 0, 0, address(0), block.timestamp);

        canClaim = true;
    }

    function loadPrivateSale(OrderPrivate[] memory orders) public onlyOwner {
        for(uint i = 0; i < orders.length; i++){
            address trainer = orders[i].trainer;
            uint256 amount = orders[i].amount;
            private_contributedAmountBNB[trainer] = amount;
            starterChoice[trainer] = orders[i].starterChoice;
            totalContributedAmount += amount;
        }
    }


    function loadPreSale(OrderPre[] memory orders) public onlyOwner {
        for(uint i = 0; i < orders.length; i++){
            address trainer = orders[i].trainer;
            uint256 amount = orders[i].amount;
            pre_contributedAmountBNB[trainer] = orders[i].amount;
            totalContributedAmount += amount;
        }
    }


    function claimPrivateSale() public {
        require(canClaim == true, "Sale: Claiming not yet activated");
        
        address blockMonTrainer = _msgSender();
        uint256 amount = private_contributedAmountBNB[blockMonTrainer];
        require( amount > 0, "MonsterPreSale: Nothing to claim");

        // reset contributed amount 
        private_contributedAmountBNB[blockMonTrainer] = 0;

        // transfer $MNSTRS
        uint256 amountTokensToTransfer = amount.mul(privateSaleRate);
        token.transfer(blockMonTrainer, amountTokensToTransfer);

        // mint starter
        nft.mintStarterBlockMon(blockMonTrainer, StarterChoice(starterChoice[blockMonTrainer]));
        emit StarterMint(blockMonTrainer);

        // mint privatesale special
        if ( amount >= 750000000000000000 ){
            nft.mintPrivateSaleSpecial(blockMonTrainer);
            emit PrivateSaleMint(blockMonTrainer);
        }
    }

    function claimPreSale() public {
        require(canClaim == true, "Sale: Claiming not yet activated");
        
        address blockMonTrainer = _msgSender();
        uint256 amount = pre_contributedAmountBNB[blockMonTrainer];
        require( amount > 0, "MonsterPreSale: Nothing to claim");

        // reset contributed amount 
        pre_contributedAmountBNB[blockMonTrainer] = 0;

        // transfer $MNSTRS
        uint256 amountTokensToTransfer = amount.mul(preSaleRate);
        token.transfer(blockMonTrainer, amountTokensToTransfer);

        // mint starter
        uint256 random = _random();
        nft.mintStarterBlockMon(blockMonTrainer, StarterChoice(random));
        emit MysteryMint(blockMonTrainer);

        // mint presale special
        if ( amount >= 750000000000000000 ){
            nft.mintPreSaleSpecial(blockMonTrainer);
            emit PreSaleMint(blockMonTrainer);
        }
    }

    function _random() private view returns (uint8) {
        // Because the weights are the same this random generator should be sufficient
        return uint8(uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty))) % 3);
    }

    function withdrawNative() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    function withdrawToken(address _token, uint256 amount) external onlyOwner {
        IERC20(_token).transfer(owner(), amount);
    }

    function setRouterAddress(address router) external onlyOwner {
        require(router != address(0), "MonsterPreSale: Cannot set Router to zero address");
        PancakeRouter = IPancakeRouter02(router);
    }

    function setMonsterTokenAddress(address monsterToken) external onlyOwner {
        token = IERC20(monsterToken);
    }

    function setMonsterNftAddress(address monsterNft) external onlyOwner {
        nft = IBlockMonsters(monsterNft);
    }
}

