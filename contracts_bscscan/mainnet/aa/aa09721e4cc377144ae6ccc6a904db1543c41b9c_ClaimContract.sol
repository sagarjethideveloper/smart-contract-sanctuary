import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Pausable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "../interfaces/IBlockMonsterAuthManager.sol";
import "../interfaces/IBlockMonsterNFT.sol";

// TODO implement IERC2981

import "hardhat/console.sol";

contract BlockMonsterNFT is
    ERC721,
    ERC721Enumerable,
    ERC721Pausable
{
    using Counters for Counters.Counter;

    IBlockMonsterAuthManager public _authManager;
    Counters.Counter private _tokenIdTracker;
    string _baseTokenURI;
    string _contractURI;

    bytes4 _interfaceHash;
    bytes32 public constant DEFAULT_ADMIN_ROLE = 0x00;

    modifier onlyPrivileged() {
        require(
            _authManager.hasPrivilegeForInterface(_interfaceHash, msg.sender),
            "BlockMonsterNFT: not privileged to call function"
        );
        _;
    }


    constructor(
        address authManager
    ) ERC721("BlockMonsters", "BLOCKMON"){
        // set authManager
        _authManager = IBlockMonsterAuthManager(authManager);

        // set interface hash
        _interfaceHash = type(IBlockMonsterNFT).interfaceId;
    }

    function initialize(
        string memory baseURI,
        string memory contractURI
    ) public {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "BlockMonsterFactory: sender is not DEFAULT_ADMIN_ROLE");

        _baseTokenURI = baseURI;
        _contractURI = contractURI;

        // allow MINTER_STARTER privilege to mint
        _authManager.registerInterfaceToPrivilege(
            _interfaceHash,
            keccak256("FACTORY_CONTRACT")
        );
    }

    event MintedBlockMonster( uint256 tokenId, address trainer );
    event BurnedBlockMonster( uint256 tokenId );

    function mintBlockMon(address to) external onlyPrivileged returns (uint256 tokenId) {
        tokenId = _tokenIdTracker.current();

        _safeMint(to, tokenId);
        _tokenIdTracker.increment();

        emit MintedBlockMonster( tokenId, to );

        return tokenId;
    }

    function burnBlockMon(uint256 tokenId) external onlyPrivileged{
        require(_exists(tokenId), "BlockMonsterNFT: BlockMonster with this tokenId does not exist");
        
        _burn( tokenId );

        emit BurnedBlockMonster( tokenId );
    }

    // ER271 FUNCTIONS ##############################################################
    function baseTokenURI() public view returns (string memory) {
        return _baseURI();
    }

    function contractURI() public view returns (string memory) {
        return _contractURI;
    }

    function _baseURI() internal view override returns ( string memory ){
        return _baseTokenURI;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721, ERC721Enumerable, ERC721Pausable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    // SETTINGS FUNCTIONS #############################################################
    function changeAuthManager( address newAuthManager ) public onlyPrivileged {
        _authManager = IBlockMonsterAuthManager(newAuthManager);
    }

    function changeBaseURI( string memory newBaseURI ) public onlyPrivileged {
        _baseTokenURI = newBaseURI;
    }
    function changeContractURI( string memory newContractURI ) public onlyPrivileged {
        _contractURI = newContractURI;
    }
}

