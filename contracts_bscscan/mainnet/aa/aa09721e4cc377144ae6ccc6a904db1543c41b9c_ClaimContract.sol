import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "../interfaces/IBlockMonsterAuthManager.sol";
import "../interfaces/IBlockMonsterFactory.sol";
import "../interfaces/IPancakePair.sol";
import "../interfaces/ISaleMinter.sol";
import "../libraries/StarterChoice.sol";

import "../interfaces/IBlockMonsters.sol";


contract SaleMinter is IBlockMonsters, Pausable {
    IBlockMonsterAuthManager _authManager;
    IBlockMonsterFactory _blockMonFactory;
    IERC20 _mnstrsToken;
    address _busdAddress;
    address _blockMonPool;
    address _wbnb_busdPool;
    uint256 _priceOfBlockMonInBUSD;

    bytes4 _interfaceHash;
    bytes32 public constant DEFAULT_ADMIN_ROLE = 0x00;

    mapping(address => uint256) public lastMinted;
    
    modifier onlyPrivileged() {
        require(
            _authManager.hasPrivilegeForInterface(_interfaceHash, msg.sender),
            "SaleMinter: not privileged to call function"
        );
        _;
    }

    event ClaimPrivateSaleSpecial( address trainer );
    event ClaimPreSaleSpecial( address trainer );
    event BuyStarterBlockMon( address trainer, uint64 monsterClassId );
    
    constructor (
        address authManager
    ) {
        _authManager = IBlockMonsterAuthManager(authManager);

        _interfaceHash = type(ISaleMinter).interfaceId;

        _pause();
    }


    function initialize(
        address mnstrsToken,
        address blockMonFactory,
        address blockMonPool,
        address wbnb_busdPool,
        address busdAddress,
        uint256 priceOfBlockMonInBUSD
    ) external {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "SaleMinter: sender is not DEFAULT_ADMIN_ROLE");

        _authManager.registerInterfaceToPrivilege(
            _interfaceHash,
            keccak256("SALE_CONTRACT")
        );
        _mnstrsToken = IERC20(mnstrsToken);
        _busdAddress = busdAddress;
        _blockMonFactory = IBlockMonsterFactory(blockMonFactory);
        _blockMonPool = blockMonPool;
        _wbnb_busdPool = wbnb_busdPool;
        _priceOfBlockMonInBUSD = priceOfBlockMonInBUSD;
    }

    function getStarterPrice() public view returns (uint256){
        uint112 reserveBUSD;
        uint112 reserveWBNB1;
        if (IPancakePair(_wbnb_busdPool).token0() == _busdAddress){
            (reserveBUSD, reserveWBNB1, ) = IPancakePair(_wbnb_busdPool).getReserves();
        } else {
            (reserveWBNB1, reserveBUSD, ) = IPancakePair(_wbnb_busdPool).getReserves();
        }

        uint112 reserveMNSTRS;
        uint112 reserveWBNB2;
        if (IPancakePair(_blockMonPool).token0() == address(_mnstrsToken)){
            (reserveMNSTRS, reserveWBNB1, ) = IPancakePair(_blockMonPool).getReserves();
        } else {
            (reserveWBNB2, reserveMNSTRS, ) = IPancakePair(_blockMonPool).getReserves();
        }

        return _priceOfBlockMonInBUSD * reserveWBNB1 * reserveMNSTRS / reserveBUSD / reserveWBNB2;
    }


    function buyStarterBlockMon() external whenNotPaused {
        address sender = msg.sender; // gas savings
        uint256 blockNr = block.number; // gas savings
        
        require(lastMinted[sender] < blockNr, "SaleMinter: Can only buy one BlockMonster per block");
        lastMinted[sender] = block.number;

        uint256 price = getStarterPrice();

        require( _mnstrsToken.balanceOf(sender) > price, "SaleMinter: Not enough $MNSTRS");
        require( _mnstrsToken.allowance(sender, address(this)) > price, "SaleMinter: Allowance of $MNSTRS Token too low");

        _mnstrsToken.approve(address(this), price);
        _mnstrsToken.transferFrom(sender, address(this), price);

        StarterChoice starter = StarterChoice(_random()); 
        
        _mintStarterBlockMon(sender, starter);
    }


    function mintStarterBlockMon(address to, StarterChoice starter) external override onlyPrivileged {
        _mintStarterBlockMon(to, starter);
    }


    function mintPrivateSaleSpecial(address to) external override onlyPrivileged {
        _blockMonFactory.mintBlockMon(
            to,
            34,         
            1,          
            1,          
            false,      
            0       
        );
        emit ClaimPrivateSaleSpecial( to );
    }

    function mintPreSaleSpecial(address to) external override onlyPrivileged {
        _blockMonFactory.mintBlockMon(
            to,
            18,         
            1,          
            1,          
            false,      
            0        
        );
        emit ClaimPreSaleSpecial( to );
    }

    function _mintStarterBlockMon(address to, StarterChoice starter) private { 
        uint64 monsterClassId;
        if ( starter == StarterChoice.Petaliz ) {
            monsterClassId = 7;
        } else if ( starter == StarterChoice.Orcalf) {
            monsterClassId = 4;
        } else if ( starter == StarterChoice.Flamisire) {
            monsterClassId = 1;
        }

        _blockMonFactory.mintBlockMon(
            to,
            monsterClassId,     
            1,                
            1,         
            false,      
            0          
        );

        emit BuyStarterBlockMon( to, monsterClassId );
    }

    function _random() private view returns (uint8) {
        return uint8(uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty))) % 3);
    }

    function pauseNFTSale() public onlyPrivileged {
        _pause();
    }

    function unpauseNFTSale() public onlyPrivileged {
        _unpause();
    }
    
    function changeAuthManager( address newAuthManager ) public {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "SaleMinter: sender is not DEFAULT_ADMIN_ROLE");
        _authManager = IBlockMonsterAuthManager(newAuthManager);
    }

    function changeStarterPrice( uint256 priceOfBlockMonInBUSD ) public {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "SaleMinter: sender is not DEFAULT_ADMIN_ROLE");
        _priceOfBlockMonInBUSD = priceOfBlockMonInBUSD;
    }

    function setupMinter(address to) external override {}

    function withdrawStuckERC20(address token, address to) external {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "SaleMinter: sender is not DEFAULT_ADMIN_ROLE");
        uint256 balance = IERC20(token).balanceOf(address(this));
        IERC20(token).transfer(to, balance);
    }

    function withdrawStuckBalance(address to) external {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "SaleMinter: sender is not DEFAULT_ADMIN_ROLE");
        payable(to).transfer(address(this).balance);
    }
}

