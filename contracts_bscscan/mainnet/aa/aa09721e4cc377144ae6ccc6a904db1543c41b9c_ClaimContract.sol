import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "../interfaces/IBlockMonsterAuthManager.sol";
import "../interfaces/IBlockMonsterDataManager.sol";
import "../interfaces/IBlockMonsterNFT.sol";
import "../interfaces/IBlockMonsterFactory.sol";
import "../libraries/Structs.sol";

contract BlockMonsterFactory {
    IBlockMonsterNFT public _blockMonNFT;

    IBlockMonsterAuthManager public _authManager;
    IBlockMonsterDataManager public _dataManager;

    bytes4 _interfaceHash;
    bytes32 public constant DEFAULT_ADMIN_ROLE = 0x00;
    
    modifier onlyPrivileged() {
        require(
            _authManager.hasPrivilegeForInterface(_interfaceHash, msg.sender),
            "BlockMonsterFactory: not privileged to call function"
        );
        _;
    }

    event CreateBlockMonInstance( BlockMonsterInstance instance );
    event BurnBlockMonInstance( BlockMonsterInstance instance );

    constructor(
        address authManager
    ) {
        _authManager = IBlockMonsterAuthManager(authManager);
        _interfaceHash = type(IBlockMonsterFactory).interfaceId;
    }
    
    
    function initialize(
        address dataManager,
        address blockMonNFT
    ) public {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "BlockMonsterFactory: sender is not DEFAULT_ADMIN_ROLE");

        _blockMonNFT = IBlockMonsterNFT(blockMonNFT);
        _dataManager = IBlockMonsterDataManager(dataManager);

        _authManager.registerInterfaceToPrivilege(
            _interfaceHash,
            keccak256("MINTER_CONTRACT")
        );
    }

    function mintBlockMon(
        address to,
        uint64 monsterClassId,
        uint16 level,
        uint32 exp,
        bool isShiny,
        uint32 specialMove
    ) external onlyPrivileged returns (uint256 monsterId){
        monsterId = _blockMonNFT.mintBlockMon(to);

        BlockMonsterClass memory monsterClass = _dataManager.getClass(monsterClassId);
        require(monsterClass.classId != 0, "BlockMonsterFactory: BlockMonster of this class is not initialized");

        monsterClass.total += 1;
        _dataManager.setClass(monsterClass);

        BlockMonsterInstance memory blockMonInstance = BlockMonsterInstance({
            monsterId:  monsterId,
            classId:    monsterClassId,
            trainer:    to,
            level:      level,
            exp:        exp,
            shiny:      isShiny,
            specialMove: specialMove,
            name:       "",
            timeOfCreation: block.timestamp,
            deathTimestamp: 0,
            mintIndex: monsterClass.total
        });

        _dataManager.setInstance( blockMonInstance );

        emit CreateBlockMonInstance( blockMonInstance );
    }

    function burnBlockMon(
        uint256 monsterId
    ) external onlyPrivileged {
        _blockMonNFT.burnBlockMon(monsterId);

        BlockMonsterInstance memory blockMonInstance = _dataManager.getInstance(monsterId);

        BlockMonsterClass memory monsterClass = _dataManager.getClass(blockMonInstance.classId);
        require(monsterClass.classId != 0, "BlockMonsterFactory: BlockMonster of this class is not initialized");

        monsterClass.total -= 1;
        _dataManager.setClass(monsterClass);

        _dataManager.removeInstance(monsterId);


        emit BurnBlockMonInstance( blockMonInstance );
    }


    function changeAuthManager( address newAuthManager ) public {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "BlockMonsterFactory: sender is not DEFAULT_ADMIN_ROLE");
        _authManager = IBlockMonsterAuthManager(newAuthManager);
    }

    function changeDataManager( address newDataManager ) public {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "BlockMonsterFactory: sender is not DEFAULT_ADMIN_ROLE");
        _dataManager = IBlockMonsterDataManager(newDataManager);
    }

    function withdrawStuckERC20(address token, address to) external {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "BlockMonsterFactory: sender is not DEFAULT_ADMIN_ROLE");
        uint256 balance = IERC20(token).balanceOf(address(this));
        IERC20(token).transfer(to, balance);
    }

    function withdrawStuckBalance(address to) external {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "BlockMonsterFactory: sender is not DEFAULT_ADMIN_ROLE");
        payable(to).transfer(address(this).balance);
    }
}

