enum BlockMonsterType {
    None,
    Normal,
    Fire,
    Water,
    Grass,
    Ghost,
    Rock,
    Electric,
    Cryptic,
    Flying,
    Dragon,
    Psychic,
    Fight
}

struct BlockMonsterClass {
    uint64 classId;
    uint64 total;

    BlockMonsterType type1;
    BlockMonsterType type2;
    uint32[] baseMoves;
    uint32 maxHealth;
    uint32 attack;
    uint32 speed;
    uint32 accuracy;
    uint32 defense;
    uint32 gas;

    bool catchable;

    uint256 basePrice;
}


