// SPDX-License-Identifier: UNLICENSED
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "../interfaces/IBlockMonsterAuthManager.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract BlockMonsterAuthManager is  IBlockMonsterAuthManager, AccessControlEnumerable, Ownable {    

    mapping(bytes4 => bytes32) _signaturePrivilegeMap;
    bytes32 public SYSTEM_CONTRACT_ROLE;
    bytes32 public SALE_CONTRACT_ROLE;
    bytes32 public MINTER_CONTRACT_ROLE;
    bytes32 public FACTORY_CONTRACT_ROLE;
    bytes32 public DATABASE_ADMIN_ROLE;

    bytes32 public GAME_MASTER_ROLE;

    bool initialized = false;

    modifier onlyAdmin() {
        _checkRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _;
    }
    modifier isNotInitialized(){
        require(!initialized, "AuthManager: is already initialized");
        _;
    }

    event RegisteredSignature(bytes4 signature, bytes32 privilege);
    event UnregisteredSignature(bytes4 signature, bytes32 privilege);
    event AssignedSystemContract(address account);
    event AssignedPrivilege(bytes32 privilege, address account);
    event RevokedPrivilege(bytes32 privilege, address account);
    event DeletedPrivilege(bytes32 privilege);


    function initialize() external onlyOwner isNotInitialized {
        initialized = true;

        SYSTEM_CONTRACT_ROLE = keccak256("SYSTEM_CONTRACT");
        SALE_CONTRACT_ROLE = keccak256("SALE_CONTRACT");
        MINTER_CONTRACT_ROLE = keccak256("MINTER_CONTRACT");
        FACTORY_CONTRACT_ROLE = keccak256("FACTORY_CONTRACT");
        DATABASE_ADMIN_ROLE = keccak256("DATABASE_ADMIN");

        GAME_MASTER_ROLE = keccak256("GAME_MASTER");

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());

        grantRole(SALE_CONTRACT_ROLE, _msgSender());
        grantRole(MINTER_CONTRACT_ROLE, _msgSender());
        grantRole(FACTORY_CONTRACT_ROLE, _msgSender());
        grantRole(DATABASE_ADMIN_ROLE, _msgSender());

        grantRole(GAME_MASTER_ROLE, _msgSender());
    }

    function hasPrivilege(bytes32 privilege, address account) external view override returns (bool) {
        return hasRole(privilege, account);
    }

    function hasPrivilegeForInterface(bytes4 funcSig, address account) external view override returns (bool) {
        bytes32 requiredPrivilege = _signaturePrivilegeMap[funcSig];
        require(requiredPrivilege != bytes32(0), "BlockMonsterAuthManager: interface not registered to privilege");
        return hasRole(requiredPrivilege, account);
    }

    function registerInterfaceToPrivilege(bytes4 funcSig, bytes32 privilege)
        external
        override
        onlyRole(SYSTEM_CONTRACT_ROLE)
    {
        _signaturePrivilegeMap[funcSig] = privilege;
        emit RegisteredSignature(funcSig, privilege);
    }

    function unregisterInterfaceFromPrivilege(bytes4 funcSig) external override onlyAdmin {
        bytes32 privilege = _signaturePrivilegeMap[funcSig];
        _signaturePrivilegeMap[funcSig] = bytes32(0);
        emit UnregisteredSignature(funcSig, privilege);
    }

    function assignSystemContract(address account) external override onlyAdmin {
        require(account != address(0), "BlockMonsterAuthManager: Cannot assign SYSTEM_CONTRACT role to address 0");
        grantRole(SYSTEM_CONTRACT_ROLE, account);
        emit AssignedSystemContract(account);
    }

    function assignPrivilege(bytes32 privilege, address account) external override onlyAdmin {
        grantRole(privilege, account);
        emit AssignedPrivilege(privilege, account);
    }

    function revokePrivilege(bytes32 privilege, address account) external onlyAdmin {
        revokeRole(privilege, account);
        emit RevokedPrivilege(privilege, account);
    }

    function deletePrivilege(bytes32 privilege) external onlyAdmin {
        require(privilege != DEFAULT_ADMIN_ROLE, "AuthManager: Can not delete DEFAULT_ADMIN_ROLE");
        uint256 roleMemberCount = getRoleMemberCount(privilege);
        uint256 deletedCounter = 0;

        for (uint256 index = 0; index < roleMemberCount; index++) {
            address member = getRoleMember(privilege, index);
            revokeRole(privilege, member);
            deletedCounter = deletedCounter + 1;
        }

        require(deletedCounter == roleMemberCount, "BlockMonsterAuthManager: error while trying to delete privilege");
        emit DeletedPrivilege(privilege);
    }
}

