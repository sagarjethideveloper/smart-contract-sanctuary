    uint256 monsterId;
    uint64 classId;
    address trainer;
    uint16 level;
    uint32 exp;
    bool shiny;
    uint32 specialMove;
    string name;
    uint256 timeOfCreation;
    uint256 deathTimestamp;
    uint256 mintIndex;
}


