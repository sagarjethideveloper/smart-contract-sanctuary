import "../interfaces/IBlockMonsterDataManager.sol";
import "../libraries/Structs.sol";
import "hardhat/console.sol";

abstract contract Database {
    mapping(uint256 => BlockMonsterClass) classTable;
    mapping(uint256 => BlockMonsterInstance) instanceTable;
    mapping(uint256 => BlockMonsterMove) moveTable;

    //event SetClassEvt(BlockMonsterClass class);
    //event SetInstanceEvt(BlockMonsterInstance instance);
    //event RemoveInstanceEvt(uint256 id);
    //event SetMoveEvt(BlockMonsterMove move);

    // Block Monster Classes #####################################
    function _getClass(uint256 id) internal view returns(BlockMonsterClass memory) {
        return classTable[id];
    }

    function _setClass(uint256 id, BlockMonsterClass memory item) internal {
        classTable[id] = item;
        //emit SetClassEvt(item);
    }

    // Block Monster Instances #####################################
    function _getInstance(uint256 id) internal view returns(BlockMonsterInstance memory) {
        return instanceTable[id];
    }

    function _setInstance(uint256 id, BlockMonsterInstance memory item) internal {
        instanceTable[id] = item;
        //emit SetInstanceEvt(item);
    }

    function _removeInstance(uint256 id) internal {
        delete instanceTable[id];
        //emit RemoveInstanceEvt(id);
    }


    // Block Monster Moves #####################################
    function _getMove(uint256 id) internal view returns(BlockMonsterMove memory) {
        return moveTable[id];
    }

    function _setMove(uint256 id, BlockMonsterMove memory item) internal {
        moveTable[id] = item;
        //emit SetMoveEvt(item);
    }
}

contract BlockMonsterDataManager is IBlockMonsterDataManager, Database {
    using SafeMath for uint256;

    IBlockMonsterAuthClient _authManager;
    IERC721Enumerable _blockMonsterNft;
    
    bytes4 _interfaceHash;
    bytes32 public constant DEFAULT_ADMIN_ROLE = 0x00;


    modifier onlyPrivileged() {
        require(
            _authManager.hasPrivilegeForInterface(_interfaceHash, msg.sender),
            "BlockMonsterNFT: not privileged to call function"
        );
        _;
    }

    constructor(
        address authManager
    ) {
        _authManager = IBlockMonsterAuthClient(authManager);

        _interfaceHash = type(IBlockMonsterDataManager).interfaceId;
    }

    function initialize(address nft) external override {
        require(_authManager.hasPrivilege( DEFAULT_ADMIN_ROLE, msg.sender), "BlockMonsterDataManager: sender is not DEFAULT_ADMIN_ROLE");

        _blockMonsterNft = IERC721Enumerable(nft);
        _authManager.registerInterfaceToPrivilege(
            _interfaceHash,
            keccak256("DATABASE_ADMIN")
        );
    }

    function kill() external override onlyPrivileged {
        selfdestruct(payable(msg.sender));
    }


    // Block Monster Instances #####################################
    function getInstance(uint256 tokenId) external view override returns (BlockMonsterInstance memory) {
        return _getInstance(tokenId);
    }

    function setInstance(BlockMonsterInstance calldata instance) external override onlyPrivileged {
        _setInstance(instance.monsterId, instance);
    }

    function removeInstance(uint256 tokenId) external override onlyPrivileged {
        // TODO check if we can even delet it
        _removeInstance(tokenId);
    }

    function getAllInstancesOfTrainer(address trainer) external override view returns (BlockMonsterInstance[] memory instances) {

        uint256 trainerBalance = _blockMonsterNft.balanceOf(trainer);
        instances = new BlockMonsterInstance[](trainerBalance);

        for(uint256 i = 0; i < trainerBalance; i++) {
            uint256 tokenId = _blockMonsterNft.tokenOfOwnerByIndex(trainer, i);
            instances[i] = _getInstance(tokenId);
        }
    }

    function loadInstances(BlockMonsterInstance[] calldata instances) external override onlyPrivileged {
        for (uint i = 0; i < instances.length; i++){
            _setInstance(instances[i].classId, instances[i]);
        }
    }


    // Block Monster Classes #####################################
    function setClass(BlockMonsterClass memory monsterClass) external override onlyPrivileged{
        _setClass(monsterClass.classId, monsterClass);
    }

    function getClass(uint256 classId) external override view returns (BlockMonsterClass memory){
        return _getClass(classId);
    }

    function loadClasses(BlockMonsterClass[] calldata classes) external override onlyPrivileged {
        for (uint i = 0; i < classes.length; i++){
            _setClass(classes[i].classId, classes[i]);
        }
    }


    // Block Monster Moves #####################################
    function setMove(BlockMonsterMove calldata move) external override onlyPrivileged {
        _setMove(move.moveId, move);
    }

    function getMove(uint256 moveId) external override view returns (BlockMonsterMove memory) {
        return _getMove(moveId);
    }

    function getMoves(uint256 tokenId) external override view returns (BlockMonsterMove[] memory moves) {
        moves = new BlockMonsterMove[](5);

        BlockMonsterInstance memory instance = _getInstance(tokenId);
        BlockMonsterClass memory class = _getClass(instance.classId);
        
        uint256 index;
        for (index = 0; index < class.baseMoves.length; index++) {
            BlockMonsterMove memory currentMove = _getMove(class.baseMoves[index]);
            moves[index] = currentMove;
        }

        // add special move
        if (instance.specialMove != 0){
            moves[4] = _getMove(instance.specialMove);
        }
    }

    function loadMoves(BlockMonsterMove[] calldata moves) external override onlyPrivileged {
        for (uint i = 0; i < moves.length; i++){
            _setMove(moves[i].moveId, moves[i]);
        }
    }
}

