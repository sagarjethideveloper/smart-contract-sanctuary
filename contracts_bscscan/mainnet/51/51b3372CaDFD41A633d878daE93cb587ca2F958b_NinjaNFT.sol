pragma solidity ^0.8.0;
pragma abicoder v2;

import "./NFTinterface.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./ERC721.sol";
import "./Utils.sol";

contract NinjaNFT is ERC721, NFTinterface{
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.UintSet;

    event PlaceOrder(uint256 indexed tokenId, address seller, uint256 price);
    event CancelOrder(uint256 indexed tokenId, address seller);
    
    event UpdatePrice(
        uint256 indexed tokenId,
        address seller,
        uint256 newPrice
    );
    
    event Gift(address _address, uint256 idNinja);
    event FillOrder(uint256 indexed tokenId, address seller);
    event CreateNinja(uint256 indexed tokenId, Ninja , address buyer);
    event PurchaseAmulets(bool isProtectionAmulets,uint amount,uint level);
    event UpgdareNinja(bool isSuccess, uint256 ninjaKeep,uint256 nijaRemove, uint256 rate);

    uint256 public latestTokenId;
    mapping(uint256 => bool) public isEvolved;

    mapping(uint256 => NinjaGame) internal ninjas;
    mapping(uint256 => ItemSale) internal markets;
    mapping(Ninja => uint256) timesLockFight;

    EnumerableSet.UintSet private tokenSales;
    mapping(address => EnumerableSet.UintSet) private sellerTokens;

    IERC20 public ninjaERC20;
    address private contractFight;

    constructor(string memory _name,string memory _symbol,address _manager,address _ninjaERC20
    ) ERC721(_name, _symbol, _manager) {
        ninjaERC20 = IERC20(_ninjaERC20);
    }
    
    modifier onlyPromise() {
        require(ownerContract==_msgSender() || contractFight==_msgSender()||manager.checkManager(msg.sender));
        _;
    }

    function _mint(address to, uint256 tokenId) internal override(ERC721) {
        super._mint(to, tokenId);
        _incrementTokenId();
    }
    
    function setContractFight(address _address) public{
        require(msg.sender==ownerContract);
        contractFight=_address;
    }

    function openNinja() external {
        (uint countGems,,,,)= manager.getPlayerOf(msg.sender);
        require(countGems>0," Not Gem");
        uint ninja =  random(0, 16);
       _createNinja(msg.sender, Ninja(ninja),1);
       manager.setGem(msg.sender,1,false);
    }

    function _createNinja(address receiver, Ninja ninja, uint256 strengthLevel) internal {
        uint256 nextTokenId = _getNextTokenId();
        _mint(receiver, nextTokenId);
        ninjas[nextTokenId] = NinjaGame({
            ninja:ninja,
            id:nextTokenId,
            owner: receiver,
            strengthLevel:strengthLevel,
            exp:0,
            timeLock:timesLockFight[ninja],
            timeLastFight:0
        });
        emit CreateNinja(nextTokenId, ninja , receiver);
    }

    function upgradeNinja(uint256 idSender, uint256 idSubNinja,uint idLuck,bool useProtection) public {
       require(ownerOf(idSender)==_msgSender()&&ownerOf(idSubNinja)==_msgSender());

        NinjaGame memory ninjaFrom= getNinja(idSender);
        NinjaGame memory ninjaTo= getNinja(idSubNinja);
        
        require(ninjaFrom.strengthLevel == ninjaTo.strengthLevel&&ninjaFrom.ninja == ninjaTo.ninja);
        require(ninjaFrom.strengthLevel<10, "Level max");
        uint256 balance=ninjaERC20.balanceOf(msg.sender);
        uint levelUpTo = (ninjaFrom.strengthLevel).add(1);
        uint256 fee = manager.getFeeUpgdare().mul(ninjaFrom.strengthLevel);
        require(balance>fee,"Not balance");
        ninjaERC20.transferFrom(msg.sender, manager.feeAddress(), fee);
        
        bool checknAmulets=false;
        (bool result,uint256 rate, bool isLuckycharms) = manager.getResultUpgrade(_msgSender(),levelUpTo, idLuck);
                uint256 tempSender=idSender;
        uint256 id= block.number.mul(1000) + random(1,10000);
        if(useProtection&&levelUpTo>=3){
            (,,,uint[8] memory protectionAmulets,) = manager.getPlayerOf(_msgSender());
            checknAmulets =  protectionAmulets[levelUpTo.sub(3)]>0; 
            if(checknAmulets)
                manager.setProtectionAmulets(msg.sender, levelUpTo.sub(3) , 1, false);
        }
        uint256 _ninjaRemove=0;
        uint256 temp=idSubNinja;
        if(result){
            ninjaTo.strengthLevel=levelUpTo;
            ninjaTo.exp=0;
            ninjas[temp]=ninjaTo;
            _burn(tempSender);
            _ninjaRemove=tempSender;
            emit UpgdareNinja(true,temp,_ninjaRemove,rate);
        } else {
            uint256 ninjaKeep = !checknAmulets ? (ninjaFrom.exp>ninjaTo.exp ? tempSender : temp) :0;
            if(!checknAmulets){
                 _ninjaRemove =  !checknAmulets ? (ninjaFrom.exp>ninjaTo.exp ? temp : tempSender) :0;
                _burn(_ninjaRemove);
            }
            emit UpgdareNinja(false, ninjaKeep, _ninjaRemove,rate);
        }
        uint lucky= isLuckycharms?idLuck:0;
        manager.addUpgrade(UpgradeObj({
                 id:id,
                 user:msg.sender,
                time: block.timestamp,
                luckyCharms:lucky,
                isProtectionAmulets:checknAmulets,
                ninjaSender:tempSender,
                ninjaSub:temp,
                strengthLevelUp:levelUpTo,
                 isSuccess:result,
                ninjaRemove:_ninjaRemove
        }));
    }
  
    function purchase(bool isGem, uint amount, bool isProtectionAmulets,uint level) public{
         (uint256 priceGem, uint256 priceLuckycharms,uint256 priceProtectionAmulets) = manager.getPrice();
         require(amount>0,"amount >0");
         if(isProtectionAmulets){
             require((level<=8 && level>=0));
         }
         uint256 price = isGem ? priceGem * amount : (isProtectionAmulets ?  (priceProtectionAmulets.mul(level.add(1))) * amount : priceLuckycharms * amount);
         uint256 tokenFight=0;
         if(contractFight!=address(0)){
             tokenFight= price.div(100).mul(manager.getRateSendFight());
             ninjaERC20.transferFrom(msg.sender,contractFight, tokenFight);
         }
         ninjaERC20.transferFrom(msg.sender, manager.feeAddress(), price.sub(tokenFight));
         if(isGem){
             manager.setGem(msg.sender,amount,true);
         } else {
             if(isProtectionAmulets){
                 manager.setProtectionAmulets(msg.sender, level, amount, true);
             } else {
                 manager.setLuckyPlus(msg.sender,0,amount,true);
             }
         } 
     }

    function getNinjasOf(address _addrees) public view returns (NinjaGame[] memory){
        uint256[] memory ids  = getTokensOf(_addrees);
        NinjaGame[] memory arr = new NinjaGame[](ids.length);
        for (uint256 index = 0; index < ids.length; index++) {
            arr[index]=ninjas[ids[index]];
        }
        return arr;
    }

