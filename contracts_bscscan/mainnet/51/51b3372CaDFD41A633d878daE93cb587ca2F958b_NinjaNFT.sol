        ninja.owner=_address;
        ninjas[idNinja]=ninja;
    }

    function random(uint from, uint to) private view   returns (uint) {
        uint nonce = 0;
        uint randomnumber = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce))) % to;
        randomnumber = randomnumber + from;
        nonce++;
        return randomnumber;
    }
